<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>BrandTemplates.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/BrandTemplate' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>BrandTemplates.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>BrandTemplates.BrandTemplate_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>BrandTemplates.BrandTemplate_Description}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>BrandTemplates.BrandTemplate_DeveloperName}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>BrandTemplates.BrandTemplate_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>BrandTemplates.BrandTemplate_IsActive}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>BrandTemplates.BrandTemplate_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>BrandTemplates.BrandTemplate_Name}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>BrandTemplates.BrandTemplate_NamespacePrefix}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>BrandTemplates.BrandTemplate_SystemModstamp}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>BrandTemplates.BrandTemplate_Value}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="BrandTemplate"/>
							<VBox>					
							<Text text="{dataModel>BrandTemplate_CreatedDate/BrandTemplate_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>BrandTemplate_Description/BrandTemplate_Description',type:'sap.ui.model.type.String'}" linkText="{i18n>BrandTemplate.BrandTemplate_Description/BrandTemplate_Description}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>BrandTemplate_DeveloperName/BrandTemplate_DeveloperName',type:'sap.ui.model.type.String'}" linkText="{i18n>BrandTemplate.BrandTemplate_DeveloperName/BrandTemplate_DeveloperName}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>BrandTemplate_Id/BrandTemplate_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>BrandTemplate.BrandTemplate_Id/BrandTemplate_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>BrandTemplate_IsActive/BrandTemplate_IsActive',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<Text text="{dataModel>BrandTemplate_LastModifiedDate/BrandTemplate_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>BrandTemplate_Name/BrandTemplate_Name',type:'sap.ui.model.type.String'}" linkText="{i18n>BrandTemplate.BrandTemplate_Name/BrandTemplate_Name}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>BrandTemplate_NamespacePrefix/BrandTemplate_NamespacePrefix',type:'sap.ui.model.type.String'}" linkText="{i18n>BrandTemplate.BrandTemplate_NamespacePrefix/BrandTemplate_NamespacePrefix}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>BrandTemplate_SystemModstamp/BrandTemplate_SystemModstamp}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>BrandTemplate_Value/BrandTemplate_Value',type:'sap.ui.model.type.String'}" linkText="{i18n>BrandTemplate.BrandTemplate_Value/BrandTemplate_Value}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>