					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexTestQueueItem.ApexTestQueueItem_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ApexTestQueueItem_Id/ApexTestQueueItem_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>ApexTestQueueItem.ApexTestQueueItem_Id/ApexTestQueueItem_Id}"/>
