						<rdf:ListResource path="dataModel>ApexTestQueueItem_ParentJobId" entitySet="AsyncApexJob"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexTestQueueItem.ApexTestQueueItem_TestRunResultId}" required="false"/> 
						<rdf:ListResource path="dataModel>ApexTestQueueItem_TestRunResultId" entitySet="ApexTestRunResult"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexTestQueueItem.ApexTestResult_QueueItemId_Inverse}" required="false"/> 
						<rdf:ListResource path="dataModel>ApexTestResult_QueueItemId_Inverse" entitySet="ApexTestResult"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>