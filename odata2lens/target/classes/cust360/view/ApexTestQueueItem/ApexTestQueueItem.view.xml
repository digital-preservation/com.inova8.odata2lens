				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexTestQueueItem.ApexTestQueueItem_Status}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ApexTestQueueItem_Status/ApexTestQueueItem_Status',type:'sap.ui.model.type.String' }" linkText="{i18n>ApexTestQueueItem.ApexTestQueueItem_Status/ApexTestQueueItem_Status}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexTestQueueItem.ApexTestQueueItem_SystemModstamp}" required="false"/> 
						<Text text=""/>
