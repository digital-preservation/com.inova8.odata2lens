						<Text text=""/>
						<rdf:ListResource path="dataModel>ApexTestResult_QueueItemId_Inverse/sfdc_ApexTestResult_QueueItemId_Inverse" entitySet="ApexTestResult"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexTestQueueItem.ApexTestQueueItem_ApexClassId}" required="false"/> 
						<rdf:ListResource path="dataModel>ApexTestQueueItem_ApexClassId" entitySet="ApexClass"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexTestQueueItem.ApexTestQueueItem_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>ApexTestQueueItem_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
