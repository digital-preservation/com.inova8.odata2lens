<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>UserRoles.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/UserRole' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>UserRoles.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserRoles.UserRole_CaseAccessForAccountOwner}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserRoles.UserRole_ContactAccessForAccountOwner}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserRoles.UserRole_DeveloperName}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserRoles.UserRole_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserRoles.UserRole_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserRoles.UserRole_MayForecastManagerShare}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserRoles.UserRole_Name}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserRoles.UserRole_OpportunityAccessForAccountOwner}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserRoles.UserRole_PortalType}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserRoles.UserRole_RollupDescription}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserRoles.UserRole_SystemModstamp}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="UserRole"/>
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>UserRole_CaseAccessForAccountOwner/UserRole_CaseAccessForAccountOwner',type:'sap.ui.model.type.String'}" linkText="{i18n>UserRole.UserRole_CaseAccessForAccountOwner/UserRole_CaseAccessForAccountOwner}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>UserRole_ContactAccessForAccountOwner/UserRole_ContactAccessForAccountOwner',type:'sap.ui.model.type.String'}" linkText="{i18n>UserRole.UserRole_ContactAccessForAccountOwner/UserRole_ContactAccessForAccountOwner}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>UserRole_DeveloperName/UserRole_DeveloperName',type:'sap.ui.model.type.String'}" linkText="{i18n>UserRole.UserRole_DeveloperName/UserRole_DeveloperName}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>UserRole_Id/UserRole_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>UserRole.UserRole_Id/UserRole_Id}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>UserRole_LastModifiedDate/UserRole_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>UserRole_MayForecastManagerShare/UserRole_MayForecastManagerShare',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>UserRole_Name/UserRole_Name',type:'sap.ui.model.type.String'}" linkText="{i18n>UserRole.UserRole_Name/UserRole_Name}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>UserRole_OpportunityAccessForAccountOwner/UserRole_OpportunityAccessForAccountOwner',type:'sap.ui.model.type.String'}" linkText="{i18n>UserRole.UserRole_OpportunityAccessForAccountOwner/UserRole_OpportunityAccessForAccountOwner}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>UserRole_PortalType/UserRole_PortalType',type:'sap.ui.model.type.String'}" linkText="{i18n>UserRole.UserRole_PortalType/UserRole_PortalType}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>UserRole_RollupDescription/UserRole_RollupDescription',type:'sap.ui.model.type.String'}" linkText="{i18n>UserRole.UserRole_RollupDescription/UserRole_RollupDescription}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>UserRole_SystemModstamp/UserRole_SystemModstamp}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>