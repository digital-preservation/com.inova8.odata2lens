					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ProcessDefinition.ProcessDefinition_LockType}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ProcessDefinition_LockType/ProcessDefinition_LockType',type:'sap.ui.model.type.String' }" linkText="{i18n>ProcessDefinition.ProcessDefinition_LockType/ProcessDefinition_LockType}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ProcessDefinition.ProcessDefinition_Name}" required="false"/> 
						<Text text=""/>
