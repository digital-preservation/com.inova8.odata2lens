					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ProcessDefinition.ProcessDefinition_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>ProcessDefinition_SystemModstamp/ProcessDefinition_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ProcessDefinition.ProcessDefinition_TableEnumOrId}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ProcessDefinition_TableEnumOrId/ProcessDefinition_TableEnumOrId',type:'sap.ui.model.type.String' }" linkText="{i18n>ProcessDefinition.ProcessDefinition_TableEnumOrId/ProcessDefinition_TableEnumOrId}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ProcessDefinition.ProcessDefinition_Type}" required="false"/> 
