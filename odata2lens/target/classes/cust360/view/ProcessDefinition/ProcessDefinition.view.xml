					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ProcessDefinition.ProcessInstance_ProcessDefinitionId_Inverse}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ProcessInstance_ProcessDefinitionId_Inverse/sfdc_ProcessInstance_ProcessDefinitionId_Inverse" entitySet="ProcessInstance"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ProcessDefinition.ProcessNode_ProcessDefinitionId_Inverse}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ProcessNode_ProcessDefinitionId_Inverse/sfdc_ProcessNode_ProcessDefinitionId_Inverse" entitySet="ProcessNode"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ProcessDefinition.ProcessDefinition_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>ProcessDefinition_CreatedById" entitySet="User"/>
