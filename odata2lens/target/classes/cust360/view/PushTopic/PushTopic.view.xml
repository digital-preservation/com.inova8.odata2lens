<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>PushTopic.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>PushTopic.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="PushTopic"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="PushTopic"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>PushTopic.PushTopic_ApiVersion}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>PushTopic_ApiVersion/PushTopic_ApiVersion',type:'sap.ui.model.type.Float', formatOptions:{'style':'short','maxFractionDigits':2} }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>PushTopic.PushTopic_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>PushTopic_CreatedById/sfdc_PushTopic_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>PushTopic.PushTopic_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>PushTopic_CreatedDate/PushTopic_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>PushTopic.PushTopic_Description}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>PushTopic_Description/PushTopic_Description',type:'sap.ui.model.type.String' }" linkText="{i18n>PushTopic.PushTopic_Description/PushTopic_Description}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>PushTopic.PushTopic_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>PushTopic_Id/PushTopic_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>PushTopic.PushTopic_Id/PushTopic_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>PushTopic.PushTopic_IsActive}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>PushTopic_IsActive/PushTopic_IsActive',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>PushTopic.PushTopic_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>PushTopic_IsDeleted/PushTopic_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>PushTopic.PushTopic_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>PushTopic_LastModifiedById/sfdc_PushTopic_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>PushTopic.PushTopic_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>PushTopic_LastModifiedDate/PushTopic_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>PushTopic.PushTopic_Name}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>PushTopic_Name/PushTopic_Name',type:'sap.ui.model.type.String' }" linkText="{i18n>PushTopic.PushTopic_Name/PushTopic_Name}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>PushTopic.PushTopic_NotifyForFields}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>PushTopic_NotifyForFields/PushTopic_NotifyForFields',type:'sap.ui.model.type.String' }" linkText="{i18n>PushTopic.PushTopic_NotifyForFields/PushTopic_NotifyForFields}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>PushTopic.PushTopic_NotifyForOperationCreate}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>PushTopic_NotifyForOperationCreate/PushTopic_NotifyForOperationCreate',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>PushTopic.PushTopic_NotifyForOperationDelete}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>PushTopic_NotifyForOperationDelete/PushTopic_NotifyForOperationDelete',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>PushTopic.PushTopic_NotifyForOperationUndelete}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>PushTopic_NotifyForOperationUndelete/PushTopic_NotifyForOperationUndelete',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>PushTopic.PushTopic_NotifyForOperationUpdate}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>PushTopic_NotifyForOperationUpdate/PushTopic_NotifyForOperationUpdate',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>PushTopic.PushTopic_NotifyForOperations}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>PushTopic_NotifyForOperations/PushTopic_NotifyForOperations',type:'sap.ui.model.type.String' }" linkText="{i18n>PushTopic.PushTopic_NotifyForOperations/PushTopic_NotifyForOperations}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>PushTopic.PushTopic_Query}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>PushTopic_Query/PushTopic_Query',type:'sap.ui.model.type.String' }" linkText="{i18n>PushTopic.PushTopic_Query/PushTopic_Query}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>PushTopic.PushTopic_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>PushTopic_SystemModstamp/PushTopic_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>PushTopic.PushTopic_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>PushTopic_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>PushTopic.PushTopic_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>PushTopic_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>