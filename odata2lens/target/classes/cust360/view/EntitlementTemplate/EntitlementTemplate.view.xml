<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>EntitlementTemplate.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>EntitlementTemplate.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="EntitlementTemplate"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="EntitlementTemplate"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EntitlementTemplate.EntitlementTemplate_BusinessHoursId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>EntitlementTemplate_BusinessHoursId/sfdc_EntitlementTemplate_BusinessHoursId" entitySet="BusinessHours"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EntitlementTemplate.EntitlementTemplate_CasesPerEntitlement}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>EntitlementTemplate_CasesPerEntitlement/EntitlementTemplate_CasesPerEntitlement',type:'sap.ui.model.type.Integer' }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EntitlementTemplate.EntitlementTemplate_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>EntitlementTemplate_CreatedById/sfdc_EntitlementTemplate_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EntitlementTemplate.EntitlementTemplate_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>EntitlementTemplate_CreatedDate/EntitlementTemplate_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EntitlementTemplate.EntitlementTemplate_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>EntitlementTemplate_Id/EntitlementTemplate_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>EntitlementTemplate.EntitlementTemplate_Id/EntitlementTemplate_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EntitlementTemplate.EntitlementTemplate_IsPerIncident}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>EntitlementTemplate_IsPerIncident/EntitlementTemplate_IsPerIncident',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EntitlementTemplate.EntitlementTemplate_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>EntitlementTemplate_LastModifiedById/sfdc_EntitlementTemplate_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EntitlementTemplate.EntitlementTemplate_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>EntitlementTemplate_LastModifiedDate/EntitlementTemplate_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EntitlementTemplate.EntitlementTemplate_Name}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>EntitlementTemplate_Name/EntitlementTemplate_Name',type:'sap.ui.model.type.String' }" linkText="{i18n>EntitlementTemplate.EntitlementTemplate_Name/EntitlementTemplate_Name}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EntitlementTemplate.EntitlementTemplate_NamespacePrefix}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>EntitlementTemplate_NamespacePrefix/EntitlementTemplate_NamespacePrefix',type:'sap.ui.model.type.String' }" linkText="{i18n>EntitlementTemplate.EntitlementTemplate_NamespacePrefix/EntitlementTemplate_NamespacePrefix}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EntitlementTemplate.EntitlementTemplate_SlaProcessId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>EntitlementTemplate_SlaProcessId/sfdc_EntitlementTemplate_SlaProcessId" entitySet="SlaProcess"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EntitlementTemplate.EntitlementTemplate_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>EntitlementTemplate_SystemModstamp/EntitlementTemplate_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EntitlementTemplate.EntitlementTemplate_Term}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>EntitlementTemplate_Term/EntitlementTemplate_Term',type:'sap.ui.model.type.Integer' }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EntitlementTemplate.EntitlementTemplate_Type}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>EntitlementTemplate_Type/EntitlementTemplate_Type',type:'sap.ui.model.type.String' }" linkText="{i18n>EntitlementTemplate.EntitlementTemplate_Type/EntitlementTemplate_Type}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EntitlementTemplate.ProductEntitlementTemplate_EntitlementTemplateId_Inverse}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ProductEntitlementTemplate_EntitlementTemplateId_Inverse/sfdc_ProductEntitlementTemplate_EntitlementTemplateId_Inverse" entitySet="ProductEntitlementTemplate"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EntitlementTemplate.EntitlementTemplate_BusinessHoursId}" required="false"/> 
						<rdf:ListResource path="dataModel>EntitlementTemplate_BusinessHoursId" entitySet="BusinessHours"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EntitlementTemplate.EntitlementTemplate_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>EntitlementTemplate_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EntitlementTemplate.EntitlementTemplate_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>EntitlementTemplate_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EntitlementTemplate.EntitlementTemplate_SlaProcessId}" required="false"/> 
						<rdf:ListResource path="dataModel>EntitlementTemplate_SlaProcessId" entitySet="SlaProcess"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EntitlementTemplate.ProductEntitlementTemplate_EntitlementTemplateId_Inverse}" required="false"/> 
						<rdf:ListResource path="dataModel>ProductEntitlementTemplate_EntitlementTemplateId_Inverse" entitySet="ProductEntitlementTemplate"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>