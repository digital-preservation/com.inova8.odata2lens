<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>Folder.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>Folder.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="Folder"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="Folder"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Folder.Dashboard_FolderId_Inverse}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>Dashboard_FolderId_Inverse/sfdc_Dashboard_FolderId_Inverse" entitySet="Dashboard"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Folder.Document_FolderId_Inverse}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>Document_FolderId_Inverse/sfdc_Document_FolderId_Inverse" entitySet="Document"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Folder.EmailTemplate_FolderId_Inverse}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>EmailTemplate_FolderId_Inverse/sfdc_EmailTemplate_FolderId_Inverse" entitySet="EmailTemplate"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Folder.Folder_AccessType}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>Folder_AccessType/Folder_AccessType',type:'sap.ui.model.type.String' }" linkText="{i18n>Folder.Folder_AccessType/Folder_AccessType}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Folder.Folder_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>Folder_CreatedById/sfdc_Folder_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Folder.Folder_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>Folder_CreatedDate/Folder_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Folder.Folder_DeveloperName}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>Folder_DeveloperName/Folder_DeveloperName',type:'sap.ui.model.type.String' }" linkText="{i18n>Folder.Folder_DeveloperName/Folder_DeveloperName}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Folder.Folder_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>Folder_Id/Folder_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>Folder.Folder_Id/Folder_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Folder.Folder_IsReadonly}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>Folder_IsReadonly/Folder_IsReadonly',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Folder.Folder_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>Folder_LastModifiedById/sfdc_Folder_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Folder.Folder_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>Folder_LastModifiedDate/Folder_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Folder.Folder_Name}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>Folder_Name/Folder_Name',type:'sap.ui.model.type.String' }" linkText="{i18n>Folder.Folder_Name/Folder_Name}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Folder.Folder_NamespacePrefix}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>Folder_NamespacePrefix/Folder_NamespacePrefix',type:'sap.ui.model.type.String' }" linkText="{i18n>Folder.Folder_NamespacePrefix/Folder_NamespacePrefix}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Folder.Folder_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>Folder_SystemModstamp/Folder_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Folder.Folder_Type}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>Folder_Type/Folder_Type',type:'sap.ui.model.type.String' }" linkText="{i18n>Folder.Folder_Type/Folder_Type}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Folder.Report_OwnerId_Inverse}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>Report_OwnerId_Inverse/sfdc_Report_OwnerId_Inverse" entitySet="Report"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Folder.Dashboard_FolderId_Inverse}" required="false"/> 
						<rdf:ListResource path="dataModel>Dashboard_FolderId_Inverse" entitySet="Dashboard"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Folder.Document_FolderId_Inverse}" required="false"/> 
						<rdf:ListResource path="dataModel>Document_FolderId_Inverse" entitySet="Document"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Folder.EmailTemplate_FolderId_Inverse}" required="false"/> 
						<rdf:ListResource path="dataModel>EmailTemplate_FolderId_Inverse" entitySet="EmailTemplate"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Folder.Folder_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>Folder_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Folder.Folder_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>Folder_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Folder.Report_OwnerId_Inverse}" required="false"/> 
						<rdf:ListResource path="dataModel>Report_OwnerId_Inverse" entitySet="Report"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>