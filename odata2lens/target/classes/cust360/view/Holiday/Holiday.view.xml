						<Text text=""/>
						<Text text="{path:'dataModel>Holiday_RecurrenceDayOfMonth/Holiday_RecurrenceDayOfMonth',type:'sap.ui.model.type.Integer' }"/>
					</cells>
				</ColumnListItem>
