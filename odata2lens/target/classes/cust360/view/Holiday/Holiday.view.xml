						<rdf:ListResource path="dataModel>Holiday_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Holiday.Holiday_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>Holiday_LastModifiedById" entitySet="User"/>
						<Text text=""/>
