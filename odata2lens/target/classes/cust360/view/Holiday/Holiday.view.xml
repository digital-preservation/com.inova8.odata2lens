				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="Holiday"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Holiday.Holiday_ActivityDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>Holiday_ActivityDate/Holiday_ActivityDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Holiday.Holiday_CreatedById}" required="false"/> 
						<Text text=""/>
