<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>ChatterExtensionLocalization.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>ChatterExtensionLocalization.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="ChatterExtensionLocalization"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="ChatterExtensionLocalization"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ChatterExtensionLocalization.ChatterExtensionLocalization_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ChatterExtensionLocalization_CreatedById/sfdc_ChatterExtensionLocalization_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ChatterExtensionLocalization.ChatterExtensionLocalization_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>ChatterExtensionLocalization_CreatedDate/ChatterExtensionLocalization_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ChatterExtensionLocalization.ChatterExtensionLocalization_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ChatterExtensionLocalization_Id/ChatterExtensionLocalization_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>ChatterExtensionLocalization.ChatterExtensionLocalization_Id/ChatterExtensionLocalization_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ChatterExtensionLocalization.ChatterExtensionLocalization_Language}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ChatterExtensionLocalization_Language/ChatterExtensionLocalization_Language',type:'sap.ui.model.type.String' }" linkText="{i18n>ChatterExtensionLocalization.ChatterExtensionLocalization_Language/ChatterExtensionLocalization_Language}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ChatterExtensionLocalization.ChatterExtensionLocalization_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ChatterExtensionLocalization_LastModifiedById/sfdc_ChatterExtensionLocalization_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ChatterExtensionLocalization.ChatterExtensionLocalization_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>ChatterExtensionLocalization_LastModifiedDate/ChatterExtensionLocalization_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ChatterExtensionLocalization.ChatterExtensionLocalization_NamespacePrefix}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ChatterExtensionLocalization_NamespacePrefix/ChatterExtensionLocalization_NamespacePrefix',type:'sap.ui.model.type.String' }" linkText="{i18n>ChatterExtensionLocalization.ChatterExtensionLocalization_NamespacePrefix/ChatterExtensionLocalization_NamespacePrefix}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ChatterExtensionLocalization.ChatterExtensionLocalization_ParentId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ChatterExtensionLocalization_ParentId/sfdc_ChatterExtensionLocalization_ParentId" entitySet="ChatterExtension"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ChatterExtensionLocalization.ChatterExtensionLocalization_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>ChatterExtensionLocalization_SystemModstamp/ChatterExtensionLocalization_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ChatterExtensionLocalization.ChatterExtensionLocalization_Value}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ChatterExtensionLocalization_Value/ChatterExtensionLocalization_Value',type:'sap.ui.model.type.String' }" linkText="{i18n>ChatterExtensionLocalization.ChatterExtensionLocalization_Value/ChatterExtensionLocalization_Value}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ChatterExtensionLocalization.ChatterExtensionLocalization_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>ChatterExtensionLocalization_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ChatterExtensionLocalization.ChatterExtensionLocalization_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>ChatterExtensionLocalization_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ChatterExtensionLocalization.ChatterExtensionLocalization_ParentId}" required="false"/> 
						<rdf:ListResource path="dataModel>ChatterExtensionLocalization_ParentId" entitySet="ChatterExtension"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>