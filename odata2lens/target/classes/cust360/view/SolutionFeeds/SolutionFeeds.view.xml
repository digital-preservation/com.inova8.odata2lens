<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>SolutionFeeds.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/SolutionFeed' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>SolutionFeeds.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>SolutionFeeds.SolutionFeed_Body}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>SolutionFeeds.SolutionFeed_CommentCount}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>SolutionFeeds.SolutionFeed_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>SolutionFeeds.SolutionFeed_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>SolutionFeeds.SolutionFeed_IsDeleted}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>SolutionFeeds.SolutionFeed_IsRichText}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>SolutionFeeds.SolutionFeed_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>SolutionFeeds.SolutionFeed_LikeCount}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>SolutionFeeds.SolutionFeed_LinkUrl}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>SolutionFeeds.SolutionFeed_SystemModstamp}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>SolutionFeeds.SolutionFeed_Title}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>SolutionFeeds.SolutionFeed_Type}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="SolutionFeed"/>
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>SolutionFeed_Body/SolutionFeed_Body',type:'sap.ui.model.type.String'}" linkText="{i18n>SolutionFeed.SolutionFeed_Body/SolutionFeed_Body}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>SolutionFeed_CommentCount/SolutionFeed_CommentCount',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>SolutionFeed_CreatedDate/SolutionFeed_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>SolutionFeed_Id/SolutionFeed_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>SolutionFeed.SolutionFeed_Id/SolutionFeed_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>SolutionFeed_IsDeleted/SolutionFeed_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>SolutionFeed_IsRichText/SolutionFeed_IsRichText',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<Text text="{dataModel>SolutionFeed_LastModifiedDate/SolutionFeed_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>SolutionFeed_LikeCount/SolutionFeed_LikeCount',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>SolutionFeed_LinkUrl/SolutionFeed_LinkUrl',type:'sap.ui.model.type.String'}" linkText="{i18n>SolutionFeed.SolutionFeed_LinkUrl/SolutionFeed_LinkUrl}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>SolutionFeed_SystemModstamp/SolutionFeed_SystemModstamp}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>SolutionFeed_Title/SolutionFeed_Title',type:'sap.ui.model.type.String'}" linkText="{i18n>SolutionFeed.SolutionFeed_Title/SolutionFeed_Title}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>SolutionFeed_Type/SolutionFeed_Type',type:'sap.ui.model.type.String'}" linkText="{i18n>SolutionFeed.SolutionFeed_Type/SolutionFeed_Type}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>