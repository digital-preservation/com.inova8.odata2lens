<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>BrandTemplate.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>BrandTemplate.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="BrandTemplate"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="BrandTemplate"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>BrandTemplate.BrandTemplate_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>BrandTemplate_CreatedById/sfdc_BrandTemplate_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>BrandTemplate.BrandTemplate_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>BrandTemplate_CreatedDate/BrandTemplate_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>BrandTemplate.BrandTemplate_Description}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>BrandTemplate_Description/BrandTemplate_Description',type:'sap.ui.model.type.String' }" linkText="{i18n>BrandTemplate.BrandTemplate_Description/BrandTemplate_Description}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>BrandTemplate.BrandTemplate_DeveloperName}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>BrandTemplate_DeveloperName/BrandTemplate_DeveloperName',type:'sap.ui.model.type.String' }" linkText="{i18n>BrandTemplate.BrandTemplate_DeveloperName/BrandTemplate_DeveloperName}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>BrandTemplate.BrandTemplate_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>BrandTemplate_Id/BrandTemplate_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>BrandTemplate.BrandTemplate_Id/BrandTemplate_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>BrandTemplate.BrandTemplate_IsActive}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>BrandTemplate_IsActive/BrandTemplate_IsActive',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>BrandTemplate.BrandTemplate_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>BrandTemplate_LastModifiedById/sfdc_BrandTemplate_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>BrandTemplate.BrandTemplate_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>BrandTemplate_LastModifiedDate/BrandTemplate_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>BrandTemplate.BrandTemplate_Name}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>BrandTemplate_Name/BrandTemplate_Name',type:'sap.ui.model.type.String' }" linkText="{i18n>BrandTemplate.BrandTemplate_Name/BrandTemplate_Name}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>BrandTemplate.BrandTemplate_NamespacePrefix}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>BrandTemplate_NamespacePrefix/BrandTemplate_NamespacePrefix',type:'sap.ui.model.type.String' }" linkText="{i18n>BrandTemplate.BrandTemplate_NamespacePrefix/BrandTemplate_NamespacePrefix}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>BrandTemplate.BrandTemplate_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>BrandTemplate_SystemModstamp/BrandTemplate_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>BrandTemplate.BrandTemplate_Value}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>BrandTemplate_Value/BrandTemplate_Value',type:'sap.ui.model.type.String' }" linkText="{i18n>BrandTemplate.BrandTemplate_Value/BrandTemplate_Value}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>BrandTemplate.EmailTemplate_BrandTemplateId_Inverse}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>EmailTemplate_BrandTemplateId_Inverse/sfdc_EmailTemplate_BrandTemplateId_Inverse" entitySet="EmailTemplate"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>BrandTemplate.BrandTemplate_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>BrandTemplate_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>BrandTemplate.BrandTemplate_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>BrandTemplate_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>BrandTemplate.EmailTemplate_BrandTemplateId_Inverse}" required="false"/> 
						<rdf:ListResource path="dataModel>EmailTemplate_BrandTemplateId_Inverse" entitySet="EmailTemplate"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>