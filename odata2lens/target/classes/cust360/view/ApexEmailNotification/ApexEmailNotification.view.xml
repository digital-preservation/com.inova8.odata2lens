<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>ApexEmailNotification.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>ApexEmailNotification.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="ApexEmailNotification"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="ApexEmailNotification"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexEmailNotification.ApexEmailNotification_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ApexEmailNotification_CreatedById/sfdc_ApexEmailNotification_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexEmailNotification.ApexEmailNotification_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>ApexEmailNotification_CreatedDate/ApexEmailNotification_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexEmailNotification.ApexEmailNotification_Email}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ApexEmailNotification_Email/ApexEmailNotification_Email',type:'sap.ui.model.type.String' }" linkText="{i18n>ApexEmailNotification.ApexEmailNotification_Email/ApexEmailNotification_Email}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexEmailNotification.ApexEmailNotification_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ApexEmailNotification_Id/ApexEmailNotification_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>ApexEmailNotification.ApexEmailNotification_Id/ApexEmailNotification_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexEmailNotification.ApexEmailNotification_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>ApexEmailNotification_IsDeleted/ApexEmailNotification_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexEmailNotification.ApexEmailNotification_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ApexEmailNotification_LastModifiedById/sfdc_ApexEmailNotification_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexEmailNotification.ApexEmailNotification_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>ApexEmailNotification_LastModifiedDate/ApexEmailNotification_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexEmailNotification.ApexEmailNotification_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>ApexEmailNotification_SystemModstamp/ApexEmailNotification_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexEmailNotification.ApexEmailNotification_UserId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ApexEmailNotification_UserId/sfdc_ApexEmailNotification_UserId" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexEmailNotification.ApexEmailNotification_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>ApexEmailNotification_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexEmailNotification.ApexEmailNotification_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>ApexEmailNotification_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexEmailNotification.ApexEmailNotification_UserId}" required="false"/> 
						<rdf:ListResource path="dataModel>ApexEmailNotification_UserId" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>