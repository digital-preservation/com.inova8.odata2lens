				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AttachedContentNote.AttachedContentNote_FileExtension}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>AttachedContentNote_FileExtension/AttachedContentNote_FileExtension',type:'sap.ui.model.type.String' }" linkText="{i18n>AttachedContentNote.AttachedContentNote_FileExtension/AttachedContentNote_FileExtension}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AttachedContentNote.AttachedContentNote_FileType}" required="false"/> 
