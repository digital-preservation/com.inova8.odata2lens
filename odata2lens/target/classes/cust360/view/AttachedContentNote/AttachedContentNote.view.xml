						<Text text=""/>
						<rdf:ListResource path="dataModel>AttachedContentNote_LinkedEntityId/sfdc_AttachedContentNote_LinkedEntityId" entitySet="Account"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AttachedContentNote.AttachedContentNote_TextPreview}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>AttachedContentNote_TextPreview/AttachedContentNote_TextPreview',type:'sap.ui.model.type.String' }" linkText="{i18n>AttachedContentNote.AttachedContentNote_TextPreview/AttachedContentNote_TextPreview}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
