						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AttachedContentNote.AttachedContentNote_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>AttachedContentNote_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
