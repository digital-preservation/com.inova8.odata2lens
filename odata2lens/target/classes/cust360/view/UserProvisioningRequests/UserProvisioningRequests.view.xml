							<VBox>					
								<Text text="{path:'dataModel>UserProvisioningRequest_IsDeleted/UserProvisioningRequest_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<Text text="{dataModel>UserProvisioningRequest_LastModifiedDate/UserProvisioningRequest_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>UserProvisioningRequest_Name/UserProvisioningRequest_Name',type:'sap.ui.model.type.String'}" linkText="{i18n>UserProvisioningRequest.UserProvisioningRequest_Name/UserProvisioningRequest_Name}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>UserProvisioningRequest_Operation/UserProvisioningRequest_Operation',type:'sap.ui.model.type.String'}" linkText="{i18n>UserProvisioningRequest.UserProvisioningRequest_Operation/UserProvisioningRequest_Operation}"/>
