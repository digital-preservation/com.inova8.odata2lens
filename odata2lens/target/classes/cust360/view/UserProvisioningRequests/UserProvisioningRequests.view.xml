							<VBox>					
							<Text text="{dataModel>UserProvisioningRequest_ScheduleDate/UserProvisioningRequest_ScheduleDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>UserProvisioningRequest_State/UserProvisioningRequest_State',type:'sap.ui.model.type.String'}" linkText="{i18n>UserProvisioningRequest.UserProvisioningRequest_State/UserProvisioningRequest_State}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>UserProvisioningRequest_SystemModstamp/UserProvisioningRequest_SystemModstamp}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
