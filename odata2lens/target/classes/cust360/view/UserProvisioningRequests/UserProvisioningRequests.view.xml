					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserProvisioningRequests.UserProvisioningRequest_ApprovalStatus}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserProvisioningRequests.UserProvisioningRequest_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserProvisioningRequests.UserProvisioningRequest_ExternalUserId}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserProvisioningRequests.UserProvisioningRequest_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserProvisioningRequests.UserProvisioningRequest_IsDeleted}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserProvisioningRequests.UserProvisioningRequest_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserProvisioningRequests.UserProvisioningRequest_Name}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserProvisioningRequests.UserProvisioningRequest_Operation}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserProvisioningRequests.UserProvisioningRequest_RetryCount}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserProvisioningRequests.UserProvisioningRequest_ScheduleDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserProvisioningRequests.UserProvisioningRequest_State}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserProvisioningRequests.UserProvisioningRequest_SystemModstamp}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="UserProvisioningRequest"/>
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>UserProvisioningRequest_AppName/UserProvisioningRequest_AppName',type:'sap.ui.model.type.String'}" linkText="{i18n>UserProvisioningRequest.UserProvisioningRequest_AppName/UserProvisioningRequest_AppName}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>UserProvisioningRequest_ApprovalStatus/UserProvisioningRequest_ApprovalStatus',type:'sap.ui.model.type.String'}" linkText="{i18n>UserProvisioningRequest.UserProvisioningRequest_ApprovalStatus/UserProvisioningRequest_ApprovalStatus}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>UserProvisioningRequest_CreatedDate/UserProvisioningRequest_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>UserProvisioningRequest_ExternalUserId/UserProvisioningRequest_ExternalUserId',type:'sap.ui.model.type.String'}" linkText="{i18n>UserProvisioningRequest.UserProvisioningRequest_ExternalUserId/UserProvisioningRequest_ExternalUserId}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>UserProvisioningRequest_Id/UserProvisioningRequest_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>UserProvisioningRequest.UserProvisioningRequest_Id/UserProvisioningRequest_Id}"/>
