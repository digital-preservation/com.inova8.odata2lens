<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>EmailTemplates.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/EmailTemplate' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>EmailTemplates.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EmailTemplates.EmailTemplate_ApiVersion}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EmailTemplates.EmailTemplate_Body}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EmailTemplates.EmailTemplate_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EmailTemplates.EmailTemplate_Description}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EmailTemplates.EmailTemplate_DeveloperName}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EmailTemplates.EmailTemplate_Encoding}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EmailTemplates.EmailTemplate_HtmlValue}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EmailTemplates.EmailTemplate_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EmailTemplates.EmailTemplate_IsActive}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EmailTemplates.EmailTemplate_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EmailTemplates.EmailTemplate_LastUsedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EmailTemplates.EmailTemplate_Markup}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EmailTemplates.EmailTemplate_Name}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EmailTemplates.EmailTemplate_NamespacePrefix}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EmailTemplates.EmailTemplate_RelatedEntityType}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EmailTemplates.EmailTemplate_Subject}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EmailTemplates.EmailTemplate_SystemModstamp}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EmailTemplates.EmailTemplate_TemplateStyle}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EmailTemplates.EmailTemplate_TemplateType}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EmailTemplates.EmailTemplate_TimesUsed}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EmailTemplates.EmailTemplate_UiType}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="EmailTemplate"/>
							<VBox>					
							<Text text="{path:'dataModel>EmailTemplate_ApiVersion/EmailTemplate_ApiVersion',type:'sap.ui.model.type.Float', formatOptions:{'style':'short','maxFractionDigits':2} }"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>EmailTemplate_Body/EmailTemplate_Body',type:'sap.ui.model.type.String'}" linkText="{i18n>EmailTemplate.EmailTemplate_Body/EmailTemplate_Body}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>EmailTemplate_CreatedDate/EmailTemplate_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>EmailTemplate_Description/EmailTemplate_Description',type:'sap.ui.model.type.String'}" linkText="{i18n>EmailTemplate.EmailTemplate_Description/EmailTemplate_Description}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>EmailTemplate_DeveloperName/EmailTemplate_DeveloperName',type:'sap.ui.model.type.String'}" linkText="{i18n>EmailTemplate.EmailTemplate_DeveloperName/EmailTemplate_DeveloperName}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>EmailTemplate_Encoding/EmailTemplate_Encoding',type:'sap.ui.model.type.String'}" linkText="{i18n>EmailTemplate.EmailTemplate_Encoding/EmailTemplate_Encoding}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>EmailTemplate_HtmlValue/EmailTemplate_HtmlValue',type:'sap.ui.model.type.String'}" linkText="{i18n>EmailTemplate.EmailTemplate_HtmlValue/EmailTemplate_HtmlValue}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>EmailTemplate_Id/EmailTemplate_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>EmailTemplate.EmailTemplate_Id/EmailTemplate_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>EmailTemplate_IsActive/EmailTemplate_IsActive',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<Text text="{dataModel>EmailTemplate_LastModifiedDate/EmailTemplate_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>EmailTemplate_LastUsedDate/EmailTemplate_LastUsedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>EmailTemplate_Markup/EmailTemplate_Markup',type:'sap.ui.model.type.String'}" linkText="{i18n>EmailTemplate.EmailTemplate_Markup/EmailTemplate_Markup}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>EmailTemplate_Name/EmailTemplate_Name',type:'sap.ui.model.type.String'}" linkText="{i18n>EmailTemplate.EmailTemplate_Name/EmailTemplate_Name}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>EmailTemplate_NamespacePrefix/EmailTemplate_NamespacePrefix',type:'sap.ui.model.type.String'}" linkText="{i18n>EmailTemplate.EmailTemplate_NamespacePrefix/EmailTemplate_NamespacePrefix}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>EmailTemplate_RelatedEntityType/EmailTemplate_RelatedEntityType',type:'sap.ui.model.type.String'}" linkText="{i18n>EmailTemplate.EmailTemplate_RelatedEntityType/EmailTemplate_RelatedEntityType}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>EmailTemplate_Subject/EmailTemplate_Subject',type:'sap.ui.model.type.String'}" linkText="{i18n>EmailTemplate.EmailTemplate_Subject/EmailTemplate_Subject}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>EmailTemplate_SystemModstamp/EmailTemplate_SystemModstamp}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>EmailTemplate_TemplateStyle/EmailTemplate_TemplateStyle',type:'sap.ui.model.type.String'}" linkText="{i18n>EmailTemplate.EmailTemplate_TemplateStyle/EmailTemplate_TemplateStyle}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>EmailTemplate_TemplateType/EmailTemplate_TemplateType',type:'sap.ui.model.type.String'}" linkText="{i18n>EmailTemplate.EmailTemplate_TemplateType/EmailTemplate_TemplateType}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>EmailTemplate_TimesUsed/EmailTemplate_TimesUsed',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>EmailTemplate_UiType/EmailTemplate_UiType',type:'sap.ui.model.type.String'}" linkText="{i18n>EmailTemplate.EmailTemplate_UiType/EmailTemplate_UiType}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>