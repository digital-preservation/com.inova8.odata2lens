<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>ContentAsset.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>ContentAsset.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="ContentAsset"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="ContentAsset"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentAsset.ChatterExtension_IconId_Inverse}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ChatterExtension_IconId_Inverse/sfdc_ChatterExtension_IconId_Inverse" entitySet="ChatterExtension"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentAsset.ContentAsset_ContentDocumentId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ContentAsset_ContentDocumentId/sfdc_ContentAsset_ContentDocumentId" entitySet="ContentDocument"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentAsset.ContentAsset_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ContentAsset_CreatedById/sfdc_ContentAsset_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentAsset.ContentAsset_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>ContentAsset_CreatedDate/ContentAsset_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentAsset.ContentAsset_DeveloperName}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ContentAsset_DeveloperName/ContentAsset_DeveloperName',type:'sap.ui.model.type.String' }" linkText="{i18n>ContentAsset.ContentAsset_DeveloperName/ContentAsset_DeveloperName}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentAsset.ContentAsset_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ContentAsset_Id/ContentAsset_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>ContentAsset.ContentAsset_Id/ContentAsset_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentAsset.ContentAsset_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>ContentAsset_IsDeleted/ContentAsset_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentAsset.ContentAsset_Language}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ContentAsset_Language/ContentAsset_Language',type:'sap.ui.model.type.String' }" linkText="{i18n>ContentAsset.ContentAsset_Language/ContentAsset_Language}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentAsset.ContentAsset_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ContentAsset_LastModifiedById/sfdc_ContentAsset_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentAsset.ContentAsset_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>ContentAsset_LastModifiedDate/ContentAsset_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentAsset.ContentAsset_MasterLabel}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ContentAsset_MasterLabel/ContentAsset_MasterLabel',type:'sap.ui.model.type.String' }" linkText="{i18n>ContentAsset.ContentAsset_MasterLabel/ContentAsset_MasterLabel}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentAsset.ContentAsset_NamespacePrefix}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ContentAsset_NamespacePrefix/ContentAsset_NamespacePrefix',type:'sap.ui.model.type.String' }" linkText="{i18n>ContentAsset.ContentAsset_NamespacePrefix/ContentAsset_NamespacePrefix}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentAsset.ContentAsset_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>ContentAsset_SystemModstamp/ContentAsset_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentAsset.ContentDocument_ContentAssetId_Inverse}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ContentDocument_ContentAssetId_Inverse/sfdc_ContentDocument_ContentAssetId_Inverse" entitySet="ContentDocument"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentAsset.CustomBrandAsset_AssetSourceId_Inverse}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>CustomBrandAsset_AssetSourceId_Inverse/sfdc_CustomBrandAsset_AssetSourceId_Inverse" entitySet="CustomBrandAsset"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentAsset.ChatterExtension_IconId_Inverse}" required="false"/> 
						<rdf:ListResource path="dataModel>ChatterExtension_IconId_Inverse" entitySet="ChatterExtension"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentAsset.ContentAsset_ContentDocumentId}" required="false"/> 
						<rdf:ListResource path="dataModel>ContentAsset_ContentDocumentId" entitySet="ContentDocument"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentAsset.ContentAsset_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>ContentAsset_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentAsset.ContentAsset_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>ContentAsset_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentAsset.ContentDocument_ContentAssetId_Inverse}" required="false"/> 
						<rdf:ListResource path="dataModel>ContentDocument_ContentAssetId_Inverse" entitySet="ContentDocument"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentAsset.CustomBrandAsset_AssetSourceId_Inverse}" required="false"/> 
						<rdf:ListResource path="dataModel>CustomBrandAsset_AssetSourceId_Inverse" entitySet="CustomBrandAsset"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>