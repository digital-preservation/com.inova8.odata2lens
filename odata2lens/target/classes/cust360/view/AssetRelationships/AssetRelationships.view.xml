<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>AssetRelationships.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/AssetRelationship' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>AssetRelationships.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AssetRelationships.AssetRelationship_AssetRelationshipNumber}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AssetRelationships.AssetRelationship_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AssetRelationships.AssetRelationship_FromDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AssetRelationships.AssetRelationship_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AssetRelationships.AssetRelationship_IsDeleted}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AssetRelationships.AssetRelationship_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AssetRelationships.AssetRelationship_LastReferencedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AssetRelationships.AssetRelationship_LastViewedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AssetRelationships.AssetRelationship_RelationshipType}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AssetRelationships.AssetRelationship_SystemModstamp}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AssetRelationships.AssetRelationship_ToDate}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="AssetRelationship"/>
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>AssetRelationship_AssetRelationshipNumber/AssetRelationship_AssetRelationshipNumber',type:'sap.ui.model.type.String'}" linkText="{i18n>AssetRelationship.AssetRelationship_AssetRelationshipNumber/AssetRelationship_AssetRelationshipNumber}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>AssetRelationship_CreatedDate/AssetRelationship_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>AssetRelationship_FromDate/AssetRelationship_FromDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>AssetRelationship_Id/AssetRelationship_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>AssetRelationship.AssetRelationship_Id/AssetRelationship_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>AssetRelationship_IsDeleted/AssetRelationship_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<Text text="{dataModel>AssetRelationship_LastModifiedDate/AssetRelationship_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>AssetRelationship_LastReferencedDate/AssetRelationship_LastReferencedDate}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>AssetRelationship_LastViewedDate/AssetRelationship_LastViewedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>AssetRelationship_RelationshipType/AssetRelationship_RelationshipType',type:'sap.ui.model.type.String'}" linkText="{i18n>AssetRelationship.AssetRelationship_RelationshipType/AssetRelationship_RelationshipType}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>AssetRelationship_SystemModstamp/AssetRelationship_SystemModstamp}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>AssetRelationship_ToDate/AssetRelationship_ToDate}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>