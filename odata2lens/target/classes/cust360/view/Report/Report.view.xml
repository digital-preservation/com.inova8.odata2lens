<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>Report.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>Report.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="Report"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="Report"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Report.ReportFeed_ParentId_Inverse}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ReportFeed_ParentId_Inverse/sfdc_ReportFeed_ParentId_Inverse" entitySet="ReportFeed"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Report.Report_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>Report_CreatedById/sfdc_Report_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Report.Report_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>Report_CreatedDate/Report_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Report.Report_Description}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>Report_Description/Report_Description',type:'sap.ui.model.type.String' }" linkText="{i18n>Report.Report_Description/Report_Description}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Report.Report_DeveloperName}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>Report_DeveloperName/Report_DeveloperName',type:'sap.ui.model.type.String' }" linkText="{i18n>Report.Report_DeveloperName/Report_DeveloperName}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Report.Report_FolderName}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>Report_FolderName/Report_FolderName',type:'sap.ui.model.type.String' }" linkText="{i18n>Report.Report_FolderName/Report_FolderName}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Report.Report_Format}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>Report_Format/Report_Format',type:'sap.ui.model.type.String' }" linkText="{i18n>Report.Report_Format/Report_Format}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Report.Report_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>Report_Id/Report_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>Report.Report_Id/Report_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Report.Report_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>Report_IsDeleted/Report_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Report.Report_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>Report_LastModifiedById/sfdc_Report_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Report.Report_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>Report_LastModifiedDate/Report_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Report.Report_LastReferencedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>Report_LastReferencedDate/Report_LastReferencedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Report.Report_LastRunDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>Report_LastRunDate/Report_LastRunDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Report.Report_LastViewedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>Report_LastViewedDate/Report_LastViewedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Report.Report_Name}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>Report_Name/Report_Name',type:'sap.ui.model.type.String' }" linkText="{i18n>Report.Report_Name/Report_Name}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Report.Report_NamespacePrefix}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>Report_NamespacePrefix/Report_NamespacePrefix',type:'sap.ui.model.type.String' }" linkText="{i18n>Report.Report_NamespacePrefix/Report_NamespacePrefix}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Report.Report_OwnerId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>Report_OwnerId/sfdc_Report_OwnerId" entitySet="Folder"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Report.Report_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>Report_SystemModstamp/Report_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Report.ReportFeed_ParentId_Inverse}" required="false"/> 
						<rdf:ListResource path="dataModel>ReportFeed_ParentId_Inverse" entitySet="ReportFeed"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Report.Report_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>Report_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Report.Report_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>Report_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Report.Report_OwnerId}" required="false"/> 
						<rdf:ListResource path="dataModel>Report_OwnerId" entitySet="Folder"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>