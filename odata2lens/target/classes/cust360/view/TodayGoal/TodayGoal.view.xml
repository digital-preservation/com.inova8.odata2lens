<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>TodayGoal.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>TodayGoal.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="TodayGoal"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="TodayGoal"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TodayGoal.TodayGoalShare_ParentId_Inverse}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>TodayGoalShare_ParentId_Inverse/sfdc_TodayGoalShare_ParentId_Inverse" entitySet="TodayGoalShare"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TodayGoal.TodayGoal_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>TodayGoal_CreatedById/sfdc_TodayGoal_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TodayGoal.TodayGoal_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>TodayGoal_CreatedDate/TodayGoal_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TodayGoal.TodayGoal_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>TodayGoal_Id/TodayGoal_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>TodayGoal.TodayGoal_Id/TodayGoal_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TodayGoal.TodayGoal_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>TodayGoal_IsDeleted/TodayGoal_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TodayGoal.TodayGoal_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>TodayGoal_LastModifiedById/sfdc_TodayGoal_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TodayGoal.TodayGoal_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>TodayGoal_LastModifiedDate/TodayGoal_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TodayGoal.TodayGoal_Name}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>TodayGoal_Name/TodayGoal_Name',type:'sap.ui.model.type.String' }" linkText="{i18n>TodayGoal.TodayGoal_Name/TodayGoal_Name}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TodayGoal.TodayGoal_OwnerId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>TodayGoal_OwnerId/sfdc_TodayGoal_OwnerId" entitySet="Group"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TodayGoal.TodayGoal_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>TodayGoal_SystemModstamp/TodayGoal_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TodayGoal.TodayGoal_UserId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>TodayGoal_UserId/sfdc_TodayGoal_UserId" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TodayGoal.TodayGoal_Value}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>TodayGoal_Value/TodayGoal_Value',type:'sap.ui.model.type.Float', formatOptions:{'style':'short','maxFractionDigits':2} }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TodayGoal.TodayGoalShare_ParentId_Inverse}" required="false"/> 
						<rdf:ListResource path="dataModel>TodayGoalShare_ParentId_Inverse" entitySet="TodayGoalShare"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TodayGoal.TodayGoal_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>TodayGoal_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TodayGoal.TodayGoal_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>TodayGoal_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TodayGoal.TodayGoal_OwnerId}" required="false"/> 
						<rdf:ListResource path="dataModel>TodayGoal_OwnerId" entitySet="Group"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TodayGoal.TodayGoal_UserId}" required="false"/> 
						<rdf:ListResource path="dataModel>TodayGoal_UserId" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>