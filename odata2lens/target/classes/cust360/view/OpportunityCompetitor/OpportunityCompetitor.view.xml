<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>OpportunityCompetitor.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>OpportunityCompetitor.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="OpportunityCompetitor"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="OpportunityCompetitor"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OpportunityCompetitor.OpportunityCompetitor_CompetitorName}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>OpportunityCompetitor_CompetitorName/OpportunityCompetitor_CompetitorName',type:'sap.ui.model.type.String' }" linkText="{i18n>OpportunityCompetitor.OpportunityCompetitor_CompetitorName/OpportunityCompetitor_CompetitorName}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OpportunityCompetitor.OpportunityCompetitor_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>OpportunityCompetitor_CreatedById/sfdc_OpportunityCompetitor_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OpportunityCompetitor.OpportunityCompetitor_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>OpportunityCompetitor_CreatedDate/OpportunityCompetitor_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OpportunityCompetitor.OpportunityCompetitor_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>OpportunityCompetitor_Id/OpportunityCompetitor_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>OpportunityCompetitor.OpportunityCompetitor_Id/OpportunityCompetitor_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OpportunityCompetitor.OpportunityCompetitor_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>OpportunityCompetitor_IsDeleted/OpportunityCompetitor_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OpportunityCompetitor.OpportunityCompetitor_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>OpportunityCompetitor_LastModifiedById/sfdc_OpportunityCompetitor_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OpportunityCompetitor.OpportunityCompetitor_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>OpportunityCompetitor_LastModifiedDate/OpportunityCompetitor_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OpportunityCompetitor.OpportunityCompetitor_OpportunityId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>OpportunityCompetitor_OpportunityId/sfdc_OpportunityCompetitor_OpportunityId" entitySet="Opportunity"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OpportunityCompetitor.OpportunityCompetitor_Strengths}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>OpportunityCompetitor_Strengths/OpportunityCompetitor_Strengths',type:'sap.ui.model.type.String' }" linkText="{i18n>OpportunityCompetitor.OpportunityCompetitor_Strengths/OpportunityCompetitor_Strengths}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OpportunityCompetitor.OpportunityCompetitor_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>OpportunityCompetitor_SystemModstamp/OpportunityCompetitor_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OpportunityCompetitor.OpportunityCompetitor_Weaknesses}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>OpportunityCompetitor_Weaknesses/OpportunityCompetitor_Weaknesses',type:'sap.ui.model.type.String' }" linkText="{i18n>OpportunityCompetitor.OpportunityCompetitor_Weaknesses/OpportunityCompetitor_Weaknesses}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OpportunityCompetitor.OpportunityCompetitor_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>OpportunityCompetitor_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OpportunityCompetitor.OpportunityCompetitor_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>OpportunityCompetitor_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OpportunityCompetitor.OpportunityCompetitor_OpportunityId}" required="false"/> 
						<rdf:ListResource path="dataModel>OpportunityCompetitor_OpportunityId" entitySet="Opportunity"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>