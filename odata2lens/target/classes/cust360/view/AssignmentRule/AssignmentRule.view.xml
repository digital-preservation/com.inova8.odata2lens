<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>AssignmentRule.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>AssignmentRule.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="AssignmentRule"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="AssignmentRule"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AssignmentRule.AssignmentRule_Active}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>AssignmentRule_Active/AssignmentRule_Active',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AssignmentRule.AssignmentRule_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>AssignmentRule_CreatedById/sfdc_AssignmentRule_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AssignmentRule.AssignmentRule_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>AssignmentRule_CreatedDate/AssignmentRule_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AssignmentRule.AssignmentRule_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>AssignmentRule_Id/AssignmentRule_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>AssignmentRule.AssignmentRule_Id/AssignmentRule_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AssignmentRule.AssignmentRule_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>AssignmentRule_LastModifiedById/sfdc_AssignmentRule_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AssignmentRule.AssignmentRule_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>AssignmentRule_LastModifiedDate/AssignmentRule_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AssignmentRule.AssignmentRule_Name}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>AssignmentRule_Name/AssignmentRule_Name',type:'sap.ui.model.type.String' }" linkText="{i18n>AssignmentRule.AssignmentRule_Name/AssignmentRule_Name}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AssignmentRule.AssignmentRule_SobjectType}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>AssignmentRule_SobjectType/AssignmentRule_SobjectType',type:'sap.ui.model.type.String' }" linkText="{i18n>AssignmentRule.AssignmentRule_SobjectType/AssignmentRule_SobjectType}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AssignmentRule.AssignmentRule_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>AssignmentRule_SystemModstamp/AssignmentRule_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AssignmentRule.AssignmentRule_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>AssignmentRule_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AssignmentRule.AssignmentRule_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>AssignmentRule_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>