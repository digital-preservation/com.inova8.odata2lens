<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>ProcessNode.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>ProcessNode.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="ProcessNode"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="ProcessNode"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ProcessNode.ProcessInstanceHistory_ProcessNodeId_Inverse}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ProcessInstanceHistory_ProcessNodeId_Inverse/sfdc_ProcessInstanceHistory_ProcessNodeId_Inverse" entitySet="ProcessInstanceHistory"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ProcessNode.ProcessInstanceNode_ProcessNodeId_Inverse}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ProcessInstanceNode_ProcessNodeId_Inverse/sfdc_ProcessInstanceNode_ProcessNodeId_Inverse" entitySet="ProcessInstanceNode"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ProcessNode.ProcessInstanceStep_StepNodeId_Inverse}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ProcessInstanceStep_StepNodeId_Inverse/sfdc_ProcessInstanceStep_StepNodeId_Inverse" entitySet="ProcessInstanceStep"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ProcessNode.ProcessNode_Description}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ProcessNode_Description/ProcessNode_Description',type:'sap.ui.model.type.String' }" linkText="{i18n>ProcessNode.ProcessNode_Description/ProcessNode_Description}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ProcessNode.ProcessNode_DeveloperName}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ProcessNode_DeveloperName/ProcessNode_DeveloperName',type:'sap.ui.model.type.String' }" linkText="{i18n>ProcessNode.ProcessNode_DeveloperName/ProcessNode_DeveloperName}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ProcessNode.ProcessNode_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ProcessNode_Id/ProcessNode_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>ProcessNode.ProcessNode_Id/ProcessNode_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ProcessNode.ProcessNode_Name}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ProcessNode_Name/ProcessNode_Name',type:'sap.ui.model.type.String' }" linkText="{i18n>ProcessNode.ProcessNode_Name/ProcessNode_Name}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ProcessNode.ProcessNode_ProcessDefinitionId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ProcessNode_ProcessDefinitionId/sfdc_ProcessNode_ProcessDefinitionId" entitySet="ProcessDefinition"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ProcessNode.ProcessNode_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>ProcessNode_SystemModstamp/ProcessNode_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ProcessNode.ProcessInstanceHistory_ProcessNodeId_Inverse}" required="false"/> 
						<rdf:ListResource path="dataModel>ProcessInstanceHistory_ProcessNodeId_Inverse" entitySet="ProcessInstanceHistory"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ProcessNode.ProcessInstanceNode_ProcessNodeId_Inverse}" required="false"/> 
						<rdf:ListResource path="dataModel>ProcessInstanceNode_ProcessNodeId_Inverse" entitySet="ProcessInstanceNode"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ProcessNode.ProcessInstanceStep_StepNodeId_Inverse}" required="false"/> 
						<rdf:ListResource path="dataModel>ProcessInstanceStep_StepNodeId_Inverse" entitySet="ProcessInstanceStep"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ProcessNode.ProcessNode_ProcessDefinitionId}" required="false"/> 
						<rdf:ListResource path="dataModel>ProcessNode_ProcessDefinitionId" entitySet="ProcessDefinition"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>