<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>AuthConfig.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>AuthConfig.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="AuthConfig"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="AuthConfig"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AuthConfig.AuthConfigProviders_AuthConfigId_Inverse}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>AuthConfigProviders_AuthConfigId_Inverse/sfdc_AuthConfigProviders_AuthConfigId_Inverse" entitySet="AuthConfigProviders"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AuthConfig.AuthConfig_AuthOptionsAuthProvider}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>AuthConfig_AuthOptionsAuthProvider/AuthConfig_AuthOptionsAuthProvider',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AuthConfig.AuthConfig_AuthOptionsSaml}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>AuthConfig_AuthOptionsSaml/AuthConfig_AuthOptionsSaml',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AuthConfig.AuthConfig_AuthOptionsUsernamePassword}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>AuthConfig_AuthOptionsUsernamePassword/AuthConfig_AuthOptionsUsernamePassword',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AuthConfig.AuthConfig_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>AuthConfig_CreatedById/sfdc_AuthConfig_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AuthConfig.AuthConfig_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>AuthConfig_CreatedDate/AuthConfig_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AuthConfig.AuthConfig_DeveloperName}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>AuthConfig_DeveloperName/AuthConfig_DeveloperName',type:'sap.ui.model.type.String' }" linkText="{i18n>AuthConfig.AuthConfig_DeveloperName/AuthConfig_DeveloperName}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AuthConfig.AuthConfig_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>AuthConfig_Id/AuthConfig_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>AuthConfig.AuthConfig_Id/AuthConfig_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AuthConfig.AuthConfig_IsActive}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>AuthConfig_IsActive/AuthConfig_IsActive',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AuthConfig.AuthConfig_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>AuthConfig_IsDeleted/AuthConfig_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AuthConfig.AuthConfig_Language}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>AuthConfig_Language/AuthConfig_Language',type:'sap.ui.model.type.String' }" linkText="{i18n>AuthConfig.AuthConfig_Language/AuthConfig_Language}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AuthConfig.AuthConfig_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>AuthConfig_LastModifiedById/sfdc_AuthConfig_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AuthConfig.AuthConfig_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>AuthConfig_LastModifiedDate/AuthConfig_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AuthConfig.AuthConfig_MasterLabel}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>AuthConfig_MasterLabel/AuthConfig_MasterLabel',type:'sap.ui.model.type.String' }" linkText="{i18n>AuthConfig.AuthConfig_MasterLabel/AuthConfig_MasterLabel}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AuthConfig.AuthConfig_NamespacePrefix}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>AuthConfig_NamespacePrefix/AuthConfig_NamespacePrefix',type:'sap.ui.model.type.String' }" linkText="{i18n>AuthConfig.AuthConfig_NamespacePrefix/AuthConfig_NamespacePrefix}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AuthConfig.AuthConfig_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>AuthConfig_SystemModstamp/AuthConfig_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AuthConfig.AuthConfig_Type}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>AuthConfig_Type/AuthConfig_Type',type:'sap.ui.model.type.String' }" linkText="{i18n>AuthConfig.AuthConfig_Type/AuthConfig_Type}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AuthConfig.AuthConfig_Url}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>AuthConfig_Url/AuthConfig_Url',type:'sap.ui.model.type.String' }" linkText="{i18n>AuthConfig.AuthConfig_Url/AuthConfig_Url}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AuthConfig.AuthConfigProviders_AuthConfigId_Inverse}" required="false"/> 
						<rdf:ListResource path="dataModel>AuthConfigProviders_AuthConfigId_Inverse" entitySet="AuthConfigProviders"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AuthConfig.AuthConfig_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>AuthConfig_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AuthConfig.AuthConfig_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>AuthConfig_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>