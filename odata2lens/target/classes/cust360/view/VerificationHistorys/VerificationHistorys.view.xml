<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>VerificationHistorys.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/VerificationHistory' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>VerificationHistorys.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>VerificationHistorys.VerificationHistory_Activity}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>VerificationHistorys.VerificationHistory_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>VerificationHistorys.VerificationHistory_EventGroup}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>VerificationHistorys.VerificationHistory_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>VerificationHistorys.VerificationHistory_IsDeleted}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>VerificationHistorys.VerificationHistory_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>VerificationHistorys.VerificationHistory_Policy}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>VerificationHistorys.VerificationHistory_Remarks}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>VerificationHistorys.VerificationHistory_SourceIp}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>VerificationHistorys.VerificationHistory_Status}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>VerificationHistorys.VerificationHistory_SystemModstamp}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>VerificationHistorys.VerificationHistory_VerificationMethod}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>VerificationHistorys.VerificationHistory_VerificationTime}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="VerificationHistory"/>
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>VerificationHistory_Activity/VerificationHistory_Activity',type:'sap.ui.model.type.String'}" linkText="{i18n>VerificationHistory.VerificationHistory_Activity/VerificationHistory_Activity}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>VerificationHistory_CreatedDate/VerificationHistory_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>VerificationHistory_EventGroup/VerificationHistory_EventGroup',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>VerificationHistory_Id/VerificationHistory_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>VerificationHistory.VerificationHistory_Id/VerificationHistory_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>VerificationHistory_IsDeleted/VerificationHistory_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<Text text="{dataModel>VerificationHistory_LastModifiedDate/VerificationHistory_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>VerificationHistory_Policy/VerificationHistory_Policy',type:'sap.ui.model.type.String'}" linkText="{i18n>VerificationHistory.VerificationHistory_Policy/VerificationHistory_Policy}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>VerificationHistory_Remarks/VerificationHistory_Remarks',type:'sap.ui.model.type.String'}" linkText="{i18n>VerificationHistory.VerificationHistory_Remarks/VerificationHistory_Remarks}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>VerificationHistory_SourceIp/VerificationHistory_SourceIp',type:'sap.ui.model.type.String'}" linkText="{i18n>VerificationHistory.VerificationHistory_SourceIp/VerificationHistory_SourceIp}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>VerificationHistory_Status/VerificationHistory_Status',type:'sap.ui.model.type.String'}" linkText="{i18n>VerificationHistory.VerificationHistory_Status/VerificationHistory_Status}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>VerificationHistory_SystemModstamp/VerificationHistory_SystemModstamp}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>VerificationHistory_VerificationMethod/VerificationHistory_VerificationMethod',type:'sap.ui.model.type.String'}" linkText="{i18n>VerificationHistory.VerificationHistory_VerificationMethod/VerificationHistory_VerificationMethod}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>VerificationHistory_VerificationTime/VerificationHistory_VerificationTime}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>