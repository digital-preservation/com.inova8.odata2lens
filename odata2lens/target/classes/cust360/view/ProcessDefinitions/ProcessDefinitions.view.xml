<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>ProcessDefinitions.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/ProcessDefinition' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>ProcessDefinitions.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ProcessDefinitions.ProcessDefinition_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ProcessDefinitions.ProcessDefinition_Description}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ProcessDefinitions.ProcessDefinition_DeveloperName}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ProcessDefinitions.ProcessDefinition_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ProcessDefinitions.ProcessDefinition_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ProcessDefinitions.ProcessDefinition_LockType}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ProcessDefinitions.ProcessDefinition_Name}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ProcessDefinitions.ProcessDefinition_State}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ProcessDefinitions.ProcessDefinition_SystemModstamp}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ProcessDefinitions.ProcessDefinition_TableEnumOrId}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ProcessDefinitions.ProcessDefinition_Type}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="ProcessDefinition"/>
							<VBox>					
							<Text text="{dataModel>ProcessDefinition_CreatedDate/ProcessDefinition_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ProcessDefinition_Description/ProcessDefinition_Description',type:'sap.ui.model.type.String'}" linkText="{i18n>ProcessDefinition.ProcessDefinition_Description/ProcessDefinition_Description}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ProcessDefinition_DeveloperName/ProcessDefinition_DeveloperName',type:'sap.ui.model.type.String'}" linkText="{i18n>ProcessDefinition.ProcessDefinition_DeveloperName/ProcessDefinition_DeveloperName}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ProcessDefinition_Id/ProcessDefinition_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>ProcessDefinition.ProcessDefinition_Id/ProcessDefinition_Id}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>ProcessDefinition_LastModifiedDate/ProcessDefinition_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ProcessDefinition_LockType/ProcessDefinition_LockType',type:'sap.ui.model.type.String'}" linkText="{i18n>ProcessDefinition.ProcessDefinition_LockType/ProcessDefinition_LockType}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ProcessDefinition_Name/ProcessDefinition_Name',type:'sap.ui.model.type.String'}" linkText="{i18n>ProcessDefinition.ProcessDefinition_Name/ProcessDefinition_Name}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ProcessDefinition_State/ProcessDefinition_State',type:'sap.ui.model.type.String'}" linkText="{i18n>ProcessDefinition.ProcessDefinition_State/ProcessDefinition_State}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>ProcessDefinition_SystemModstamp/ProcessDefinition_SystemModstamp}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ProcessDefinition_TableEnumOrId/ProcessDefinition_TableEnumOrId',type:'sap.ui.model.type.String'}" linkText="{i18n>ProcessDefinition.ProcessDefinition_TableEnumOrId/ProcessDefinition_TableEnumOrId}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ProcessDefinition_Type/ProcessDefinition_Type',type:'sap.ui.model.type.String'}" linkText="{i18n>ProcessDefinition.ProcessDefinition_Type/ProcessDefinition_Type}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>