					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentNote.ContentNote_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>ContentNote_IsDeleted/ContentNote_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentNote.ContentNote_IsReadOnly}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>ContentNote_IsReadOnly/ContentNote_IsReadOnly',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentNote.ContentNote_LastModifiedById}" required="false"/> 
						<Text text=""/>
