						<Text text="{dataModel>ContentNote_LastModifiedDate/ContentNote_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentNote.ContentNote_LastViewedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>ContentNote_LastViewedDate/ContentNote_LastViewedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentNote.ContentNote_LatestPublishedVersionId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ContentNote_LatestPublishedVersionId/sfdc_ContentNote_LatestPublishedVersionId" entitySet="ContentVersion"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentNote.ContentNote_OwnerId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ContentNote_OwnerId/sfdc_ContentNote_OwnerId" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentNote.ContentNote_SharingPrivacy}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ContentNote_SharingPrivacy/ContentNote_SharingPrivacy',type:'sap.ui.model.type.String' }" linkText="{i18n>ContentNote.ContentNote_SharingPrivacy/ContentNote_SharingPrivacy}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentNote.ContentNote_TextPreview}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ContentNote_TextPreview/ContentNote_TextPreview',type:'sap.ui.model.type.String' }" linkText="{i18n>ContentNote.ContentNote_TextPreview/ContentNote_TextPreview}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentNote.ContentNote_Title}" required="false"/> 
						<Text text=""/>
