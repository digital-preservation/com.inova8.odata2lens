<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>Solutions.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/Solution' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>Solutions.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Solutions.Solution_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Solutions.Solution_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Solutions.Solution_IsDeleted}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Solutions.Solution_IsHtml}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Solutions.Solution_IsPublished}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Solutions.Solution_IsPublishedInPublicKb}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Solutions.Solution_IsReviewed}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Solutions.Solution_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Solutions.Solution_LastReferencedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Solutions.Solution_LastViewedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Solutions.Solution_SolutionName}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Solutions.Solution_SolutionNote}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Solutions.Solution_SolutionNumber}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Solutions.Solution_Status}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Solutions.Solution_SystemModstamp}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Solutions.Solution_TimesUsed}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="Solution"/>
							<VBox>					
							<Text text="{dataModel>Solution_CreatedDate/Solution_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Solution_Id/Solution_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>Solution.Solution_Id/Solution_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>Solution_IsDeleted/Solution_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>Solution_IsHtml/Solution_IsHtml',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>Solution_IsPublished/Solution_IsPublished',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>Solution_IsPublishedInPublicKb/Solution_IsPublishedInPublicKb',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>Solution_IsReviewed/Solution_IsReviewed',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<Text text="{dataModel>Solution_LastModifiedDate/Solution_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>Solution_LastReferencedDate/Solution_LastReferencedDate}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>Solution_LastViewedDate/Solution_LastViewedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Solution_SolutionName/Solution_SolutionName',type:'sap.ui.model.type.String'}" linkText="{i18n>Solution.Solution_SolutionName/Solution_SolutionName}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Solution_SolutionNote/Solution_SolutionNote',type:'sap.ui.model.type.String'}" linkText="{i18n>Solution.Solution_SolutionNote/Solution_SolutionNote}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Solution_SolutionNumber/Solution_SolutionNumber',type:'sap.ui.model.type.String'}" linkText="{i18n>Solution.Solution_SolutionNumber/Solution_SolutionNumber}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Solution_Status/Solution_Status',type:'sap.ui.model.type.String'}" linkText="{i18n>Solution.Solution_Status/Solution_Status}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>Solution_SystemModstamp/Solution_SystemModstamp}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>Solution_TimesUsed/Solution_TimesUsed',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>