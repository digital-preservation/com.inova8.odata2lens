					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContactFeed.ContactFeed_ParentId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ContactFeed_ParentId/sfdc_ContactFeed_ParentId" entitySet="Contact"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContactFeed.ContactFeed_RelatedRecordId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ContactFeed_RelatedRecordId/sfdc_ContactFeed_RelatedRecordId" entitySet="ContentVersion"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContactFeed.ContactFeed_SystemModstamp}" required="false"/> 
