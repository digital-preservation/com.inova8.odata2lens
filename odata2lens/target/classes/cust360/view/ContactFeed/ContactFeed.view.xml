					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContactFeed.ContactFeed_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ContactFeed_Id/ContactFeed_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>ContactFeed.ContactFeed_Id/ContactFeed_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContactFeed.ContactFeed_InsertedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ContactFeed_InsertedById/sfdc_ContactFeed_InsertedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContactFeed.ContactFeed_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>ContactFeed_IsDeleted/ContactFeed_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContactFeed.ContactFeed_IsRichText}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>ContactFeed_IsRichText/ContactFeed_IsRichText',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContactFeed.ContactFeed_LastModifiedDate}" required="false"/> 
						<Text text=""/>
