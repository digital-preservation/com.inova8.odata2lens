<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>UserEntityAccesss.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/UserEntityAccess' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>UserEntityAccesss.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserEntityAccesss.UserEntityAccess_DurableId}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserEntityAccesss.UserEntityAccess_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserEntityAccesss.UserEntityAccess_IsActivateable}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserEntityAccesss.UserEntityAccess_IsCreatable}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserEntityAccesss.UserEntityAccess_IsDeletable}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserEntityAccesss.UserEntityAccess_IsEditable}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserEntityAccesss.UserEntityAccess_IsFlsUpdatable}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserEntityAccesss.UserEntityAccess_IsMergeable}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserEntityAccesss.UserEntityAccess_IsReadable}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserEntityAccesss.UserEntityAccess_IsUndeletable}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserEntityAccesss.UserEntityAccess_IsUpdatable}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="UserEntityAccess"/>
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>UserEntityAccess_DurableId/UserEntityAccess_DurableId',type:'sap.ui.model.type.String'}" linkText="{i18n>UserEntityAccess.UserEntityAccess_DurableId/UserEntityAccess_DurableId}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>UserEntityAccess_Id/UserEntityAccess_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>UserEntityAccess.UserEntityAccess_Id/UserEntityAccess_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>UserEntityAccess_IsActivateable/UserEntityAccess_IsActivateable',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>UserEntityAccess_IsCreatable/UserEntityAccess_IsCreatable',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>UserEntityAccess_IsDeletable/UserEntityAccess_IsDeletable',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>UserEntityAccess_IsEditable/UserEntityAccess_IsEditable',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>UserEntityAccess_IsFlsUpdatable/UserEntityAccess_IsFlsUpdatable',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>UserEntityAccess_IsMergeable/UserEntityAccess_IsMergeable',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>UserEntityAccess_IsReadable/UserEntityAccess_IsReadable',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>UserEntityAccess_IsUndeletable/UserEntityAccess_IsUndeletable',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>UserEntityAccess_IsUpdatable/UserEntityAccess_IsUpdatable',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>