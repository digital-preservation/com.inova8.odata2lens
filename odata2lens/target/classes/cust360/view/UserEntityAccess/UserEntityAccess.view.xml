<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>UserEntityAccess.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>UserEntityAccess.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="UserEntityAccess"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="UserEntityAccess"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserEntityAccess.EntityDefinition_RunningUserEntityAccessId_Inverse}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>EntityDefinition_RunningUserEntityAccessId_Inverse/sfdc_EntityDefinition_RunningUserEntityAccessId_Inverse" entitySet="EntityDefinition"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserEntityAccess.UserEntityAccess_DurableId}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>UserEntityAccess_DurableId/UserEntityAccess_DurableId',type:'sap.ui.model.type.String' }" linkText="{i18n>UserEntityAccess.UserEntityAccess_DurableId/UserEntityAccess_DurableId}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserEntityAccess.UserEntityAccess_EntityDefinitionId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>UserEntityAccess_EntityDefinitionId/sfdc_UserEntityAccess_EntityDefinitionId" entitySet="EntityDefinition"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserEntityAccess.UserEntityAccess_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>UserEntityAccess_Id/UserEntityAccess_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>UserEntityAccess.UserEntityAccess_Id/UserEntityAccess_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserEntityAccess.UserEntityAccess_IsActivateable}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>UserEntityAccess_IsActivateable/UserEntityAccess_IsActivateable',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserEntityAccess.UserEntityAccess_IsCreatable}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>UserEntityAccess_IsCreatable/UserEntityAccess_IsCreatable',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserEntityAccess.UserEntityAccess_IsDeletable}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>UserEntityAccess_IsDeletable/UserEntityAccess_IsDeletable',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserEntityAccess.UserEntityAccess_IsEditable}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>UserEntityAccess_IsEditable/UserEntityAccess_IsEditable',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserEntityAccess.UserEntityAccess_IsFlsUpdatable}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>UserEntityAccess_IsFlsUpdatable/UserEntityAccess_IsFlsUpdatable',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserEntityAccess.UserEntityAccess_IsMergeable}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>UserEntityAccess_IsMergeable/UserEntityAccess_IsMergeable',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserEntityAccess.UserEntityAccess_IsReadable}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>UserEntityAccess_IsReadable/UserEntityAccess_IsReadable',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserEntityAccess.UserEntityAccess_IsUndeletable}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>UserEntityAccess_IsUndeletable/UserEntityAccess_IsUndeletable',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserEntityAccess.UserEntityAccess_IsUpdatable}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>UserEntityAccess_IsUpdatable/UserEntityAccess_IsUpdatable',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserEntityAccess.UserEntityAccess_UserId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>UserEntityAccess_UserId/sfdc_UserEntityAccess_UserId" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserEntityAccess.EntityDefinition_RunningUserEntityAccessId_Inverse}" required="false"/> 
						<rdf:ListResource path="dataModel>EntityDefinition_RunningUserEntityAccessId_Inverse" entitySet="EntityDefinition"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserEntityAccess.UserEntityAccess_EntityDefinitionId}" required="false"/> 
						<rdf:ListResource path="dataModel>UserEntityAccess_EntityDefinitionId" entitySet="EntityDefinition"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserEntityAccess.UserEntityAccess_UserId}" required="false"/> 
						<rdf:ListResource path="dataModel>UserEntityAccess_UserId" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>