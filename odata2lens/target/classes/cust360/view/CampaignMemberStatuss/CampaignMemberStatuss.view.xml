<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>CampaignMemberStatuss.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/CampaignMemberStatus' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>CampaignMemberStatuss.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CampaignMemberStatuss.CampaignMemberStatus_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CampaignMemberStatuss.CampaignMemberStatus_HasResponded}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CampaignMemberStatuss.CampaignMemberStatus_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CampaignMemberStatuss.CampaignMemberStatus_IsDefault}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CampaignMemberStatuss.CampaignMemberStatus_IsDeleted}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CampaignMemberStatuss.CampaignMemberStatus_Label}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CampaignMemberStatuss.CampaignMemberStatus_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CampaignMemberStatuss.CampaignMemberStatus_SortOrder}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CampaignMemberStatuss.CampaignMemberStatus_SystemModstamp}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="CampaignMemberStatus"/>
							<VBox>					
							<Text text="{dataModel>CampaignMemberStatus_CreatedDate/CampaignMemberStatus_CreatedDate}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>CampaignMemberStatus_HasResponded/CampaignMemberStatus_HasResponded',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>CampaignMemberStatus_Id/CampaignMemberStatus_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>CampaignMemberStatus.CampaignMemberStatus_Id/CampaignMemberStatus_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>CampaignMemberStatus_IsDefault/CampaignMemberStatus_IsDefault',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>CampaignMemberStatus_IsDeleted/CampaignMemberStatus_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>CampaignMemberStatus_Label/CampaignMemberStatus_Label',type:'sap.ui.model.type.String'}" linkText="{i18n>CampaignMemberStatus.CampaignMemberStatus_Label/CampaignMemberStatus_Label}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>CampaignMemberStatus_LastModifiedDate/CampaignMemberStatus_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>CampaignMemberStatus_SortOrder/CampaignMemberStatus_SortOrder',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>CampaignMemberStatus_SystemModstamp/CampaignMemberStatus_SystemModstamp}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>