<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>CallCenters.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/CallCenter' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>CallCenters.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CallCenters.CallCenter_AdapterUrl}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CallCenters.CallCenter_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CallCenters.CallCenter_CustomSettings}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CallCenters.CallCenter_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CallCenters.CallCenter_InternalName}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CallCenters.CallCenter_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CallCenters.CallCenter_Name}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CallCenters.CallCenter_SystemModstamp}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CallCenters.CallCenter_Version}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="CallCenter"/>
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>CallCenter_AdapterUrl/CallCenter_AdapterUrl',type:'sap.ui.model.type.String'}" linkText="{i18n>CallCenter.CallCenter_AdapterUrl/CallCenter_AdapterUrl}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>CallCenter_CreatedDate/CallCenter_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>CallCenter_CustomSettings/CallCenter_CustomSettings',type:'sap.ui.model.type.String'}" linkText="{i18n>CallCenter.CallCenter_CustomSettings/CallCenter_CustomSettings}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>CallCenter_Id/CallCenter_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>CallCenter.CallCenter_Id/CallCenter_Id}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>CallCenter_InternalName/CallCenter_InternalName',type:'sap.ui.model.type.String'}" linkText="{i18n>CallCenter.CallCenter_InternalName/CallCenter_InternalName}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>CallCenter_LastModifiedDate/CallCenter_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>CallCenter_Name/CallCenter_Name',type:'sap.ui.model.type.String'}" linkText="{i18n>CallCenter.CallCenter_Name/CallCenter_Name}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>CallCenter_SystemModstamp/CallCenter_SystemModstamp}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>CallCenter_Version/CallCenter_Version',type:'sap.ui.model.type.Float', formatOptions:{'style':'short','maxFractionDigits':2} }"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>