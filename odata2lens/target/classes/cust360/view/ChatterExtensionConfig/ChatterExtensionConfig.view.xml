<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>ChatterExtensionConfig.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>ChatterExtensionConfig.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="ChatterExtensionConfig"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="ChatterExtensionConfig"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ChatterExtensionConfig.ChatterExtensionConfig_CanCreate}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>ChatterExtensionConfig_CanCreate/ChatterExtensionConfig_CanCreate',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ChatterExtensionConfig.ChatterExtensionConfig_CanRead}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>ChatterExtensionConfig_CanRead/ChatterExtensionConfig_CanRead',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ChatterExtensionConfig.ChatterExtensionConfig_ChatterExtensionId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ChatterExtensionConfig_ChatterExtensionId/sfdc_ChatterExtensionConfig_ChatterExtensionId" entitySet="ChatterExtension"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ChatterExtensionConfig.ChatterExtensionConfig_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ChatterExtensionConfig_CreatedById/sfdc_ChatterExtensionConfig_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ChatterExtensionConfig.ChatterExtensionConfig_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>ChatterExtensionConfig_CreatedDate/ChatterExtensionConfig_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ChatterExtensionConfig.ChatterExtensionConfig_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ChatterExtensionConfig_Id/ChatterExtensionConfig_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>ChatterExtensionConfig.ChatterExtensionConfig_Id/ChatterExtensionConfig_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ChatterExtensionConfig.ChatterExtensionConfig_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>ChatterExtensionConfig_IsDeleted/ChatterExtensionConfig_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ChatterExtensionConfig.ChatterExtensionConfig_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ChatterExtensionConfig_LastModifiedById/sfdc_ChatterExtensionConfig_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ChatterExtensionConfig.ChatterExtensionConfig_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>ChatterExtensionConfig_LastModifiedDate/ChatterExtensionConfig_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ChatterExtensionConfig.ChatterExtensionConfig_Position}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>ChatterExtensionConfig_Position/ChatterExtensionConfig_Position',type:'sap.ui.model.type.Integer' }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ChatterExtensionConfig.ChatterExtensionConfig_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>ChatterExtensionConfig_SystemModstamp/ChatterExtensionConfig_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ChatterExtensionConfig.ChatterExtensionConfig_ChatterExtensionId}" required="false"/> 
						<rdf:ListResource path="dataModel>ChatterExtensionConfig_ChatterExtensionId" entitySet="ChatterExtension"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ChatterExtensionConfig.ChatterExtensionConfig_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>ChatterExtensionConfig_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ChatterExtensionConfig.ChatterExtensionConfig_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>ChatterExtensionConfig_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>