<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>Assets.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/Asset' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>Assets.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Assets.Asset_AssetLevel}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Assets.Asset_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Assets.Asset_Description}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Assets.Asset_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Assets.Asset_InstallDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Assets.Asset_IsCompetitorProduct}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Assets.Asset_IsDeleted}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Assets.Asset_IsInternal}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Assets.Asset_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Assets.Asset_LastReferencedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Assets.Asset_LastViewedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Assets.Asset_Name}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Assets.Asset_Price}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Assets.Asset_ProductCode}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Assets.Asset_PurchaseDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Assets.Asset_Quantity}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Assets.Asset_SerialNumber}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Assets.Asset_Status}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Assets.Asset_StockKeepingUnit}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Assets.Asset_SystemModstamp}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Assets.Asset_UsageEndDate}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="Asset"/>
							<VBox>					
							<Text text="{path:'dataModel>Asset_AssetLevel/Asset_AssetLevel',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>Asset_CreatedDate/Asset_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Asset_Description/Asset_Description',type:'sap.ui.model.type.String'}" linkText="{i18n>Asset.Asset_Description/Asset_Description}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Asset_Id/Asset_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>Asset.Asset_Id/Asset_Id}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>Asset_InstallDate/Asset_InstallDate}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>Asset_IsCompetitorProduct/Asset_IsCompetitorProduct',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>Asset_IsDeleted/Asset_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>Asset_IsInternal/Asset_IsInternal',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<Text text="{dataModel>Asset_LastModifiedDate/Asset_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>Asset_LastReferencedDate/Asset_LastReferencedDate}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>Asset_LastViewedDate/Asset_LastViewedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Asset_Name/Asset_Name',type:'sap.ui.model.type.String'}" linkText="{i18n>Asset.Asset_Name/Asset_Name}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>Asset_Price/Asset_Price',type:'sap.ui.model.type.Float', formatOptions:{'style':'short','maxFractionDigits':2} }"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Asset_ProductCode/Asset_ProductCode',type:'sap.ui.model.type.String'}" linkText="{i18n>Asset.Asset_ProductCode/Asset_ProductCode}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>Asset_PurchaseDate/Asset_PurchaseDate}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>Asset_Quantity/Asset_Quantity',type:'sap.ui.model.type.Float', formatOptions:{'style':'short','maxFractionDigits':2} }"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Asset_SerialNumber/Asset_SerialNumber',type:'sap.ui.model.type.String'}" linkText="{i18n>Asset.Asset_SerialNumber/Asset_SerialNumber}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Asset_Status/Asset_Status',type:'sap.ui.model.type.String'}" linkText="{i18n>Asset.Asset_Status/Asset_Status}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Asset_StockKeepingUnit/Asset_StockKeepingUnit',type:'sap.ui.model.type.String'}" linkText="{i18n>Asset.Asset_StockKeepingUnit/Asset_StockKeepingUnit}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>Asset_SystemModstamp/Asset_SystemModstamp}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>Asset_UsageEndDate/Asset_UsageEndDate}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>