<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>SocialPersonaHistory.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>SocialPersonaHistory.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="SocialPersonaHistory"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="SocialPersonaHistory"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SocialPersonaHistory.SocialPersonaHistory_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>SocialPersonaHistory_CreatedById/sfdc_SocialPersonaHistory_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SocialPersonaHistory.SocialPersonaHistory_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>SocialPersonaHistory_CreatedDate/SocialPersonaHistory_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SocialPersonaHistory.SocialPersonaHistory_Field}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>SocialPersonaHistory_Field/SocialPersonaHistory_Field',type:'sap.ui.model.type.String' }" linkText="{i18n>SocialPersonaHistory.SocialPersonaHistory_Field/SocialPersonaHistory_Field}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SocialPersonaHistory.SocialPersonaHistory_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>SocialPersonaHistory_Id/SocialPersonaHistory_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>SocialPersonaHistory.SocialPersonaHistory_Id/SocialPersonaHistory_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SocialPersonaHistory.SocialPersonaHistory_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>SocialPersonaHistory_IsDeleted/SocialPersonaHistory_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SocialPersonaHistory.SocialPersonaHistory_NewValue}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>SocialPersonaHistory_NewValue/SocialPersonaHistory_NewValue',type:'sap.ui.model.type.String' }" linkText="{i18n>SocialPersonaHistory.SocialPersonaHistory_NewValue/SocialPersonaHistory_NewValue}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SocialPersonaHistory.SocialPersonaHistory_OldValue}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>SocialPersonaHistory_OldValue/SocialPersonaHistory_OldValue',type:'sap.ui.model.type.String' }" linkText="{i18n>SocialPersonaHistory.SocialPersonaHistory_OldValue/SocialPersonaHistory_OldValue}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SocialPersonaHistory.SocialPersonaHistory_SocialPersonaId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>SocialPersonaHistory_SocialPersonaId/sfdc_SocialPersonaHistory_SocialPersonaId" entitySet="SocialPersona"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SocialPersonaHistory.SocialPersonaHistory_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>SocialPersonaHistory_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SocialPersonaHistory.SocialPersonaHistory_SocialPersonaId}" required="false"/> 
						<rdf:ListResource path="dataModel>SocialPersonaHistory_SocialPersonaId" entitySet="SocialPersona"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>