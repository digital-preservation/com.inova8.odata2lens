<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>EmbeddedServiceDetails.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/EmbeddedServiceDetail' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>EmbeddedServiceDetails.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EmbeddedServiceDetails.EmbeddedServiceDetail_ContrastPrimaryColor}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EmbeddedServiceDetails.EmbeddedServiceDetail_DurableId}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EmbeddedServiceDetails.EmbeddedServiceDetail_FlowDeveloperName}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EmbeddedServiceDetails.EmbeddedServiceDetail_Font}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EmbeddedServiceDetails.EmbeddedServiceDetail_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EmbeddedServiceDetails.EmbeddedServiceDetail_IsPrechatEnabled}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EmbeddedServiceDetails.EmbeddedServiceDetail_NavBarColor}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EmbeddedServiceDetails.EmbeddedServiceDetail_PrimaryColor}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EmbeddedServiceDetails.EmbeddedServiceDetail_SecondaryColor}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EmbeddedServiceDetails.EmbeddedServiceDetail_Site}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="EmbeddedServiceDetail"/>
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>EmbeddedServiceDetail_ContrastPrimaryColor/EmbeddedServiceDetail_ContrastPrimaryColor',type:'sap.ui.model.type.String'}" linkText="{i18n>EmbeddedServiceDetail.EmbeddedServiceDetail_ContrastPrimaryColor/EmbeddedServiceDetail_ContrastPrimaryColor}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>EmbeddedServiceDetail_DurableId/EmbeddedServiceDetail_DurableId',type:'sap.ui.model.type.String'}" linkText="{i18n>EmbeddedServiceDetail.EmbeddedServiceDetail_DurableId/EmbeddedServiceDetail_DurableId}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>EmbeddedServiceDetail_FlowDeveloperName/EmbeddedServiceDetail_FlowDeveloperName',type:'sap.ui.model.type.String'}" linkText="{i18n>EmbeddedServiceDetail.EmbeddedServiceDetail_FlowDeveloperName/EmbeddedServiceDetail_FlowDeveloperName}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>EmbeddedServiceDetail_Font/EmbeddedServiceDetail_Font',type:'sap.ui.model.type.String'}" linkText="{i18n>EmbeddedServiceDetail.EmbeddedServiceDetail_Font/EmbeddedServiceDetail_Font}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>EmbeddedServiceDetail_Id/EmbeddedServiceDetail_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>EmbeddedServiceDetail.EmbeddedServiceDetail_Id/EmbeddedServiceDetail_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>EmbeddedServiceDetail_IsPrechatEnabled/EmbeddedServiceDetail_IsPrechatEnabled',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>EmbeddedServiceDetail_NavBarColor/EmbeddedServiceDetail_NavBarColor',type:'sap.ui.model.type.String'}" linkText="{i18n>EmbeddedServiceDetail.EmbeddedServiceDetail_NavBarColor/EmbeddedServiceDetail_NavBarColor}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>EmbeddedServiceDetail_PrimaryColor/EmbeddedServiceDetail_PrimaryColor',type:'sap.ui.model.type.String'}" linkText="{i18n>EmbeddedServiceDetail.EmbeddedServiceDetail_PrimaryColor/EmbeddedServiceDetail_PrimaryColor}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>EmbeddedServiceDetail_SecondaryColor/EmbeddedServiceDetail_SecondaryColor',type:'sap.ui.model.type.String'}" linkText="{i18n>EmbeddedServiceDetail.EmbeddedServiceDetail_SecondaryColor/EmbeddedServiceDetail_SecondaryColor}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>EmbeddedServiceDetail_Site/EmbeddedServiceDetail_Site',type:'sap.ui.model.type.String'}" linkText="{i18n>EmbeddedServiceDetail.EmbeddedServiceDetail_Site/EmbeddedServiceDetail_Site}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>