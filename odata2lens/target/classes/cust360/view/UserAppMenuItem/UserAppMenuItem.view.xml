<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>UserAppMenuItem.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>UserAppMenuItem.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="UserAppMenuItem"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="UserAppMenuItem"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserAppMenuItem.UserAppMenuItem_AppMenuItemId}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>UserAppMenuItem_AppMenuItemId/UserAppMenuItem_AppMenuItemId',type:'sap.ui.model.type.String' }" linkText="{i18n>UserAppMenuItem.UserAppMenuItem_AppMenuItemId/UserAppMenuItem_AppMenuItemId}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserAppMenuItem.UserAppMenuItem_ApplicationId}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>UserAppMenuItem_ApplicationId/UserAppMenuItem_ApplicationId',type:'sap.ui.model.type.String' }" linkText="{i18n>UserAppMenuItem.UserAppMenuItem_ApplicationId/UserAppMenuItem_ApplicationId}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserAppMenuItem.UserAppMenuItem_Description}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>UserAppMenuItem_Description/UserAppMenuItem_Description',type:'sap.ui.model.type.String' }" linkText="{i18n>UserAppMenuItem.UserAppMenuItem_Description/UserAppMenuItem_Description}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserAppMenuItem.UserAppMenuItem_IconUrl}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>UserAppMenuItem_IconUrl/UserAppMenuItem_IconUrl',type:'sap.ui.model.type.String' }" linkText="{i18n>UserAppMenuItem.UserAppMenuItem_IconUrl/UserAppMenuItem_IconUrl}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserAppMenuItem.UserAppMenuItem_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>UserAppMenuItem_Id/UserAppMenuItem_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>UserAppMenuItem.UserAppMenuItem_Id/UserAppMenuItem_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserAppMenuItem.UserAppMenuItem_InfoUrl}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>UserAppMenuItem_InfoUrl/UserAppMenuItem_InfoUrl',type:'sap.ui.model.type.String' }" linkText="{i18n>UserAppMenuItem.UserAppMenuItem_InfoUrl/UserAppMenuItem_InfoUrl}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserAppMenuItem.UserAppMenuItem_IsUsingAdminAuthorization}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>UserAppMenuItem_IsUsingAdminAuthorization/UserAppMenuItem_IsUsingAdminAuthorization',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserAppMenuItem.UserAppMenuItem_IsVisible}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>UserAppMenuItem_IsVisible/UserAppMenuItem_IsVisible',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserAppMenuItem.UserAppMenuItem_Label}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>UserAppMenuItem_Label/UserAppMenuItem_Label',type:'sap.ui.model.type.String' }" linkText="{i18n>UserAppMenuItem.UserAppMenuItem_Label/UserAppMenuItem_Label}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserAppMenuItem.UserAppMenuItem_LogoUrl}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>UserAppMenuItem_LogoUrl/UserAppMenuItem_LogoUrl',type:'sap.ui.model.type.String' }" linkText="{i18n>UserAppMenuItem.UserAppMenuItem_LogoUrl/UserAppMenuItem_LogoUrl}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserAppMenuItem.UserAppMenuItem_MobileStartUrl}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>UserAppMenuItem_MobileStartUrl/UserAppMenuItem_MobileStartUrl',type:'sap.ui.model.type.String' }" linkText="{i18n>UserAppMenuItem.UserAppMenuItem_MobileStartUrl/UserAppMenuItem_MobileStartUrl}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserAppMenuItem.UserAppMenuItem_Name}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>UserAppMenuItem_Name/UserAppMenuItem_Name',type:'sap.ui.model.type.String' }" linkText="{i18n>UserAppMenuItem.UserAppMenuItem_Name/UserAppMenuItem_Name}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserAppMenuItem.UserAppMenuItem_SortOrder}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>UserAppMenuItem_SortOrder/UserAppMenuItem_SortOrder',type:'sap.ui.model.type.Integer' }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserAppMenuItem.UserAppMenuItem_StartUrl}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>UserAppMenuItem_StartUrl/UserAppMenuItem_StartUrl',type:'sap.ui.model.type.String' }" linkText="{i18n>UserAppMenuItem.UserAppMenuItem_StartUrl/UserAppMenuItem_StartUrl}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserAppMenuItem.UserAppMenuItem_Type}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>UserAppMenuItem_Type/UserAppMenuItem_Type',type:'sap.ui.model.type.String' }" linkText="{i18n>UserAppMenuItem.UserAppMenuItem_Type/UserAppMenuItem_Type}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserAppMenuItem.UserAppMenuItem_UserSortOrder}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>UserAppMenuItem_UserSortOrder/UserAppMenuItem_UserSortOrder',type:'sap.ui.model.type.Integer' }"/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>