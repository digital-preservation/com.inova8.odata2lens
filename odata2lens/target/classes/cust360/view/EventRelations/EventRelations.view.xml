<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>EventRelations.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/EventRelation' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>EventRelations.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EventRelations.EventRelation_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EventRelations.EventRelation_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EventRelations.EventRelation_IsDeleted}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EventRelations.EventRelation_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EventRelations.EventRelation_RespondedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EventRelations.EventRelation_Response}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EventRelations.EventRelation_Status}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EventRelations.EventRelation_SystemModstamp}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="EventRelation"/>
							<VBox>					
							<Text text="{dataModel>EventRelation_CreatedDate/EventRelation_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>EventRelation_Id/EventRelation_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>EventRelation.EventRelation_Id/EventRelation_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>EventRelation_IsDeleted/EventRelation_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<Text text="{dataModel>EventRelation_LastModifiedDate/EventRelation_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>EventRelation_RespondedDate/EventRelation_RespondedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>EventRelation_Response/EventRelation_Response',type:'sap.ui.model.type.String'}" linkText="{i18n>EventRelation.EventRelation_Response/EventRelation_Response}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>EventRelation_Status/EventRelation_Status',type:'sap.ui.model.type.String'}" linkText="{i18n>EventRelation.EventRelation_Status/EventRelation_Status}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>EventRelation_SystemModstamp/EventRelation_SystemModstamp}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>