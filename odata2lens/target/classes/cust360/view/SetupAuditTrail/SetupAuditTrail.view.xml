<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>SetupAuditTrail.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>SetupAuditTrail.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="SetupAuditTrail"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="SetupAuditTrail"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SetupAuditTrail.SetupAuditTrail_Action}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>SetupAuditTrail_Action/SetupAuditTrail_Action',type:'sap.ui.model.type.String' }" linkText="{i18n>SetupAuditTrail.SetupAuditTrail_Action/SetupAuditTrail_Action}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SetupAuditTrail.SetupAuditTrail_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>SetupAuditTrail_CreatedById/sfdc_SetupAuditTrail_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SetupAuditTrail.SetupAuditTrail_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>SetupAuditTrail_CreatedDate/SetupAuditTrail_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SetupAuditTrail.SetupAuditTrail_DelegateUser}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>SetupAuditTrail_DelegateUser/SetupAuditTrail_DelegateUser',type:'sap.ui.model.type.String' }" linkText="{i18n>SetupAuditTrail.SetupAuditTrail_DelegateUser/SetupAuditTrail_DelegateUser}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SetupAuditTrail.SetupAuditTrail_Display}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>SetupAuditTrail_Display/SetupAuditTrail_Display',type:'sap.ui.model.type.String' }" linkText="{i18n>SetupAuditTrail.SetupAuditTrail_Display/SetupAuditTrail_Display}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SetupAuditTrail.SetupAuditTrail_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>SetupAuditTrail_Id/SetupAuditTrail_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>SetupAuditTrail.SetupAuditTrail_Id/SetupAuditTrail_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SetupAuditTrail.SetupAuditTrail_ResponsibleNamespacePrefix}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>SetupAuditTrail_ResponsibleNamespacePrefix/SetupAuditTrail_ResponsibleNamespacePrefix',type:'sap.ui.model.type.String' }" linkText="{i18n>SetupAuditTrail.SetupAuditTrail_ResponsibleNamespacePrefix/SetupAuditTrail_ResponsibleNamespacePrefix}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SetupAuditTrail.SetupAuditTrail_Section}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>SetupAuditTrail_Section/SetupAuditTrail_Section',type:'sap.ui.model.type.String' }" linkText="{i18n>SetupAuditTrail.SetupAuditTrail_Section/SetupAuditTrail_Section}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SetupAuditTrail.SetupAuditTrail_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>SetupAuditTrail_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>