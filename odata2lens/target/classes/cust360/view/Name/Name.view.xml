<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>Name.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>Name.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="Name"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="Name"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Name.Name_Alias}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>Name_Alias/Name_Alias',type:'sap.ui.model.type.String' }" linkText="{i18n>Name.Name_Alias/Name_Alias}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Name.Name_Email}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>Name_Email/Name_Email',type:'sap.ui.model.type.String' }" linkText="{i18n>Name.Name_Email/Name_Email}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Name.Name_FirstName}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>Name_FirstName/Name_FirstName',type:'sap.ui.model.type.String' }" linkText="{i18n>Name.Name_FirstName/Name_FirstName}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Name.Name_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>Name_Id/Name_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>Name.Name_Id/Name_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Name.Name_IsActive}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>Name_IsActive/Name_IsActive',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Name.Name_LastName}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>Name_LastName/Name_LastName',type:'sap.ui.model.type.String' }" linkText="{i18n>Name.Name_LastName/Name_LastName}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Name.Name_LastReferencedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>Name_LastReferencedDate/Name_LastReferencedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Name.Name_LastViewedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>Name_LastViewedDate/Name_LastViewedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Name.Name_Name}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>Name_Name/Name_Name',type:'sap.ui.model.type.String' }" linkText="{i18n>Name.Name_Name/Name_Name}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Name.Name_NameOrAlias}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>Name_NameOrAlias/Name_NameOrAlias',type:'sap.ui.model.type.String' }" linkText="{i18n>Name.Name_NameOrAlias/Name_NameOrAlias}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Name.Name_Phone}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>Name_Phone/Name_Phone',type:'sap.ui.model.type.String' }" linkText="{i18n>Name.Name_Phone/Name_Phone}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Name.Name_ProfileId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>Name_ProfileId/sfdc_Name_ProfileId" entitySet="Profile"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Name.Name_RecordTypeId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>Name_RecordTypeId/sfdc_Name_RecordTypeId" entitySet="RecordType"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Name.Name_Title}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>Name_Title/Name_Title',type:'sap.ui.model.type.String' }" linkText="{i18n>Name.Name_Title/Name_Title}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Name.Name_Type}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>Name_Type/Name_Type',type:'sap.ui.model.type.String' }" linkText="{i18n>Name.Name_Type/Name_Type}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Name.Name_UserRoleId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>Name_UserRoleId/sfdc_Name_UserRoleId" entitySet="UserRole"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Name.Name_Username}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>Name_Username/Name_Username',type:'sap.ui.model.type.String' }" linkText="{i18n>Name.Name_Username/Name_Username}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Name.Name_ProfileId}" required="false"/> 
						<rdf:ListResource path="dataModel>Name_ProfileId" entitySet="Profile"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Name.Name_RecordTypeId}" required="false"/> 
						<rdf:ListResource path="dataModel>Name_RecordTypeId" entitySet="RecordType"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Name.Name_UserRoleId}" required="false"/> 
						<rdf:ListResource path="dataModel>Name_UserRoleId" entitySet="UserRole"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>