<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>AttachedContentNotes.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/AttachedContentNote' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>AttachedContentNotes.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AttachedContentNotes.AttachedContentNote_ContentSize}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AttachedContentNotes.AttachedContentNote_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AttachedContentNotes.AttachedContentNote_FileExtension}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AttachedContentNotes.AttachedContentNote_FileType}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AttachedContentNotes.AttachedContentNote_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AttachedContentNotes.AttachedContentNote_IsDeleted}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AttachedContentNotes.AttachedContentNote_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AttachedContentNotes.AttachedContentNote_TextPreview}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AttachedContentNotes.AttachedContentNote_Title}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="AttachedContentNote"/>
							<VBox>					
							<Text text="{path:'dataModel>AttachedContentNote_ContentSize/AttachedContentNote_ContentSize',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>AttachedContentNote_CreatedDate/AttachedContentNote_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>AttachedContentNote_FileExtension/AttachedContentNote_FileExtension',type:'sap.ui.model.type.String'}" linkText="{i18n>AttachedContentNote.AttachedContentNote_FileExtension/AttachedContentNote_FileExtension}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>AttachedContentNote_FileType/AttachedContentNote_FileType',type:'sap.ui.model.type.String'}" linkText="{i18n>AttachedContentNote.AttachedContentNote_FileType/AttachedContentNote_FileType}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>AttachedContentNote_Id/AttachedContentNote_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>AttachedContentNote.AttachedContentNote_Id/AttachedContentNote_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>AttachedContentNote_IsDeleted/AttachedContentNote_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<Text text="{dataModel>AttachedContentNote_LastModifiedDate/AttachedContentNote_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>AttachedContentNote_TextPreview/AttachedContentNote_TextPreview',type:'sap.ui.model.type.String'}" linkText="{i18n>AttachedContentNote.AttachedContentNote_TextPreview/AttachedContentNote_TextPreview}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>AttachedContentNote_Title/AttachedContentNote_Title',type:'sap.ui.model.type.String'}" linkText="{i18n>AttachedContentNote.AttachedContentNote_Title/AttachedContentNote_Title}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>