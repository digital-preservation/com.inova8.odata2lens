<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>ObjectPermissions.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>ObjectPermissions.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="ObjectPermissions"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="ObjectPermissions"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ObjectPermissions.ObjectPermissions_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ObjectPermissions_CreatedById/sfdc_ObjectPermissions_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ObjectPermissions.ObjectPermissions_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>ObjectPermissions_CreatedDate/ObjectPermissions_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ObjectPermissions.ObjectPermissions_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ObjectPermissions_Id/ObjectPermissions_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>ObjectPermissions.ObjectPermissions_Id/ObjectPermissions_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ObjectPermissions.ObjectPermissions_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ObjectPermissions_LastModifiedById/sfdc_ObjectPermissions_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ObjectPermissions.ObjectPermissions_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>ObjectPermissions_LastModifiedDate/ObjectPermissions_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ObjectPermissions.ObjectPermissions_ParentId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ObjectPermissions_ParentId/sfdc_ObjectPermissions_ParentId" entitySet="PermissionSet"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ObjectPermissions.ObjectPermissions_PermissionsCreate}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>ObjectPermissions_PermissionsCreate/ObjectPermissions_PermissionsCreate',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ObjectPermissions.ObjectPermissions_PermissionsDelete}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>ObjectPermissions_PermissionsDelete/ObjectPermissions_PermissionsDelete',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ObjectPermissions.ObjectPermissions_PermissionsEdit}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>ObjectPermissions_PermissionsEdit/ObjectPermissions_PermissionsEdit',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ObjectPermissions.ObjectPermissions_PermissionsModifyAllRecords}" required="false"/> 
						<Text text=""/>
