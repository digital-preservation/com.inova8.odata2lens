						<Text text=""/>
						<Text text="{path:'dataModel>ObjectPermissions_PermissionsRead/ObjectPermissions_PermissionsRead',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ObjectPermissions.ObjectPermissions_PermissionsViewAllRecords}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>ObjectPermissions_PermissionsViewAllRecords/ObjectPermissions_PermissionsViewAllRecords',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ObjectPermissions.ObjectPermissions_SobjectType}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ObjectPermissions_SobjectType/ObjectPermissions_SobjectType',type:'sap.ui.model.type.String' }" linkText="{i18n>ObjectPermissions.ObjectPermissions_SobjectType/ObjectPermissions_SobjectType}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ObjectPermissions.ObjectPermissions_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>ObjectPermissions_SystemModstamp/ObjectPermissions_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ObjectPermissions.ObjectPermissions_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>ObjectPermissions_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ObjectPermissions.ObjectPermissions_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>ObjectPermissions_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ObjectPermissions.ObjectPermissions_ParentId}" required="false"/> 
						<rdf:ListResource path="dataModel>ObjectPermissions_ParentId" entitySet="PermissionSet"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
