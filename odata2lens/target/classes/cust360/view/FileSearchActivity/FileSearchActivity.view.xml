<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>FileSearchActivity.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>FileSearchActivity.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="FileSearchActivity"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="FileSearchActivity"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>FileSearchActivity.FileSearchActivity_AvgNumResults}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>FileSearchActivity_AvgNumResults/FileSearchActivity_AvgNumResults',type:'sap.ui.model.type.Float', formatOptions:{'style':'short','maxFractionDigits':2} }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>FileSearchActivity.FileSearchActivity_ClickRank}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>FileSearchActivity_ClickRank/FileSearchActivity_ClickRank',type:'sap.ui.model.type.Float', formatOptions:{'style':'short','maxFractionDigits':2} }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>FileSearchActivity.FileSearchActivity_CountQueries}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>FileSearchActivity_CountQueries/FileSearchActivity_CountQueries',type:'sap.ui.model.type.Integer' }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>FileSearchActivity.FileSearchActivity_CountUsers}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>FileSearchActivity_CountUsers/FileSearchActivity_CountUsers',type:'sap.ui.model.type.Integer' }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>FileSearchActivity.FileSearchActivity_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>FileSearchActivity_CreatedById/sfdc_FileSearchActivity_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>FileSearchActivity.FileSearchActivity_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>FileSearchActivity_CreatedDate/FileSearchActivity_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>FileSearchActivity.FileSearchActivity_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>FileSearchActivity_Id/FileSearchActivity_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>FileSearchActivity.FileSearchActivity_Id/FileSearchActivity_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>FileSearchActivity.FileSearchActivity_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>FileSearchActivity_IsDeleted/FileSearchActivity_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>FileSearchActivity.FileSearchActivity_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>FileSearchActivity_LastModifiedById/sfdc_FileSearchActivity_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>FileSearchActivity.FileSearchActivity_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>FileSearchActivity_LastModifiedDate/FileSearchActivity_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>FileSearchActivity.FileSearchActivity_Name}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>FileSearchActivity_Name/FileSearchActivity_Name',type:'sap.ui.model.type.String' }" linkText="{i18n>FileSearchActivity.FileSearchActivity_Name/FileSearchActivity_Name}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>FileSearchActivity.FileSearchActivity_Period}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>FileSearchActivity_Period/FileSearchActivity_Period',type:'sap.ui.model.type.String' }" linkText="{i18n>FileSearchActivity.FileSearchActivity_Period/FileSearchActivity_Period}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>FileSearchActivity.FileSearchActivity_QueryDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>FileSearchActivity_QueryDate/FileSearchActivity_QueryDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>FileSearchActivity.FileSearchActivity_QueryLanguage}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>FileSearchActivity_QueryLanguage/FileSearchActivity_QueryLanguage',type:'sap.ui.model.type.String' }" linkText="{i18n>FileSearchActivity.FileSearchActivity_QueryLanguage/FileSearchActivity_QueryLanguage}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>FileSearchActivity.FileSearchActivity_SearchTerm}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>FileSearchActivity_SearchTerm/FileSearchActivity_SearchTerm',type:'sap.ui.model.type.String' }" linkText="{i18n>FileSearchActivity.FileSearchActivity_SearchTerm/FileSearchActivity_SearchTerm}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>FileSearchActivity.FileSearchActivity_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>FileSearchActivity_SystemModstamp/FileSearchActivity_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>FileSearchActivity.FileSearchActivity_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>FileSearchActivity_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>FileSearchActivity.FileSearchActivity_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>FileSearchActivity_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>