<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>SignupRequestFeeds.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/SignupRequestFeed' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>SignupRequestFeeds.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>SignupRequestFeeds.SignupRequestFeed_Body}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>SignupRequestFeeds.SignupRequestFeed_CommentCount}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>SignupRequestFeeds.SignupRequestFeed_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>SignupRequestFeeds.SignupRequestFeed_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>SignupRequestFeeds.SignupRequestFeed_IsDeleted}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>SignupRequestFeeds.SignupRequestFeed_IsRichText}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>SignupRequestFeeds.SignupRequestFeed_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>SignupRequestFeeds.SignupRequestFeed_LikeCount}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>SignupRequestFeeds.SignupRequestFeed_LinkUrl}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>SignupRequestFeeds.SignupRequestFeed_SystemModstamp}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>SignupRequestFeeds.SignupRequestFeed_Title}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>SignupRequestFeeds.SignupRequestFeed_Type}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="SignupRequestFeed"/>
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>SignupRequestFeed_Body/SignupRequestFeed_Body',type:'sap.ui.model.type.String'}" linkText="{i18n>SignupRequestFeed.SignupRequestFeed_Body/SignupRequestFeed_Body}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>SignupRequestFeed_CommentCount/SignupRequestFeed_CommentCount',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>SignupRequestFeed_CreatedDate/SignupRequestFeed_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>SignupRequestFeed_Id/SignupRequestFeed_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>SignupRequestFeed.SignupRequestFeed_Id/SignupRequestFeed_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>SignupRequestFeed_IsDeleted/SignupRequestFeed_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>SignupRequestFeed_IsRichText/SignupRequestFeed_IsRichText',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<Text text="{dataModel>SignupRequestFeed_LastModifiedDate/SignupRequestFeed_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>SignupRequestFeed_LikeCount/SignupRequestFeed_LikeCount',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>SignupRequestFeed_LinkUrl/SignupRequestFeed_LinkUrl',type:'sap.ui.model.type.String'}" linkText="{i18n>SignupRequestFeed.SignupRequestFeed_LinkUrl/SignupRequestFeed_LinkUrl}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>SignupRequestFeed_SystemModstamp/SignupRequestFeed_SystemModstamp}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>SignupRequestFeed_Title/SignupRequestFeed_Title',type:'sap.ui.model.type.String'}" linkText="{i18n>SignupRequestFeed.SignupRequestFeed_Title/SignupRequestFeed_Title}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>SignupRequestFeed_Type/SignupRequestFeed_Type',type:'sap.ui.model.type.String'}" linkText="{i18n>SignupRequestFeed.SignupRequestFeed_Type/SignupRequestFeed_Type}"/>
							</VBox>							
