<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>Note.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>Note.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="Note"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="Note"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Note.Note_Body}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>Note_Body/Note_Body',type:'sap.ui.model.type.String' }" linkText="{i18n>Note.Note_Body/Note_Body}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Note.Note_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>Note_CreatedById/sfdc_Note_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Note.Note_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>Note_CreatedDate/Note_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Note.Note_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>Note_Id/Note_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>Note.Note_Id/Note_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Note.Note_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>Note_IsDeleted/Note_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Note.Note_IsPrivate}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>Note_IsPrivate/Note_IsPrivate',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Note.Note_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>Note_LastModifiedById/sfdc_Note_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Note.Note_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>Note_LastModifiedDate/Note_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Note.Note_OwnerId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>Note_OwnerId/sfdc_Note_OwnerId" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Note.Note_ParentId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>Note_ParentId/sfdc_Note_ParentId" entitySet="Account"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Note.Note_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>Note_SystemModstamp/Note_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Note.Note_Title}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>Note_Title/Note_Title',type:'sap.ui.model.type.String' }" linkText="{i18n>Note.Note_Title/Note_Title}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Note.Note_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>Note_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Note.Note_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>Note_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Note.Note_OwnerId}" required="false"/> 
						<rdf:ListResource path="dataModel>Note_OwnerId" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Note.Note_ParentId}" required="false"/> 
						<rdf:ListResource path="dataModel>Note_ParentId" entitySet="Account"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>