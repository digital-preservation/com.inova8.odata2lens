<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>ListViewChart.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>ListViewChart.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="ListViewChart"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="ListViewChart"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ListViewChart.ListViewChartInstance_ListViewChartId_Inverse}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ListViewChartInstance_ListViewChartId_Inverse/sfdc_ListViewChartInstance_ListViewChartId_Inverse" entitySet="ListViewChartInstance"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ListViewChart.ListViewChart_AggregateField}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ListViewChart_AggregateField/ListViewChart_AggregateField',type:'sap.ui.model.type.String' }" linkText="{i18n>ListViewChart.ListViewChart_AggregateField/ListViewChart_AggregateField}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ListViewChart.ListViewChart_AggregateType}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ListViewChart_AggregateType/ListViewChart_AggregateType',type:'sap.ui.model.type.String' }" linkText="{i18n>ListViewChart.ListViewChart_AggregateType/ListViewChart_AggregateType}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ListViewChart.ListViewChart_ChartType}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ListViewChart_ChartType/ListViewChart_ChartType',type:'sap.ui.model.type.String' }" linkText="{i18n>ListViewChart.ListViewChart_ChartType/ListViewChart_ChartType}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ListViewChart.ListViewChart_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ListViewChart_CreatedById/sfdc_ListViewChart_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ListViewChart.ListViewChart_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>ListViewChart_CreatedDate/ListViewChart_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ListViewChart.ListViewChart_DeveloperName}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ListViewChart_DeveloperName/ListViewChart_DeveloperName',type:'sap.ui.model.type.String' }" linkText="{i18n>ListViewChart.ListViewChart_DeveloperName/ListViewChart_DeveloperName}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ListViewChart.ListViewChart_GroupingField}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ListViewChart_GroupingField/ListViewChart_GroupingField',type:'sap.ui.model.type.String' }" linkText="{i18n>ListViewChart.ListViewChart_GroupingField/ListViewChart_GroupingField}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ListViewChart.ListViewChart_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ListViewChart_Id/ListViewChart_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>ListViewChart.ListViewChart_Id/ListViewChart_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ListViewChart.ListViewChart_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>ListViewChart_IsDeleted/ListViewChart_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ListViewChart.ListViewChart_Language}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ListViewChart_Language/ListViewChart_Language',type:'sap.ui.model.type.String' }" linkText="{i18n>ListViewChart.ListViewChart_Language/ListViewChart_Language}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ListViewChart.ListViewChart_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ListViewChart_LastModifiedById/sfdc_ListViewChart_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ListViewChart.ListViewChart_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>ListViewChart_LastModifiedDate/ListViewChart_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ListViewChart.ListViewChart_MasterLabel}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ListViewChart_MasterLabel/ListViewChart_MasterLabel',type:'sap.ui.model.type.String' }" linkText="{i18n>ListViewChart.ListViewChart_MasterLabel/ListViewChart_MasterLabel}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ListViewChart.ListViewChart_OwnerId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ListViewChart_OwnerId/sfdc_ListViewChart_OwnerId" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ListViewChart.ListViewChart_SobjectType}" required="false"/> 
