					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ListViewChart.ListViewChart_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>ListViewChart_SystemModstamp/ListViewChart_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ListViewChart.ListViewChartInstance_ListViewChartId_Inverse}" required="false"/> 
						<rdf:ListResource path="dataModel>ListViewChartInstance_ListViewChartId_Inverse" entitySet="ListViewChartInstance"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ListViewChart.ListViewChart_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>ListViewChart_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
