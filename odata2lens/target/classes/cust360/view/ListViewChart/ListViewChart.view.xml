						<rdf:ListResource path="dataModel>ListViewChart_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ListViewChart.ListViewChart_OwnerId}" required="false"/> 
						<rdf:ListResource path="dataModel>ListViewChart_OwnerId" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>