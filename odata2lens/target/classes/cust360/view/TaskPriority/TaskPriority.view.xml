<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>TaskPriority.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>TaskPriority.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="TaskPriority"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="TaskPriority"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TaskPriority.TaskPriority_ApiName}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>TaskPriority_ApiName/TaskPriority_ApiName',type:'sap.ui.model.type.String' }" linkText="{i18n>TaskPriority.TaskPriority_ApiName/TaskPriority_ApiName}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TaskPriority.TaskPriority_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>TaskPriority_CreatedById/sfdc_TaskPriority_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TaskPriority.TaskPriority_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>TaskPriority_CreatedDate/TaskPriority_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TaskPriority.TaskPriority_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>TaskPriority_Id/TaskPriority_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>TaskPriority.TaskPriority_Id/TaskPriority_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TaskPriority.TaskPriority_IsDefault}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>TaskPriority_IsDefault/TaskPriority_IsDefault',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TaskPriority.TaskPriority_IsHighPriority}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>TaskPriority_IsHighPriority/TaskPriority_IsHighPriority',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TaskPriority.TaskPriority_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>TaskPriority_LastModifiedById/sfdc_TaskPriority_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TaskPriority.TaskPriority_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>TaskPriority_LastModifiedDate/TaskPriority_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TaskPriority.TaskPriority_MasterLabel}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>TaskPriority_MasterLabel/TaskPriority_MasterLabel',type:'sap.ui.model.type.String' }" linkText="{i18n>TaskPriority.TaskPriority_MasterLabel/TaskPriority_MasterLabel}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TaskPriority.TaskPriority_SortOrder}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>TaskPriority_SortOrder/TaskPriority_SortOrder',type:'sap.ui.model.type.Integer' }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TaskPriority.TaskPriority_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>TaskPriority_SystemModstamp/TaskPriority_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TaskPriority.TaskPriority_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>TaskPriority_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TaskPriority.TaskPriority_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>TaskPriority_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>