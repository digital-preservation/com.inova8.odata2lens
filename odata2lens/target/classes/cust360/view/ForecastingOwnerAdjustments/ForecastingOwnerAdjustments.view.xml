<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>ForecastingOwnerAdjustments.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/ForecastingOwnerAdjustment' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>ForecastingOwnerAdjustments.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ForecastingOwnerAdjustments.ForecastingOwnerAdjustment_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ForecastingOwnerAdjustments.ForecastingOwnerAdjustment_ForecastCategoryName}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ForecastingOwnerAdjustments.ForecastingOwnerAdjustment_ForecastingItemCategory}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ForecastingOwnerAdjustments.ForecastingOwnerAdjustment_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ForecastingOwnerAdjustments.ForecastingOwnerAdjustment_IsAmount}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ForecastingOwnerAdjustments.ForecastingOwnerAdjustment_IsDeleted}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ForecastingOwnerAdjustments.ForecastingOwnerAdjustment_IsQuantity}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ForecastingOwnerAdjustments.ForecastingOwnerAdjustment_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ForecastingOwnerAdjustments.ForecastingOwnerAdjustment_OwnerAdjustedAmount}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ForecastingOwnerAdjustments.ForecastingOwnerAdjustment_OwnerAdjustedQuantity}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ForecastingOwnerAdjustments.ForecastingOwnerAdjustment_OwnerAdjustmentNote}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ForecastingOwnerAdjustments.ForecastingOwnerAdjustment_ProductFamily}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ForecastingOwnerAdjustments.ForecastingOwnerAdjustment_StartDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ForecastingOwnerAdjustments.ForecastingOwnerAdjustment_SystemModstamp}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="ForecastingOwnerAdjustment"/>
							<VBox>					
							<Text text="{dataModel>ForecastingOwnerAdjustment_CreatedDate/ForecastingOwnerAdjustment_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ForecastingOwnerAdjustment_ForecastCategoryName/ForecastingOwnerAdjustment_ForecastCategoryName',type:'sap.ui.model.type.String'}" linkText="{i18n>ForecastingOwnerAdjustment.ForecastingOwnerAdjustment_ForecastCategoryName/ForecastingOwnerAdjustment_ForecastCategoryName}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ForecastingOwnerAdjustment_ForecastingItemCategory/ForecastingOwnerAdjustment_ForecastingItemCategory',type:'sap.ui.model.type.String'}" linkText="{i18n>ForecastingOwnerAdjustment.ForecastingOwnerAdjustment_ForecastingItemCategory/ForecastingOwnerAdjustment_ForecastingItemCategory}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ForecastingOwnerAdjustment_Id/ForecastingOwnerAdjustment_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>ForecastingOwnerAdjustment.ForecastingOwnerAdjustment_Id/ForecastingOwnerAdjustment_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>ForecastingOwnerAdjustment_IsAmount/ForecastingOwnerAdjustment_IsAmount',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>ForecastingOwnerAdjustment_IsDeleted/ForecastingOwnerAdjustment_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>ForecastingOwnerAdjustment_IsQuantity/ForecastingOwnerAdjustment_IsQuantity',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<Text text="{dataModel>ForecastingOwnerAdjustment_LastModifiedDate/ForecastingOwnerAdjustment_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>ForecastingOwnerAdjustment_OwnerAdjustedAmount/ForecastingOwnerAdjustment_OwnerAdjustedAmount',type:'sap.ui.model.type.Float', formatOptions:{'style':'short','maxFractionDigits':2} }"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>ForecastingOwnerAdjustment_OwnerAdjustedQuantity/ForecastingOwnerAdjustment_OwnerAdjustedQuantity',type:'sap.ui.model.type.Float', formatOptions:{'style':'short','maxFractionDigits':2} }"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ForecastingOwnerAdjustment_OwnerAdjustmentNote/ForecastingOwnerAdjustment_OwnerAdjustmentNote',type:'sap.ui.model.type.String'}" linkText="{i18n>ForecastingOwnerAdjustment.ForecastingOwnerAdjustment_OwnerAdjustmentNote/ForecastingOwnerAdjustment_OwnerAdjustmentNote}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ForecastingOwnerAdjustment_ProductFamily/ForecastingOwnerAdjustment_ProductFamily',type:'sap.ui.model.type.String'}" linkText="{i18n>ForecastingOwnerAdjustment.ForecastingOwnerAdjustment_ProductFamily/ForecastingOwnerAdjustment_ProductFamily}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>ForecastingOwnerAdjustment_StartDate/ForecastingOwnerAdjustment_StartDate}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>ForecastingOwnerAdjustment_SystemModstamp/ForecastingOwnerAdjustment_SystemModstamp}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>