					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>QuoteFeed.QuoteFeed_ParentId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>QuoteFeed_ParentId/sfdc_QuoteFeed_ParentId" entitySet="Quote"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>QuoteFeed.QuoteFeed_RelatedRecordId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>QuoteFeed_RelatedRecordId/sfdc_QuoteFeed_RelatedRecordId" entitySet="ContentVersion"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>QuoteFeed.QuoteFeed_SystemModstamp}" required="false"/> 
						<Text text=""/>
