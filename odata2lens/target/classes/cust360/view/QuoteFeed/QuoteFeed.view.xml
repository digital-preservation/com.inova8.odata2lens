					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>QuoteFeed.QuoteFeed_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>QuoteFeed_Id/QuoteFeed_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>QuoteFeed.QuoteFeed_Id/QuoteFeed_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>QuoteFeed.QuoteFeed_InsertedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>QuoteFeed_InsertedById/sfdc_QuoteFeed_InsertedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>QuoteFeed.QuoteFeed_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>QuoteFeed_IsDeleted/QuoteFeed_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>QuoteFeed.QuoteFeed_IsRichText}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>QuoteFeed_IsRichText/QuoteFeed_IsRichText',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>QuoteFeed.QuoteFeed_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>QuoteFeed_LastModifiedDate/QuoteFeed_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>QuoteFeed.QuoteFeed_LikeCount}" required="false"/> 
						<Text text=""/>
