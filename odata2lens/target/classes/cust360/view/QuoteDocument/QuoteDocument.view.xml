<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>QuoteDocument.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>QuoteDocument.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="QuoteDocument"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="QuoteDocument"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>QuoteDocument.QuoteDocument_ContentVersionDocumentId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>QuoteDocument_ContentVersionDocumentId/sfdc_QuoteDocument_ContentVersionDocumentId" entitySet="ContentVersion"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>QuoteDocument.QuoteDocument_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>QuoteDocument_CreatedById/sfdc_QuoteDocument_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>QuoteDocument.QuoteDocument_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>QuoteDocument_CreatedDate/QuoteDocument_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>QuoteDocument.QuoteDocument_Discount}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>QuoteDocument_Discount/QuoteDocument_Discount',type:'sap.ui.model.type.Float', formatOptions:{'style':'short','maxFractionDigits':2} }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>QuoteDocument.QuoteDocument_Document}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>QuoteDocument_Document/QuoteDocument_Document}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>QuoteDocument.QuoteDocument_GrandTotal}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>QuoteDocument_GrandTotal/QuoteDocument_GrandTotal',type:'sap.ui.model.type.Float', formatOptions:{'style':'short','maxFractionDigits':2} }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>QuoteDocument.QuoteDocument_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>QuoteDocument_Id/QuoteDocument_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>QuoteDocument.QuoteDocument_Id/QuoteDocument_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>QuoteDocument.QuoteDocument_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>QuoteDocument_IsDeleted/QuoteDocument_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>QuoteDocument.QuoteDocument_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>QuoteDocument_LastModifiedById/sfdc_QuoteDocument_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>QuoteDocument.QuoteDocument_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>QuoteDocument_LastModifiedDate/QuoteDocument_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>QuoteDocument.QuoteDocument_Name}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>QuoteDocument_Name/QuoteDocument_Name',type:'sap.ui.model.type.String' }" linkText="{i18n>QuoteDocument.QuoteDocument_Name/QuoteDocument_Name}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>QuoteDocument.QuoteDocument_QuoteId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>QuoteDocument_QuoteId/sfdc_QuoteDocument_QuoteId" entitySet="Quote"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>QuoteDocument.QuoteDocument_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>QuoteDocument_SystemModstamp/QuoteDocument_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>QuoteDocument.QuoteDocument_ContentVersionDocumentId}" required="false"/> 
						<rdf:ListResource path="dataModel>QuoteDocument_ContentVersionDocumentId" entitySet="ContentVersion"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>QuoteDocument.QuoteDocument_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>QuoteDocument_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>QuoteDocument.QuoteDocument_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>QuoteDocument_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>QuoteDocument.QuoteDocument_QuoteId}" required="false"/> 
						<rdf:ListResource path="dataModel>QuoteDocument_QuoteId" entitySet="Quote"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>