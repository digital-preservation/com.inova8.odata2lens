<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>ContentNotes.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/ContentNote' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>ContentNotes.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContentNotes.ContentNote_Content}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContentNotes.ContentNote_ContentSize}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContentNotes.ContentNote_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContentNotes.ContentNote_FileExtension}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContentNotes.ContentNote_FileType}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContentNotes.ContentNote_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContentNotes.ContentNote_IsDeleted}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContentNotes.ContentNote_IsReadOnly}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContentNotes.ContentNote_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContentNotes.ContentNote_LastViewedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContentNotes.ContentNote_SharingPrivacy}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContentNotes.ContentNote_TextPreview}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContentNotes.ContentNote_Title}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="ContentNote"/>
							<VBox>					
								<Text text="{dataModel>ContentNote_Content/ContentNote_Content}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>ContentNote_ContentSize/ContentNote_ContentSize',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>ContentNote_CreatedDate/ContentNote_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ContentNote_FileExtension/ContentNote_FileExtension',type:'sap.ui.model.type.String'}" linkText="{i18n>ContentNote.ContentNote_FileExtension/ContentNote_FileExtension}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ContentNote_FileType/ContentNote_FileType',type:'sap.ui.model.type.String'}" linkText="{i18n>ContentNote.ContentNote_FileType/ContentNote_FileType}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ContentNote_Id/ContentNote_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>ContentNote.ContentNote_Id/ContentNote_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>ContentNote_IsDeleted/ContentNote_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>ContentNote_IsReadOnly/ContentNote_IsReadOnly',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<Text text="{dataModel>ContentNote_LastModifiedDate/ContentNote_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>ContentNote_LastViewedDate/ContentNote_LastViewedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ContentNote_SharingPrivacy/ContentNote_SharingPrivacy',type:'sap.ui.model.type.String'}" linkText="{i18n>ContentNote.ContentNote_SharingPrivacy/ContentNote_SharingPrivacy}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ContentNote_TextPreview/ContentNote_TextPreview',type:'sap.ui.model.type.String'}" linkText="{i18n>ContentNote.ContentNote_TextPreview/ContentNote_TextPreview}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ContentNote_Title/ContentNote_Title',type:'sap.ui.model.type.String'}" linkText="{i18n>ContentNote.ContentNote_Title/ContentNote_Title}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>