<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>CustomBrandAsset.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>CustomBrandAsset.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="CustomBrandAsset"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="CustomBrandAsset"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CustomBrandAsset.CustomBrandAsset_AssetCategory}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>CustomBrandAsset_AssetCategory/CustomBrandAsset_AssetCategory',type:'sap.ui.model.type.String' }" linkText="{i18n>CustomBrandAsset.CustomBrandAsset_AssetCategory/CustomBrandAsset_AssetCategory}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CustomBrandAsset.CustomBrandAsset_AssetSourceId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>CustomBrandAsset_AssetSourceId/sfdc_CustomBrandAsset_AssetSourceId" entitySet="ContentAsset"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CustomBrandAsset.CustomBrandAsset_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>CustomBrandAsset_CreatedById/sfdc_CustomBrandAsset_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CustomBrandAsset.CustomBrandAsset_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>CustomBrandAsset_CreatedDate/CustomBrandAsset_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CustomBrandAsset.CustomBrandAsset_CustomBrandId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>CustomBrandAsset_CustomBrandId/sfdc_CustomBrandAsset_CustomBrandId" entitySet="CustomBrand"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CustomBrandAsset.CustomBrandAsset_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>CustomBrandAsset_Id/CustomBrandAsset_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>CustomBrandAsset.CustomBrandAsset_Id/CustomBrandAsset_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CustomBrandAsset.CustomBrandAsset_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>CustomBrandAsset_LastModifiedById/sfdc_CustomBrandAsset_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CustomBrandAsset.CustomBrandAsset_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>CustomBrandAsset_LastModifiedDate/CustomBrandAsset_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CustomBrandAsset.CustomBrandAsset_TextAsset}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>CustomBrandAsset_TextAsset/CustomBrandAsset_TextAsset',type:'sap.ui.model.type.String' }" linkText="{i18n>CustomBrandAsset.CustomBrandAsset_TextAsset/CustomBrandAsset_TextAsset}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CustomBrandAsset.CustomBrandAsset_AssetSourceId}" required="false"/> 
						<rdf:ListResource path="dataModel>CustomBrandAsset_AssetSourceId" entitySet="ContentAsset"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CustomBrandAsset.CustomBrandAsset_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>CustomBrandAsset_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CustomBrandAsset.CustomBrandAsset_CustomBrandId}" required="false"/> 
						<rdf:ListResource path="dataModel>CustomBrandAsset_CustomBrandId" entitySet="CustomBrand"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CustomBrandAsset.CustomBrandAsset_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>CustomBrandAsset_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>