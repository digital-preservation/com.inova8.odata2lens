<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>FeedSignal.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>FeedSignal.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="FeedSignal"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="FeedSignal"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>FeedSignal.FeedSignal_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>FeedSignal_CreatedById/sfdc_FeedSignal_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>FeedSignal.FeedSignal_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>FeedSignal_CreatedDate/FeedSignal_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>FeedSignal.FeedSignal_FeedEntityId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>FeedSignal_FeedEntityId/sfdc_FeedSignal_FeedEntityId" entitySet="AccountFeed"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>FeedSignal.FeedSignal_FeedItemId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>FeedSignal_FeedItemId/sfdc_FeedSignal_FeedItemId" entitySet="AccountFeed"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>FeedSignal.FeedSignal_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>FeedSignal_Id/FeedSignal_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>FeedSignal.FeedSignal_Id/FeedSignal_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>FeedSignal.FeedSignal_InsertedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>FeedSignal_InsertedById/sfdc_FeedSignal_InsertedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>FeedSignal.FeedSignal_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>FeedSignal_IsDeleted/FeedSignal_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>FeedSignal.FeedSignal_SignalType}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>FeedSignal_SignalType/FeedSignal_SignalType',type:'sap.ui.model.type.String' }" linkText="{i18n>FeedSignal.FeedSignal_SignalType/FeedSignal_SignalType}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>FeedSignal.FeedSignal_SignalValue}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>FeedSignal_SignalValue/FeedSignal_SignalValue',type:'sap.ui.model.type.Integer' }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>FeedSignal.FeedSignal_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>FeedSignal_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>FeedSignal.FeedSignal_FeedEntityId}" required="false"/> 
						<rdf:ListResource path="dataModel>FeedSignal_FeedEntityId" entitySet="AccountFeed"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>FeedSignal.FeedSignal_FeedItemId}" required="false"/> 
						<rdf:ListResource path="dataModel>FeedSignal_FeedItemId" entitySet="AccountFeed"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>FeedSignal.FeedSignal_InsertedById}" required="false"/> 
						<rdf:ListResource path="dataModel>FeedSignal_InsertedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>