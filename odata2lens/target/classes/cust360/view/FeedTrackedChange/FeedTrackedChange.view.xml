<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>FeedTrackedChange.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>FeedTrackedChange.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="FeedTrackedChange"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="FeedTrackedChange"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>FeedTrackedChange.FeedTrackedChange_FeedItemId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>FeedTrackedChange_FeedItemId/sfdc_FeedTrackedChange_FeedItemId" entitySet="AccountFeed"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>FeedTrackedChange.FeedTrackedChange_FieldName}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>FeedTrackedChange_FieldName/FeedTrackedChange_FieldName',type:'sap.ui.model.type.String' }" linkText="{i18n>FeedTrackedChange.FeedTrackedChange_FieldName/FeedTrackedChange_FieldName}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>FeedTrackedChange.FeedTrackedChange_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>FeedTrackedChange_Id/FeedTrackedChange_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>FeedTrackedChange.FeedTrackedChange_Id/FeedTrackedChange_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>FeedTrackedChange.FeedTrackedChange_NewValue}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>FeedTrackedChange_NewValue/FeedTrackedChange_NewValue',type:'sap.ui.model.type.String' }" linkText="{i18n>FeedTrackedChange.FeedTrackedChange_NewValue/FeedTrackedChange_NewValue}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>FeedTrackedChange.FeedTrackedChange_OldValue}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>FeedTrackedChange_OldValue/FeedTrackedChange_OldValue',type:'sap.ui.model.type.String' }" linkText="{i18n>FeedTrackedChange.FeedTrackedChange_OldValue/FeedTrackedChange_OldValue}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>FeedTrackedChange.FeedTrackedChange_FeedItemId}" required="false"/> 
						<rdf:ListResource path="dataModel>FeedTrackedChange_FeedItemId" entitySet="AccountFeed"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>