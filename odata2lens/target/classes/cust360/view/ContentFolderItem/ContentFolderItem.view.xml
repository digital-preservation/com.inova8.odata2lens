<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>ContentFolderItem.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>ContentFolderItem.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="ContentFolderItem"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="ContentFolderItem"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentFolderItem.ContentFolderItem_ContentSize}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>ContentFolderItem_ContentSize/ContentFolderItem_ContentSize',type:'sap.ui.model.type.Integer' }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentFolderItem.ContentFolderItem_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ContentFolderItem_CreatedById/sfdc_ContentFolderItem_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentFolderItem.ContentFolderItem_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>ContentFolderItem_CreatedDate/ContentFolderItem_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentFolderItem.ContentFolderItem_FileExtension}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ContentFolderItem_FileExtension/ContentFolderItem_FileExtension',type:'sap.ui.model.type.String' }" linkText="{i18n>ContentFolderItem.ContentFolderItem_FileExtension/ContentFolderItem_FileExtension}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentFolderItem.ContentFolderItem_FileType}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ContentFolderItem_FileType/ContentFolderItem_FileType',type:'sap.ui.model.type.String' }" linkText="{i18n>ContentFolderItem.ContentFolderItem_FileType/ContentFolderItem_FileType}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentFolderItem.ContentFolderItem_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ContentFolderItem_Id/ContentFolderItem_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>ContentFolderItem.ContentFolderItem_Id/ContentFolderItem_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentFolderItem.ContentFolderItem_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>ContentFolderItem_IsDeleted/ContentFolderItem_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentFolderItem.ContentFolderItem_IsFolder}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>ContentFolderItem_IsFolder/ContentFolderItem_IsFolder',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentFolderItem.ContentFolderItem_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ContentFolderItem_LastModifiedById/sfdc_ContentFolderItem_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentFolderItem.ContentFolderItem_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>ContentFolderItem_LastModifiedDate/ContentFolderItem_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentFolderItem.ContentFolderItem_ParentContentFolderId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ContentFolderItem_ParentContentFolderId/sfdc_ContentFolderItem_ParentContentFolderId" entitySet="ContentFolder"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentFolderItem.ContentFolderItem_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>ContentFolderItem_SystemModstamp/ContentFolderItem_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentFolderItem.ContentFolderItem_Title}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ContentFolderItem_Title/ContentFolderItem_Title',type:'sap.ui.model.type.String' }" linkText="{i18n>ContentFolderItem.ContentFolderItem_Title/ContentFolderItem_Title}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentFolderItem.ContentFolderItem_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>ContentFolderItem_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentFolderItem.ContentFolderItem_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>ContentFolderItem_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentFolderItem.ContentFolderItem_ParentContentFolderId}" required="false"/> 
						<rdf:ListResource path="dataModel>ContentFolderItem_ParentContentFolderId" entitySet="ContentFolder"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>