					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>OpenActivitys.OpenActivity_CallDisposition}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>OpenActivitys.OpenActivity_CallDurationInSeconds}"/>
