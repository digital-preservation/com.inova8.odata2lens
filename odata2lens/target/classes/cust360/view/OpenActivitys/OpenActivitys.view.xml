							<VBox>					
								<Text text="{path:'dataModel>OpenActivity_IsTask/OpenActivity_IsTask',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>OpenActivity_IsVisibleInSelfService/OpenActivity_IsVisibleInSelfService',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<Text text="{dataModel>OpenActivity_LastModifiedDate/OpenActivity_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>OpenActivity_Location/OpenActivity_Location',type:'sap.ui.model.type.String'}" linkText="{i18n>OpenActivity.OpenActivity_Location/OpenActivity_Location}"/>
							</VBox>							
							<VBox>					
