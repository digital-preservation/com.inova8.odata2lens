<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>CaseStatus.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>CaseStatus.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="CaseStatus"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="CaseStatus"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CaseStatus.CaseStatus_ApiName}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>CaseStatus_ApiName/CaseStatus_ApiName',type:'sap.ui.model.type.String' }" linkText="{i18n>CaseStatus.CaseStatus_ApiName/CaseStatus_ApiName}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CaseStatus.CaseStatus_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>CaseStatus_CreatedById/sfdc_CaseStatus_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CaseStatus.CaseStatus_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>CaseStatus_CreatedDate/CaseStatus_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CaseStatus.CaseStatus_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>CaseStatus_Id/CaseStatus_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>CaseStatus.CaseStatus_Id/CaseStatus_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CaseStatus.CaseStatus_IsClosed}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>CaseStatus_IsClosed/CaseStatus_IsClosed',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CaseStatus.CaseStatus_IsDefault}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>CaseStatus_IsDefault/CaseStatus_IsDefault',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CaseStatus.CaseStatus_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>CaseStatus_LastModifiedById/sfdc_CaseStatus_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CaseStatus.CaseStatus_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>CaseStatus_LastModifiedDate/CaseStatus_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CaseStatus.CaseStatus_MasterLabel}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>CaseStatus_MasterLabel/CaseStatus_MasterLabel',type:'sap.ui.model.type.String' }" linkText="{i18n>CaseStatus.CaseStatus_MasterLabel/CaseStatus_MasterLabel}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CaseStatus.CaseStatus_SortOrder}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>CaseStatus_SortOrder/CaseStatus_SortOrder',type:'sap.ui.model.type.Integer' }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CaseStatus.CaseStatus_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>CaseStatus_SystemModstamp/CaseStatus_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CaseStatus.CaseStatus_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>CaseStatus_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CaseStatus.CaseStatus_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>CaseStatus_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>