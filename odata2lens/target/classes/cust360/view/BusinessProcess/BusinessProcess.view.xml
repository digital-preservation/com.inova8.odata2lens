<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>BusinessProcess.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>BusinessProcess.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="BusinessProcess"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="BusinessProcess"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>BusinessProcess.BusinessProcess_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>BusinessProcess_CreatedById/sfdc_BusinessProcess_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>BusinessProcess.BusinessProcess_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>BusinessProcess_CreatedDate/BusinessProcess_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>BusinessProcess.BusinessProcess_Description}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>BusinessProcess_Description/BusinessProcess_Description',type:'sap.ui.model.type.String' }" linkText="{i18n>BusinessProcess.BusinessProcess_Description/BusinessProcess_Description}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>BusinessProcess.BusinessProcess_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>BusinessProcess_Id/BusinessProcess_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>BusinessProcess.BusinessProcess_Id/BusinessProcess_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>BusinessProcess.BusinessProcess_IsActive}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>BusinessProcess_IsActive/BusinessProcess_IsActive',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>BusinessProcess.BusinessProcess_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>BusinessProcess_LastModifiedById/sfdc_BusinessProcess_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>BusinessProcess.BusinessProcess_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>BusinessProcess_LastModifiedDate/BusinessProcess_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>BusinessProcess.BusinessProcess_Name}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>BusinessProcess_Name/BusinessProcess_Name',type:'sap.ui.model.type.String' }" linkText="{i18n>BusinessProcess.BusinessProcess_Name/BusinessProcess_Name}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>BusinessProcess.BusinessProcess_NamespacePrefix}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>BusinessProcess_NamespacePrefix/BusinessProcess_NamespacePrefix',type:'sap.ui.model.type.String' }" linkText="{i18n>BusinessProcess.BusinessProcess_NamespacePrefix/BusinessProcess_NamespacePrefix}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>BusinessProcess.BusinessProcess_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>BusinessProcess_SystemModstamp/BusinessProcess_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>BusinessProcess.BusinessProcess_TableEnumOrId}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>BusinessProcess_TableEnumOrId/BusinessProcess_TableEnumOrId',type:'sap.ui.model.type.String' }" linkText="{i18n>BusinessProcess.BusinessProcess_TableEnumOrId/BusinessProcess_TableEnumOrId}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>BusinessProcess.RecordType_BusinessProcessId_Inverse}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>RecordType_BusinessProcessId_Inverse/sfdc_RecordType_BusinessProcessId_Inverse" entitySet="RecordType"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>BusinessProcess.BusinessProcess_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>BusinessProcess_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>BusinessProcess.BusinessProcess_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>BusinessProcess_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>BusinessProcess.RecordType_BusinessProcessId_Inverse}" required="false"/> 
						<rdf:ListResource path="dataModel>RecordType_BusinessProcessId_Inverse" entitySet="RecordType"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>