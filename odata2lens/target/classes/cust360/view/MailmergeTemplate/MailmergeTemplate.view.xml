<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>MailmergeTemplate.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>MailmergeTemplate.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="MailmergeTemplate"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="MailmergeTemplate"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>MailmergeTemplate.MailmergeTemplate_Body}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>MailmergeTemplate_Body/MailmergeTemplate_Body}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>MailmergeTemplate.MailmergeTemplate_BodyLength}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>MailmergeTemplate_BodyLength/MailmergeTemplate_BodyLength',type:'sap.ui.model.type.Integer' }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>MailmergeTemplate.MailmergeTemplate_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>MailmergeTemplate_CreatedById/sfdc_MailmergeTemplate_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>MailmergeTemplate.MailmergeTemplate_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>MailmergeTemplate_CreatedDate/MailmergeTemplate_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>MailmergeTemplate.MailmergeTemplate_Description}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>MailmergeTemplate_Description/MailmergeTemplate_Description',type:'sap.ui.model.type.String' }" linkText="{i18n>MailmergeTemplate.MailmergeTemplate_Description/MailmergeTemplate_Description}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>MailmergeTemplate.MailmergeTemplate_Filename}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>MailmergeTemplate_Filename/MailmergeTemplate_Filename',type:'sap.ui.model.type.String' }" linkText="{i18n>MailmergeTemplate.MailmergeTemplate_Filename/MailmergeTemplate_Filename}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>MailmergeTemplate.MailmergeTemplate_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>MailmergeTemplate_Id/MailmergeTemplate_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>MailmergeTemplate.MailmergeTemplate_Id/MailmergeTemplate_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>MailmergeTemplate.MailmergeTemplate_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>MailmergeTemplate_IsDeleted/MailmergeTemplate_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>MailmergeTemplate.MailmergeTemplate_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>MailmergeTemplate_LastModifiedById/sfdc_MailmergeTemplate_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>MailmergeTemplate.MailmergeTemplate_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>MailmergeTemplate_LastModifiedDate/MailmergeTemplate_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>MailmergeTemplate.MailmergeTemplate_LastUsedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>MailmergeTemplate_LastUsedDate/MailmergeTemplate_LastUsedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>MailmergeTemplate.MailmergeTemplate_Name}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>MailmergeTemplate_Name/MailmergeTemplate_Name',type:'sap.ui.model.type.String' }" linkText="{i18n>MailmergeTemplate.MailmergeTemplate_Name/MailmergeTemplate_Name}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>MailmergeTemplate.MailmergeTemplate_SecurityOptionsAttachmentHasFlash}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>MailmergeTemplate_SecurityOptionsAttachmentHasFlash/MailmergeTemplate_SecurityOptionsAttachmentHasFlash',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>MailmergeTemplate.MailmergeTemplate_SecurityOptionsAttachmentHasXSSThreat}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>MailmergeTemplate_SecurityOptionsAttachmentHasXSSThreat/MailmergeTemplate_SecurityOptionsAttachmentHasXSSThreat',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>MailmergeTemplate.MailmergeTemplate_SecurityOptionsAttachmentScannedForXSS}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>MailmergeTemplate_SecurityOptionsAttachmentScannedForXSS/MailmergeTemplate_SecurityOptionsAttachmentScannedForXSS',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>MailmergeTemplate.MailmergeTemplate_SecurityOptionsAttachmentScannedforFlash}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>MailmergeTemplate_SecurityOptionsAttachmentScannedforFlash/MailmergeTemplate_SecurityOptionsAttachmentScannedforFlash',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>MailmergeTemplate.MailmergeTemplate_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>MailmergeTemplate_SystemModstamp/MailmergeTemplate_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>MailmergeTemplate.MailmergeTemplate_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>MailmergeTemplate_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>MailmergeTemplate.MailmergeTemplate_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>MailmergeTemplate_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>