<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>WorkOrderStatuss.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/WorkOrderStatus' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>WorkOrderStatuss.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>WorkOrderStatuss.WorkOrderStatus_ApiName}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>WorkOrderStatuss.WorkOrderStatus_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>WorkOrderStatuss.WorkOrderStatus_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>WorkOrderStatuss.WorkOrderStatus_IsDefault}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>WorkOrderStatuss.WorkOrderStatus_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>WorkOrderStatuss.WorkOrderStatus_MasterLabel}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>WorkOrderStatuss.WorkOrderStatus_SortOrder}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>WorkOrderStatuss.WorkOrderStatus_StatusCode}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>WorkOrderStatuss.WorkOrderStatus_SystemModstamp}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="WorkOrderStatus"/>
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>WorkOrderStatus_ApiName/WorkOrderStatus_ApiName',type:'sap.ui.model.type.String'}" linkText="{i18n>WorkOrderStatus.WorkOrderStatus_ApiName/WorkOrderStatus_ApiName}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>WorkOrderStatus_CreatedDate/WorkOrderStatus_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>WorkOrderStatus_Id/WorkOrderStatus_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>WorkOrderStatus.WorkOrderStatus_Id/WorkOrderStatus_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>WorkOrderStatus_IsDefault/WorkOrderStatus_IsDefault',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<Text text="{dataModel>WorkOrderStatus_LastModifiedDate/WorkOrderStatus_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>WorkOrderStatus_MasterLabel/WorkOrderStatus_MasterLabel',type:'sap.ui.model.type.String'}" linkText="{i18n>WorkOrderStatus.WorkOrderStatus_MasterLabel/WorkOrderStatus_MasterLabel}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>WorkOrderStatus_SortOrder/WorkOrderStatus_SortOrder',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>WorkOrderStatus_StatusCode/WorkOrderStatus_StatusCode',type:'sap.ui.model.type.String'}" linkText="{i18n>WorkOrderStatus.WorkOrderStatus_StatusCode/WorkOrderStatus_StatusCode}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>WorkOrderStatus_SystemModstamp/WorkOrderStatus_SystemModstamp}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>