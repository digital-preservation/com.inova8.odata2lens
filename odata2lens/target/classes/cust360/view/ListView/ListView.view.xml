<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>ListView.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>ListView.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="ListView"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="ListView"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ListView.ListEmailRecipientSource_SourceListId_Inverse}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ListEmailRecipientSource_SourceListId_Inverse/sfdc_ListEmailRecipientSource_SourceListId_Inverse" entitySet="ListEmailRecipientSource"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ListView.ListViewChartInstance_ListViewContextId_Inverse}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ListViewChartInstance_ListViewContextId_Inverse/sfdc_ListViewChartInstance_ListViewContextId_Inverse" entitySet="ListViewChartInstance"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ListView.ListView_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ListView_CreatedById/sfdc_ListView_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ListView.ListView_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>ListView_CreatedDate/ListView_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ListView.ListView_DeveloperName}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ListView_DeveloperName/ListView_DeveloperName',type:'sap.ui.model.type.String' }" linkText="{i18n>ListView.ListView_DeveloperName/ListView_DeveloperName}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ListView.ListView_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ListView_Id/ListView_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>ListView.ListView_Id/ListView_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ListView.ListView_IsSoqlCompatible}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>ListView_IsSoqlCompatible/ListView_IsSoqlCompatible',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ListView.ListView_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ListView_LastModifiedById/sfdc_ListView_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ListView.ListView_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>ListView_LastModifiedDate/ListView_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ListView.ListView_LastReferencedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>ListView_LastReferencedDate/ListView_LastReferencedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ListView.ListView_LastViewedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>ListView_LastViewedDate/ListView_LastViewedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ListView.ListView_Name}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ListView_Name/ListView_Name',type:'sap.ui.model.type.String' }" linkText="{i18n>ListView.ListView_Name/ListView_Name}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ListView.ListView_NamespacePrefix}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ListView_NamespacePrefix/ListView_NamespacePrefix',type:'sap.ui.model.type.String' }" linkText="{i18n>ListView.ListView_NamespacePrefix/ListView_NamespacePrefix}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ListView.ListView_SobjectType}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ListView_SobjectType/ListView_SobjectType',type:'sap.ui.model.type.String' }" linkText="{i18n>ListView.ListView_SobjectType/ListView_SobjectType}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ListView.ListView_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>ListView_SystemModstamp/ListView_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ListView.UserListView_ListViewId_Inverse}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>UserListView_ListViewId_Inverse/sfdc_UserListView_ListViewId_Inverse" entitySet="UserListView"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ListView.ListEmailRecipientSource_SourceListId_Inverse}" required="false"/> 
						<rdf:ListResource path="dataModel>ListEmailRecipientSource_SourceListId_Inverse" entitySet="ListEmailRecipientSource"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ListView.ListViewChartInstance_ListViewContextId_Inverse}" required="false"/> 
						<rdf:ListResource path="dataModel>ListViewChartInstance_ListViewContextId_Inverse" entitySet="ListViewChartInstance"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ListView.ListView_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>ListView_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ListView.ListView_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>ListView_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ListView.UserListView_ListViewId_Inverse}" required="false"/> 
						<rdf:ListResource path="dataModel>UserListView_ListViewId_Inverse" entitySet="UserListView"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>