<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>UserFeed.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>UserFeed.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="UserFeed"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="UserFeed"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserFeed.UserFeed_Body}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>UserFeed_Body/UserFeed_Body',type:'sap.ui.model.type.String' }" linkText="{i18n>UserFeed.UserFeed_Body/UserFeed_Body}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserFeed.UserFeed_CommentCount}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>UserFeed_CommentCount/UserFeed_CommentCount',type:'sap.ui.model.type.Integer' }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserFeed.UserFeed_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>UserFeed_CreatedById/sfdc_UserFeed_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserFeed.UserFeed_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>UserFeed_CreatedDate/UserFeed_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserFeed.UserFeed_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>UserFeed_Id/UserFeed_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>UserFeed.UserFeed_Id/UserFeed_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserFeed.UserFeed_InsertedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>UserFeed_InsertedById/sfdc_UserFeed_InsertedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserFeed.UserFeed_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>UserFeed_IsDeleted/UserFeed_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserFeed.UserFeed_IsRichText}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>UserFeed_IsRichText/UserFeed_IsRichText',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserFeed.UserFeed_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>UserFeed_LastModifiedDate/UserFeed_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserFeed.UserFeed_LikeCount}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>UserFeed_LikeCount/UserFeed_LikeCount',type:'sap.ui.model.type.Integer' }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserFeed.UserFeed_LinkUrl}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>UserFeed_LinkUrl/UserFeed_LinkUrl',type:'sap.ui.model.type.String' }" linkText="{i18n>UserFeed.UserFeed_LinkUrl/UserFeed_LinkUrl}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserFeed.UserFeed_ParentId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>UserFeed_ParentId/sfdc_UserFeed_ParentId" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserFeed.UserFeed_RelatedRecordId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>UserFeed_RelatedRecordId/sfdc_UserFeed_RelatedRecordId" entitySet="ContentVersion"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserFeed.UserFeed_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>UserFeed_SystemModstamp/UserFeed_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserFeed.UserFeed_Title}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>UserFeed_Title/UserFeed_Title',type:'sap.ui.model.type.String' }" linkText="{i18n>UserFeed.UserFeed_Title/UserFeed_Title}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserFeed.UserFeed_Type}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>UserFeed_Type/UserFeed_Type',type:'sap.ui.model.type.String' }" linkText="{i18n>UserFeed.UserFeed_Type/UserFeed_Type}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserFeed.UserFeed_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>UserFeed_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserFeed.UserFeed_InsertedById}" required="false"/> 
						<rdf:ListResource path="dataModel>UserFeed_InsertedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserFeed.UserFeed_ParentId}" required="false"/> 
						<rdf:ListResource path="dataModel>UserFeed_ParentId" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserFeed.UserFeed_RelatedRecordId}" required="false"/> 
						<rdf:ListResource path="dataModel>UserFeed_RelatedRecordId" entitySet="ContentVersion"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>