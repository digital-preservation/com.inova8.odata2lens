						<rdf:TextLink text="{path:'dataModel>ProcessInstanceNode_Id/ProcessInstanceNode_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>ProcessInstanceNode.ProcessInstanceNode_Id/ProcessInstanceNode_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ProcessInstanceNode.ProcessInstanceNode_IsDeleted}" required="false"/> 
