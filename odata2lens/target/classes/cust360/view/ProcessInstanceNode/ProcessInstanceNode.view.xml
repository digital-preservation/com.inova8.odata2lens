						<Text text=""/>
						<Text text="{dataModel>ProcessInstanceNode_LastModifiedDate/ProcessInstanceNode_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ProcessInstanceNode.ProcessInstanceNode_NodeStatus}" required="false"/> 
