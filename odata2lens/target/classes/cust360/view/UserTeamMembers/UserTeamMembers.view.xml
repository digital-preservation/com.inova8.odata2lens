<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>UserTeamMembers.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/UserTeamMember' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>UserTeamMembers.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserTeamMembers.UserTeamMember_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserTeamMembers.UserTeamMember_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserTeamMembers.UserTeamMember_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserTeamMembers.UserTeamMember_OpportunityAccessLevel}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserTeamMembers.UserTeamMember_SystemModstamp}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserTeamMembers.UserTeamMember_TeamMemberRole}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="UserTeamMember"/>
							<VBox>					
							<Text text="{dataModel>UserTeamMember_CreatedDate/UserTeamMember_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>UserTeamMember_Id/UserTeamMember_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>UserTeamMember.UserTeamMember_Id/UserTeamMember_Id}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>UserTeamMember_LastModifiedDate/UserTeamMember_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>UserTeamMember_OpportunityAccessLevel/UserTeamMember_OpportunityAccessLevel',type:'sap.ui.model.type.String'}" linkText="{i18n>UserTeamMember.UserTeamMember_OpportunityAccessLevel/UserTeamMember_OpportunityAccessLevel}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>UserTeamMember_SystemModstamp/UserTeamMember_SystemModstamp}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>UserTeamMember_TeamMemberRole/UserTeamMember_TeamMemberRole',type:'sap.ui.model.type.String'}" linkText="{i18n>UserTeamMember.UserTeamMember_TeamMemberRole/UserTeamMember_TeamMemberRole}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>