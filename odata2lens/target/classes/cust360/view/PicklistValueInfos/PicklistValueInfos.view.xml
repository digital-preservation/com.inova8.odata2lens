<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>PicklistValueInfos.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/PicklistValueInfo' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>PicklistValueInfos.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>PicklistValueInfos.PicklistValueInfo_DurableId}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>PicklistValueInfos.PicklistValueInfo_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>PicklistValueInfos.PicklistValueInfo_IsActive}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>PicklistValueInfos.PicklistValueInfo_IsDefaultValue}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>PicklistValueInfos.PicklistValueInfo_Label}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>PicklistValueInfos.PicklistValueInfo_ValidFor}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>PicklistValueInfos.PicklistValueInfo_Value}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="PicklistValueInfo"/>
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>PicklistValueInfo_DurableId/PicklistValueInfo_DurableId',type:'sap.ui.model.type.String'}" linkText="{i18n>PicklistValueInfo.PicklistValueInfo_DurableId/PicklistValueInfo_DurableId}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>PicklistValueInfo_Id/PicklistValueInfo_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>PicklistValueInfo.PicklistValueInfo_Id/PicklistValueInfo_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>PicklistValueInfo_IsActive/PicklistValueInfo_IsActive',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>PicklistValueInfo_IsDefaultValue/PicklistValueInfo_IsDefaultValue',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>PicklistValueInfo_Label/PicklistValueInfo_Label',type:'sap.ui.model.type.String'}" linkText="{i18n>PicklistValueInfo.PicklistValueInfo_Label/PicklistValueInfo_Label}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>PicklistValueInfo_ValidFor/PicklistValueInfo_ValidFor',type:'sap.ui.model.type.String'}" linkText="{i18n>PicklistValueInfo.PicklistValueInfo_ValidFor/PicklistValueInfo_ValidFor}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>PicklistValueInfo_Value/PicklistValueInfo_Value',type:'sap.ui.model.type.String'}" linkText="{i18n>PicklistValueInfo.PicklistValueInfo_Value/PicklistValueInfo_Value}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>