<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>FeedItems.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/FeedItem' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>FeedItems.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>FeedItems.FeedItem_Body}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>FeedItems.FeedItem_CommentCount}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>FeedItems.FeedItem_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>FeedItems.FeedItem_HasContent}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>FeedItems.FeedItem_HasFeedEntity}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>FeedItems.FeedItem_HasLink}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>FeedItems.FeedItem_HasVerifiedComment}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>FeedItems.FeedItem_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>FeedItems.FeedItem_IsDeleted}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>FeedItems.FeedItem_IsRichText}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>FeedItems.FeedItem_LastEditDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>FeedItems.FeedItem_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>FeedItems.FeedItem_LikeCount}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>FeedItems.FeedItem_LinkUrl}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>FeedItems.FeedItem_Revision}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>FeedItems.FeedItem_Status}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>FeedItems.FeedItem_SystemModstamp}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>FeedItems.FeedItem_Title}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>FeedItems.FeedItem_Type}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="FeedItem"/>
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>FeedItem_Body/FeedItem_Body',type:'sap.ui.model.type.String'}" linkText="{i18n>FeedItem.FeedItem_Body/FeedItem_Body}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>FeedItem_CommentCount/FeedItem_CommentCount',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>FeedItem_CreatedDate/FeedItem_CreatedDate}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>FeedItem_HasContent/FeedItem_HasContent',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>FeedItem_HasFeedEntity/FeedItem_HasFeedEntity',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>FeedItem_HasLink/FeedItem_HasLink',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>FeedItem_HasVerifiedComment/FeedItem_HasVerifiedComment',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>FeedItem_Id/FeedItem_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>FeedItem.FeedItem_Id/FeedItem_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>FeedItem_IsDeleted/FeedItem_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>FeedItem_IsRichText/FeedItem_IsRichText',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<Text text="{dataModel>FeedItem_LastEditDate/FeedItem_LastEditDate}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>FeedItem_LastModifiedDate/FeedItem_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>FeedItem_LikeCount/FeedItem_LikeCount',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>FeedItem_LinkUrl/FeedItem_LinkUrl',type:'sap.ui.model.type.String'}" linkText="{i18n>FeedItem.FeedItem_LinkUrl/FeedItem_LinkUrl}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>FeedItem_Revision/FeedItem_Revision',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>FeedItem_Status/FeedItem_Status',type:'sap.ui.model.type.String'}" linkText="{i18n>FeedItem.FeedItem_Status/FeedItem_Status}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>FeedItem_SystemModstamp/FeedItem_SystemModstamp}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>FeedItem_Title/FeedItem_Title',type:'sap.ui.model.type.String'}" linkText="{i18n>FeedItem.FeedItem_Title/FeedItem_Title}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>FeedItem_Type/FeedItem_Type',type:'sap.ui.model.type.String'}" linkText="{i18n>FeedItem.FeedItem_Type/FeedItem_Type}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>