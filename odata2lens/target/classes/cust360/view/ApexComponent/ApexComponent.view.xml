<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>ApexComponent.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>ApexComponent.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="ApexComponent"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="ApexComponent"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexComponent.ApexComponent_ApiVersion}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>ApexComponent_ApiVersion/ApexComponent_ApiVersion',type:'sap.ui.model.type.Float', formatOptions:{'style':'short','maxFractionDigits':2} }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexComponent.ApexComponent_ControllerKey}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ApexComponent_ControllerKey/ApexComponent_ControllerKey',type:'sap.ui.model.type.String' }" linkText="{i18n>ApexComponent.ApexComponent_ControllerKey/ApexComponent_ControllerKey}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexComponent.ApexComponent_ControllerType}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ApexComponent_ControllerType/ApexComponent_ControllerType',type:'sap.ui.model.type.String' }" linkText="{i18n>ApexComponent.ApexComponent_ControllerType/ApexComponent_ControllerType}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexComponent.ApexComponent_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ApexComponent_CreatedById/sfdc_ApexComponent_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexComponent.ApexComponent_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>ApexComponent_CreatedDate/ApexComponent_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexComponent.ApexComponent_Description}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ApexComponent_Description/ApexComponent_Description',type:'sap.ui.model.type.String' }" linkText="{i18n>ApexComponent.ApexComponent_Description/ApexComponent_Description}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexComponent.ApexComponent_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ApexComponent_Id/ApexComponent_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>ApexComponent.ApexComponent_Id/ApexComponent_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexComponent.ApexComponent_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ApexComponent_LastModifiedById/sfdc_ApexComponent_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexComponent.ApexComponent_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>ApexComponent_LastModifiedDate/ApexComponent_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexComponent.ApexComponent_Markup}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ApexComponent_Markup/ApexComponent_Markup',type:'sap.ui.model.type.String' }" linkText="{i18n>ApexComponent.ApexComponent_Markup/ApexComponent_Markup}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexComponent.ApexComponent_MasterLabel}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ApexComponent_MasterLabel/ApexComponent_MasterLabel',type:'sap.ui.model.type.String' }" linkText="{i18n>ApexComponent.ApexComponent_MasterLabel/ApexComponent_MasterLabel}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexComponent.ApexComponent_Name}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ApexComponent_Name/ApexComponent_Name',type:'sap.ui.model.type.String' }" linkText="{i18n>ApexComponent.ApexComponent_Name/ApexComponent_Name}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexComponent.ApexComponent_NamespacePrefix}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ApexComponent_NamespacePrefix/ApexComponent_NamespacePrefix',type:'sap.ui.model.type.String' }" linkText="{i18n>ApexComponent.ApexComponent_NamespacePrefix/ApexComponent_NamespacePrefix}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexComponent.ApexComponent_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>ApexComponent_SystemModstamp/ApexComponent_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexComponent.ApexComponent_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>ApexComponent_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexComponent.ApexComponent_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>ApexComponent_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>