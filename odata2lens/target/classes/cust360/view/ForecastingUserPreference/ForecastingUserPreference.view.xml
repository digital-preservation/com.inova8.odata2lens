					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ForecastingUserPreference.ForecastingUserPreference_IsForecastingShowQuantity}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>ForecastingUserPreference_IsForecastingShowQuantity/ForecastingUserPreference_IsForecastingShowQuantity',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ForecastingUserPreference.ForecastingUserPreference_IsHideForecastingGuidedTour}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>ForecastingUserPreference_IsHideForecastingGuidedTour/ForecastingUserPreference_IsHideForecastingGuidedTour',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ForecastingUserPreference.ForecastingUserPreference_IsHideForecastingQuotaColumn}" required="false"/> 
