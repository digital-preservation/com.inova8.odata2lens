					<cells>
						<Label text="{i18n>ForecastingUserPreference.ForecastingUserPreference_IsShowForecastingQuotaAttainment}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>ForecastingUserPreference_IsShowForecastingQuotaAttainment/ForecastingUserPreference_IsShowForecastingQuotaAttainment',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ForecastingUserPreference.ForecastingUserPreference_ForecastingDisplayedTypeId}" required="false"/> 
						<rdf:ListResource path="dataModel>ForecastingUserPreference_ForecastingDisplayedTypeId" entitySet="ForecastingType"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>