						<rdf:TextLink text="{path:'dataModel>ForecastingUserPreference_ForecastingPeriodType/ForecastingUserPreference_ForecastingPeriodType',type:'sap.ui.model.type.String' }" linkText="{i18n>ForecastingUserPreference.ForecastingUserPreference_ForecastingPeriodType/ForecastingUserPreference_ForecastingPeriodType}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ForecastingUserPreference.ForecastingUserPreference_ForecastingStartPeriod}" required="false"/> 
