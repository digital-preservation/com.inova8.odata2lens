						<rdf:TextLink text="{path:'dataModel>ForecastingUserPreference_ExternalId/ForecastingUserPreference_ExternalId',type:'sap.ui.model.type.String' }" linkText="{i18n>ForecastingUserPreference.ForecastingUserPreference_ExternalId/ForecastingUserPreference_ExternalId}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ForecastingUserPreference.ForecastingUserPreference_ForecastingDisplayedTypeId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ForecastingUserPreference_ForecastingDisplayedTypeId/sfdc_ForecastingUserPreference_ForecastingDisplayedTypeId" entitySet="ForecastingType"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ForecastingUserPreference.ForecastingUserPreference_ForecastingPeriodDuration}" required="false"/> 
