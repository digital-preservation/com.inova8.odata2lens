				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="ForecastingUserPreference"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ForecastingUserPreference.ForecastingUserPreference_ExternalId}" required="false"/> 
