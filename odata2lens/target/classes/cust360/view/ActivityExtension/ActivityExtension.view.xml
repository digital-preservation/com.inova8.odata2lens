<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>ActivityExtension.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>ActivityExtension.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="ActivityExtension"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="ActivityExtension"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ActivityExtension.ActivityExtension_BaseId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ActivityExtension_BaseId/sfdc_ActivityExtension_BaseId" entitySet="Event"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ActivityExtension.ActivityExtension_BaseType}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ActivityExtension_BaseType/ActivityExtension_BaseType',type:'sap.ui.model.type.String' }" linkText="{i18n>ActivityExtension.ActivityExtension_BaseType/ActivityExtension_BaseType}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ActivityExtension.ActivityExtension_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ActivityExtension_Id/ActivityExtension_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>ActivityExtension.ActivityExtension_Id/ActivityExtension_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ActivityExtension.ActivityExtension_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>ActivityExtension_IsDeleted/ActivityExtension_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ActivityExtension.ActivityExtension_IsException}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ActivityExtension_IsException/ActivityExtension_IsException',type:'sap.ui.model.type.String' }" linkText="{i18n>ActivityExtension.ActivityExtension_IsException/ActivityExtension_IsException}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ActivityExtension.ActivityExtension_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>ActivityExtension_LastModifiedDate/ActivityExtension_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ActivityExtension.ActivityExtension_RecurrenceId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ActivityExtension_RecurrenceId/sfdc_ActivityExtension_RecurrenceId" entitySet="ActivityRecurrence"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ActivityExtension.ActivityExtension_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>ActivityExtension_SystemModstamp/ActivityExtension_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ActivityExtension.Event_ActivityExtensionId_Inverse}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>Event_ActivityExtensionId_Inverse/sfdc_Event_ActivityExtensionId_Inverse" entitySet="Event"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ActivityExtension.ActivityExtension_BaseId}" required="false"/> 
						<rdf:ListResource path="dataModel>ActivityExtension_BaseId" entitySet="Event"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ActivityExtension.ActivityExtension_RecurrenceId}" required="false"/> 
						<rdf:ListResource path="dataModel>ActivityExtension_RecurrenceId" entitySet="ActivityRecurrence"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ActivityExtension.Event_ActivityExtensionId_Inverse}" required="false"/> 
						<rdf:ListResource path="dataModel>Event_ActivityExtensionId_Inverse" entitySet="Event"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>