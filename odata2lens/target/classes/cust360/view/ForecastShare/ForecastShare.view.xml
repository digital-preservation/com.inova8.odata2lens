<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>ForecastShare.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>ForecastShare.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="ForecastShare"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="ForecastShare"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ForecastShare.ForecastShare_AccessLevel}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ForecastShare_AccessLevel/ForecastShare_AccessLevel',type:'sap.ui.model.type.String' }" linkText="{i18n>ForecastShare.ForecastShare_AccessLevel/ForecastShare_AccessLevel}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ForecastShare.ForecastShare_CanSubmit}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>ForecastShare_CanSubmit/ForecastShare_CanSubmit',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ForecastShare.ForecastShare_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ForecastShare_Id/ForecastShare_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>ForecastShare.ForecastShare_Id/ForecastShare_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ForecastShare.ForecastShare_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ForecastShare_LastModifiedById/sfdc_ForecastShare_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ForecastShare.ForecastShare_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>ForecastShare_LastModifiedDate/ForecastShare_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ForecastShare.ForecastShare_RowCause}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ForecastShare_RowCause/ForecastShare_RowCause',type:'sap.ui.model.type.String' }" linkText="{i18n>ForecastShare.ForecastShare_RowCause/ForecastShare_RowCause}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ForecastShare.ForecastShare_UserOrGroupId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ForecastShare_UserOrGroupId/sfdc_ForecastShare_UserOrGroupId" entitySet="Group"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ForecastShare.ForecastShare_UserRoleId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ForecastShare_UserRoleId/sfdc_ForecastShare_UserRoleId" entitySet="UserRole"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ForecastShare.ForecastShare_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>ForecastShare_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ForecastShare.ForecastShare_UserOrGroupId}" required="false"/> 
						<rdf:ListResource path="dataModel>ForecastShare_UserOrGroupId" entitySet="Group"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ForecastShare.ForecastShare_UserRoleId}" required="false"/> 
						<rdf:ListResource path="dataModel>ForecastShare_UserRoleId" entitySet="UserRole"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>