<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>ApexTestRunResults.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/ApexTestRunResult' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>ApexTestRunResults.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexTestRunResults.ApexTestRunResult_ClassesCompleted}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexTestRunResults.ApexTestRunResult_ClassesEnqueued}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexTestRunResults.ApexTestRunResult_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexTestRunResults.ApexTestRunResult_EndTime}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexTestRunResults.ApexTestRunResult_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexTestRunResults.ApexTestRunResult_IsAllTests}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexTestRunResults.ApexTestRunResult_IsDeleted}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexTestRunResults.ApexTestRunResult_JobName}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexTestRunResults.ApexTestRunResult_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexTestRunResults.ApexTestRunResult_MethodsCompleted}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexTestRunResults.ApexTestRunResult_MethodsEnqueued}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexTestRunResults.ApexTestRunResult_MethodsFailed}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexTestRunResults.ApexTestRunResult_Source}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexTestRunResults.ApexTestRunResult_StartTime}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexTestRunResults.ApexTestRunResult_Status}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexTestRunResults.ApexTestRunResult_SystemModstamp}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexTestRunResults.ApexTestRunResult_TestTime}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="ApexTestRunResult"/>
							<VBox>					
							<Text text="{path:'dataModel>ApexTestRunResult_ClassesCompleted/ApexTestRunResult_ClassesCompleted',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>ApexTestRunResult_ClassesEnqueued/ApexTestRunResult_ClassesEnqueued',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>ApexTestRunResult_CreatedDate/ApexTestRunResult_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>ApexTestRunResult_EndTime/ApexTestRunResult_EndTime}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ApexTestRunResult_Id/ApexTestRunResult_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>ApexTestRunResult.ApexTestRunResult_Id/ApexTestRunResult_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>ApexTestRunResult_IsAllTests/ApexTestRunResult_IsAllTests',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>ApexTestRunResult_IsDeleted/ApexTestRunResult_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ApexTestRunResult_JobName/ApexTestRunResult_JobName',type:'sap.ui.model.type.String'}" linkText="{i18n>ApexTestRunResult.ApexTestRunResult_JobName/ApexTestRunResult_JobName}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>ApexTestRunResult_LastModifiedDate/ApexTestRunResult_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>ApexTestRunResult_MethodsCompleted/ApexTestRunResult_MethodsCompleted',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>ApexTestRunResult_MethodsEnqueued/ApexTestRunResult_MethodsEnqueued',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>ApexTestRunResult_MethodsFailed/ApexTestRunResult_MethodsFailed',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ApexTestRunResult_Source/ApexTestRunResult_Source',type:'sap.ui.model.type.String'}" linkText="{i18n>ApexTestRunResult.ApexTestRunResult_Source/ApexTestRunResult_Source}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>ApexTestRunResult_StartTime/ApexTestRunResult_StartTime}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ApexTestRunResult_Status/ApexTestRunResult_Status',type:'sap.ui.model.type.String'}" linkText="{i18n>ApexTestRunResult.ApexTestRunResult_Status/ApexTestRunResult_Status}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>ApexTestRunResult_SystemModstamp/ApexTestRunResult_SystemModstamp}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>ApexTestRunResult_TestTime/ApexTestRunResult_TestTime',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>