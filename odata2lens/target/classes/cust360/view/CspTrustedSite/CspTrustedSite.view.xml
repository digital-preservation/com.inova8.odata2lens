<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>CspTrustedSite.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>CspTrustedSite.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="CspTrustedSite"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="CspTrustedSite"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CspTrustedSite.CspTrustedSite_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>CspTrustedSite_CreatedById/sfdc_CspTrustedSite_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CspTrustedSite.CspTrustedSite_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>CspTrustedSite_CreatedDate/CspTrustedSite_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CspTrustedSite.CspTrustedSite_Description}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>CspTrustedSite_Description/CspTrustedSite_Description',type:'sap.ui.model.type.String' }" linkText="{i18n>CspTrustedSite.CspTrustedSite_Description/CspTrustedSite_Description}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CspTrustedSite.CspTrustedSite_DeveloperName}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>CspTrustedSite_DeveloperName/CspTrustedSite_DeveloperName',type:'sap.ui.model.type.String' }" linkText="{i18n>CspTrustedSite.CspTrustedSite_DeveloperName/CspTrustedSite_DeveloperName}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CspTrustedSite.CspTrustedSite_EndpointUrl}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>CspTrustedSite_EndpointUrl/CspTrustedSite_EndpointUrl',type:'sap.ui.model.type.String' }" linkText="{i18n>CspTrustedSite.CspTrustedSite_EndpointUrl/CspTrustedSite_EndpointUrl}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CspTrustedSite.CspTrustedSite_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>CspTrustedSite_Id/CspTrustedSite_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>CspTrustedSite.CspTrustedSite_Id/CspTrustedSite_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CspTrustedSite.CspTrustedSite_IsActive}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>CspTrustedSite_IsActive/CspTrustedSite_IsActive',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CspTrustedSite.CspTrustedSite_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>CspTrustedSite_IsDeleted/CspTrustedSite_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CspTrustedSite.CspTrustedSite_Language}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>CspTrustedSite_Language/CspTrustedSite_Language',type:'sap.ui.model.type.String' }" linkText="{i18n>CspTrustedSite.CspTrustedSite_Language/CspTrustedSite_Language}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CspTrustedSite.CspTrustedSite_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>CspTrustedSite_LastModifiedById/sfdc_CspTrustedSite_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CspTrustedSite.CspTrustedSite_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>CspTrustedSite_LastModifiedDate/CspTrustedSite_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CspTrustedSite.CspTrustedSite_MasterLabel}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>CspTrustedSite_MasterLabel/CspTrustedSite_MasterLabel',type:'sap.ui.model.type.String' }" linkText="{i18n>CspTrustedSite.CspTrustedSite_MasterLabel/CspTrustedSite_MasterLabel}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CspTrustedSite.CspTrustedSite_NamespacePrefix}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>CspTrustedSite_NamespacePrefix/CspTrustedSite_NamespacePrefix',type:'sap.ui.model.type.String' }" linkText="{i18n>CspTrustedSite.CspTrustedSite_NamespacePrefix/CspTrustedSite_NamespacePrefix}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CspTrustedSite.CspTrustedSite_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>CspTrustedSite_SystemModstamp/CspTrustedSite_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CspTrustedSite.CspTrustedSite_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>CspTrustedSite_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CspTrustedSite.CspTrustedSite_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>CspTrustedSite_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>