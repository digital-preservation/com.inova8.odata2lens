<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>AuraDefinitionInfo.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>AuraDefinitionInfo.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="AuraDefinitionInfo"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="AuraDefinitionInfo"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AuraDefinitionInfo.AuraDefinitionInfo_AuraDefinitionBundleInfoId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>AuraDefinitionInfo_AuraDefinitionBundleInfoId/sfdc_AuraDefinitionInfo_AuraDefinitionBundleInfoId" entitySet="AuraDefinitionBundleInfo"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AuraDefinitionInfo.AuraDefinitionInfo_AuraDefinitionId}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>AuraDefinitionInfo_AuraDefinitionId/AuraDefinitionInfo_AuraDefinitionId',type:'sap.ui.model.type.String' }" linkText="{i18n>AuraDefinitionInfo.AuraDefinitionInfo_AuraDefinitionId/AuraDefinitionInfo_AuraDefinitionId}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AuraDefinitionInfo.AuraDefinitionInfo_DefType}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>AuraDefinitionInfo_DefType/AuraDefinitionInfo_DefType',type:'sap.ui.model.type.String' }" linkText="{i18n>AuraDefinitionInfo.AuraDefinitionInfo_DefType/AuraDefinitionInfo_DefType}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AuraDefinitionInfo.AuraDefinitionInfo_DeveloperName}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>AuraDefinitionInfo_DeveloperName/AuraDefinitionInfo_DeveloperName',type:'sap.ui.model.type.String' }" linkText="{i18n>AuraDefinitionInfo.AuraDefinitionInfo_DeveloperName/AuraDefinitionInfo_DeveloperName}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AuraDefinitionInfo.AuraDefinitionInfo_DurableId}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>AuraDefinitionInfo_DurableId/AuraDefinitionInfo_DurableId',type:'sap.ui.model.type.String' }" linkText="{i18n>AuraDefinitionInfo.AuraDefinitionInfo_DurableId/AuraDefinitionInfo_DurableId}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AuraDefinitionInfo.AuraDefinitionInfo_Format}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>AuraDefinitionInfo_Format/AuraDefinitionInfo_Format',type:'sap.ui.model.type.String' }" linkText="{i18n>AuraDefinitionInfo.AuraDefinitionInfo_Format/AuraDefinitionInfo_Format}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AuraDefinitionInfo.AuraDefinitionInfo_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>AuraDefinitionInfo_Id/AuraDefinitionInfo_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>AuraDefinitionInfo.AuraDefinitionInfo_Id/AuraDefinitionInfo_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AuraDefinitionInfo.AuraDefinitionInfo_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>AuraDefinitionInfo_LastModifiedDate/AuraDefinitionInfo_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AuraDefinitionInfo.AuraDefinitionInfo_NamespacePrefix}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>AuraDefinitionInfo_NamespacePrefix/AuraDefinitionInfo_NamespacePrefix',type:'sap.ui.model.type.String' }" linkText="{i18n>AuraDefinitionInfo.AuraDefinitionInfo_NamespacePrefix/AuraDefinitionInfo_NamespacePrefix}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AuraDefinitionInfo.AuraDefinitionInfo_Source}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>AuraDefinitionInfo_Source/AuraDefinitionInfo_Source',type:'sap.ui.model.type.String' }" linkText="{i18n>AuraDefinitionInfo.AuraDefinitionInfo_Source/AuraDefinitionInfo_Source}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AuraDefinitionInfo.AuraDefinitionInfo_AuraDefinitionBundleInfoId}" required="false"/> 
						<rdf:ListResource path="dataModel>AuraDefinitionInfo_AuraDefinitionBundleInfoId" entitySet="AuraDefinitionBundleInfo"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>