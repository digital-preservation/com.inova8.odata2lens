<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>BackgroundOperations.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/BackgroundOperation' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>BackgroundOperations.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>BackgroundOperations.BackgroundOperation_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>BackgroundOperations.BackgroundOperation_Error}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>BackgroundOperations.BackgroundOperation_ExecutionGroup}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>BackgroundOperations.BackgroundOperation_ExpiresAt}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>BackgroundOperations.BackgroundOperation_FinishedAt}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>BackgroundOperations.BackgroundOperation_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>BackgroundOperations.BackgroundOperation_IsDeleted}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>BackgroundOperations.BackgroundOperation_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>BackgroundOperations.BackgroundOperation_Name}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>BackgroundOperations.BackgroundOperation_NumFollowers}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>BackgroundOperations.BackgroundOperation_ParentKey}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>BackgroundOperations.BackgroundOperation_ProcessAfter}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>BackgroundOperations.BackgroundOperation_RetryBackoff}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>BackgroundOperations.BackgroundOperation_RetryCount}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>BackgroundOperations.BackgroundOperation_RetryLimit}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>BackgroundOperations.BackgroundOperation_SequenceGroup}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>BackgroundOperations.BackgroundOperation_SequenceNumber}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>BackgroundOperations.BackgroundOperation_StartedAt}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>BackgroundOperations.BackgroundOperation_Status}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>BackgroundOperations.BackgroundOperation_SubmittedAt}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>BackgroundOperations.BackgroundOperation_SystemModstamp}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>BackgroundOperations.BackgroundOperation_Timeout}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>BackgroundOperations.BackgroundOperation_WorkerUri}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="BackgroundOperation"/>
							<VBox>					
							<Text text="{dataModel>BackgroundOperation_CreatedDate/BackgroundOperation_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>BackgroundOperation_Error/BackgroundOperation_Error',type:'sap.ui.model.type.String'}" linkText="{i18n>BackgroundOperation.BackgroundOperation_Error/BackgroundOperation_Error}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>BackgroundOperation_ExecutionGroup/BackgroundOperation_ExecutionGroup',type:'sap.ui.model.type.String'}" linkText="{i18n>BackgroundOperation.BackgroundOperation_ExecutionGroup/BackgroundOperation_ExecutionGroup}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>BackgroundOperation_ExpiresAt/BackgroundOperation_ExpiresAt}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>BackgroundOperation_FinishedAt/BackgroundOperation_FinishedAt}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>BackgroundOperation_Id/BackgroundOperation_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>BackgroundOperation.BackgroundOperation_Id/BackgroundOperation_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>BackgroundOperation_IsDeleted/BackgroundOperation_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<Text text="{dataModel>BackgroundOperation_LastModifiedDate/BackgroundOperation_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>BackgroundOperation_Name/BackgroundOperation_Name',type:'sap.ui.model.type.String'}" linkText="{i18n>BackgroundOperation.BackgroundOperation_Name/BackgroundOperation_Name}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>BackgroundOperation_NumFollowers/BackgroundOperation_NumFollowers',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>BackgroundOperation_ParentKey/BackgroundOperation_ParentKey',type:'sap.ui.model.type.String'}" linkText="{i18n>BackgroundOperation.BackgroundOperation_ParentKey/BackgroundOperation_ParentKey}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>BackgroundOperation_ProcessAfter/BackgroundOperation_ProcessAfter}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>BackgroundOperation_RetryBackoff/BackgroundOperation_RetryBackoff',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>BackgroundOperation_RetryCount/BackgroundOperation_RetryCount',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>BackgroundOperation_RetryLimit/BackgroundOperation_RetryLimit',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>BackgroundOperation_SequenceGroup/BackgroundOperation_SequenceGroup',type:'sap.ui.model.type.String'}" linkText="{i18n>BackgroundOperation.BackgroundOperation_SequenceGroup/BackgroundOperation_SequenceGroup}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>BackgroundOperation_SequenceNumber/BackgroundOperation_SequenceNumber',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>BackgroundOperation_StartedAt/BackgroundOperation_StartedAt}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>BackgroundOperation_Status/BackgroundOperation_Status',type:'sap.ui.model.type.String'}" linkText="{i18n>BackgroundOperation.BackgroundOperation_Status/BackgroundOperation_Status}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>BackgroundOperation_SubmittedAt/BackgroundOperation_SubmittedAt}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>BackgroundOperation_SystemModstamp/BackgroundOperation_SystemModstamp}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>BackgroundOperation_Timeout/BackgroundOperation_Timeout',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>BackgroundOperation_WorkerUri/BackgroundOperation_WorkerUri',type:'sap.ui.model.type.String'}" linkText="{i18n>BackgroundOperation.BackgroundOperation_WorkerUri/BackgroundOperation_WorkerUri}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>