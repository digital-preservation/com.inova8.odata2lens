<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>TopicLocalization.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>TopicLocalization.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="TopicLocalization"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="TopicLocalization"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TopicLocalization.TopicLocalization_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>TopicLocalization_CreatedById/sfdc_TopicLocalization_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TopicLocalization.TopicLocalization_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>TopicLocalization_CreatedDate/TopicLocalization_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TopicLocalization.TopicLocalization_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>TopicLocalization_Id/TopicLocalization_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>TopicLocalization.TopicLocalization_Id/TopicLocalization_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TopicLocalization.TopicLocalization_Language}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>TopicLocalization_Language/TopicLocalization_Language',type:'sap.ui.model.type.String' }" linkText="{i18n>TopicLocalization.TopicLocalization_Language/TopicLocalization_Language}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TopicLocalization.TopicLocalization_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>TopicLocalization_LastModifiedById/sfdc_TopicLocalization_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TopicLocalization.TopicLocalization_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>TopicLocalization_LastModifiedDate/TopicLocalization_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TopicLocalization.TopicLocalization_NamespacePrefix}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>TopicLocalization_NamespacePrefix/TopicLocalization_NamespacePrefix',type:'sap.ui.model.type.String' }" linkText="{i18n>TopicLocalization.TopicLocalization_NamespacePrefix/TopicLocalization_NamespacePrefix}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TopicLocalization.TopicLocalization_ParentId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>TopicLocalization_ParentId/sfdc_TopicLocalization_ParentId" entitySet="Topic"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TopicLocalization.TopicLocalization_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>TopicLocalization_SystemModstamp/TopicLocalization_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TopicLocalization.TopicLocalization_Value}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>TopicLocalization_Value/TopicLocalization_Value',type:'sap.ui.model.type.String' }" linkText="{i18n>TopicLocalization.TopicLocalization_Value/TopicLocalization_Value}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TopicLocalization.TopicLocalization_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>TopicLocalization_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TopicLocalization.TopicLocalization_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>TopicLocalization_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TopicLocalization.TopicLocalization_ParentId}" required="false"/> 
						<rdf:ListResource path="dataModel>TopicLocalization_ParentId" entitySet="Topic"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>