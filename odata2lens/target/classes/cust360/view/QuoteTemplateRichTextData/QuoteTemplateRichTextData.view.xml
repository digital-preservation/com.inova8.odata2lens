<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>QuoteTemplateRichTextData.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>QuoteTemplateRichTextData.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="QuoteTemplateRichTextData"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="QuoteTemplateRichTextData"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>QuoteTemplateRichTextData.QuoteTemplateRichTextData_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>QuoteTemplateRichTextData_CreatedById/sfdc_QuoteTemplateRichTextData_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>QuoteTemplateRichTextData.QuoteTemplateRichTextData_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>QuoteTemplateRichTextData_CreatedDate/QuoteTemplateRichTextData_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>QuoteTemplateRichTextData.QuoteTemplateRichTextData_Data}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>QuoteTemplateRichTextData_Data/QuoteTemplateRichTextData_Data',type:'sap.ui.model.type.String' }" linkText="{i18n>QuoteTemplateRichTextData.QuoteTemplateRichTextData_Data/QuoteTemplateRichTextData_Data}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>QuoteTemplateRichTextData.QuoteTemplateRichTextData_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>QuoteTemplateRichTextData_Id/QuoteTemplateRichTextData_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>QuoteTemplateRichTextData.QuoteTemplateRichTextData_Id/QuoteTemplateRichTextData_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>QuoteTemplateRichTextData.QuoteTemplateRichTextData_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>QuoteTemplateRichTextData_IsDeleted/QuoteTemplateRichTextData_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>QuoteTemplateRichTextData.QuoteTemplateRichTextData_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>QuoteTemplateRichTextData_LastModifiedById/sfdc_QuoteTemplateRichTextData_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>QuoteTemplateRichTextData.QuoteTemplateRichTextData_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>QuoteTemplateRichTextData_LastModifiedDate/QuoteTemplateRichTextData_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>QuoteTemplateRichTextData.QuoteTemplateRichTextData_Name}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>QuoteTemplateRichTextData_Name/QuoteTemplateRichTextData_Name',type:'sap.ui.model.type.String' }" linkText="{i18n>QuoteTemplateRichTextData.QuoteTemplateRichTextData_Name/QuoteTemplateRichTextData_Name}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>QuoteTemplateRichTextData.QuoteTemplateRichTextData_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>QuoteTemplateRichTextData_SystemModstamp/QuoteTemplateRichTextData_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>QuoteTemplateRichTextData.QuoteTemplateRichTextData_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>QuoteTemplateRichTextData_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>QuoteTemplateRichTextData.QuoteTemplateRichTextData_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>QuoteTemplateRichTextData_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>