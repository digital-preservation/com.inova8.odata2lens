<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>UserAppMenuItems.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/UserAppMenuItem' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>UserAppMenuItems.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserAppMenuItems.UserAppMenuItem_AppMenuItemId}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserAppMenuItems.UserAppMenuItem_ApplicationId}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserAppMenuItems.UserAppMenuItem_Description}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserAppMenuItems.UserAppMenuItem_IconUrl}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserAppMenuItems.UserAppMenuItem_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserAppMenuItems.UserAppMenuItem_InfoUrl}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserAppMenuItems.UserAppMenuItem_IsUsingAdminAuthorization}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserAppMenuItems.UserAppMenuItem_IsVisible}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserAppMenuItems.UserAppMenuItem_Label}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserAppMenuItems.UserAppMenuItem_LogoUrl}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserAppMenuItems.UserAppMenuItem_MobileStartUrl}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserAppMenuItems.UserAppMenuItem_Name}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserAppMenuItems.UserAppMenuItem_SortOrder}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserAppMenuItems.UserAppMenuItem_StartUrl}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserAppMenuItems.UserAppMenuItem_Type}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserAppMenuItems.UserAppMenuItem_UserSortOrder}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="UserAppMenuItem"/>
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>UserAppMenuItem_AppMenuItemId/UserAppMenuItem_AppMenuItemId',type:'sap.ui.model.type.String'}" linkText="{i18n>UserAppMenuItem.UserAppMenuItem_AppMenuItemId/UserAppMenuItem_AppMenuItemId}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>UserAppMenuItem_ApplicationId/UserAppMenuItem_ApplicationId',type:'sap.ui.model.type.String'}" linkText="{i18n>UserAppMenuItem.UserAppMenuItem_ApplicationId/UserAppMenuItem_ApplicationId}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>UserAppMenuItem_Description/UserAppMenuItem_Description',type:'sap.ui.model.type.String'}" linkText="{i18n>UserAppMenuItem.UserAppMenuItem_Description/UserAppMenuItem_Description}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>UserAppMenuItem_IconUrl/UserAppMenuItem_IconUrl',type:'sap.ui.model.type.String'}" linkText="{i18n>UserAppMenuItem.UserAppMenuItem_IconUrl/UserAppMenuItem_IconUrl}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>UserAppMenuItem_Id/UserAppMenuItem_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>UserAppMenuItem.UserAppMenuItem_Id/UserAppMenuItem_Id}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>UserAppMenuItem_InfoUrl/UserAppMenuItem_InfoUrl',type:'sap.ui.model.type.String'}" linkText="{i18n>UserAppMenuItem.UserAppMenuItem_InfoUrl/UserAppMenuItem_InfoUrl}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>UserAppMenuItem_IsUsingAdminAuthorization/UserAppMenuItem_IsUsingAdminAuthorization',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>UserAppMenuItem_IsVisible/UserAppMenuItem_IsVisible',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>UserAppMenuItem_Label/UserAppMenuItem_Label',type:'sap.ui.model.type.String'}" linkText="{i18n>UserAppMenuItem.UserAppMenuItem_Label/UserAppMenuItem_Label}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>UserAppMenuItem_LogoUrl/UserAppMenuItem_LogoUrl',type:'sap.ui.model.type.String'}" linkText="{i18n>UserAppMenuItem.UserAppMenuItem_LogoUrl/UserAppMenuItem_LogoUrl}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>UserAppMenuItem_MobileStartUrl/UserAppMenuItem_MobileStartUrl',type:'sap.ui.model.type.String'}" linkText="{i18n>UserAppMenuItem.UserAppMenuItem_MobileStartUrl/UserAppMenuItem_MobileStartUrl}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>UserAppMenuItem_Name/UserAppMenuItem_Name',type:'sap.ui.model.type.String'}" linkText="{i18n>UserAppMenuItem.UserAppMenuItem_Name/UserAppMenuItem_Name}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>UserAppMenuItem_SortOrder/UserAppMenuItem_SortOrder',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>UserAppMenuItem_StartUrl/UserAppMenuItem_StartUrl',type:'sap.ui.model.type.String'}" linkText="{i18n>UserAppMenuItem.UserAppMenuItem_StartUrl/UserAppMenuItem_StartUrl}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>UserAppMenuItem_Type/UserAppMenuItem_Type',type:'sap.ui.model.type.String'}" linkText="{i18n>UserAppMenuItem.UserAppMenuItem_Type/UserAppMenuItem_Type}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>UserAppMenuItem_UserSortOrder/UserAppMenuItem_UserSortOrder',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>