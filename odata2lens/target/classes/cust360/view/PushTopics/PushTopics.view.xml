<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>PushTopics.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/PushTopic' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>PushTopics.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>PushTopics.PushTopic_ApiVersion}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>PushTopics.PushTopic_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>PushTopics.PushTopic_Description}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>PushTopics.PushTopic_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>PushTopics.PushTopic_IsActive}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>PushTopics.PushTopic_IsDeleted}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>PushTopics.PushTopic_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>PushTopics.PushTopic_Name}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>PushTopics.PushTopic_NotifyForFields}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>PushTopics.PushTopic_NotifyForOperationCreate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>PushTopics.PushTopic_NotifyForOperationDelete}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>PushTopics.PushTopic_NotifyForOperationUndelete}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>PushTopics.PushTopic_NotifyForOperationUpdate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>PushTopics.PushTopic_NotifyForOperations}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>PushTopics.PushTopic_Query}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>PushTopics.PushTopic_SystemModstamp}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="PushTopic"/>
							<VBox>					
							<Text text="{path:'dataModel>PushTopic_ApiVersion/PushTopic_ApiVersion',type:'sap.ui.model.type.Float', formatOptions:{'style':'short','maxFractionDigits':2} }"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>PushTopic_CreatedDate/PushTopic_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>PushTopic_Description/PushTopic_Description',type:'sap.ui.model.type.String'}" linkText="{i18n>PushTopic.PushTopic_Description/PushTopic_Description}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>PushTopic_Id/PushTopic_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>PushTopic.PushTopic_Id/PushTopic_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>PushTopic_IsActive/PushTopic_IsActive',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>PushTopic_IsDeleted/PushTopic_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<Text text="{dataModel>PushTopic_LastModifiedDate/PushTopic_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>PushTopic_Name/PushTopic_Name',type:'sap.ui.model.type.String'}" linkText="{i18n>PushTopic.PushTopic_Name/PushTopic_Name}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>PushTopic_NotifyForFields/PushTopic_NotifyForFields',type:'sap.ui.model.type.String'}" linkText="{i18n>PushTopic.PushTopic_NotifyForFields/PushTopic_NotifyForFields}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>PushTopic_NotifyForOperationCreate/PushTopic_NotifyForOperationCreate',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>PushTopic_NotifyForOperationDelete/PushTopic_NotifyForOperationDelete',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>PushTopic_NotifyForOperationUndelete/PushTopic_NotifyForOperationUndelete',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>PushTopic_NotifyForOperationUpdate/PushTopic_NotifyForOperationUpdate',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>PushTopic_NotifyForOperations/PushTopic_NotifyForOperations',type:'sap.ui.model.type.String'}" linkText="{i18n>PushTopic.PushTopic_NotifyForOperations/PushTopic_NotifyForOperations}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>PushTopic_Query/PushTopic_Query',type:'sap.ui.model.type.String'}" linkText="{i18n>PushTopic.PushTopic_Query/PushTopic_Query}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>PushTopic_SystemModstamp/PushTopic_SystemModstamp}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>