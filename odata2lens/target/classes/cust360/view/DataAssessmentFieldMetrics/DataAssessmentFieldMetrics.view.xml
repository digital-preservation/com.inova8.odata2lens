<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>DataAssessmentFieldMetrics.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/DataAssessmentFieldMetric' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>DataAssessmentFieldMetrics.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>DataAssessmentFieldMetrics.DataAssessmentFieldMetric_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>DataAssessmentFieldMetrics.DataAssessmentFieldMetric_FieldName}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>DataAssessmentFieldMetrics.DataAssessmentFieldMetric_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>DataAssessmentFieldMetrics.DataAssessmentFieldMetric_IsDeleted}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>DataAssessmentFieldMetrics.DataAssessmentFieldMetric_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>DataAssessmentFieldMetrics.DataAssessmentFieldMetric_Name}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>DataAssessmentFieldMetrics.DataAssessmentFieldMetric_NumMatchedBlanks}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>DataAssessmentFieldMetrics.DataAssessmentFieldMetric_NumMatchedDifferent}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>DataAssessmentFieldMetrics.DataAssessmentFieldMetric_NumMatchedInSync}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>DataAssessmentFieldMetrics.DataAssessmentFieldMetric_NumUnmatchedBlanks}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>DataAssessmentFieldMetrics.DataAssessmentFieldMetric_SystemModstamp}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="DataAssessmentFieldMetric"/>
							<VBox>					
							<Text text="{dataModel>DataAssessmentFieldMetric_CreatedDate/DataAssessmentFieldMetric_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>DataAssessmentFieldMetric_FieldName/DataAssessmentFieldMetric_FieldName',type:'sap.ui.model.type.String'}" linkText="{i18n>DataAssessmentFieldMetric.DataAssessmentFieldMetric_FieldName/DataAssessmentFieldMetric_FieldName}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>DataAssessmentFieldMetric_Id/DataAssessmentFieldMetric_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>DataAssessmentFieldMetric.DataAssessmentFieldMetric_Id/DataAssessmentFieldMetric_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>DataAssessmentFieldMetric_IsDeleted/DataAssessmentFieldMetric_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<Text text="{dataModel>DataAssessmentFieldMetric_LastModifiedDate/DataAssessmentFieldMetric_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>DataAssessmentFieldMetric_Name/DataAssessmentFieldMetric_Name',type:'sap.ui.model.type.String'}" linkText="{i18n>DataAssessmentFieldMetric.DataAssessmentFieldMetric_Name/DataAssessmentFieldMetric_Name}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>DataAssessmentFieldMetric_NumMatchedBlanks/DataAssessmentFieldMetric_NumMatchedBlanks',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>DataAssessmentFieldMetric_NumMatchedDifferent/DataAssessmentFieldMetric_NumMatchedDifferent',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>DataAssessmentFieldMetric_NumMatchedInSync/DataAssessmentFieldMetric_NumMatchedInSync',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>DataAssessmentFieldMetric_NumUnmatchedBlanks/DataAssessmentFieldMetric_NumUnmatchedBlanks',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>DataAssessmentFieldMetric_SystemModstamp/DataAssessmentFieldMetric_SystemModstamp}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>