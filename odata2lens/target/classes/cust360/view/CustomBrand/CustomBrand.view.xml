<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>CustomBrand.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>CustomBrand.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="CustomBrand"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="CustomBrand"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CustomBrand.CustomBrandAsset_CustomBrandId_Inverse}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>CustomBrandAsset_CustomBrandId_Inverse/sfdc_CustomBrandAsset_CustomBrandId_Inverse" entitySet="CustomBrandAsset"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CustomBrand.CustomBrand_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>CustomBrand_CreatedById/sfdc_CustomBrand_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CustomBrand.CustomBrand_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>CustomBrand_CreatedDate/CustomBrand_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CustomBrand.CustomBrand_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>CustomBrand_Id/CustomBrand_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>CustomBrand.CustomBrand_Id/CustomBrand_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CustomBrand.CustomBrand_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>CustomBrand_LastModifiedById/sfdc_CustomBrand_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CustomBrand.CustomBrand_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>CustomBrand_LastModifiedDate/CustomBrand_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CustomBrand.CustomBrand_ParentId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>CustomBrand_ParentId/sfdc_CustomBrand_ParentId" entitySet="Organization"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CustomBrand.CustomBrandAsset_CustomBrandId_Inverse}" required="false"/> 
						<rdf:ListResource path="dataModel>CustomBrandAsset_CustomBrandId_Inverse" entitySet="CustomBrandAsset"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CustomBrand.CustomBrand_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>CustomBrand_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CustomBrand.CustomBrand_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>CustomBrand_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CustomBrand.CustomBrand_ParentId}" required="false"/> 
						<rdf:ListResource path="dataModel>CustomBrand_ParentId" entitySet="Organization"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>