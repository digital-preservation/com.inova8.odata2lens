<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>ApexClasss.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/ApexClass' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>ApexClasss.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexClasss.ApexClass_ApiVersion}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexClasss.ApexClass_Body}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexClasss.ApexClass_BodyCrc}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexClasss.ApexClass_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexClasss.ApexClass_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexClasss.ApexClass_IsValid}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexClasss.ApexClass_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexClasss.ApexClass_LengthWithoutComments}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexClasss.ApexClass_Name}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexClasss.ApexClass_NamespacePrefix}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexClasss.ApexClass_Status}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexClasss.ApexClass_SystemModstamp}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="ApexClass"/>
							<VBox>					
							<Text text="{path:'dataModel>ApexClass_ApiVersion/ApexClass_ApiVersion',type:'sap.ui.model.type.Float', formatOptions:{'style':'short','maxFractionDigits':2} }"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ApexClass_Body/ApexClass_Body',type:'sap.ui.model.type.String'}" linkText="{i18n>ApexClass.ApexClass_Body/ApexClass_Body}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>ApexClass_BodyCrc/ApexClass_BodyCrc',type:'sap.ui.model.type.Float', formatOptions:{'style':'short','maxFractionDigits':2} }"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>ApexClass_CreatedDate/ApexClass_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ApexClass_Id/ApexClass_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>ApexClass.ApexClass_Id/ApexClass_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>ApexClass_IsValid/ApexClass_IsValid',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<Text text="{dataModel>ApexClass_LastModifiedDate/ApexClass_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>ApexClass_LengthWithoutComments/ApexClass_LengthWithoutComments',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ApexClass_Name/ApexClass_Name',type:'sap.ui.model.type.String'}" linkText="{i18n>ApexClass.ApexClass_Name/ApexClass_Name}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ApexClass_NamespacePrefix/ApexClass_NamespacePrefix',type:'sap.ui.model.type.String'}" linkText="{i18n>ApexClass.ApexClass_NamespacePrefix/ApexClass_NamespacePrefix}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ApexClass_Status/ApexClass_Status',type:'sap.ui.model.type.String'}" linkText="{i18n>ApexClass.ApexClass_Status/ApexClass_Status}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>ApexClass_SystemModstamp/ApexClass_SystemModstamp}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>