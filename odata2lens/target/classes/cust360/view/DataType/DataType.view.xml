<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>DataType.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>DataType.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="DataType"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="DataType"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>DataType.DataType_ContextServiceDataTypeId}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>DataType_ContextServiceDataTypeId/DataType_ContextServiceDataTypeId',type:'sap.ui.model.type.String' }" linkText="{i18n>DataType.DataType_ContextServiceDataTypeId/DataType_ContextServiceDataTypeId}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>DataType.DataType_ContextWsdlDataTypeId}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>DataType_ContextWsdlDataTypeId/DataType_ContextWsdlDataTypeId',type:'sap.ui.model.type.String' }" linkText="{i18n>DataType.DataType_ContextWsdlDataTypeId/DataType_ContextWsdlDataTypeId}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>DataType.DataType_DeveloperName}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>DataType_DeveloperName/DataType_DeveloperName',type:'sap.ui.model.type.String' }" linkText="{i18n>DataType.DataType_DeveloperName/DataType_DeveloperName}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>DataType.DataType_DurableId}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>DataType_DurableId/DataType_DurableId',type:'sap.ui.model.type.String' }" linkText="{i18n>DataType.DataType_DurableId/DataType_DurableId}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>DataType.DataType_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>DataType_Id/DataType_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>DataType.DataType_Id/DataType_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>DataType.DataType_IsComplex}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>DataType_IsComplex/DataType_IsComplex',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>DataType.EntityParticle_ServiceDataTypeId_Inverse}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>EntityParticle_ServiceDataTypeId_Inverse/sfdc_EntityParticle_ServiceDataTypeId_Inverse" entitySet="EntityParticle"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>DataType.EntityParticle_ValueTypeId_Inverse}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>EntityParticle_ValueTypeId_Inverse/sfdc_EntityParticle_ValueTypeId_Inverse" entitySet="EntityParticle"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>DataType.FieldDefinition_ServiceDataTypeId_Inverse}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>FieldDefinition_ServiceDataTypeId_Inverse/sfdc_FieldDefinition_ServiceDataTypeId_Inverse" entitySet="FieldDefinition"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>DataType.FieldDefinition_ValueTypeId_Inverse}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>FieldDefinition_ValueTypeId_Inverse/sfdc_FieldDefinition_ValueTypeId_Inverse" entitySet="FieldDefinition"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>DataType.EntityParticle_ServiceDataTypeId_Inverse}" required="false"/> 
						<rdf:ListResource path="dataModel>EntityParticle_ServiceDataTypeId_Inverse" entitySet="EntityParticle"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>DataType.EntityParticle_ValueTypeId_Inverse}" required="false"/> 
						<rdf:ListResource path="dataModel>EntityParticle_ValueTypeId_Inverse" entitySet="EntityParticle"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>DataType.FieldDefinition_ServiceDataTypeId_Inverse}" required="false"/> 
						<rdf:ListResource path="dataModel>FieldDefinition_ServiceDataTypeId_Inverse" entitySet="FieldDefinition"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>DataType.FieldDefinition_ValueTypeId_Inverse}" required="false"/> 
						<rdf:ListResource path="dataModel>FieldDefinition_ValueTypeId_Inverse" entitySet="FieldDefinition"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>