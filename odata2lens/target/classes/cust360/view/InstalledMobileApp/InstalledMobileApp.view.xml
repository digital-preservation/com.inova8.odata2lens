<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>InstalledMobileApp.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>InstalledMobileApp.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="InstalledMobileApp"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="InstalledMobileApp"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>InstalledMobileApp.InstalledMobileApp_ConnectedApplicationId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>InstalledMobileApp_ConnectedApplicationId/sfdc_InstalledMobileApp_ConnectedApplicationId" entitySet="ConnectedApplication"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>InstalledMobileApp.InstalledMobileApp_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>InstalledMobileApp_CreatedById/sfdc_InstalledMobileApp_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>InstalledMobileApp.InstalledMobileApp_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>InstalledMobileApp_CreatedDate/InstalledMobileApp_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>InstalledMobileApp.InstalledMobileApp_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>InstalledMobileApp_Id/InstalledMobileApp_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>InstalledMobileApp.InstalledMobileApp_Id/InstalledMobileApp_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>InstalledMobileApp.InstalledMobileApp_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>InstalledMobileApp_IsDeleted/InstalledMobileApp_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>InstalledMobileApp.InstalledMobileApp_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>InstalledMobileApp_LastModifiedById/sfdc_InstalledMobileApp_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>InstalledMobileApp.InstalledMobileApp_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>InstalledMobileApp_LastModifiedDate/InstalledMobileApp_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>InstalledMobileApp.InstalledMobileApp_Name}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>InstalledMobileApp_Name/InstalledMobileApp_Name',type:'sap.ui.model.type.String' }" linkText="{i18n>InstalledMobileApp.InstalledMobileApp_Name/InstalledMobileApp_Name}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>InstalledMobileApp.InstalledMobileApp_Status}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>InstalledMobileApp_Status/InstalledMobileApp_Status',type:'sap.ui.model.type.String' }" linkText="{i18n>InstalledMobileApp.InstalledMobileApp_Status/InstalledMobileApp_Status}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>InstalledMobileApp.InstalledMobileApp_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>InstalledMobileApp_SystemModstamp/InstalledMobileApp_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>InstalledMobileApp.InstalledMobileApp_UserId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>InstalledMobileApp_UserId/sfdc_InstalledMobileApp_UserId" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>InstalledMobileApp.InstalledMobileApp_Version}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>InstalledMobileApp_Version/InstalledMobileApp_Version',type:'sap.ui.model.type.String' }" linkText="{i18n>InstalledMobileApp.InstalledMobileApp_Version/InstalledMobileApp_Version}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>InstalledMobileApp.InstalledMobileApp_ConnectedApplicationId}" required="false"/> 
						<rdf:ListResource path="dataModel>InstalledMobileApp_ConnectedApplicationId" entitySet="ConnectedApplication"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>InstalledMobileApp.InstalledMobileApp_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>InstalledMobileApp_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>InstalledMobileApp.InstalledMobileApp_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>InstalledMobileApp_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>InstalledMobileApp.InstalledMobileApp_UserId}" required="false"/> 
						<rdf:ListResource path="dataModel>InstalledMobileApp_UserId" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>