<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>ActionLinkTemplates.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/ActionLinkTemplate' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>ActionLinkTemplates.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ActionLinkTemplates.ActionLinkTemplate_ActionUrl}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ActionLinkTemplates.ActionLinkTemplate_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ActionLinkTemplates.ActionLinkTemplate_Headers}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ActionLinkTemplates.ActionLinkTemplate_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ActionLinkTemplates.ActionLinkTemplate_IsConfirmationRequired}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ActionLinkTemplates.ActionLinkTemplate_IsDeleted}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ActionLinkTemplates.ActionLinkTemplate_IsGroupDefault}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ActionLinkTemplates.ActionLinkTemplate_Label}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ActionLinkTemplates.ActionLinkTemplate_LabelKey}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ActionLinkTemplates.ActionLinkTemplate_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ActionLinkTemplates.ActionLinkTemplate_LinkType}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ActionLinkTemplates.ActionLinkTemplate_Method}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ActionLinkTemplates.ActionLinkTemplate_Position}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ActionLinkTemplates.ActionLinkTemplate_RequestBody}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ActionLinkTemplates.ActionLinkTemplate_SystemModstamp}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ActionLinkTemplates.ActionLinkTemplate_UserAlias}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ActionLinkTemplates.ActionLinkTemplate_UserVisibility}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="ActionLinkTemplate"/>
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ActionLinkTemplate_ActionUrl/ActionLinkTemplate_ActionUrl',type:'sap.ui.model.type.String'}" linkText="{i18n>ActionLinkTemplate.ActionLinkTemplate_ActionUrl/ActionLinkTemplate_ActionUrl}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>ActionLinkTemplate_CreatedDate/ActionLinkTemplate_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ActionLinkTemplate_Headers/ActionLinkTemplate_Headers',type:'sap.ui.model.type.String'}" linkText="{i18n>ActionLinkTemplate.ActionLinkTemplate_Headers/ActionLinkTemplate_Headers}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ActionLinkTemplate_Id/ActionLinkTemplate_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>ActionLinkTemplate.ActionLinkTemplate_Id/ActionLinkTemplate_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>ActionLinkTemplate_IsConfirmationRequired/ActionLinkTemplate_IsConfirmationRequired',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>ActionLinkTemplate_IsDeleted/ActionLinkTemplate_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>ActionLinkTemplate_IsGroupDefault/ActionLinkTemplate_IsGroupDefault',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ActionLinkTemplate_Label/ActionLinkTemplate_Label',type:'sap.ui.model.type.String'}" linkText="{i18n>ActionLinkTemplate.ActionLinkTemplate_Label/ActionLinkTemplate_Label}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ActionLinkTemplate_LabelKey/ActionLinkTemplate_LabelKey',type:'sap.ui.model.type.String'}" linkText="{i18n>ActionLinkTemplate.ActionLinkTemplate_LabelKey/ActionLinkTemplate_LabelKey}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>ActionLinkTemplate_LastModifiedDate/ActionLinkTemplate_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ActionLinkTemplate_LinkType/ActionLinkTemplate_LinkType',type:'sap.ui.model.type.String'}" linkText="{i18n>ActionLinkTemplate.ActionLinkTemplate_LinkType/ActionLinkTemplate_LinkType}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ActionLinkTemplate_Method/ActionLinkTemplate_Method',type:'sap.ui.model.type.String'}" linkText="{i18n>ActionLinkTemplate.ActionLinkTemplate_Method/ActionLinkTemplate_Method}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>ActionLinkTemplate_Position/ActionLinkTemplate_Position',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ActionLinkTemplate_RequestBody/ActionLinkTemplate_RequestBody',type:'sap.ui.model.type.String'}" linkText="{i18n>ActionLinkTemplate.ActionLinkTemplate_RequestBody/ActionLinkTemplate_RequestBody}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>ActionLinkTemplate_SystemModstamp/ActionLinkTemplate_SystemModstamp}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ActionLinkTemplate_UserAlias/ActionLinkTemplate_UserAlias',type:'sap.ui.model.type.String'}" linkText="{i18n>ActionLinkTemplate.ActionLinkTemplate_UserAlias/ActionLinkTemplate_UserAlias}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ActionLinkTemplate_UserVisibility/ActionLinkTemplate_UserVisibility',type:'sap.ui.model.type.String'}" linkText="{i18n>ActionLinkTemplate.ActionLinkTemplate_UserVisibility/ActionLinkTemplate_UserVisibility}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>