<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>AssetRelationshipFeeds.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/AssetRelationshipFeed' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>AssetRelationshipFeeds.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AssetRelationshipFeeds.AssetRelationshipFeed_Body}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AssetRelationshipFeeds.AssetRelationshipFeed_CommentCount}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AssetRelationshipFeeds.AssetRelationshipFeed_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AssetRelationshipFeeds.AssetRelationshipFeed_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AssetRelationshipFeeds.AssetRelationshipFeed_IsDeleted}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AssetRelationshipFeeds.AssetRelationshipFeed_IsRichText}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AssetRelationshipFeeds.AssetRelationshipFeed_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AssetRelationshipFeeds.AssetRelationshipFeed_LikeCount}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AssetRelationshipFeeds.AssetRelationshipFeed_LinkUrl}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AssetRelationshipFeeds.AssetRelationshipFeed_SystemModstamp}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AssetRelationshipFeeds.AssetRelationshipFeed_Title}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AssetRelationshipFeeds.AssetRelationshipFeed_Type}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="AssetRelationshipFeed"/>
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>AssetRelationshipFeed_Body/AssetRelationshipFeed_Body',type:'sap.ui.model.type.String'}" linkText="{i18n>AssetRelationshipFeed.AssetRelationshipFeed_Body/AssetRelationshipFeed_Body}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>AssetRelationshipFeed_CommentCount/AssetRelationshipFeed_CommentCount',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>AssetRelationshipFeed_CreatedDate/AssetRelationshipFeed_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>AssetRelationshipFeed_Id/AssetRelationshipFeed_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>AssetRelationshipFeed.AssetRelationshipFeed_Id/AssetRelationshipFeed_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>AssetRelationshipFeed_IsDeleted/AssetRelationshipFeed_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>AssetRelationshipFeed_IsRichText/AssetRelationshipFeed_IsRichText',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<Text text="{dataModel>AssetRelationshipFeed_LastModifiedDate/AssetRelationshipFeed_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>AssetRelationshipFeed_LikeCount/AssetRelationshipFeed_LikeCount',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>AssetRelationshipFeed_LinkUrl/AssetRelationshipFeed_LinkUrl',type:'sap.ui.model.type.String'}" linkText="{i18n>AssetRelationshipFeed.AssetRelationshipFeed_LinkUrl/AssetRelationshipFeed_LinkUrl}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>AssetRelationshipFeed_SystemModstamp/AssetRelationshipFeed_SystemModstamp}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>AssetRelationshipFeed_Title/AssetRelationshipFeed_Title',type:'sap.ui.model.type.String'}" linkText="{i18n>AssetRelationshipFeed.AssetRelationshipFeed_Title/AssetRelationshipFeed_Title}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>AssetRelationshipFeed_Type/AssetRelationshipFeed_Type',type:'sap.ui.model.type.String'}" linkText="{i18n>AssetRelationshipFeed.AssetRelationshipFeed_Type/AssetRelationshipFeed_Type}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>