<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>TopicFeed.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>TopicFeed.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="TopicFeed"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="TopicFeed"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TopicFeed.TopicFeed_Body}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>TopicFeed_Body/TopicFeed_Body',type:'sap.ui.model.type.String' }" linkText="{i18n>TopicFeed.TopicFeed_Body/TopicFeed_Body}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TopicFeed.TopicFeed_CommentCount}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>TopicFeed_CommentCount/TopicFeed_CommentCount',type:'sap.ui.model.type.Integer' }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TopicFeed.TopicFeed_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>TopicFeed_CreatedById/sfdc_TopicFeed_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TopicFeed.TopicFeed_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>TopicFeed_CreatedDate/TopicFeed_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TopicFeed.TopicFeed_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>TopicFeed_Id/TopicFeed_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>TopicFeed.TopicFeed_Id/TopicFeed_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TopicFeed.TopicFeed_InsertedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>TopicFeed_InsertedById/sfdc_TopicFeed_InsertedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TopicFeed.TopicFeed_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>TopicFeed_IsDeleted/TopicFeed_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TopicFeed.TopicFeed_IsRichText}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>TopicFeed_IsRichText/TopicFeed_IsRichText',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TopicFeed.TopicFeed_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>TopicFeed_LastModifiedDate/TopicFeed_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TopicFeed.TopicFeed_LikeCount}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>TopicFeed_LikeCount/TopicFeed_LikeCount',type:'sap.ui.model.type.Integer' }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TopicFeed.TopicFeed_LinkUrl}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>TopicFeed_LinkUrl/TopicFeed_LinkUrl',type:'sap.ui.model.type.String' }" linkText="{i18n>TopicFeed.TopicFeed_LinkUrl/TopicFeed_LinkUrl}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TopicFeed.TopicFeed_ParentId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>TopicFeed_ParentId/sfdc_TopicFeed_ParentId" entitySet="Topic"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TopicFeed.TopicFeed_RelatedRecordId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>TopicFeed_RelatedRecordId/sfdc_TopicFeed_RelatedRecordId" entitySet="ContentVersion"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TopicFeed.TopicFeed_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>TopicFeed_SystemModstamp/TopicFeed_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TopicFeed.TopicFeed_Title}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>TopicFeed_Title/TopicFeed_Title',type:'sap.ui.model.type.String' }" linkText="{i18n>TopicFeed.TopicFeed_Title/TopicFeed_Title}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TopicFeed.TopicFeed_Type}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>TopicFeed_Type/TopicFeed_Type',type:'sap.ui.model.type.String' }" linkText="{i18n>TopicFeed.TopicFeed_Type/TopicFeed_Type}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TopicFeed.TopicFeed_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>TopicFeed_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TopicFeed.TopicFeed_InsertedById}" required="false"/> 
						<rdf:ListResource path="dataModel>TopicFeed_InsertedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TopicFeed.TopicFeed_ParentId}" required="false"/> 
						<rdf:ListResource path="dataModel>TopicFeed_ParentId" entitySet="Topic"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TopicFeed.TopicFeed_RelatedRecordId}" required="false"/> 
						<rdf:ListResource path="dataModel>TopicFeed_RelatedRecordId" entitySet="ContentVersion"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>