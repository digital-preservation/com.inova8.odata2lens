<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>SolutionFeed.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>SolutionFeed.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="SolutionFeed"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="SolutionFeed"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SolutionFeed.SolutionFeed_Body}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>SolutionFeed_Body/SolutionFeed_Body',type:'sap.ui.model.type.String' }" linkText="{i18n>SolutionFeed.SolutionFeed_Body/SolutionFeed_Body}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SolutionFeed.SolutionFeed_CommentCount}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>SolutionFeed_CommentCount/SolutionFeed_CommentCount',type:'sap.ui.model.type.Integer' }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SolutionFeed.SolutionFeed_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>SolutionFeed_CreatedById/sfdc_SolutionFeed_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SolutionFeed.SolutionFeed_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>SolutionFeed_CreatedDate/SolutionFeed_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SolutionFeed.SolutionFeed_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>SolutionFeed_Id/SolutionFeed_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>SolutionFeed.SolutionFeed_Id/SolutionFeed_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SolutionFeed.SolutionFeed_InsertedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>SolutionFeed_InsertedById/sfdc_SolutionFeed_InsertedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SolutionFeed.SolutionFeed_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>SolutionFeed_IsDeleted/SolutionFeed_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SolutionFeed.SolutionFeed_IsRichText}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>SolutionFeed_IsRichText/SolutionFeed_IsRichText',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SolutionFeed.SolutionFeed_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>SolutionFeed_LastModifiedDate/SolutionFeed_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SolutionFeed.SolutionFeed_LikeCount}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>SolutionFeed_LikeCount/SolutionFeed_LikeCount',type:'sap.ui.model.type.Integer' }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SolutionFeed.SolutionFeed_LinkUrl}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>SolutionFeed_LinkUrl/SolutionFeed_LinkUrl',type:'sap.ui.model.type.String' }" linkText="{i18n>SolutionFeed.SolutionFeed_LinkUrl/SolutionFeed_LinkUrl}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SolutionFeed.SolutionFeed_ParentId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>SolutionFeed_ParentId/sfdc_SolutionFeed_ParentId" entitySet="Solution"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SolutionFeed.SolutionFeed_RelatedRecordId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>SolutionFeed_RelatedRecordId/sfdc_SolutionFeed_RelatedRecordId" entitySet="ContentVersion"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SolutionFeed.SolutionFeed_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>SolutionFeed_SystemModstamp/SolutionFeed_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SolutionFeed.SolutionFeed_Title}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>SolutionFeed_Title/SolutionFeed_Title',type:'sap.ui.model.type.String' }" linkText="{i18n>SolutionFeed.SolutionFeed_Title/SolutionFeed_Title}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SolutionFeed.SolutionFeed_Type}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>SolutionFeed_Type/SolutionFeed_Type',type:'sap.ui.model.type.String' }" linkText="{i18n>SolutionFeed.SolutionFeed_Type/SolutionFeed_Type}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SolutionFeed.SolutionFeed_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>SolutionFeed_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SolutionFeed.SolutionFeed_InsertedById}" required="false"/> 
						<rdf:ListResource path="dataModel>SolutionFeed_InsertedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SolutionFeed.SolutionFeed_ParentId}" required="false"/> 
						<rdf:ListResource path="dataModel>SolutionFeed_ParentId" entitySet="Solution"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SolutionFeed.SolutionFeed_RelatedRecordId}" required="false"/> 
						<rdf:ListResource path="dataModel>SolutionFeed_RelatedRecordId" entitySet="ContentVersion"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>