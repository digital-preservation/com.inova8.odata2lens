					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CombinedAttachment.CombinedAttachment_ContentUrl}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>CombinedAttachment_ContentUrl/CombinedAttachment_ContentUrl',type:'sap.ui.model.type.String' }" linkText="{i18n>CombinedAttachment.CombinedAttachment_ContentUrl/CombinedAttachment_ContentUrl}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CombinedAttachment.CombinedAttachment_CreatedById}" required="false"/> 
