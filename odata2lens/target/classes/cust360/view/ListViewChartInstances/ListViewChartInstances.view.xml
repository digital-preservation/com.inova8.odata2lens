<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>ListViewChartInstances.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/ListViewChartInstance' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>ListViewChartInstances.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ListViewChartInstances.ListViewChartInstance_AggregateField}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ListViewChartInstances.ListViewChartInstance_AggregateType}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ListViewChartInstances.ListViewChartInstance_ChartType}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ListViewChartInstances.ListViewChartInstance_DataQuery}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ListViewChartInstances.ListViewChartInstance_DeveloperName}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ListViewChartInstances.ListViewChartInstance_ExternalId}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ListViewChartInstances.ListViewChartInstance_GroupingField}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ListViewChartInstances.ListViewChartInstance_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ListViewChartInstances.ListViewChartInstance_IsDeletable}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ListViewChartInstances.ListViewChartInstance_IsEditable}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ListViewChartInstances.ListViewChartInstance_IsLastViewed}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ListViewChartInstances.ListViewChartInstance_Label}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ListViewChartInstances.ListViewChartInstance_SourceEntity}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="ListViewChartInstance"/>
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ListViewChartInstance_AggregateField/ListViewChartInstance_AggregateField',type:'sap.ui.model.type.String'}" linkText="{i18n>ListViewChartInstance.ListViewChartInstance_AggregateField/ListViewChartInstance_AggregateField}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ListViewChartInstance_AggregateType/ListViewChartInstance_AggregateType',type:'sap.ui.model.type.String'}" linkText="{i18n>ListViewChartInstance.ListViewChartInstance_AggregateType/ListViewChartInstance_AggregateType}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ListViewChartInstance_ChartType/ListViewChartInstance_ChartType',type:'sap.ui.model.type.String'}" linkText="{i18n>ListViewChartInstance.ListViewChartInstance_ChartType/ListViewChartInstance_ChartType}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ListViewChartInstance_DataQuery/ListViewChartInstance_DataQuery',type:'sap.ui.model.type.String'}" linkText="{i18n>ListViewChartInstance.ListViewChartInstance_DataQuery/ListViewChartInstance_DataQuery}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ListViewChartInstance_DeveloperName/ListViewChartInstance_DeveloperName',type:'sap.ui.model.type.String'}" linkText="{i18n>ListViewChartInstance.ListViewChartInstance_DeveloperName/ListViewChartInstance_DeveloperName}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ListViewChartInstance_ExternalId/ListViewChartInstance_ExternalId',type:'sap.ui.model.type.String'}" linkText="{i18n>ListViewChartInstance.ListViewChartInstance_ExternalId/ListViewChartInstance_ExternalId}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ListViewChartInstance_GroupingField/ListViewChartInstance_GroupingField',type:'sap.ui.model.type.String'}" linkText="{i18n>ListViewChartInstance.ListViewChartInstance_GroupingField/ListViewChartInstance_GroupingField}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ListViewChartInstance_Id/ListViewChartInstance_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>ListViewChartInstance.ListViewChartInstance_Id/ListViewChartInstance_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>ListViewChartInstance_IsDeletable/ListViewChartInstance_IsDeletable',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>ListViewChartInstance_IsEditable/ListViewChartInstance_IsEditable',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>ListViewChartInstance_IsLastViewed/ListViewChartInstance_IsLastViewed',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ListViewChartInstance_Label/ListViewChartInstance_Label',type:'sap.ui.model.type.String'}" linkText="{i18n>ListViewChartInstance.ListViewChartInstance_Label/ListViewChartInstance_Label}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ListViewChartInstance_SourceEntity/ListViewChartInstance_SourceEntity',type:'sap.ui.model.type.String'}" linkText="{i18n>ListViewChartInstance.ListViewChartInstance_SourceEntity/ListViewChartInstance_SourceEntity}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>