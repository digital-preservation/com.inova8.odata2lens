<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>FeedPollChoice.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>FeedPollChoice.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="FeedPollChoice"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="FeedPollChoice"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>FeedPollChoice.FeedPollChoice_ChoiceBody}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>FeedPollChoice_ChoiceBody/FeedPollChoice_ChoiceBody',type:'sap.ui.model.type.String' }" linkText="{i18n>FeedPollChoice.FeedPollChoice_ChoiceBody/FeedPollChoice_ChoiceBody}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>FeedPollChoice.FeedPollChoice_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>FeedPollChoice_CreatedById/sfdc_FeedPollChoice_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>FeedPollChoice.FeedPollChoice_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>FeedPollChoice_CreatedDate/FeedPollChoice_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>FeedPollChoice.FeedPollChoice_FeedItemId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>FeedPollChoice_FeedItemId/sfdc_FeedPollChoice_FeedItemId" entitySet="AccountFeed"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>FeedPollChoice.FeedPollChoice_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>FeedPollChoice_Id/FeedPollChoice_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>FeedPollChoice.FeedPollChoice_Id/FeedPollChoice_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>FeedPollChoice.FeedPollChoice_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>FeedPollChoice_IsDeleted/FeedPollChoice_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>FeedPollChoice.FeedPollChoice_Position}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>FeedPollChoice_Position/FeedPollChoice_Position',type:'sap.ui.model.type.Integer' }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>FeedPollChoice.FeedPollVote_ChoiceId_Inverse}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>FeedPollVote_ChoiceId_Inverse/sfdc_FeedPollVote_ChoiceId_Inverse" entitySet="FeedPollVote"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>FeedPollChoice.FeedPollChoice_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>FeedPollChoice_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>FeedPollChoice.FeedPollChoice_FeedItemId}" required="false"/> 
						<rdf:ListResource path="dataModel>FeedPollChoice_FeedItemId" entitySet="AccountFeed"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>FeedPollChoice.FeedPollVote_ChoiceId_Inverse}" required="false"/> 
						<rdf:ListResource path="dataModel>FeedPollVote_ChoiceId_Inverse" entitySet="FeedPollVote"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>