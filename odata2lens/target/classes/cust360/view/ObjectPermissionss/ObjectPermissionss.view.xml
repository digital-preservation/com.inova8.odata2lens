<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>ObjectPermissionss.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/ObjectPermissions' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>ObjectPermissionss.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ObjectPermissionss.ObjectPermissions_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ObjectPermissionss.ObjectPermissions_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ObjectPermissionss.ObjectPermissions_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ObjectPermissionss.ObjectPermissions_PermissionsCreate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ObjectPermissionss.ObjectPermissions_PermissionsDelete}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ObjectPermissionss.ObjectPermissions_PermissionsEdit}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ObjectPermissionss.ObjectPermissions_PermissionsModifyAllRecords}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ObjectPermissionss.ObjectPermissions_PermissionsRead}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ObjectPermissionss.ObjectPermissions_PermissionsViewAllRecords}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ObjectPermissionss.ObjectPermissions_SobjectType}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ObjectPermissionss.ObjectPermissions_SystemModstamp}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="ObjectPermissions"/>
							<VBox>					
							<Text text="{dataModel>ObjectPermissions_CreatedDate/ObjectPermissions_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ObjectPermissions_Id/ObjectPermissions_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>ObjectPermissions.ObjectPermissions_Id/ObjectPermissions_Id}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>ObjectPermissions_LastModifiedDate/ObjectPermissions_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>ObjectPermissions_PermissionsCreate/ObjectPermissions_PermissionsCreate',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>ObjectPermissions_PermissionsDelete/ObjectPermissions_PermissionsDelete',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>ObjectPermissions_PermissionsEdit/ObjectPermissions_PermissionsEdit',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>ObjectPermissions_PermissionsModifyAllRecords/ObjectPermissions_PermissionsModifyAllRecords',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>ObjectPermissions_PermissionsRead/ObjectPermissions_PermissionsRead',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>ObjectPermissions_PermissionsViewAllRecords/ObjectPermissions_PermissionsViewAllRecords',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ObjectPermissions_SobjectType/ObjectPermissions_SobjectType',type:'sap.ui.model.type.String'}" linkText="{i18n>ObjectPermissions.ObjectPermissions_SobjectType/ObjectPermissions_SobjectType}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>ObjectPermissions_SystemModstamp/ObjectPermissions_SystemModstamp}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>