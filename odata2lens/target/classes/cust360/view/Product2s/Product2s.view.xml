<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>Product2s.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/Product2' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>Product2s.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Product2s.Product2_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Product2s.Product2_Description}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Product2s.Product2_DisplayUrl}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Product2s.Product2_ExternalId}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Product2s.Product2_Family}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Product2s.Product2_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Product2s.Product2_IsActive}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Product2s.Product2_IsDeleted}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Product2s.Product2_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Product2s.Product2_LastReferencedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Product2s.Product2_LastViewedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Product2s.Product2_Name}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Product2s.Product2_ProductCode}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Product2s.Product2_QuantityUnitOfMeasure}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Product2s.Product2_StockKeepingUnit}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Product2s.Product2_SystemModstamp}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="Product2"/>
							<VBox>					
							<Text text="{dataModel>Product2_CreatedDate/Product2_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Product2_Description/Product2_Description',type:'sap.ui.model.type.String'}" linkText="{i18n>Product2.Product2_Description/Product2_Description}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Product2_DisplayUrl/Product2_DisplayUrl',type:'sap.ui.model.type.String'}" linkText="{i18n>Product2.Product2_DisplayUrl/Product2_DisplayUrl}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Product2_ExternalId/Product2_ExternalId',type:'sap.ui.model.type.String'}" linkText="{i18n>Product2.Product2_ExternalId/Product2_ExternalId}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Product2_Family/Product2_Family',type:'sap.ui.model.type.String'}" linkText="{i18n>Product2.Product2_Family/Product2_Family}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Product2_Id/Product2_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>Product2.Product2_Id/Product2_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>Product2_IsActive/Product2_IsActive',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>Product2_IsDeleted/Product2_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<Text text="{dataModel>Product2_LastModifiedDate/Product2_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>Product2_LastReferencedDate/Product2_LastReferencedDate}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>Product2_LastViewedDate/Product2_LastViewedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Product2_Name/Product2_Name',type:'sap.ui.model.type.String'}" linkText="{i18n>Product2.Product2_Name/Product2_Name}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Product2_ProductCode/Product2_ProductCode',type:'sap.ui.model.type.String'}" linkText="{i18n>Product2.Product2_ProductCode/Product2_ProductCode}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Product2_QuantityUnitOfMeasure/Product2_QuantityUnitOfMeasure',type:'sap.ui.model.type.String'}" linkText="{i18n>Product2.Product2_QuantityUnitOfMeasure/Product2_QuantityUnitOfMeasure}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Product2_StockKeepingUnit/Product2_StockKeepingUnit',type:'sap.ui.model.type.String'}" linkText="{i18n>Product2.Product2_StockKeepingUnit/Product2_StockKeepingUnit}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>Product2_SystemModstamp/Product2_SystemModstamp}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>