<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>Attachment.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>Attachment.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="Attachment"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="Attachment"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Attachment.Attachment_Body}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>Attachment_Body/Attachment_Body}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Attachment.Attachment_BodyLength}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>Attachment_BodyLength/Attachment_BodyLength',type:'sap.ui.model.type.Integer' }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Attachment.Attachment_ContentType}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>Attachment_ContentType/Attachment_ContentType',type:'sap.ui.model.type.String' }" linkText="{i18n>Attachment.Attachment_ContentType/Attachment_ContentType}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Attachment.Attachment_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>Attachment_CreatedById/sfdc_Attachment_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Attachment.Attachment_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>Attachment_CreatedDate/Attachment_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Attachment.Attachment_Description}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>Attachment_Description/Attachment_Description',type:'sap.ui.model.type.String' }" linkText="{i18n>Attachment.Attachment_Description/Attachment_Description}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Attachment.Attachment_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>Attachment_Id/Attachment_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>Attachment.Attachment_Id/Attachment_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Attachment.Attachment_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>Attachment_IsDeleted/Attachment_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Attachment.Attachment_IsPrivate}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>Attachment_IsPrivate/Attachment_IsPrivate',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Attachment.Attachment_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>Attachment_LastModifiedById/sfdc_Attachment_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Attachment.Attachment_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>Attachment_LastModifiedDate/Attachment_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Attachment.Attachment_Name}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>Attachment_Name/Attachment_Name',type:'sap.ui.model.type.String' }" linkText="{i18n>Attachment.Attachment_Name/Attachment_Name}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Attachment.Attachment_OwnerId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>Attachment_OwnerId/sfdc_Attachment_OwnerId" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Attachment.Attachment_ParentId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>Attachment_ParentId/sfdc_Attachment_ParentId" entitySet="Account"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Attachment.Attachment_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>Attachment_SystemModstamp/Attachment_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Attachment.Attachment_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>Attachment_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Attachment.Attachment_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>Attachment_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Attachment.Attachment_OwnerId}" required="false"/> 
						<rdf:ListResource path="dataModel>Attachment_OwnerId" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Attachment.Attachment_ParentId}" required="false"/> 
						<rdf:ListResource path="dataModel>Attachment_ParentId" entitySet="Account"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>