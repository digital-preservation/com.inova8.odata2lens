						<rdf:ListResource path="dataModel>ContractFeed_ParentId" entitySet="Contract"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContractFeed.ContractFeed_RelatedRecordId}" required="false"/> 
						<rdf:ListResource path="dataModel>ContractFeed_RelatedRecordId" entitySet="ContentVersion"/>
