					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContractFeed.ContractFeed_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ContractFeed_Id/ContractFeed_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>ContractFeed.ContractFeed_Id/ContractFeed_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContractFeed.ContractFeed_InsertedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ContractFeed_InsertedById/sfdc_ContractFeed_InsertedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContractFeed.ContractFeed_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>ContractFeed_IsDeleted/ContractFeed_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContractFeed.ContractFeed_IsRichText}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>ContractFeed_IsRichText/ContractFeed_IsRichText',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContractFeed.ContractFeed_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>ContractFeed_LastModifiedDate/ContractFeed_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContractFeed.ContractFeed_LikeCount}" required="false"/> 
						<Text text=""/>
