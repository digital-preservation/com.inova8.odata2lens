					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContractFeed.ContractFeed_Title}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ContractFeed_Title/ContractFeed_Title',type:'sap.ui.model.type.String' }" linkText="{i18n>ContractFeed.ContractFeed_Title/ContractFeed_Title}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContractFeed.ContractFeed_Type}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ContractFeed_Type/ContractFeed_Type',type:'sap.ui.model.type.String' }" linkText="{i18n>ContractFeed.ContractFeed_Type/ContractFeed_Type}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContractFeed.ContractFeed_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>ContractFeed_CreatedById" entitySet="User"/>
						<Text text=""/>
