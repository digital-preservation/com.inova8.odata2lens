<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>OauthToken.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>OauthToken.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="OauthToken"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="OauthToken"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OauthToken.OauthToken_AccessToken}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>OauthToken_AccessToken/OauthToken_AccessToken',type:'sap.ui.model.type.String' }" linkText="{i18n>OauthToken.OauthToken_AccessToken/OauthToken_AccessToken}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OauthToken.OauthToken_AppMenuItemId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>OauthToken_AppMenuItemId/sfdc_OauthToken_AppMenuItemId" entitySet="AppMenuItem"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OauthToken.OauthToken_AppName}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>OauthToken_AppName/OauthToken_AppName',type:'sap.ui.model.type.String' }" linkText="{i18n>OauthToken.OauthToken_AppName/OauthToken_AppName}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OauthToken.OauthToken_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>OauthToken_CreatedDate/OauthToken_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OauthToken.OauthToken_DeleteToken}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>OauthToken_DeleteToken/OauthToken_DeleteToken',type:'sap.ui.model.type.String' }" linkText="{i18n>OauthToken.OauthToken_DeleteToken/OauthToken_DeleteToken}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OauthToken.OauthToken_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>OauthToken_Id/OauthToken_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>OauthToken.OauthToken_Id/OauthToken_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OauthToken.OauthToken_LastUsedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>OauthToken_LastUsedDate/OauthToken_LastUsedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OauthToken.OauthToken_RequestToken}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>OauthToken_RequestToken/OauthToken_RequestToken',type:'sap.ui.model.type.String' }" linkText="{i18n>OauthToken.OauthToken_RequestToken/OauthToken_RequestToken}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OauthToken.OauthToken_UseCount}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>OauthToken_UseCount/OauthToken_UseCount',type:'sap.ui.model.type.Integer' }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OauthToken.OauthToken_UserId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>OauthToken_UserId/sfdc_OauthToken_UserId" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OauthToken.OauthToken_AppMenuItemId}" required="false"/> 
						<rdf:ListResource path="dataModel>OauthToken_AppMenuItemId" entitySet="AppMenuItem"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OauthToken.OauthToken_UserId}" required="false"/> 
						<rdf:ListResource path="dataModel>OauthToken_UserId" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>