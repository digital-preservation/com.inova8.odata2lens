<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>OrderFeed.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>OrderFeed.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="OrderFeed"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="OrderFeed"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OrderFeed.OrderFeed_Body}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>OrderFeed_Body/OrderFeed_Body',type:'sap.ui.model.type.String' }" linkText="{i18n>OrderFeed.OrderFeed_Body/OrderFeed_Body}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OrderFeed.OrderFeed_CommentCount}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>OrderFeed_CommentCount/OrderFeed_CommentCount',type:'sap.ui.model.type.Integer' }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OrderFeed.OrderFeed_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>OrderFeed_CreatedById/sfdc_OrderFeed_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OrderFeed.OrderFeed_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>OrderFeed_CreatedDate/OrderFeed_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OrderFeed.OrderFeed_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>OrderFeed_Id/OrderFeed_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>OrderFeed.OrderFeed_Id/OrderFeed_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OrderFeed.OrderFeed_InsertedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>OrderFeed_InsertedById/sfdc_OrderFeed_InsertedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OrderFeed.OrderFeed_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>OrderFeed_IsDeleted/OrderFeed_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OrderFeed.OrderFeed_IsRichText}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>OrderFeed_IsRichText/OrderFeed_IsRichText',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OrderFeed.OrderFeed_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>OrderFeed_LastModifiedDate/OrderFeed_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OrderFeed.OrderFeed_LikeCount}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>OrderFeed_LikeCount/OrderFeed_LikeCount',type:'sap.ui.model.type.Integer' }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OrderFeed.OrderFeed_LinkUrl}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>OrderFeed_LinkUrl/OrderFeed_LinkUrl',type:'sap.ui.model.type.String' }" linkText="{i18n>OrderFeed.OrderFeed_LinkUrl/OrderFeed_LinkUrl}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OrderFeed.OrderFeed_ParentId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>OrderFeed_ParentId/sfdc_OrderFeed_ParentId" entitySet="Order"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OrderFeed.OrderFeed_RelatedRecordId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>OrderFeed_RelatedRecordId/sfdc_OrderFeed_RelatedRecordId" entitySet="ContentVersion"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OrderFeed.OrderFeed_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>OrderFeed_SystemModstamp/OrderFeed_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OrderFeed.OrderFeed_Title}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>OrderFeed_Title/OrderFeed_Title',type:'sap.ui.model.type.String' }" linkText="{i18n>OrderFeed.OrderFeed_Title/OrderFeed_Title}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OrderFeed.OrderFeed_Type}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>OrderFeed_Type/OrderFeed_Type',type:'sap.ui.model.type.String' }" linkText="{i18n>OrderFeed.OrderFeed_Type/OrderFeed_Type}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OrderFeed.OrderFeed_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>OrderFeed_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OrderFeed.OrderFeed_InsertedById}" required="false"/> 
						<rdf:ListResource path="dataModel>OrderFeed_InsertedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OrderFeed.OrderFeed_ParentId}" required="false"/> 
						<rdf:ListResource path="dataModel>OrderFeed_ParentId" entitySet="Order"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OrderFeed.OrderFeed_RelatedRecordId}" required="false"/> 
						<rdf:ListResource path="dataModel>OrderFeed_RelatedRecordId" entitySet="ContentVersion"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>