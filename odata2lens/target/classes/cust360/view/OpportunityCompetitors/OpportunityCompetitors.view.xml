<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>OpportunityCompetitors.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/OpportunityCompetitor' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>OpportunityCompetitors.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>OpportunityCompetitors.OpportunityCompetitor_CompetitorName}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>OpportunityCompetitors.OpportunityCompetitor_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>OpportunityCompetitors.OpportunityCompetitor_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>OpportunityCompetitors.OpportunityCompetitor_IsDeleted}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>OpportunityCompetitors.OpportunityCompetitor_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>OpportunityCompetitors.OpportunityCompetitor_Strengths}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>OpportunityCompetitors.OpportunityCompetitor_SystemModstamp}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>OpportunityCompetitors.OpportunityCompetitor_Weaknesses}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="OpportunityCompetitor"/>
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>OpportunityCompetitor_CompetitorName/OpportunityCompetitor_CompetitorName',type:'sap.ui.model.type.String'}" linkText="{i18n>OpportunityCompetitor.OpportunityCompetitor_CompetitorName/OpportunityCompetitor_CompetitorName}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>OpportunityCompetitor_CreatedDate/OpportunityCompetitor_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>OpportunityCompetitor_Id/OpportunityCompetitor_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>OpportunityCompetitor.OpportunityCompetitor_Id/OpportunityCompetitor_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>OpportunityCompetitor_IsDeleted/OpportunityCompetitor_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<Text text="{dataModel>OpportunityCompetitor_LastModifiedDate/OpportunityCompetitor_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>OpportunityCompetitor_Strengths/OpportunityCompetitor_Strengths',type:'sap.ui.model.type.String'}" linkText="{i18n>OpportunityCompetitor.OpportunityCompetitor_Strengths/OpportunityCompetitor_Strengths}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>OpportunityCompetitor_SystemModstamp/OpportunityCompetitor_SystemModstamp}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>OpportunityCompetitor_Weaknesses/OpportunityCompetitor_Weaknesses',type:'sap.ui.model.type.String'}" linkText="{i18n>OpportunityCompetitor.OpportunityCompetitor_Weaknesses/OpportunityCompetitor_Weaknesses}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>