<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>OrgLifecycleNotification.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>OrgLifecycleNotification.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="OrgLifecycleNotification"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="OrgLifecycleNotification"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OrgLifecycleNotification.OrgLifecycleNotification_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>OrgLifecycleNotification_CreatedById/sfdc_OrgLifecycleNotification_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OrgLifecycleNotification.OrgLifecycleNotification_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>OrgLifecycleNotification_CreatedDate/OrgLifecycleNotification_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OrgLifecycleNotification.OrgLifecycleNotification_LifecycleRequestId}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>OrgLifecycleNotification_LifecycleRequestId/OrgLifecycleNotification_LifecycleRequestId',type:'sap.ui.model.type.String' }" linkText="{i18n>OrgLifecycleNotification.OrgLifecycleNotification_LifecycleRequestId/OrgLifecycleNotification_LifecycleRequestId}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OrgLifecycleNotification.OrgLifecycleNotification_LifecycleRequestType}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>OrgLifecycleNotification_LifecycleRequestType/OrgLifecycleNotification_LifecycleRequestType',type:'sap.ui.model.type.String' }" linkText="{i18n>OrgLifecycleNotification.OrgLifecycleNotification_LifecycleRequestType/OrgLifecycleNotification_LifecycleRequestType}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OrgLifecycleNotification.OrgLifecycleNotification_OrgId}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>OrgLifecycleNotification_OrgId/OrgLifecycleNotification_OrgId',type:'sap.ui.model.type.String' }" linkText="{i18n>OrgLifecycleNotification.OrgLifecycleNotification_OrgId/OrgLifecycleNotification_OrgId}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OrgLifecycleNotification.OrgLifecycleNotification_ReplayId}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>OrgLifecycleNotification_ReplayId/OrgLifecycleNotification_ReplayId',type:'sap.ui.model.type.String' }" linkText="{i18n>OrgLifecycleNotification.OrgLifecycleNotification_ReplayId/OrgLifecycleNotification_ReplayId}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OrgLifecycleNotification.OrgLifecycleNotification_Status}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>OrgLifecycleNotification_Status/OrgLifecycleNotification_Status',type:'sap.ui.model.type.String' }" linkText="{i18n>OrgLifecycleNotification.OrgLifecycleNotification_Status/OrgLifecycleNotification_Status}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OrgLifecycleNotification.OrgLifecycleNotification_StatusCode}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>OrgLifecycleNotification_StatusCode/OrgLifecycleNotification_StatusCode',type:'sap.ui.model.type.String' }" linkText="{i18n>OrgLifecycleNotification.OrgLifecycleNotification_StatusCode/OrgLifecycleNotification_StatusCode}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OrgLifecycleNotification.OrgLifecycleNotification_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>OrgLifecycleNotification_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>