<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>ThirdPartyAccountLinks.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/ThirdPartyAccountLink' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>ThirdPartyAccountLinks.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ThirdPartyAccountLinks.ThirdPartyAccountLink_Handle}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ThirdPartyAccountLinks.ThirdPartyAccountLink_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ThirdPartyAccountLinks.ThirdPartyAccountLink_IsNotSsoUsable}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ThirdPartyAccountLinks.ThirdPartyAccountLink_Provider}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ThirdPartyAccountLinks.ThirdPartyAccountLink_RemoteIdentifier}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ThirdPartyAccountLinks.ThirdPartyAccountLink_SsoProviderName}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ThirdPartyAccountLinks.ThirdPartyAccountLink_ThirdPartyAccountLinkKey}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="ThirdPartyAccountLink"/>
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ThirdPartyAccountLink_Handle/ThirdPartyAccountLink_Handle',type:'sap.ui.model.type.String'}" linkText="{i18n>ThirdPartyAccountLink.ThirdPartyAccountLink_Handle/ThirdPartyAccountLink_Handle}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ThirdPartyAccountLink_Id/ThirdPartyAccountLink_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>ThirdPartyAccountLink.ThirdPartyAccountLink_Id/ThirdPartyAccountLink_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>ThirdPartyAccountLink_IsNotSsoUsable/ThirdPartyAccountLink_IsNotSsoUsable',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ThirdPartyAccountLink_Provider/ThirdPartyAccountLink_Provider',type:'sap.ui.model.type.String'}" linkText="{i18n>ThirdPartyAccountLink.ThirdPartyAccountLink_Provider/ThirdPartyAccountLink_Provider}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ThirdPartyAccountLink_RemoteIdentifier/ThirdPartyAccountLink_RemoteIdentifier',type:'sap.ui.model.type.String'}" linkText="{i18n>ThirdPartyAccountLink.ThirdPartyAccountLink_RemoteIdentifier/ThirdPartyAccountLink_RemoteIdentifier}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ThirdPartyAccountLink_SsoProviderName/ThirdPartyAccountLink_SsoProviderName',type:'sap.ui.model.type.String'}" linkText="{i18n>ThirdPartyAccountLink.ThirdPartyAccountLink_SsoProviderName/ThirdPartyAccountLink_SsoProviderName}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ThirdPartyAccountLink_ThirdPartyAccountLinkKey/ThirdPartyAccountLink_ThirdPartyAccountLinkKey',type:'sap.ui.model.type.String'}" linkText="{i18n>ThirdPartyAccountLink.ThirdPartyAccountLink_ThirdPartyAccountLinkKey/ThirdPartyAccountLink_ThirdPartyAccountLinkKey}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>