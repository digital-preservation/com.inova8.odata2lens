<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>TestSuiteMembership.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>TestSuiteMembership.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="TestSuiteMembership"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="TestSuiteMembership"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TestSuiteMembership.TestSuiteMembership_ApexClassId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>TestSuiteMembership_ApexClassId/sfdc_TestSuiteMembership_ApexClassId" entitySet="ApexClass"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TestSuiteMembership.TestSuiteMembership_ApexTestSuiteId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>TestSuiteMembership_ApexTestSuiteId/sfdc_TestSuiteMembership_ApexTestSuiteId" entitySet="ApexTestSuite"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TestSuiteMembership.TestSuiteMembership_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>TestSuiteMembership_CreatedById/sfdc_TestSuiteMembership_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TestSuiteMembership.TestSuiteMembership_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>TestSuiteMembership_CreatedDate/TestSuiteMembership_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TestSuiteMembership.TestSuiteMembership_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>TestSuiteMembership_Id/TestSuiteMembership_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>TestSuiteMembership.TestSuiteMembership_Id/TestSuiteMembership_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TestSuiteMembership.TestSuiteMembership_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>TestSuiteMembership_IsDeleted/TestSuiteMembership_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TestSuiteMembership.TestSuiteMembership_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>TestSuiteMembership_LastModifiedById/sfdc_TestSuiteMembership_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TestSuiteMembership.TestSuiteMembership_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>TestSuiteMembership_LastModifiedDate/TestSuiteMembership_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TestSuiteMembership.TestSuiteMembership_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>TestSuiteMembership_SystemModstamp/TestSuiteMembership_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TestSuiteMembership.TestSuiteMembership_ApexClassId}" required="false"/> 
						<rdf:ListResource path="dataModel>TestSuiteMembership_ApexClassId" entitySet="ApexClass"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TestSuiteMembership.TestSuiteMembership_ApexTestSuiteId}" required="false"/> 
						<rdf:ListResource path="dataModel>TestSuiteMembership_ApexTestSuiteId" entitySet="ApexTestSuite"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TestSuiteMembership.TestSuiteMembership_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>TestSuiteMembership_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TestSuiteMembership.TestSuiteMembership_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>TestSuiteMembership_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>