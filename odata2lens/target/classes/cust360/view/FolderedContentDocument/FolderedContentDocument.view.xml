						<rdf:ListResource path="dataModel>FolderedContentDocument_LastModifiedById/sfdc_FolderedContentDocument_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>FolderedContentDocument.FolderedContentDocument_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>FolderedContentDocument_LastModifiedDate/FolderedContentDocument_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
