						<rdf:TextLink text="{path:'dataModel>FolderedContentDocument_Id/FolderedContentDocument_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>FolderedContentDocument.FolderedContentDocument_Id/FolderedContentDocument_Id}"/>
