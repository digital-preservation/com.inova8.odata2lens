				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>FolderedContentDocument.FolderedContentDocument_FileExtension}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>FolderedContentDocument_FileExtension/FolderedContentDocument_FileExtension',type:'sap.ui.model.type.String' }" linkText="{i18n>FolderedContentDocument.FolderedContentDocument_FileExtension/FolderedContentDocument_FileExtension}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>FolderedContentDocument.FolderedContentDocument_FileType}" required="false"/> 
