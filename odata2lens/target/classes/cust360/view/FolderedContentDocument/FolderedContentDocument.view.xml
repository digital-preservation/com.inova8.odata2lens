						<rdf:ListResource path="dataModel>FolderedContentDocument_ContentDocumentId/sfdc_FolderedContentDocument_ContentDocumentId" entitySet="ContentDocument"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>FolderedContentDocument.FolderedContentDocument_ContentSize}" required="false"/> 
