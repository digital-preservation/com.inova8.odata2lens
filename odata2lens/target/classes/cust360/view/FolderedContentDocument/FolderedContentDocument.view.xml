						<rdf:TextLink text="{path:'dataModel>FolderedContentDocument_Title/FolderedContentDocument_Title',type:'sap.ui.model.type.String' }" linkText="{i18n>FolderedContentDocument.FolderedContentDocument_Title/FolderedContentDocument_Title}"/>
