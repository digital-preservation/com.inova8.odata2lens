<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>ScontrolLocalization.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>ScontrolLocalization.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="ScontrolLocalization"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="ScontrolLocalization"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ScontrolLocalization.ScontrolLocalization_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ScontrolLocalization_CreatedById/sfdc_ScontrolLocalization_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ScontrolLocalization.ScontrolLocalization_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>ScontrolLocalization_CreatedDate/ScontrolLocalization_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ScontrolLocalization.ScontrolLocalization_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ScontrolLocalization_Id/ScontrolLocalization_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>ScontrolLocalization.ScontrolLocalization_Id/ScontrolLocalization_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ScontrolLocalization.ScontrolLocalization_Language}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ScontrolLocalization_Language/ScontrolLocalization_Language',type:'sap.ui.model.type.String' }" linkText="{i18n>ScontrolLocalization.ScontrolLocalization_Language/ScontrolLocalization_Language}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ScontrolLocalization.ScontrolLocalization_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ScontrolLocalization_LastModifiedById/sfdc_ScontrolLocalization_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ScontrolLocalization.ScontrolLocalization_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>ScontrolLocalization_LastModifiedDate/ScontrolLocalization_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ScontrolLocalization.ScontrolLocalization_NamespacePrefix}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ScontrolLocalization_NamespacePrefix/ScontrolLocalization_NamespacePrefix',type:'sap.ui.model.type.String' }" linkText="{i18n>ScontrolLocalization.ScontrolLocalization_NamespacePrefix/ScontrolLocalization_NamespacePrefix}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ScontrolLocalization.ScontrolLocalization_ScontrolId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ScontrolLocalization_ScontrolId/sfdc_ScontrolLocalization_ScontrolId" entitySet="Scontrol"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ScontrolLocalization.ScontrolLocalization_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>ScontrolLocalization_SystemModstamp/ScontrolLocalization_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ScontrolLocalization.ScontrolLocalization_Value}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ScontrolLocalization_Value/ScontrolLocalization_Value',type:'sap.ui.model.type.String' }" linkText="{i18n>ScontrolLocalization.ScontrolLocalization_Value/ScontrolLocalization_Value}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ScontrolLocalization.ScontrolLocalization_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>ScontrolLocalization_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ScontrolLocalization.ScontrolLocalization_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>ScontrolLocalization_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ScontrolLocalization.ScontrolLocalization_ScontrolId}" required="false"/> 
						<rdf:ListResource path="dataModel>ScontrolLocalization_ScontrolId" entitySet="Scontrol"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>