<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>UserPackageLicense.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>UserPackageLicense.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="UserPackageLicense"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="UserPackageLicense"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserPackageLicense.UserPackageLicense_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>UserPackageLicense_CreatedById/sfdc_UserPackageLicense_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserPackageLicense.UserPackageLicense_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>UserPackageLicense_CreatedDate/UserPackageLicense_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserPackageLicense.UserPackageLicense_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>UserPackageLicense_Id/UserPackageLicense_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>UserPackageLicense.UserPackageLicense_Id/UserPackageLicense_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserPackageLicense.UserPackageLicense_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>UserPackageLicense_LastModifiedById/sfdc_UserPackageLicense_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserPackageLicense.UserPackageLicense_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>UserPackageLicense_LastModifiedDate/UserPackageLicense_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserPackageLicense.UserPackageLicense_PackageLicenseId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>UserPackageLicense_PackageLicenseId/sfdc_UserPackageLicense_PackageLicenseId" entitySet="PackageLicense"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserPackageLicense.UserPackageLicense_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>UserPackageLicense_SystemModstamp/UserPackageLicense_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserPackageLicense.UserPackageLicense_UserId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>UserPackageLicense_UserId/sfdc_UserPackageLicense_UserId" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserPackageLicense.UserPackageLicense_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>UserPackageLicense_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserPackageLicense.UserPackageLicense_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>UserPackageLicense_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserPackageLicense.UserPackageLicense_PackageLicenseId}" required="false"/> 
						<rdf:ListResource path="dataModel>UserPackageLicense_PackageLicenseId" entitySet="PackageLicense"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserPackageLicense.UserPackageLicense_UserId}" required="false"/> 
						<rdf:ListResource path="dataModel>UserPackageLicense_UserId" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>