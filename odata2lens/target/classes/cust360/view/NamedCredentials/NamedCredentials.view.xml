<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>NamedCredentials.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/NamedCredential' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>NamedCredentials.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>NamedCredentials.NamedCredential_CalloutOptionsAllowMergeFieldsInBody}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>NamedCredentials.NamedCredential_CalloutOptionsAllowMergeFieldsInHeader}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>NamedCredentials.NamedCredential_CalloutOptionsGenerateAuthorizationHeader}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>NamedCredentials.NamedCredential_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>NamedCredentials.NamedCredential_DeveloperName}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>NamedCredentials.NamedCredential_Endpoint}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>NamedCredentials.NamedCredential_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>NamedCredentials.NamedCredential_IsDeleted}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>NamedCredentials.NamedCredential_Language}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>NamedCredentials.NamedCredential_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>NamedCredentials.NamedCredential_MasterLabel}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>NamedCredentials.NamedCredential_NamespacePrefix}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>NamedCredentials.NamedCredential_PrincipalType}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>NamedCredentials.NamedCredential_SystemModstamp}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="NamedCredential"/>
							<VBox>					
								<Text text="{path:'dataModel>NamedCredential_CalloutOptionsAllowMergeFieldsInBody/NamedCredential_CalloutOptionsAllowMergeFieldsInBody',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>NamedCredential_CalloutOptionsAllowMergeFieldsInHeader/NamedCredential_CalloutOptionsAllowMergeFieldsInHeader',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>NamedCredential_CalloutOptionsGenerateAuthorizationHeader/NamedCredential_CalloutOptionsGenerateAuthorizationHeader',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<Text text="{dataModel>NamedCredential_CreatedDate/NamedCredential_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>NamedCredential_DeveloperName/NamedCredential_DeveloperName',type:'sap.ui.model.type.String'}" linkText="{i18n>NamedCredential.NamedCredential_DeveloperName/NamedCredential_DeveloperName}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>NamedCredential_Endpoint/NamedCredential_Endpoint',type:'sap.ui.model.type.String'}" linkText="{i18n>NamedCredential.NamedCredential_Endpoint/NamedCredential_Endpoint}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>NamedCredential_Id/NamedCredential_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>NamedCredential.NamedCredential_Id/NamedCredential_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>NamedCredential_IsDeleted/NamedCredential_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>NamedCredential_Language/NamedCredential_Language',type:'sap.ui.model.type.String'}" linkText="{i18n>NamedCredential.NamedCredential_Language/NamedCredential_Language}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>NamedCredential_LastModifiedDate/NamedCredential_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>NamedCredential_MasterLabel/NamedCredential_MasterLabel',type:'sap.ui.model.type.String'}" linkText="{i18n>NamedCredential.NamedCredential_MasterLabel/NamedCredential_MasterLabel}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>NamedCredential_NamespacePrefix/NamedCredential_NamespacePrefix',type:'sap.ui.model.type.String'}" linkText="{i18n>NamedCredential.NamedCredential_NamespacePrefix/NamedCredential_NamespacePrefix}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>NamedCredential_PrincipalType/NamedCredential_PrincipalType',type:'sap.ui.model.type.String'}" linkText="{i18n>NamedCredential.NamedCredential_PrincipalType/NamedCredential_PrincipalType}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>NamedCredential_SystemModstamp/NamedCredential_SystemModstamp}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>