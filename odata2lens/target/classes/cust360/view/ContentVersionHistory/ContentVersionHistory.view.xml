<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>ContentVersionHistory.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>ContentVersionHistory.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="ContentVersionHistory"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="ContentVersionHistory"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentVersionHistory.ContentVersionHistory_ContentVersionId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ContentVersionHistory_ContentVersionId/sfdc_ContentVersionHistory_ContentVersionId" entitySet="ContentVersion"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentVersionHistory.ContentVersionHistory_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ContentVersionHistory_CreatedById/sfdc_ContentVersionHistory_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentVersionHistory.ContentVersionHistory_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>ContentVersionHistory_CreatedDate/ContentVersionHistory_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentVersionHistory.ContentVersionHistory_Field}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ContentVersionHistory_Field/ContentVersionHistory_Field',type:'sap.ui.model.type.String' }" linkText="{i18n>ContentVersionHistory.ContentVersionHistory_Field/ContentVersionHistory_Field}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentVersionHistory.ContentVersionHistory_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ContentVersionHistory_Id/ContentVersionHistory_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>ContentVersionHistory.ContentVersionHistory_Id/ContentVersionHistory_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentVersionHistory.ContentVersionHistory_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>ContentVersionHistory_IsDeleted/ContentVersionHistory_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentVersionHistory.ContentVersionHistory_NewValue}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ContentVersionHistory_NewValue/ContentVersionHistory_NewValue',type:'sap.ui.model.type.String' }" linkText="{i18n>ContentVersionHistory.ContentVersionHistory_NewValue/ContentVersionHistory_NewValue}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentVersionHistory.ContentVersionHistory_OldValue}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ContentVersionHistory_OldValue/ContentVersionHistory_OldValue',type:'sap.ui.model.type.String' }" linkText="{i18n>ContentVersionHistory.ContentVersionHistory_OldValue/ContentVersionHistory_OldValue}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentVersionHistory.ContentVersionHistory_ContentVersionId}" required="false"/> 
						<rdf:ListResource path="dataModel>ContentVersionHistory_ContentVersionId" entitySet="ContentVersion"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentVersionHistory.ContentVersionHistory_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>ContentVersionHistory_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>