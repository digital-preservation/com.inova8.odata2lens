<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>EventFeed.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>EventFeed.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="EventFeed"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="EventFeed"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EventFeed.EventFeed_Body}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>EventFeed_Body/EventFeed_Body',type:'sap.ui.model.type.String' }" linkText="{i18n>EventFeed.EventFeed_Body/EventFeed_Body}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EventFeed.EventFeed_CommentCount}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>EventFeed_CommentCount/EventFeed_CommentCount',type:'sap.ui.model.type.Integer' }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EventFeed.EventFeed_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>EventFeed_CreatedById/sfdc_EventFeed_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EventFeed.EventFeed_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>EventFeed_CreatedDate/EventFeed_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EventFeed.EventFeed_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>EventFeed_Id/EventFeed_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>EventFeed.EventFeed_Id/EventFeed_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EventFeed.EventFeed_InsertedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>EventFeed_InsertedById/sfdc_EventFeed_InsertedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EventFeed.EventFeed_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>EventFeed_IsDeleted/EventFeed_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EventFeed.EventFeed_IsRichText}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>EventFeed_IsRichText/EventFeed_IsRichText',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EventFeed.EventFeed_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>EventFeed_LastModifiedDate/EventFeed_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EventFeed.EventFeed_LikeCount}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>EventFeed_LikeCount/EventFeed_LikeCount',type:'sap.ui.model.type.Integer' }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EventFeed.EventFeed_LinkUrl}" required="false"/> 
						<Text text=""/>
