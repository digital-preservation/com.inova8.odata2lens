					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EventFeed.EventFeed_ParentId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>EventFeed_ParentId/sfdc_EventFeed_ParentId" entitySet="Event"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EventFeed.EventFeed_RelatedRecordId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>EventFeed_RelatedRecordId/sfdc_EventFeed_RelatedRecordId" entitySet="ContentVersion"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EventFeed.EventFeed_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>EventFeed_SystemModstamp/EventFeed_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EventFeed.EventFeed_Title}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>EventFeed_Title/EventFeed_Title',type:'sap.ui.model.type.String' }" linkText="{i18n>EventFeed.EventFeed_Title/EventFeed_Title}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EventFeed.EventFeed_Type}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>EventFeed_Type/EventFeed_Type',type:'sap.ui.model.type.String' }" linkText="{i18n>EventFeed.EventFeed_Type/EventFeed_Type}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EventFeed.EventFeed_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>EventFeed_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
