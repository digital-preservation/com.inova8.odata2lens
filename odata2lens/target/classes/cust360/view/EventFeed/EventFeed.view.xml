					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EventFeed.EventFeed_ParentId}" required="false"/> 
						<rdf:ListResource path="dataModel>EventFeed_ParentId" entitySet="Event"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EventFeed.EventFeed_RelatedRecordId}" required="false"/> 
						<rdf:ListResource path="dataModel>EventFeed_RelatedRecordId" entitySet="ContentVersion"/>
						<Text text=""/>
