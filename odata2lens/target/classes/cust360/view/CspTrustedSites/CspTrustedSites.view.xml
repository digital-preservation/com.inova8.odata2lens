<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>CspTrustedSites.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/CspTrustedSite' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>CspTrustedSites.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CspTrustedSites.CspTrustedSite_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CspTrustedSites.CspTrustedSite_Description}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CspTrustedSites.CspTrustedSite_DeveloperName}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CspTrustedSites.CspTrustedSite_EndpointUrl}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CspTrustedSites.CspTrustedSite_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CspTrustedSites.CspTrustedSite_IsActive}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CspTrustedSites.CspTrustedSite_IsDeleted}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CspTrustedSites.CspTrustedSite_Language}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CspTrustedSites.CspTrustedSite_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CspTrustedSites.CspTrustedSite_MasterLabel}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CspTrustedSites.CspTrustedSite_NamespacePrefix}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CspTrustedSites.CspTrustedSite_SystemModstamp}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="CspTrustedSite"/>
							<VBox>					
							<Text text="{dataModel>CspTrustedSite_CreatedDate/CspTrustedSite_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>CspTrustedSite_Description/CspTrustedSite_Description',type:'sap.ui.model.type.String'}" linkText="{i18n>CspTrustedSite.CspTrustedSite_Description/CspTrustedSite_Description}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>CspTrustedSite_DeveloperName/CspTrustedSite_DeveloperName',type:'sap.ui.model.type.String'}" linkText="{i18n>CspTrustedSite.CspTrustedSite_DeveloperName/CspTrustedSite_DeveloperName}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>CspTrustedSite_EndpointUrl/CspTrustedSite_EndpointUrl',type:'sap.ui.model.type.String'}" linkText="{i18n>CspTrustedSite.CspTrustedSite_EndpointUrl/CspTrustedSite_EndpointUrl}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>CspTrustedSite_Id/CspTrustedSite_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>CspTrustedSite.CspTrustedSite_Id/CspTrustedSite_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>CspTrustedSite_IsActive/CspTrustedSite_IsActive',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>CspTrustedSite_IsDeleted/CspTrustedSite_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>CspTrustedSite_Language/CspTrustedSite_Language',type:'sap.ui.model.type.String'}" linkText="{i18n>CspTrustedSite.CspTrustedSite_Language/CspTrustedSite_Language}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>CspTrustedSite_LastModifiedDate/CspTrustedSite_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>CspTrustedSite_MasterLabel/CspTrustedSite_MasterLabel',type:'sap.ui.model.type.String'}" linkText="{i18n>CspTrustedSite.CspTrustedSite_MasterLabel/CspTrustedSite_MasterLabel}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>CspTrustedSite_NamespacePrefix/CspTrustedSite_NamespacePrefix',type:'sap.ui.model.type.String'}" linkText="{i18n>CspTrustedSite.CspTrustedSite_NamespacePrefix/CspTrustedSite_NamespacePrefix}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>CspTrustedSite_SystemModstamp/CspTrustedSite_SystemModstamp}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>