<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>AuthSessions.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/AuthSession' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>AuthSessions.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AuthSessions.AuthSession_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AuthSessions.AuthSession_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AuthSessions.AuthSession_IsCurrent}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AuthSessions.AuthSession_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AuthSessions.AuthSession_LoginType}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AuthSessions.AuthSession_LogoutUrl}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AuthSessions.AuthSession_NumSecondsValid}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AuthSessions.AuthSession_SessionSecurityLevel}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AuthSessions.AuthSession_SessionType}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AuthSessions.AuthSession_SourceIp}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AuthSessions.AuthSession_UserType}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="AuthSession"/>
							<VBox>					
							<Text text="{dataModel>AuthSession_CreatedDate/AuthSession_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>AuthSession_Id/AuthSession_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>AuthSession.AuthSession_Id/AuthSession_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>AuthSession_IsCurrent/AuthSession_IsCurrent',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<Text text="{dataModel>AuthSession_LastModifiedDate/AuthSession_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>AuthSession_LoginType/AuthSession_LoginType',type:'sap.ui.model.type.String'}" linkText="{i18n>AuthSession.AuthSession_LoginType/AuthSession_LoginType}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>AuthSession_LogoutUrl/AuthSession_LogoutUrl',type:'sap.ui.model.type.String'}" linkText="{i18n>AuthSession.AuthSession_LogoutUrl/AuthSession_LogoutUrl}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>AuthSession_NumSecondsValid/AuthSession_NumSecondsValid',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>AuthSession_SessionSecurityLevel/AuthSession_SessionSecurityLevel',type:'sap.ui.model.type.String'}" linkText="{i18n>AuthSession.AuthSession_SessionSecurityLevel/AuthSession_SessionSecurityLevel}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>AuthSession_SessionType/AuthSession_SessionType',type:'sap.ui.model.type.String'}" linkText="{i18n>AuthSession.AuthSession_SessionType/AuthSession_SessionType}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>AuthSession_SourceIp/AuthSession_SourceIp',type:'sap.ui.model.type.String'}" linkText="{i18n>AuthSession.AuthSession_SourceIp/AuthSession_SourceIp}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>AuthSession_UserType/AuthSession_UserType',type:'sap.ui.model.type.String'}" linkText="{i18n>AuthSession.AuthSession_UserType/AuthSession_UserType}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>