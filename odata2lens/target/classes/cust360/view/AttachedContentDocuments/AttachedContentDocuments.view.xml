<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>AttachedContentDocuments.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/AttachedContentDocument' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>AttachedContentDocuments.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AttachedContentDocuments.AttachedContentDocument_ContentSize}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AttachedContentDocuments.AttachedContentDocument_ContentUrl}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AttachedContentDocuments.AttachedContentDocument_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AttachedContentDocuments.AttachedContentDocument_ExternalDataSourceName}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AttachedContentDocuments.AttachedContentDocument_ExternalDataSourceType}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AttachedContentDocuments.AttachedContentDocument_FileExtension}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AttachedContentDocuments.AttachedContentDocument_FileType}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AttachedContentDocuments.AttachedContentDocument_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AttachedContentDocuments.AttachedContentDocument_IsDeleted}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AttachedContentDocuments.AttachedContentDocument_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AttachedContentDocuments.AttachedContentDocument_SharingOption}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AttachedContentDocuments.AttachedContentDocument_Title}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="AttachedContentDocument"/>
							<VBox>					
							<Text text="{path:'dataModel>AttachedContentDocument_ContentSize/AttachedContentDocument_ContentSize',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>AttachedContentDocument_ContentUrl/AttachedContentDocument_ContentUrl',type:'sap.ui.model.type.String'}" linkText="{i18n>AttachedContentDocument.AttachedContentDocument_ContentUrl/AttachedContentDocument_ContentUrl}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>AttachedContentDocument_CreatedDate/AttachedContentDocument_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>AttachedContentDocument_ExternalDataSourceName/AttachedContentDocument_ExternalDataSourceName',type:'sap.ui.model.type.String'}" linkText="{i18n>AttachedContentDocument.AttachedContentDocument_ExternalDataSourceName/AttachedContentDocument_ExternalDataSourceName}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>AttachedContentDocument_ExternalDataSourceType/AttachedContentDocument_ExternalDataSourceType',type:'sap.ui.model.type.String'}" linkText="{i18n>AttachedContentDocument.AttachedContentDocument_ExternalDataSourceType/AttachedContentDocument_ExternalDataSourceType}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>AttachedContentDocument_FileExtension/AttachedContentDocument_FileExtension',type:'sap.ui.model.type.String'}" linkText="{i18n>AttachedContentDocument.AttachedContentDocument_FileExtension/AttachedContentDocument_FileExtension}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>AttachedContentDocument_FileType/AttachedContentDocument_FileType',type:'sap.ui.model.type.String'}" linkText="{i18n>AttachedContentDocument.AttachedContentDocument_FileType/AttachedContentDocument_FileType}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>AttachedContentDocument_Id/AttachedContentDocument_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>AttachedContentDocument.AttachedContentDocument_Id/AttachedContentDocument_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>AttachedContentDocument_IsDeleted/AttachedContentDocument_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<Text text="{dataModel>AttachedContentDocument_LastModifiedDate/AttachedContentDocument_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>AttachedContentDocument_SharingOption/AttachedContentDocument_SharingOption',type:'sap.ui.model.type.String'}" linkText="{i18n>AttachedContentDocument.AttachedContentDocument_SharingOption/AttachedContentDocument_SharingOption}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>AttachedContentDocument_Title/AttachedContentDocument_Title',type:'sap.ui.model.type.String'}" linkText="{i18n>AttachedContentDocument.AttachedContentDocument_Title/AttachedContentDocument_Title}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>