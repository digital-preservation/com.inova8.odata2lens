<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>ContentDocumentLink.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>ContentDocumentLink.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="ContentDocumentLink"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="ContentDocumentLink"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentDocumentLink.ContentDocumentLink_ContentDocumentId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ContentDocumentLink_ContentDocumentId/sfdc_ContentDocumentLink_ContentDocumentId" entitySet="ContentDocument"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentDocumentLink.ContentDocumentLink_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ContentDocumentLink_Id/ContentDocumentLink_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>ContentDocumentLink.ContentDocumentLink_Id/ContentDocumentLink_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentDocumentLink.ContentDocumentLink_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>ContentDocumentLink_IsDeleted/ContentDocumentLink_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentDocumentLink.ContentDocumentLink_LinkedEntityId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ContentDocumentLink_LinkedEntityId/sfdc_ContentDocumentLink_LinkedEntityId" entitySet="Account"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentDocumentLink.ContentDocumentLink_ShareType}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ContentDocumentLink_ShareType/ContentDocumentLink_ShareType',type:'sap.ui.model.type.String' }" linkText="{i18n>ContentDocumentLink.ContentDocumentLink_ShareType/ContentDocumentLink_ShareType}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentDocumentLink.ContentDocumentLink_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>ContentDocumentLink_SystemModstamp/ContentDocumentLink_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentDocumentLink.ContentDocumentLink_Visibility}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ContentDocumentLink_Visibility/ContentDocumentLink_Visibility',type:'sap.ui.model.type.String' }" linkText="{i18n>ContentDocumentLink.ContentDocumentLink_Visibility/ContentDocumentLink_Visibility}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentDocumentLink.ContentDocumentLink_ContentDocumentId}" required="false"/> 
						<rdf:ListResource path="dataModel>ContentDocumentLink_ContentDocumentId" entitySet="ContentDocument"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentDocumentLink.ContentDocumentLink_LinkedEntityId}" required="false"/> 
						<rdf:ListResource path="dataModel>ContentDocumentLink_LinkedEntityId" entitySet="Account"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>