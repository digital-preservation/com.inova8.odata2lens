<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>CaseMilestones.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/CaseMilestone' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>CaseMilestones.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CaseMilestones.CaseMilestone_CompletionDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CaseMilestones.CaseMilestone_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CaseMilestones.CaseMilestone_ElapsedTimeInDays}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CaseMilestones.CaseMilestone_ElapsedTimeInHrs}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CaseMilestones.CaseMilestone_ElapsedTimeInMins}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CaseMilestones.CaseMilestone_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CaseMilestones.CaseMilestone_IsCompleted}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CaseMilestones.CaseMilestone_IsDeleted}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CaseMilestones.CaseMilestone_IsViolated}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CaseMilestones.CaseMilestone_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CaseMilestones.CaseMilestone_StartDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CaseMilestones.CaseMilestone_SystemModstamp}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CaseMilestones.CaseMilestone_TargetDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CaseMilestones.CaseMilestone_TargetResponseInDays}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CaseMilestones.CaseMilestone_TargetResponseInHrs}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CaseMilestones.CaseMilestone_TargetResponseInMins}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CaseMilestones.CaseMilestone_TimeRemainingInMins}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CaseMilestones.CaseMilestone_TimeSinceTargetInMins}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="CaseMilestone"/>
							<VBox>					
							<Text text="{dataModel>CaseMilestone_CompletionDate/CaseMilestone_CompletionDate}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>CaseMilestone_CreatedDate/CaseMilestone_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>CaseMilestone_ElapsedTimeInDays/CaseMilestone_ElapsedTimeInDays',type:'sap.ui.model.type.Float', formatOptions:{'style':'short','maxFractionDigits':2} }"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>CaseMilestone_ElapsedTimeInHrs/CaseMilestone_ElapsedTimeInHrs',type:'sap.ui.model.type.Float', formatOptions:{'style':'short','maxFractionDigits':2} }"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>CaseMilestone_ElapsedTimeInMins/CaseMilestone_ElapsedTimeInMins',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>CaseMilestone_Id/CaseMilestone_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>CaseMilestone.CaseMilestone_Id/CaseMilestone_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>CaseMilestone_IsCompleted/CaseMilestone_IsCompleted',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>CaseMilestone_IsDeleted/CaseMilestone_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>CaseMilestone_IsViolated/CaseMilestone_IsViolated',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<Text text="{dataModel>CaseMilestone_LastModifiedDate/CaseMilestone_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>CaseMilestone_StartDate/CaseMilestone_StartDate}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>CaseMilestone_SystemModstamp/CaseMilestone_SystemModstamp}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>CaseMilestone_TargetDate/CaseMilestone_TargetDate}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>CaseMilestone_TargetResponseInDays/CaseMilestone_TargetResponseInDays',type:'sap.ui.model.type.Float', formatOptions:{'style':'short','maxFractionDigits':2} }"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>CaseMilestone_TargetResponseInHrs/CaseMilestone_TargetResponseInHrs',type:'sap.ui.model.type.Float', formatOptions:{'style':'short','maxFractionDigits':2} }"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>CaseMilestone_TargetResponseInMins/CaseMilestone_TargetResponseInMins',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>CaseMilestone_TimeRemainingInMins/CaseMilestone_TimeRemainingInMins',type:'sap.ui.model.type.String'}" linkText="{i18n>CaseMilestone.CaseMilestone_TimeRemainingInMins/CaseMilestone_TimeRemainingInMins}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>CaseMilestone_TimeSinceTargetInMins/CaseMilestone_TimeSinceTargetInMins',type:'sap.ui.model.type.String'}" linkText="{i18n>CaseMilestone.CaseMilestone_TimeSinceTargetInMins/CaseMilestone_TimeSinceTargetInMins}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>