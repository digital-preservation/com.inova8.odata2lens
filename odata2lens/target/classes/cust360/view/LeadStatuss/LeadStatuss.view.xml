<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>LeadStatuss.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/LeadStatus' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>LeadStatuss.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>LeadStatuss.LeadStatus_ApiName}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>LeadStatuss.LeadStatus_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>LeadStatuss.LeadStatus_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>LeadStatuss.LeadStatus_IsConverted}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>LeadStatuss.LeadStatus_IsDefault}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>LeadStatuss.LeadStatus_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>LeadStatuss.LeadStatus_MasterLabel}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>LeadStatuss.LeadStatus_SortOrder}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>LeadStatuss.LeadStatus_SystemModstamp}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="LeadStatus"/>
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>LeadStatus_ApiName/LeadStatus_ApiName',type:'sap.ui.model.type.String'}" linkText="{i18n>LeadStatus.LeadStatus_ApiName/LeadStatus_ApiName}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>LeadStatus_CreatedDate/LeadStatus_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>LeadStatus_Id/LeadStatus_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>LeadStatus.LeadStatus_Id/LeadStatus_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>LeadStatus_IsConverted/LeadStatus_IsConverted',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>LeadStatus_IsDefault/LeadStatus_IsDefault',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<Text text="{dataModel>LeadStatus_LastModifiedDate/LeadStatus_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>LeadStatus_MasterLabel/LeadStatus_MasterLabel',type:'sap.ui.model.type.String'}" linkText="{i18n>LeadStatus.LeadStatus_MasterLabel/LeadStatus_MasterLabel}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>LeadStatus_SortOrder/LeadStatus_SortOrder',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>LeadStatus_SystemModstamp/LeadStatus_SystemModstamp}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>