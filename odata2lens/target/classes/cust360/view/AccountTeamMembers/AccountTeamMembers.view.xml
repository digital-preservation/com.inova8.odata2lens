<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>AccountTeamMembers.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/AccountTeamMember' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>AccountTeamMembers.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AccountTeamMembers.AccountTeamMember_AccountAccessLevel}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AccountTeamMembers.AccountTeamMember_CaseAccessLevel}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AccountTeamMembers.AccountTeamMember_ContactAccessLevel}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AccountTeamMembers.AccountTeamMember_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AccountTeamMembers.AccountTeamMember_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AccountTeamMembers.AccountTeamMember_IsDeleted}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AccountTeamMembers.AccountTeamMember_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AccountTeamMembers.AccountTeamMember_OpportunityAccessLevel}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AccountTeamMembers.AccountTeamMember_PhotoUrl}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AccountTeamMembers.AccountTeamMember_SystemModstamp}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AccountTeamMembers.AccountTeamMember_TeamMemberRole}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AccountTeamMembers.AccountTeamMember_Title}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="AccountTeamMember"/>
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>AccountTeamMember_AccountAccessLevel/AccountTeamMember_AccountAccessLevel',type:'sap.ui.model.type.String'}" linkText="{i18n>AccountTeamMember.AccountTeamMember_AccountAccessLevel/AccountTeamMember_AccountAccessLevel}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>AccountTeamMember_CaseAccessLevel/AccountTeamMember_CaseAccessLevel',type:'sap.ui.model.type.String'}" linkText="{i18n>AccountTeamMember.AccountTeamMember_CaseAccessLevel/AccountTeamMember_CaseAccessLevel}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>AccountTeamMember_ContactAccessLevel/AccountTeamMember_ContactAccessLevel',type:'sap.ui.model.type.String'}" linkText="{i18n>AccountTeamMember.AccountTeamMember_ContactAccessLevel/AccountTeamMember_ContactAccessLevel}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>AccountTeamMember_CreatedDate/AccountTeamMember_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>AccountTeamMember_Id/AccountTeamMember_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>AccountTeamMember.AccountTeamMember_Id/AccountTeamMember_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>AccountTeamMember_IsDeleted/AccountTeamMember_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<Text text="{dataModel>AccountTeamMember_LastModifiedDate/AccountTeamMember_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>AccountTeamMember_OpportunityAccessLevel/AccountTeamMember_OpportunityAccessLevel',type:'sap.ui.model.type.String'}" linkText="{i18n>AccountTeamMember.AccountTeamMember_OpportunityAccessLevel/AccountTeamMember_OpportunityAccessLevel}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>AccountTeamMember_PhotoUrl/AccountTeamMember_PhotoUrl',type:'sap.ui.model.type.String'}" linkText="{i18n>AccountTeamMember.AccountTeamMember_PhotoUrl/AccountTeamMember_PhotoUrl}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>AccountTeamMember_SystemModstamp/AccountTeamMember_SystemModstamp}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>AccountTeamMember_TeamMemberRole/AccountTeamMember_TeamMemberRole',type:'sap.ui.model.type.String'}" linkText="{i18n>AccountTeamMember.AccountTeamMember_TeamMemberRole/AccountTeamMember_TeamMemberRole}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>AccountTeamMember_Title/AccountTeamMember_Title',type:'sap.ui.model.type.String'}" linkText="{i18n>AccountTeamMember.AccountTeamMember_Title/AccountTeamMember_Title}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>