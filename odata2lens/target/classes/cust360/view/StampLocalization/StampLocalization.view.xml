<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>StampLocalization.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>StampLocalization.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="StampLocalization"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="StampLocalization"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>StampLocalization.StampLocalization_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>StampLocalization_CreatedById/sfdc_StampLocalization_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>StampLocalization.StampLocalization_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>StampLocalization_CreatedDate/StampLocalization_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>StampLocalization.StampLocalization_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>StampLocalization_Id/StampLocalization_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>StampLocalization.StampLocalization_Id/StampLocalization_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>StampLocalization.StampLocalization_Language}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>StampLocalization_Language/StampLocalization_Language',type:'sap.ui.model.type.String' }" linkText="{i18n>StampLocalization.StampLocalization_Language/StampLocalization_Language}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>StampLocalization.StampLocalization_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>StampLocalization_LastModifiedById/sfdc_StampLocalization_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>StampLocalization.StampLocalization_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>StampLocalization_LastModifiedDate/StampLocalization_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>StampLocalization.StampLocalization_NamespacePrefix}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>StampLocalization_NamespacePrefix/StampLocalization_NamespacePrefix',type:'sap.ui.model.type.String' }" linkText="{i18n>StampLocalization.StampLocalization_NamespacePrefix/StampLocalization_NamespacePrefix}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>StampLocalization.StampLocalization_ParentId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>StampLocalization_ParentId/sfdc_StampLocalization_ParentId" entitySet="Stamp"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>StampLocalization.StampLocalization_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>StampLocalization_SystemModstamp/StampLocalization_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>StampLocalization.StampLocalization_Value}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>StampLocalization_Value/StampLocalization_Value',type:'sap.ui.model.type.String' }" linkText="{i18n>StampLocalization.StampLocalization_Value/StampLocalization_Value}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>StampLocalization.StampLocalization_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>StampLocalization_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>StampLocalization.StampLocalization_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>StampLocalization_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>StampLocalization.StampLocalization_ParentId}" required="false"/> 
						<rdf:ListResource path="dataModel>StampLocalization_ParentId" entitySet="Stamp"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>