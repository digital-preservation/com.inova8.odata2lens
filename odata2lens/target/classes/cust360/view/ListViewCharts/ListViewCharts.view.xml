<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>ListViewCharts.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/ListViewChart' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>ListViewCharts.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ListViewCharts.ListViewChart_AggregateField}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ListViewCharts.ListViewChart_AggregateType}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ListViewCharts.ListViewChart_ChartType}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ListViewCharts.ListViewChart_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ListViewCharts.ListViewChart_DeveloperName}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ListViewCharts.ListViewChart_GroupingField}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ListViewCharts.ListViewChart_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ListViewCharts.ListViewChart_IsDeleted}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ListViewCharts.ListViewChart_Language}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ListViewCharts.ListViewChart_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ListViewCharts.ListViewChart_MasterLabel}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ListViewCharts.ListViewChart_SobjectType}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ListViewCharts.ListViewChart_SystemModstamp}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="ListViewChart"/>
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ListViewChart_AggregateField/ListViewChart_AggregateField',type:'sap.ui.model.type.String'}" linkText="{i18n>ListViewChart.ListViewChart_AggregateField/ListViewChart_AggregateField}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ListViewChart_AggregateType/ListViewChart_AggregateType',type:'sap.ui.model.type.String'}" linkText="{i18n>ListViewChart.ListViewChart_AggregateType/ListViewChart_AggregateType}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ListViewChart_ChartType/ListViewChart_ChartType',type:'sap.ui.model.type.String'}" linkText="{i18n>ListViewChart.ListViewChart_ChartType/ListViewChart_ChartType}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>ListViewChart_CreatedDate/ListViewChart_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ListViewChart_DeveloperName/ListViewChart_DeveloperName',type:'sap.ui.model.type.String'}" linkText="{i18n>ListViewChart.ListViewChart_DeveloperName/ListViewChart_DeveloperName}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ListViewChart_GroupingField/ListViewChart_GroupingField',type:'sap.ui.model.type.String'}" linkText="{i18n>ListViewChart.ListViewChart_GroupingField/ListViewChart_GroupingField}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ListViewChart_Id/ListViewChart_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>ListViewChart.ListViewChart_Id/ListViewChart_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>ListViewChart_IsDeleted/ListViewChart_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ListViewChart_Language/ListViewChart_Language',type:'sap.ui.model.type.String'}" linkText="{i18n>ListViewChart.ListViewChart_Language/ListViewChart_Language}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>ListViewChart_LastModifiedDate/ListViewChart_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ListViewChart_MasterLabel/ListViewChart_MasterLabel',type:'sap.ui.model.type.String'}" linkText="{i18n>ListViewChart.ListViewChart_MasterLabel/ListViewChart_MasterLabel}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ListViewChart_SobjectType/ListViewChart_SobjectType',type:'sap.ui.model.type.String'}" linkText="{i18n>ListViewChart.ListViewChart_SobjectType/ListViewChart_SobjectType}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>ListViewChart_SystemModstamp/ListViewChart_SystemModstamp}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>