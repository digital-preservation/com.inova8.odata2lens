<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>CallCenter.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>CallCenter.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="CallCenter"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="CallCenter"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CallCenter.AdditionalNumber_CallCenterId_Inverse}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>AdditionalNumber_CallCenterId_Inverse/sfdc_AdditionalNumber_CallCenterId_Inverse" entitySet="AdditionalNumber"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CallCenter.CallCenter_AdapterUrl}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>CallCenter_AdapterUrl/CallCenter_AdapterUrl',type:'sap.ui.model.type.String' }" linkText="{i18n>CallCenter.CallCenter_AdapterUrl/CallCenter_AdapterUrl}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CallCenter.CallCenter_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>CallCenter_CreatedById/sfdc_CallCenter_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CallCenter.CallCenter_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>CallCenter_CreatedDate/CallCenter_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CallCenter.CallCenter_CustomSettings}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>CallCenter_CustomSettings/CallCenter_CustomSettings',type:'sap.ui.model.type.String' }" linkText="{i18n>CallCenter.CallCenter_CustomSettings/CallCenter_CustomSettings}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CallCenter.CallCenter_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>CallCenter_Id/CallCenter_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>CallCenter.CallCenter_Id/CallCenter_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CallCenter.CallCenter_InternalName}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>CallCenter_InternalName/CallCenter_InternalName',type:'sap.ui.model.type.String' }" linkText="{i18n>CallCenter.CallCenter_InternalName/CallCenter_InternalName}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CallCenter.CallCenter_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>CallCenter_LastModifiedById/sfdc_CallCenter_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CallCenter.CallCenter_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>CallCenter_LastModifiedDate/CallCenter_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CallCenter.CallCenter_Name}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>CallCenter_Name/CallCenter_Name',type:'sap.ui.model.type.String' }" linkText="{i18n>CallCenter.CallCenter_Name/CallCenter_Name}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CallCenter.CallCenter_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>CallCenter_SystemModstamp/CallCenter_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CallCenter.CallCenter_Version}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>CallCenter_Version/CallCenter_Version',type:'sap.ui.model.type.Float', formatOptions:{'style':'short','maxFractionDigits':2} }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CallCenter.User_CallCenterId_Inverse}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>User_CallCenterId_Inverse/sfdc_User_CallCenterId_Inverse" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CallCenter.AdditionalNumber_CallCenterId_Inverse}" required="false"/> 
						<rdf:ListResource path="dataModel>AdditionalNumber_CallCenterId_Inverse" entitySet="AdditionalNumber"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CallCenter.CallCenter_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>CallCenter_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CallCenter.CallCenter_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>CallCenter_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CallCenter.User_CallCenterId_Inverse}" required="false"/> 
						<rdf:ListResource path="dataModel>User_CallCenterId_Inverse" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>