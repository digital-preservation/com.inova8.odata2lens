<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>LoginGeo.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>LoginGeo.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="LoginGeo"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="LoginGeo"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>LoginGeo.AuthSession_LoginGeoId_Inverse}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>AuthSession_LoginGeoId_Inverse/sfdc_AuthSession_LoginGeoId_Inverse" entitySet="AuthSession"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>LoginGeo.LoginGeo_City}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>LoginGeo_City/LoginGeo_City',type:'sap.ui.model.type.String' }" linkText="{i18n>LoginGeo.LoginGeo_City/LoginGeo_City}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>LoginGeo.LoginGeo_Country}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>LoginGeo_Country/LoginGeo_Country',type:'sap.ui.model.type.String' }" linkText="{i18n>LoginGeo.LoginGeo_Country/LoginGeo_Country}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>LoginGeo.LoginGeo_CountryIso}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>LoginGeo_CountryIso/LoginGeo_CountryIso',type:'sap.ui.model.type.String' }" linkText="{i18n>LoginGeo.LoginGeo_CountryIso/LoginGeo_CountryIso}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>LoginGeo.LoginGeo_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>LoginGeo_CreatedById/sfdc_LoginGeo_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>LoginGeo.LoginGeo_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>LoginGeo_CreatedDate/LoginGeo_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>LoginGeo.LoginGeo_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>LoginGeo_Id/LoginGeo_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>LoginGeo.LoginGeo_Id/LoginGeo_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>LoginGeo.LoginGeo_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>LoginGeo_IsDeleted/LoginGeo_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>LoginGeo.LoginGeo_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>LoginGeo_LastModifiedById/sfdc_LoginGeo_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>LoginGeo.LoginGeo_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>LoginGeo_LastModifiedDate/LoginGeo_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>LoginGeo.LoginGeo_Latitude}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>LoginGeo_Latitude/LoginGeo_Latitude',type:'sap.ui.model.type.Float', formatOptions:{'style':'short','maxFractionDigits':2} }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>LoginGeo.LoginGeo_LoginTime}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>LoginGeo_LoginTime/LoginGeo_LoginTime}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>LoginGeo.LoginGeo_Longitude}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>LoginGeo_Longitude/LoginGeo_Longitude',type:'sap.ui.model.type.Float', formatOptions:{'style':'short','maxFractionDigits':2} }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>LoginGeo.LoginGeo_PostalCode}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>LoginGeo_PostalCode/LoginGeo_PostalCode',type:'sap.ui.model.type.String' }" linkText="{i18n>LoginGeo.LoginGeo_PostalCode/LoginGeo_PostalCode}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>LoginGeo.LoginGeo_Subdivision}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>LoginGeo_Subdivision/LoginGeo_Subdivision',type:'sap.ui.model.type.String' }" linkText="{i18n>LoginGeo.LoginGeo_Subdivision/LoginGeo_Subdivision}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>LoginGeo.LoginGeo_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>LoginGeo_SystemModstamp/LoginGeo_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>LoginGeo.LoginHistory_LoginGeoId_Inverse}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>LoginHistory_LoginGeoId_Inverse/sfdc_LoginHistory_LoginGeoId_Inverse" entitySet="LoginHistory"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>LoginGeo.VerificationHistory_LoginGeoId_Inverse}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>VerificationHistory_LoginGeoId_Inverse/sfdc_VerificationHistory_LoginGeoId_Inverse" entitySet="VerificationHistory"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>LoginGeo.AuthSession_LoginGeoId_Inverse}" required="false"/> 
						<rdf:ListResource path="dataModel>AuthSession_LoginGeoId_Inverse" entitySet="AuthSession"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>LoginGeo.LoginGeo_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>LoginGeo_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>LoginGeo.LoginGeo_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>LoginGeo_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>LoginGeo.LoginHistory_LoginGeoId_Inverse}" required="false"/> 
						<rdf:ListResource path="dataModel>LoginHistory_LoginGeoId_Inverse" entitySet="LoginHistory"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>LoginGeo.VerificationHistory_LoginGeoId_Inverse}" required="false"/> 
						<rdf:ListResource path="dataModel>VerificationHistory_LoginGeoId_Inverse" entitySet="VerificationHistory"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>