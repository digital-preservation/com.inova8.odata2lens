<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>WebLinkLocalizations.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/WebLinkLocalization' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>WebLinkLocalizations.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>WebLinkLocalizations.WebLinkLocalization_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>WebLinkLocalizations.WebLinkLocalization_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>WebLinkLocalizations.WebLinkLocalization_Language}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>WebLinkLocalizations.WebLinkLocalization_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>WebLinkLocalizations.WebLinkLocalization_NamespacePrefix}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>WebLinkLocalizations.WebLinkLocalization_SystemModstamp}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>WebLinkLocalizations.WebLinkLocalization_Value}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="WebLinkLocalization"/>
							<VBox>					
							<Text text="{dataModel>WebLinkLocalization_CreatedDate/WebLinkLocalization_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>WebLinkLocalization_Id/WebLinkLocalization_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>WebLinkLocalization.WebLinkLocalization_Id/WebLinkLocalization_Id}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>WebLinkLocalization_Language/WebLinkLocalization_Language',type:'sap.ui.model.type.String'}" linkText="{i18n>WebLinkLocalization.WebLinkLocalization_Language/WebLinkLocalization_Language}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>WebLinkLocalization_LastModifiedDate/WebLinkLocalization_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>WebLinkLocalization_NamespacePrefix/WebLinkLocalization_NamespacePrefix',type:'sap.ui.model.type.String'}" linkText="{i18n>WebLinkLocalization.WebLinkLocalization_NamespacePrefix/WebLinkLocalization_NamespacePrefix}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>WebLinkLocalization_SystemModstamp/WebLinkLocalization_SystemModstamp}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>WebLinkLocalization_Value/WebLinkLocalization_Value',type:'sap.ui.model.type.String'}" linkText="{i18n>WebLinkLocalization.WebLinkLocalization_Value/WebLinkLocalization_Value}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>