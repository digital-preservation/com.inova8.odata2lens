<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>OutgoingEmailRelation.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>OutgoingEmailRelation.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="OutgoingEmailRelation"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="OutgoingEmailRelation"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OutgoingEmailRelation.OutgoingEmailRelation_ExternalId}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>OutgoingEmailRelation_ExternalId/OutgoingEmailRelation_ExternalId',type:'sap.ui.model.type.String' }" linkText="{i18n>OutgoingEmailRelation.OutgoingEmailRelation_ExternalId/OutgoingEmailRelation_ExternalId}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OutgoingEmailRelation.OutgoingEmailRelation_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>OutgoingEmailRelation_Id/OutgoingEmailRelation_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>OutgoingEmailRelation.OutgoingEmailRelation_Id/OutgoingEmailRelation_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OutgoingEmailRelation.OutgoingEmailRelation_OutgoingEmailId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>OutgoingEmailRelation_OutgoingEmailId/sfdc_OutgoingEmailRelation_OutgoingEmailId" entitySet="OutgoingEmail"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OutgoingEmailRelation.OutgoingEmailRelation_RelationAddress}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>OutgoingEmailRelation_RelationAddress/OutgoingEmailRelation_RelationAddress',type:'sap.ui.model.type.String' }" linkText="{i18n>OutgoingEmailRelation.OutgoingEmailRelation_RelationAddress/OutgoingEmailRelation_RelationAddress}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OutgoingEmailRelation.OutgoingEmailRelation_RelationId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>OutgoingEmailRelation_RelationId/sfdc_OutgoingEmailRelation_RelationId" entitySet="Contact"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OutgoingEmailRelation.OutgoingEmailRelation_OutgoingEmailId}" required="false"/> 
						<rdf:ListResource path="dataModel>OutgoingEmailRelation_OutgoingEmailId" entitySet="OutgoingEmail"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OutgoingEmailRelation.OutgoingEmailRelation_RelationId}" required="false"/> 
						<rdf:ListResource path="dataModel>OutgoingEmailRelation_RelationId" entitySet="Contact"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>