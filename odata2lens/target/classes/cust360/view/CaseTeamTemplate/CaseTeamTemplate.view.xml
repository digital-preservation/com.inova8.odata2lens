				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CaseTeamTemplate.CaseTeamTemplate_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>CaseTeamTemplate_CreatedById/sfdc_CaseTeamTemplate_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CaseTeamTemplate.CaseTeamTemplate_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>CaseTeamTemplate_CreatedDate/CaseTeamTemplate_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CaseTeamTemplate.CaseTeamTemplate_Description}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>CaseTeamTemplate_Description/CaseTeamTemplate_Description',type:'sap.ui.model.type.String' }" linkText="{i18n>CaseTeamTemplate.CaseTeamTemplate_Description/CaseTeamTemplate_Description}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CaseTeamTemplate.CaseTeamTemplate_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>CaseTeamTemplate_Id/CaseTeamTemplate_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>CaseTeamTemplate.CaseTeamTemplate_Id/CaseTeamTemplate_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CaseTeamTemplate.CaseTeamTemplate_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>CaseTeamTemplate_LastModifiedById/sfdc_CaseTeamTemplate_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CaseTeamTemplate.CaseTeamTemplate_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>CaseTeamTemplate_LastModifiedDate/CaseTeamTemplate_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CaseTeamTemplate.CaseTeamTemplate_Name}" required="false"/> 
						<Text text=""/>
