				<ColumnListItem >
					<cells>
						<Label text="{i18n>CaseTeamTemplate.CaseTeamTemplateMember_TeamTemplateId_Inverse}" required="false"/> 
						<rdf:ListResource path="dataModel>CaseTeamTemplateMember_TeamTemplateId_Inverse" entitySet="CaseTeamTemplateMember"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CaseTeamTemplate.CaseTeamTemplateRecord_TeamTemplateId_Inverse}" required="false"/> 
						<rdf:ListResource path="dataModel>CaseTeamTemplateRecord_TeamTemplateId_Inverse" entitySet="CaseTeamTemplateRecord"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CaseTeamTemplate.CaseTeamTemplate_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>CaseTeamTemplate_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CaseTeamTemplate.CaseTeamTemplate_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>CaseTeamTemplate_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>