<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>SecureAgentsCluster.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>SecureAgentsCluster.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="SecureAgentsCluster"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="SecureAgentsCluster"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SecureAgentsCluster.SecureAgentsCluster_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>SecureAgentsCluster_CreatedById/sfdc_SecureAgentsCluster_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SecureAgentsCluster.SecureAgentsCluster_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>SecureAgentsCluster_CreatedDate/SecureAgentsCluster_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SecureAgentsCluster.SecureAgentsCluster_Description}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>SecureAgentsCluster_Description/SecureAgentsCluster_Description',type:'sap.ui.model.type.String' }" linkText="{i18n>SecureAgentsCluster.SecureAgentsCluster_Description/SecureAgentsCluster_Description}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SecureAgentsCluster.SecureAgentsCluster_DeveloperName}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>SecureAgentsCluster_DeveloperName/SecureAgentsCluster_DeveloperName',type:'sap.ui.model.type.String' }" linkText="{i18n>SecureAgentsCluster.SecureAgentsCluster_DeveloperName/SecureAgentsCluster_DeveloperName}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SecureAgentsCluster.SecureAgentsCluster_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>SecureAgentsCluster_Id/SecureAgentsCluster_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>SecureAgentsCluster.SecureAgentsCluster_Id/SecureAgentsCluster_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SecureAgentsCluster.SecureAgentsCluster_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>SecureAgentsCluster_IsDeleted/SecureAgentsCluster_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SecureAgentsCluster.SecureAgentsCluster_Language}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>SecureAgentsCluster_Language/SecureAgentsCluster_Language',type:'sap.ui.model.type.String' }" linkText="{i18n>SecureAgentsCluster.SecureAgentsCluster_Language/SecureAgentsCluster_Language}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SecureAgentsCluster.SecureAgentsCluster_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>SecureAgentsCluster_LastModifiedById/sfdc_SecureAgentsCluster_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SecureAgentsCluster.SecureAgentsCluster_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>SecureAgentsCluster_LastModifiedDate/SecureAgentsCluster_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SecureAgentsCluster.SecureAgentsCluster_MasterLabel}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>SecureAgentsCluster_MasterLabel/SecureAgentsCluster_MasterLabel',type:'sap.ui.model.type.String' }" linkText="{i18n>SecureAgentsCluster.SecureAgentsCluster_MasterLabel/SecureAgentsCluster_MasterLabel}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SecureAgentsCluster.SecureAgentsCluster_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>SecureAgentsCluster_SystemModstamp/SecureAgentsCluster_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SecureAgentsCluster.SecureAgentsCluster_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>SecureAgentsCluster_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SecureAgentsCluster.SecureAgentsCluster_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>SecureAgentsCluster_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>