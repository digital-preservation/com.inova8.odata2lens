<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>ContractStatus.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>ContractStatus.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="ContractStatus"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="ContractStatus"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContractStatus.ContractStatus_ApiName}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ContractStatus_ApiName/ContractStatus_ApiName',type:'sap.ui.model.type.String' }" linkText="{i18n>ContractStatus.ContractStatus_ApiName/ContractStatus_ApiName}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContractStatus.ContractStatus_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ContractStatus_CreatedById/sfdc_ContractStatus_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContractStatus.ContractStatus_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>ContractStatus_CreatedDate/ContractStatus_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContractStatus.ContractStatus_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ContractStatus_Id/ContractStatus_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>ContractStatus.ContractStatus_Id/ContractStatus_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContractStatus.ContractStatus_IsDefault}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>ContractStatus_IsDefault/ContractStatus_IsDefault',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContractStatus.ContractStatus_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ContractStatus_LastModifiedById/sfdc_ContractStatus_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContractStatus.ContractStatus_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>ContractStatus_LastModifiedDate/ContractStatus_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContractStatus.ContractStatus_MasterLabel}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ContractStatus_MasterLabel/ContractStatus_MasterLabel',type:'sap.ui.model.type.String' }" linkText="{i18n>ContractStatus.ContractStatus_MasterLabel/ContractStatus_MasterLabel}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContractStatus.ContractStatus_SortOrder}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>ContractStatus_SortOrder/ContractStatus_SortOrder',type:'sap.ui.model.type.Integer' }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContractStatus.ContractStatus_StatusCode}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ContractStatus_StatusCode/ContractStatus_StatusCode',type:'sap.ui.model.type.String' }" linkText="{i18n>ContractStatus.ContractStatus_StatusCode/ContractStatus_StatusCode}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContractStatus.ContractStatus_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>ContractStatus_SystemModstamp/ContractStatus_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContractStatus.ContractStatus_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>ContractStatus_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContractStatus.ContractStatus_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>ContractStatus_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>