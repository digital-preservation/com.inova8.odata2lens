<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>AdditionalNumber.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>AdditionalNumber.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="AdditionalNumber"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="AdditionalNumber"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AdditionalNumber.AdditionalNumber_CallCenterId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>AdditionalNumber_CallCenterId/sfdc_AdditionalNumber_CallCenterId" entitySet="CallCenter"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AdditionalNumber.AdditionalNumber_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>AdditionalNumber_CreatedById/sfdc_AdditionalNumber_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AdditionalNumber.AdditionalNumber_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>AdditionalNumber_CreatedDate/AdditionalNumber_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AdditionalNumber.AdditionalNumber_Description}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>AdditionalNumber_Description/AdditionalNumber_Description',type:'sap.ui.model.type.String' }" linkText="{i18n>AdditionalNumber.AdditionalNumber_Description/AdditionalNumber_Description}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AdditionalNumber.AdditionalNumber_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>AdditionalNumber_Id/AdditionalNumber_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>AdditionalNumber.AdditionalNumber_Id/AdditionalNumber_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AdditionalNumber.AdditionalNumber_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>AdditionalNumber_IsDeleted/AdditionalNumber_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AdditionalNumber.AdditionalNumber_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>AdditionalNumber_LastModifiedById/sfdc_AdditionalNumber_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AdditionalNumber.AdditionalNumber_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>AdditionalNumber_LastModifiedDate/AdditionalNumber_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AdditionalNumber.AdditionalNumber_Name}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>AdditionalNumber_Name/AdditionalNumber_Name',type:'sap.ui.model.type.String' }" linkText="{i18n>AdditionalNumber.AdditionalNumber_Name/AdditionalNumber_Name}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AdditionalNumber.AdditionalNumber_Phone}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>AdditionalNumber_Phone/AdditionalNumber_Phone',type:'sap.ui.model.type.String' }" linkText="{i18n>AdditionalNumber.AdditionalNumber_Phone/AdditionalNumber_Phone}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AdditionalNumber.AdditionalNumber_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>AdditionalNumber_SystemModstamp/AdditionalNumber_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AdditionalNumber.AdditionalNumber_CallCenterId}" required="false"/> 
						<rdf:ListResource path="dataModel>AdditionalNumber_CallCenterId" entitySet="CallCenter"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AdditionalNumber.AdditionalNumber_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>AdditionalNumber_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AdditionalNumber.AdditionalNumber_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>AdditionalNumber_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>