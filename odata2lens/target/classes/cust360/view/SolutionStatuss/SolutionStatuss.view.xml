<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>SolutionStatuss.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/SolutionStatus' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>SolutionStatuss.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>SolutionStatuss.SolutionStatus_ApiName}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>SolutionStatuss.SolutionStatus_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>SolutionStatuss.SolutionStatus_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>SolutionStatuss.SolutionStatus_IsDefault}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>SolutionStatuss.SolutionStatus_IsReviewed}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>SolutionStatuss.SolutionStatus_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>SolutionStatuss.SolutionStatus_MasterLabel}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>SolutionStatuss.SolutionStatus_SortOrder}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>SolutionStatuss.SolutionStatus_SystemModstamp}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="SolutionStatus"/>
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>SolutionStatus_ApiName/SolutionStatus_ApiName',type:'sap.ui.model.type.String'}" linkText="{i18n>SolutionStatus.SolutionStatus_ApiName/SolutionStatus_ApiName}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>SolutionStatus_CreatedDate/SolutionStatus_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>SolutionStatus_Id/SolutionStatus_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>SolutionStatus.SolutionStatus_Id/SolutionStatus_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>SolutionStatus_IsDefault/SolutionStatus_IsDefault',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>SolutionStatus_IsReviewed/SolutionStatus_IsReviewed',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<Text text="{dataModel>SolutionStatus_LastModifiedDate/SolutionStatus_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>SolutionStatus_MasterLabel/SolutionStatus_MasterLabel',type:'sap.ui.model.type.String'}" linkText="{i18n>SolutionStatus.SolutionStatus_MasterLabel/SolutionStatus_MasterLabel}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>SolutionStatus_SortOrder/SolutionStatus_SortOrder',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>SolutionStatus_SystemModstamp/SolutionStatus_SystemModstamp}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>