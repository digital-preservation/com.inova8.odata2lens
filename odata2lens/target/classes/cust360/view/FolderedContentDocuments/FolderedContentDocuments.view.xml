<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>FolderedContentDocuments.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/FolderedContentDocument' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>FolderedContentDocuments.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>FolderedContentDocuments.FolderedContentDocument_ContentSize}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>FolderedContentDocuments.FolderedContentDocument_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>FolderedContentDocuments.FolderedContentDocument_FileExtension}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>FolderedContentDocuments.FolderedContentDocument_FileType}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>FolderedContentDocuments.FolderedContentDocument_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>FolderedContentDocuments.FolderedContentDocument_IsDeleted}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>FolderedContentDocuments.FolderedContentDocument_IsFolder}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>FolderedContentDocuments.FolderedContentDocument_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>FolderedContentDocuments.FolderedContentDocument_SystemModstamp}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>FolderedContentDocuments.FolderedContentDocument_Title}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="FolderedContentDocument"/>
							<VBox>					
							<Text text="{path:'dataModel>FolderedContentDocument_ContentSize/FolderedContentDocument_ContentSize',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>FolderedContentDocument_CreatedDate/FolderedContentDocument_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>FolderedContentDocument_FileExtension/FolderedContentDocument_FileExtension',type:'sap.ui.model.type.String'}" linkText="{i18n>FolderedContentDocument.FolderedContentDocument_FileExtension/FolderedContentDocument_FileExtension}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>FolderedContentDocument_FileType/FolderedContentDocument_FileType',type:'sap.ui.model.type.String'}" linkText="{i18n>FolderedContentDocument.FolderedContentDocument_FileType/FolderedContentDocument_FileType}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>FolderedContentDocument_Id/FolderedContentDocument_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>FolderedContentDocument.FolderedContentDocument_Id/FolderedContentDocument_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>FolderedContentDocument_IsDeleted/FolderedContentDocument_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>FolderedContentDocument_IsFolder/FolderedContentDocument_IsFolder',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<Text text="{dataModel>FolderedContentDocument_LastModifiedDate/FolderedContentDocument_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>FolderedContentDocument_SystemModstamp/FolderedContentDocument_SystemModstamp}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>FolderedContentDocument_Title/FolderedContentDocument_Title',type:'sap.ui.model.type.String'}" linkText="{i18n>FolderedContentDocument.FolderedContentDocument_Title/FolderedContentDocument_Title}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>