						<rdf:TextLink text="{path:'dataModel>FiscalYearSettings_QuarterPrefix/FiscalYearSettings_QuarterPrefix',type:'sap.ui.model.type.String' }" linkText="{i18n>FiscalYearSettings.FiscalYearSettings_QuarterPrefix/FiscalYearSettings_QuarterPrefix}"/>
