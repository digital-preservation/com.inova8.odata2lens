<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>TopicAssignment.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>TopicAssignment.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="TopicAssignment"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="TopicAssignment"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TopicAssignment.TopicAssignment_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>TopicAssignment_CreatedById/sfdc_TopicAssignment_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TopicAssignment.TopicAssignment_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>TopicAssignment_CreatedDate/TopicAssignment_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TopicAssignment.TopicAssignment_EntityId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>TopicAssignment_EntityId/sfdc_TopicAssignment_EntityId" entitySet="Account"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TopicAssignment.TopicAssignment_EntityKeyPrefix}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>TopicAssignment_EntityKeyPrefix/TopicAssignment_EntityKeyPrefix',type:'sap.ui.model.type.String' }" linkText="{i18n>TopicAssignment.TopicAssignment_EntityKeyPrefix/TopicAssignment_EntityKeyPrefix}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TopicAssignment.TopicAssignment_EntityType}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>TopicAssignment_EntityType/TopicAssignment_EntityType',type:'sap.ui.model.type.String' }" linkText="{i18n>TopicAssignment.TopicAssignment_EntityType/TopicAssignment_EntityType}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TopicAssignment.TopicAssignment_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>TopicAssignment_Id/TopicAssignment_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>TopicAssignment.TopicAssignment_Id/TopicAssignment_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TopicAssignment.TopicAssignment_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>TopicAssignment_IsDeleted/TopicAssignment_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TopicAssignment.TopicAssignment_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>TopicAssignment_SystemModstamp/TopicAssignment_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TopicAssignment.TopicAssignment_TopicId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>TopicAssignment_TopicId/sfdc_TopicAssignment_TopicId" entitySet="Topic"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TopicAssignment.TopicAssignment_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>TopicAssignment_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TopicAssignment.TopicAssignment_EntityId}" required="false"/> 
						<rdf:ListResource path="dataModel>TopicAssignment_EntityId" entitySet="Account"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>TopicAssignment.TopicAssignment_TopicId}" required="false"/> 
						<rdf:ListResource path="dataModel>TopicAssignment_TopicId" entitySet="Topic"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>