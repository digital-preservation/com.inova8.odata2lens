<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>ContractFeeds.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/ContractFeed' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>ContractFeeds.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContractFeeds.ContractFeed_Body}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContractFeeds.ContractFeed_CommentCount}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContractFeeds.ContractFeed_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContractFeeds.ContractFeed_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContractFeeds.ContractFeed_IsDeleted}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContractFeeds.ContractFeed_IsRichText}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContractFeeds.ContractFeed_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContractFeeds.ContractFeed_LikeCount}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContractFeeds.ContractFeed_LinkUrl}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContractFeeds.ContractFeed_SystemModstamp}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContractFeeds.ContractFeed_Title}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContractFeeds.ContractFeed_Type}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="ContractFeed"/>
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ContractFeed_Body/ContractFeed_Body',type:'sap.ui.model.type.String'}" linkText="{i18n>ContractFeed.ContractFeed_Body/ContractFeed_Body}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>ContractFeed_CommentCount/ContractFeed_CommentCount',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>ContractFeed_CreatedDate/ContractFeed_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ContractFeed_Id/ContractFeed_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>ContractFeed.ContractFeed_Id/ContractFeed_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>ContractFeed_IsDeleted/ContractFeed_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>ContractFeed_IsRichText/ContractFeed_IsRichText',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<Text text="{dataModel>ContractFeed_LastModifiedDate/ContractFeed_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>ContractFeed_LikeCount/ContractFeed_LikeCount',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ContractFeed_LinkUrl/ContractFeed_LinkUrl',type:'sap.ui.model.type.String'}" linkText="{i18n>ContractFeed.ContractFeed_LinkUrl/ContractFeed_LinkUrl}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>ContractFeed_SystemModstamp/ContractFeed_SystemModstamp}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ContractFeed_Title/ContractFeed_Title',type:'sap.ui.model.type.String'}" linkText="{i18n>ContractFeed.ContractFeed_Title/ContractFeed_Title}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ContractFeed_Type/ContractFeed_Type',type:'sap.ui.model.type.String'}" linkText="{i18n>ContractFeed.ContractFeed_Type/ContractFeed_Type}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>