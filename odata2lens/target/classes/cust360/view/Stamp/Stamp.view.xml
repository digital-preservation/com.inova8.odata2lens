<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>Stamp.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>Stamp.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="Stamp"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="Stamp"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Stamp.StampAssignment_StampId_Inverse}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>StampAssignment_StampId_Inverse/sfdc_StampAssignment_StampId_Inverse" entitySet="StampAssignment"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Stamp.StampLocalization_ParentId_Inverse}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>StampLocalization_ParentId_Inverse/sfdc_StampLocalization_ParentId_Inverse" entitySet="StampLocalization"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Stamp.Stamp_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>Stamp_CreatedById/sfdc_Stamp_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Stamp.Stamp_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>Stamp_CreatedDate/Stamp_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Stamp.Stamp_Description}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>Stamp_Description/Stamp_Description',type:'sap.ui.model.type.String' }" linkText="{i18n>Stamp.Stamp_Description/Stamp_Description}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Stamp.Stamp_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>Stamp_Id/Stamp_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>Stamp.Stamp_Id/Stamp_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Stamp.Stamp_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>Stamp_IsDeleted/Stamp_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Stamp.Stamp_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>Stamp_LastModifiedById/sfdc_Stamp_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Stamp.Stamp_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>Stamp_LastModifiedDate/Stamp_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Stamp.Stamp_MasterLabel}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>Stamp_MasterLabel/Stamp_MasterLabel',type:'sap.ui.model.type.String' }" linkText="{i18n>Stamp.Stamp_MasterLabel/Stamp_MasterLabel}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Stamp.Stamp_ParentId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>Stamp_ParentId/sfdc_Stamp_ParentId" entitySet="Organization"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Stamp.Stamp_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>Stamp_SystemModstamp/Stamp_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Stamp.StampAssignment_StampId_Inverse}" required="false"/> 
						<rdf:ListResource path="dataModel>StampAssignment_StampId_Inverse" entitySet="StampAssignment"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Stamp.StampLocalization_ParentId_Inverse}" required="false"/> 
						<rdf:ListResource path="dataModel>StampLocalization_ParentId_Inverse" entitySet="StampLocalization"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Stamp.Stamp_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>Stamp_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Stamp.Stamp_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>Stamp_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Stamp.Stamp_ParentId}" required="false"/> 
						<rdf:ListResource path="dataModel>Stamp_ParentId" entitySet="Organization"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>