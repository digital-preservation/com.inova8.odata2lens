<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>EventBusSubscriber.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>EventBusSubscriber.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="EventBusSubscriber"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="EventBusSubscriber"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EventBusSubscriber.EventBusSubscriber_ExternalId}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>EventBusSubscriber_ExternalId/EventBusSubscriber_ExternalId',type:'sap.ui.model.type.String' }" linkText="{i18n>EventBusSubscriber.EventBusSubscriber_ExternalId/EventBusSubscriber_ExternalId}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EventBusSubscriber.EventBusSubscriber_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>EventBusSubscriber_Id/EventBusSubscriber_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>EventBusSubscriber.EventBusSubscriber_Id/EventBusSubscriber_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EventBusSubscriber.EventBusSubscriber_Name}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>EventBusSubscriber_Name/EventBusSubscriber_Name',type:'sap.ui.model.type.String' }" linkText="{i18n>EventBusSubscriber.EventBusSubscriber_Name/EventBusSubscriber_Name}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EventBusSubscriber.EventBusSubscriber_Position}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>EventBusSubscriber_Position/EventBusSubscriber_Position',type:'sap.ui.model.type.Integer' }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EventBusSubscriber.EventBusSubscriber_Status}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>EventBusSubscriber_Status/EventBusSubscriber_Status',type:'sap.ui.model.type.String' }" linkText="{i18n>EventBusSubscriber.EventBusSubscriber_Status/EventBusSubscriber_Status}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EventBusSubscriber.EventBusSubscriber_Tip}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>EventBusSubscriber_Tip/EventBusSubscriber_Tip',type:'sap.ui.model.type.Integer' }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EventBusSubscriber.EventBusSubscriber_Topic}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>EventBusSubscriber_Topic/EventBusSubscriber_Topic',type:'sap.ui.model.type.String' }" linkText="{i18n>EventBusSubscriber.EventBusSubscriber_Topic/EventBusSubscriber_Topic}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EventBusSubscriber.EventBusSubscriber_Type}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>EventBusSubscriber_Type/EventBusSubscriber_Type',type:'sap.ui.model.type.String' }" linkText="{i18n>EventBusSubscriber.EventBusSubscriber_Type/EventBusSubscriber_Type}"/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>