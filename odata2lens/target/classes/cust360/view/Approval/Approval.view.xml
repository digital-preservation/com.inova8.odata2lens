<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>Approval.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>Approval.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="Approval"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="Approval"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Approval.Approval_ApproveComment}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>Approval_ApproveComment/Approval_ApproveComment',type:'sap.ui.model.type.String' }" linkText="{i18n>Approval.Approval_ApproveComment/Approval_ApproveComment}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Approval.Approval_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>Approval_CreatedById/sfdc_Approval_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Approval.Approval_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>Approval_CreatedDate/Approval_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Approval.Approval_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>Approval_Id/Approval_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>Approval.Approval_Id/Approval_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Approval.Approval_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>Approval_IsDeleted/Approval_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Approval.Approval_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>Approval_LastModifiedById/sfdc_Approval_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Approval.Approval_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>Approval_LastModifiedDate/Approval_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Approval.Approval_OwnerId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>Approval_OwnerId/sfdc_Approval_OwnerId" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Approval.Approval_ParentId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>Approval_ParentId/sfdc_Approval_ParentId" entitySet="Contract"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Approval.Approval_RequestComment}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>Approval_RequestComment/Approval_RequestComment',type:'sap.ui.model.type.String' }" linkText="{i18n>Approval.Approval_RequestComment/Approval_RequestComment}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Approval.Approval_Status}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>Approval_Status/Approval_Status',type:'sap.ui.model.type.String' }" linkText="{i18n>Approval.Approval_Status/Approval_Status}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Approval.Approval_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>Approval_SystemModstamp/Approval_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Approval.Approval_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>Approval_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Approval.Approval_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>Approval_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Approval.Approval_OwnerId}" required="false"/> 
						<rdf:ListResource path="dataModel>Approval_OwnerId" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Approval.Approval_ParentId}" required="false"/> 
						<rdf:ListResource path="dataModel>Approval_ParentId" entitySet="Contract"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>