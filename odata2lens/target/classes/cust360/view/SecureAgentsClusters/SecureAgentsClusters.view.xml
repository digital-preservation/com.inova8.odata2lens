<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>SecureAgentsClusters.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/SecureAgentsCluster' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>SecureAgentsClusters.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>SecureAgentsClusters.SecureAgentsCluster_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>SecureAgentsClusters.SecureAgentsCluster_Description}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>SecureAgentsClusters.SecureAgentsCluster_DeveloperName}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>SecureAgentsClusters.SecureAgentsCluster_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>SecureAgentsClusters.SecureAgentsCluster_IsDeleted}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>SecureAgentsClusters.SecureAgentsCluster_Language}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>SecureAgentsClusters.SecureAgentsCluster_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>SecureAgentsClusters.SecureAgentsCluster_MasterLabel}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>SecureAgentsClusters.SecureAgentsCluster_SystemModstamp}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="SecureAgentsCluster"/>
							<VBox>					
							<Text text="{dataModel>SecureAgentsCluster_CreatedDate/SecureAgentsCluster_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>SecureAgentsCluster_Description/SecureAgentsCluster_Description',type:'sap.ui.model.type.String'}" linkText="{i18n>SecureAgentsCluster.SecureAgentsCluster_Description/SecureAgentsCluster_Description}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>SecureAgentsCluster_DeveloperName/SecureAgentsCluster_DeveloperName',type:'sap.ui.model.type.String'}" linkText="{i18n>SecureAgentsCluster.SecureAgentsCluster_DeveloperName/SecureAgentsCluster_DeveloperName}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>SecureAgentsCluster_Id/SecureAgentsCluster_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>SecureAgentsCluster.SecureAgentsCluster_Id/SecureAgentsCluster_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>SecureAgentsCluster_IsDeleted/SecureAgentsCluster_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>SecureAgentsCluster_Language/SecureAgentsCluster_Language',type:'sap.ui.model.type.String'}" linkText="{i18n>SecureAgentsCluster.SecureAgentsCluster_Language/SecureAgentsCluster_Language}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>SecureAgentsCluster_LastModifiedDate/SecureAgentsCluster_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>SecureAgentsCluster_MasterLabel/SecureAgentsCluster_MasterLabel',type:'sap.ui.model.type.String'}" linkText="{i18n>SecureAgentsCluster.SecureAgentsCluster_MasterLabel/SecureAgentsCluster_MasterLabel}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>SecureAgentsCluster_SystemModstamp/SecureAgentsCluster_SystemModstamp}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>