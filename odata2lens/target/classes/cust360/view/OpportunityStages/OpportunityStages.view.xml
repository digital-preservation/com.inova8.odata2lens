<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>OpportunityStages.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/OpportunityStage' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>OpportunityStages.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>OpportunityStages.OpportunityStage_ApiName}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>OpportunityStages.OpportunityStage_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>OpportunityStages.OpportunityStage_DefaultProbability}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>OpportunityStages.OpportunityStage_Description}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>OpportunityStages.OpportunityStage_ForecastCategory}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>OpportunityStages.OpportunityStage_ForecastCategoryName}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>OpportunityStages.OpportunityStage_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>OpportunityStages.OpportunityStage_IsActive}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>OpportunityStages.OpportunityStage_IsClosed}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>OpportunityStages.OpportunityStage_IsWon}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>OpportunityStages.OpportunityStage_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>OpportunityStages.OpportunityStage_MasterLabel}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>OpportunityStages.OpportunityStage_SortOrder}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>OpportunityStages.OpportunityStage_SystemModstamp}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="OpportunityStage"/>
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>OpportunityStage_ApiName/OpportunityStage_ApiName',type:'sap.ui.model.type.String'}" linkText="{i18n>OpportunityStage.OpportunityStage_ApiName/OpportunityStage_ApiName}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>OpportunityStage_CreatedDate/OpportunityStage_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>OpportunityStage_DefaultProbability/OpportunityStage_DefaultProbability',type:'sap.ui.model.type.Float', formatOptions:{'style':'short','maxFractionDigits':2} }"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>OpportunityStage_Description/OpportunityStage_Description',type:'sap.ui.model.type.String'}" linkText="{i18n>OpportunityStage.OpportunityStage_Description/OpportunityStage_Description}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>OpportunityStage_ForecastCategory/OpportunityStage_ForecastCategory',type:'sap.ui.model.type.String'}" linkText="{i18n>OpportunityStage.OpportunityStage_ForecastCategory/OpportunityStage_ForecastCategory}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>OpportunityStage_ForecastCategoryName/OpportunityStage_ForecastCategoryName',type:'sap.ui.model.type.String'}" linkText="{i18n>OpportunityStage.OpportunityStage_ForecastCategoryName/OpportunityStage_ForecastCategoryName}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>OpportunityStage_Id/OpportunityStage_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>OpportunityStage.OpportunityStage_Id/OpportunityStage_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>OpportunityStage_IsActive/OpportunityStage_IsActive',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>OpportunityStage_IsClosed/OpportunityStage_IsClosed',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>OpportunityStage_IsWon/OpportunityStage_IsWon',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<Text text="{dataModel>OpportunityStage_LastModifiedDate/OpportunityStage_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>OpportunityStage_MasterLabel/OpportunityStage_MasterLabel',type:'sap.ui.model.type.String'}" linkText="{i18n>OpportunityStage.OpportunityStage_MasterLabel/OpportunityStage_MasterLabel}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>OpportunityStage_SortOrder/OpportunityStage_SortOrder',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>OpportunityStage_SystemModstamp/OpportunityStage_SystemModstamp}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>