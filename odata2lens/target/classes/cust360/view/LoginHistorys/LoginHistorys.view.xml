<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>LoginHistorys.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/LoginHistory' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>LoginHistorys.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>LoginHistorys.LoginHistory_ApiType}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>LoginHistorys.LoginHistory_ApiVersion}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>LoginHistorys.LoginHistory_Application}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>LoginHistorys.LoginHistory_Browser}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>LoginHistorys.LoginHistory_CipherSuite}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>LoginHistorys.LoginHistory_ClientVersion}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>LoginHistorys.LoginHistory_CountryIso}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>LoginHistorys.LoginHistory_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>LoginHistorys.LoginHistory_LoginTime}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>LoginHistorys.LoginHistory_LoginType}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>LoginHistorys.LoginHistory_LoginUrl}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>LoginHistorys.LoginHistory_Platform}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>LoginHistorys.LoginHistory_SourceIp}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>LoginHistorys.LoginHistory_Status}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>LoginHistorys.LoginHistory_TlsProtocol}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="LoginHistory"/>
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>LoginHistory_ApiType/LoginHistory_ApiType',type:'sap.ui.model.type.String'}" linkText="{i18n>LoginHistory.LoginHistory_ApiType/LoginHistory_ApiType}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>LoginHistory_ApiVersion/LoginHistory_ApiVersion',type:'sap.ui.model.type.String'}" linkText="{i18n>LoginHistory.LoginHistory_ApiVersion/LoginHistory_ApiVersion}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>LoginHistory_Application/LoginHistory_Application',type:'sap.ui.model.type.String'}" linkText="{i18n>LoginHistory.LoginHistory_Application/LoginHistory_Application}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>LoginHistory_Browser/LoginHistory_Browser',type:'sap.ui.model.type.String'}" linkText="{i18n>LoginHistory.LoginHistory_Browser/LoginHistory_Browser}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>LoginHistory_CipherSuite/LoginHistory_CipherSuite',type:'sap.ui.model.type.String'}" linkText="{i18n>LoginHistory.LoginHistory_CipherSuite/LoginHistory_CipherSuite}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>LoginHistory_ClientVersion/LoginHistory_ClientVersion',type:'sap.ui.model.type.String'}" linkText="{i18n>LoginHistory.LoginHistory_ClientVersion/LoginHistory_ClientVersion}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>LoginHistory_CountryIso/LoginHistory_CountryIso',type:'sap.ui.model.type.String'}" linkText="{i18n>LoginHistory.LoginHistory_CountryIso/LoginHistory_CountryIso}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>LoginHistory_Id/LoginHistory_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>LoginHistory.LoginHistory_Id/LoginHistory_Id}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>LoginHistory_LoginTime/LoginHistory_LoginTime}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>LoginHistory_LoginType/LoginHistory_LoginType',type:'sap.ui.model.type.String'}" linkText="{i18n>LoginHistory.LoginHistory_LoginType/LoginHistory_LoginType}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>LoginHistory_LoginUrl/LoginHistory_LoginUrl',type:'sap.ui.model.type.String'}" linkText="{i18n>LoginHistory.LoginHistory_LoginUrl/LoginHistory_LoginUrl}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>LoginHistory_Platform/LoginHistory_Platform',type:'sap.ui.model.type.String'}" linkText="{i18n>LoginHistory.LoginHistory_Platform/LoginHistory_Platform}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>LoginHistory_SourceIp/LoginHistory_SourceIp',type:'sap.ui.model.type.String'}" linkText="{i18n>LoginHistory.LoginHistory_SourceIp/LoginHistory_SourceIp}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>LoginHistory_Status/LoginHistory_Status',type:'sap.ui.model.type.String'}" linkText="{i18n>LoginHistory.LoginHistory_Status/LoginHistory_Status}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>LoginHistory_TlsProtocol/LoginHistory_TlsProtocol',type:'sap.ui.model.type.String'}" linkText="{i18n>LoginHistory.LoginHistory_TlsProtocol/LoginHistory_TlsProtocol}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>