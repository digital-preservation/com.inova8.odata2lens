<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>UserProvisioningLogs.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/UserProvisioningLog' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>UserProvisioningLogs.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserProvisioningLogs.UserProvisioningLog_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserProvisioningLogs.UserProvisioningLog_Details}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserProvisioningLogs.UserProvisioningLog_ExternalUserId}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserProvisioningLogs.UserProvisioningLog_ExternalUsername}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserProvisioningLogs.UserProvisioningLog_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserProvisioningLogs.UserProvisioningLog_IsDeleted}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserProvisioningLogs.UserProvisioningLog_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserProvisioningLogs.UserProvisioningLog_Name}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserProvisioningLogs.UserProvisioningLog_Status}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserProvisioningLogs.UserProvisioningLog_SystemModstamp}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="UserProvisioningLog"/>
							<VBox>					
							<Text text="{dataModel>UserProvisioningLog_CreatedDate/UserProvisioningLog_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>UserProvisioningLog_Details/UserProvisioningLog_Details',type:'sap.ui.model.type.String'}" linkText="{i18n>UserProvisioningLog.UserProvisioningLog_Details/UserProvisioningLog_Details}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>UserProvisioningLog_ExternalUserId/UserProvisioningLog_ExternalUserId',type:'sap.ui.model.type.String'}" linkText="{i18n>UserProvisioningLog.UserProvisioningLog_ExternalUserId/UserProvisioningLog_ExternalUserId}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>UserProvisioningLog_ExternalUsername/UserProvisioningLog_ExternalUsername',type:'sap.ui.model.type.String'}" linkText="{i18n>UserProvisioningLog.UserProvisioningLog_ExternalUsername/UserProvisioningLog_ExternalUsername}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>UserProvisioningLog_Id/UserProvisioningLog_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>UserProvisioningLog.UserProvisioningLog_Id/UserProvisioningLog_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>UserProvisioningLog_IsDeleted/UserProvisioningLog_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<Text text="{dataModel>UserProvisioningLog_LastModifiedDate/UserProvisioningLog_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>UserProvisioningLog_Name/UserProvisioningLog_Name',type:'sap.ui.model.type.String'}" linkText="{i18n>UserProvisioningLog.UserProvisioningLog_Name/UserProvisioningLog_Name}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>UserProvisioningLog_Status/UserProvisioningLog_Status',type:'sap.ui.model.type.String'}" linkText="{i18n>UserProvisioningLog.UserProvisioningLog_Status/UserProvisioningLog_Status}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>UserProvisioningLog_SystemModstamp/UserProvisioningLog_SystemModstamp}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>