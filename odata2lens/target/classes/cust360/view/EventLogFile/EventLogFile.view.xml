<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>EventLogFile.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>EventLogFile.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="EventLogFile"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="EventLogFile"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EventLogFile.EventLogFile_ApiVersion}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>EventLogFile_ApiVersion/EventLogFile_ApiVersion',type:'sap.ui.model.type.Float', formatOptions:{'style':'short','maxFractionDigits':2} }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EventLogFile.EventLogFile_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>EventLogFile_CreatedById/sfdc_EventLogFile_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EventLogFile.EventLogFile_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>EventLogFile_CreatedDate/EventLogFile_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EventLogFile.EventLogFile_EventType}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>EventLogFile_EventType/EventLogFile_EventType',type:'sap.ui.model.type.String' }" linkText="{i18n>EventLogFile.EventLogFile_EventType/EventLogFile_EventType}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EventLogFile.EventLogFile_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>EventLogFile_Id/EventLogFile_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>EventLogFile.EventLogFile_Id/EventLogFile_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EventLogFile.EventLogFile_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>EventLogFile_IsDeleted/EventLogFile_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EventLogFile.EventLogFile_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>EventLogFile_LastModifiedById/sfdc_EventLogFile_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EventLogFile.EventLogFile_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>EventLogFile_LastModifiedDate/EventLogFile_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EventLogFile.EventLogFile_LogDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>EventLogFile_LogDate/EventLogFile_LogDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EventLogFile.EventLogFile_LogFile}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>EventLogFile_LogFile/EventLogFile_LogFile}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EventLogFile.EventLogFile_LogFileContentType}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>EventLogFile_LogFileContentType/EventLogFile_LogFileContentType',type:'sap.ui.model.type.String' }" linkText="{i18n>EventLogFile.EventLogFile_LogFileContentType/EventLogFile_LogFileContentType}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EventLogFile.EventLogFile_LogFileFieldNames}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>EventLogFile_LogFileFieldNames/EventLogFile_LogFileFieldNames',type:'sap.ui.model.type.String' }" linkText="{i18n>EventLogFile.EventLogFile_LogFileFieldNames/EventLogFile_LogFileFieldNames}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EventLogFile.EventLogFile_LogFileFieldTypes}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>EventLogFile_LogFileFieldTypes/EventLogFile_LogFileFieldTypes',type:'sap.ui.model.type.String' }" linkText="{i18n>EventLogFile.EventLogFile_LogFileFieldTypes/EventLogFile_LogFileFieldTypes}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EventLogFile.EventLogFile_LogFileLength}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>EventLogFile_LogFileLength/EventLogFile_LogFileLength',type:'sap.ui.model.type.Float', formatOptions:{'style':'short','maxFractionDigits':2} }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EventLogFile.EventLogFile_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>EventLogFile_SystemModstamp/EventLogFile_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EventLogFile.EventLogFile_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>EventLogFile_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EventLogFile.EventLogFile_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>EventLogFile_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>