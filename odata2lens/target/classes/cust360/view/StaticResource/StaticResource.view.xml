<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>StaticResource.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>StaticResource.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="StaticResource"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="StaticResource"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>StaticResource.ExternalDataSource_LargeIconId_Inverse}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ExternalDataSource_LargeIconId_Inverse/sfdc_ExternalDataSource_LargeIconId_Inverse" entitySet="ExternalDataSource"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>StaticResource.ExternalDataSource_SmallIconId_Inverse}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ExternalDataSource_SmallIconId_Inverse/sfdc_ExternalDataSource_SmallIconId_Inverse" entitySet="ExternalDataSource"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>StaticResource.StaticResource_Body}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>StaticResource_Body/StaticResource_Body}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>StaticResource.StaticResource_BodyLength}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>StaticResource_BodyLength/StaticResource_BodyLength',type:'sap.ui.model.type.Integer' }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>StaticResource.StaticResource_CacheControl}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>StaticResource_CacheControl/StaticResource_CacheControl',type:'sap.ui.model.type.String' }" linkText="{i18n>StaticResource.StaticResource_CacheControl/StaticResource_CacheControl}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>StaticResource.StaticResource_ContentType}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>StaticResource_ContentType/StaticResource_ContentType',type:'sap.ui.model.type.String' }" linkText="{i18n>StaticResource.StaticResource_ContentType/StaticResource_ContentType}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>StaticResource.StaticResource_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>StaticResource_CreatedById/sfdc_StaticResource_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>StaticResource.StaticResource_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>StaticResource_CreatedDate/StaticResource_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>StaticResource.StaticResource_Description}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>StaticResource_Description/StaticResource_Description',type:'sap.ui.model.type.String' }" linkText="{i18n>StaticResource.StaticResource_Description/StaticResource_Description}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>StaticResource.StaticResource_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>StaticResource_Id/StaticResource_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>StaticResource.StaticResource_Id/StaticResource_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>StaticResource.StaticResource_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>StaticResource_LastModifiedById/sfdc_StaticResource_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>StaticResource.StaticResource_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>StaticResource_LastModifiedDate/StaticResource_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>StaticResource.StaticResource_Name}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>StaticResource_Name/StaticResource_Name',type:'sap.ui.model.type.String' }" linkText="{i18n>StaticResource.StaticResource_Name/StaticResource_Name}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>StaticResource.StaticResource_NamespacePrefix}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>StaticResource_NamespacePrefix/StaticResource_NamespacePrefix',type:'sap.ui.model.type.String' }" linkText="{i18n>StaticResource.StaticResource_NamespacePrefix/StaticResource_NamespacePrefix}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>StaticResource.StaticResource_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>StaticResource_SystemModstamp/StaticResource_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>StaticResource.ExternalDataSource_LargeIconId_Inverse}" required="false"/> 
						<rdf:ListResource path="dataModel>ExternalDataSource_LargeIconId_Inverse" entitySet="ExternalDataSource"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>StaticResource.ExternalDataSource_SmallIconId_Inverse}" required="false"/> 
						<rdf:ListResource path="dataModel>ExternalDataSource_SmallIconId_Inverse" entitySet="ExternalDataSource"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>StaticResource.StaticResource_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>StaticResource_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>StaticResource.StaticResource_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>StaticResource_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>