<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>ApexPage.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>ApexPage.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="ApexPage"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="ApexPage"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexPage.ApexPage_ApiVersion}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>ApexPage_ApiVersion/ApexPage_ApiVersion',type:'sap.ui.model.type.Float', formatOptions:{'style':'short','maxFractionDigits':2} }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexPage.ApexPage_ControllerKey}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ApexPage_ControllerKey/ApexPage_ControllerKey',type:'sap.ui.model.type.String' }" linkText="{i18n>ApexPage.ApexPage_ControllerKey/ApexPage_ControllerKey}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexPage.ApexPage_ControllerType}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ApexPage_ControllerType/ApexPage_ControllerType',type:'sap.ui.model.type.String' }" linkText="{i18n>ApexPage.ApexPage_ControllerType/ApexPage_ControllerType}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexPage.ApexPage_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ApexPage_CreatedById/sfdc_ApexPage_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexPage.ApexPage_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>ApexPage_CreatedDate/ApexPage_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexPage.ApexPage_Description}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ApexPage_Description/ApexPage_Description',type:'sap.ui.model.type.String' }" linkText="{i18n>ApexPage.ApexPage_Description/ApexPage_Description}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexPage.ApexPage_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ApexPage_Id/ApexPage_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>ApexPage.ApexPage_Id/ApexPage_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexPage.ApexPage_IsAvailableInTouch}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>ApexPage_IsAvailableInTouch/ApexPage_IsAvailableInTouch',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexPage.ApexPage_IsConfirmationTokenRequired}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>ApexPage_IsConfirmationTokenRequired/ApexPage_IsConfirmationTokenRequired',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexPage.ApexPage_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ApexPage_LastModifiedById/sfdc_ApexPage_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexPage.ApexPage_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>ApexPage_LastModifiedDate/ApexPage_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexPage.ApexPage_Markup}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ApexPage_Markup/ApexPage_Markup',type:'sap.ui.model.type.String' }" linkText="{i18n>ApexPage.ApexPage_Markup/ApexPage_Markup}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexPage.ApexPage_MasterLabel}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ApexPage_MasterLabel/ApexPage_MasterLabel',type:'sap.ui.model.type.String' }" linkText="{i18n>ApexPage.ApexPage_MasterLabel/ApexPage_MasterLabel}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexPage.ApexPage_Name}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ApexPage_Name/ApexPage_Name',type:'sap.ui.model.type.String' }" linkText="{i18n>ApexPage.ApexPage_Name/ApexPage_Name}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexPage.ApexPage_NamespacePrefix}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ApexPage_NamespacePrefix/ApexPage_NamespacePrefix',type:'sap.ui.model.type.String' }" linkText="{i18n>ApexPage.ApexPage_NamespacePrefix/ApexPage_NamespacePrefix}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexPage.ApexPage_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>ApexPage_SystemModstamp/ApexPage_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexPage.VisualforceAccessMetrics_ApexPageId_Inverse}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>VisualforceAccessMetrics_ApexPageId_Inverse/sfdc_VisualforceAccessMetrics_ApexPageId_Inverse" entitySet="VisualforceAccessMetrics"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexPage.WebLink_ScontrolId_Inverse}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>WebLink_ScontrolId_Inverse/sfdc_WebLink_ScontrolId_Inverse" entitySet="WebLink"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexPage.ApexPage_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>ApexPage_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexPage.ApexPage_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>ApexPage_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexPage.VisualforceAccessMetrics_ApexPageId_Inverse}" required="false"/> 
						<rdf:ListResource path="dataModel>VisualforceAccessMetrics_ApexPageId_Inverse" entitySet="VisualforceAccessMetrics"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexPage.WebLink_ScontrolId_Inverse}" required="false"/> 
						<rdf:ListResource path="dataModel>WebLink_ScontrolId_Inverse" entitySet="WebLink"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>