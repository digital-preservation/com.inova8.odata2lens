<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>UserShare.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>UserShare.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="UserShare"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="UserShare"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserShare.UserShare_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>UserShare_Id/UserShare_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>UserShare.UserShare_Id/UserShare_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserShare.UserShare_IsActive}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>UserShare_IsActive/UserShare_IsActive',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserShare.UserShare_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>UserShare_LastModifiedById/sfdc_UserShare_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserShare.UserShare_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>UserShare_LastModifiedDate/UserShare_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserShare.UserShare_RowCause}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>UserShare_RowCause/UserShare_RowCause',type:'sap.ui.model.type.String' }" linkText="{i18n>UserShare.UserShare_RowCause/UserShare_RowCause}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserShare.UserShare_UserAccessLevel}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>UserShare_UserAccessLevel/UserShare_UserAccessLevel',type:'sap.ui.model.type.String' }" linkText="{i18n>UserShare.UserShare_UserAccessLevel/UserShare_UserAccessLevel}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserShare.UserShare_UserId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>UserShare_UserId/sfdc_UserShare_UserId" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserShare.UserShare_UserOrGroupId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>UserShare_UserOrGroupId/sfdc_UserShare_UserOrGroupId" entitySet="Group"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserShare.UserShare_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>UserShare_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserShare.UserShare_UserId}" required="false"/> 
						<rdf:ListResource path="dataModel>UserShare_UserId" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserShare.UserShare_UserOrGroupId}" required="false"/> 
						<rdf:ListResource path="dataModel>UserShare_UserOrGroupId" entitySet="Group"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>