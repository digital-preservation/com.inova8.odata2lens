<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>SearchActivity.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>SearchActivity.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="SearchActivity"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="SearchActivity"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SearchActivity.SearchActivity_AvgNumResults}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>SearchActivity_AvgNumResults/SearchActivity_AvgNumResults',type:'sap.ui.model.type.Float', formatOptions:{'style':'short','maxFractionDigits':2} }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SearchActivity.SearchActivity_ClickRank}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>SearchActivity_ClickRank/SearchActivity_ClickRank',type:'sap.ui.model.type.Float', formatOptions:{'style':'short','maxFractionDigits':2} }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SearchActivity.SearchActivity_ClickedRecordName}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>SearchActivity_ClickedRecordName/SearchActivity_ClickedRecordName',type:'sap.ui.model.type.String' }" linkText="{i18n>SearchActivity.SearchActivity_ClickedRecordName/SearchActivity_ClickedRecordName}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SearchActivity.SearchActivity_CountQueries}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>SearchActivity_CountQueries/SearchActivity_CountQueries',type:'sap.ui.model.type.Integer' }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SearchActivity.SearchActivity_CountUsers}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>SearchActivity_CountUsers/SearchActivity_CountUsers',type:'sap.ui.model.type.Integer' }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SearchActivity.SearchActivity_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>SearchActivity_CreatedById/sfdc_SearchActivity_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SearchActivity.SearchActivity_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>SearchActivity_CreatedDate/SearchActivity_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SearchActivity.SearchActivity_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>SearchActivity_Id/SearchActivity_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>SearchActivity.SearchActivity_Id/SearchActivity_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SearchActivity.SearchActivity_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>SearchActivity_IsDeleted/SearchActivity_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SearchActivity.SearchActivity_KbChannel}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>SearchActivity_KbChannel/SearchActivity_KbChannel',type:'sap.ui.model.type.String' }" linkText="{i18n>SearchActivity.SearchActivity_KbChannel/SearchActivity_KbChannel}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SearchActivity.SearchActivity_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>SearchActivity_LastModifiedById/sfdc_SearchActivity_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SearchActivity.SearchActivity_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>SearchActivity_LastModifiedDate/SearchActivity_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SearchActivity.SearchActivity_Name}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>SearchActivity_Name/SearchActivity_Name',type:'sap.ui.model.type.String' }" linkText="{i18n>SearchActivity.SearchActivity_Name/SearchActivity_Name}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SearchActivity.SearchActivity_Period}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>SearchActivity_Period/SearchActivity_Period',type:'sap.ui.model.type.String' }" linkText="{i18n>SearchActivity.SearchActivity_Period/SearchActivity_Period}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SearchActivity.SearchActivity_QueryDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>SearchActivity_QueryDate/SearchActivity_QueryDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SearchActivity.SearchActivity_QueryLanguage}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>SearchActivity_QueryLanguage/SearchActivity_QueryLanguage',type:'sap.ui.model.type.String' }" linkText="{i18n>SearchActivity.SearchActivity_QueryLanguage/SearchActivity_QueryLanguage}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SearchActivity.SearchActivity_SearchTerm}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>SearchActivity_SearchTerm/SearchActivity_SearchTerm',type:'sap.ui.model.type.String' }" linkText="{i18n>SearchActivity.SearchActivity_SearchTerm/SearchActivity_SearchTerm}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SearchActivity.SearchActivity_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>SearchActivity_SystemModstamp/SearchActivity_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SearchActivity.SearchActivity_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>SearchActivity_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SearchActivity.SearchActivity_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>SearchActivity_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>