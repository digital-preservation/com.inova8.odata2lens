<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>ApexPageInfos.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/ApexPageInfo' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>ApexPageInfos.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexPageInfos.ApexPageInfo_ApexPageId}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexPageInfos.ApexPageInfo_ApiVersion}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexPageInfos.ApexPageInfo_Description}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexPageInfos.ApexPageInfo_DurableId}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexPageInfos.ApexPageInfo_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexPageInfos.ApexPageInfo_IsAvailableInTouch}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexPageInfos.ApexPageInfo_IsShowHeader}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexPageInfos.ApexPageInfo_MasterLabel}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexPageInfos.ApexPageInfo_Name}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexPageInfos.ApexPageInfo_NameSpacePrefix}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="ApexPageInfo"/>
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ApexPageInfo_ApexPageId/ApexPageInfo_ApexPageId',type:'sap.ui.model.type.String'}" linkText="{i18n>ApexPageInfo.ApexPageInfo_ApexPageId/ApexPageInfo_ApexPageId}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>ApexPageInfo_ApiVersion/ApexPageInfo_ApiVersion',type:'sap.ui.model.type.Float', formatOptions:{'style':'short','maxFractionDigits':2} }"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ApexPageInfo_Description/ApexPageInfo_Description',type:'sap.ui.model.type.String'}" linkText="{i18n>ApexPageInfo.ApexPageInfo_Description/ApexPageInfo_Description}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ApexPageInfo_DurableId/ApexPageInfo_DurableId',type:'sap.ui.model.type.String'}" linkText="{i18n>ApexPageInfo.ApexPageInfo_DurableId/ApexPageInfo_DurableId}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ApexPageInfo_Id/ApexPageInfo_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>ApexPageInfo.ApexPageInfo_Id/ApexPageInfo_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>ApexPageInfo_IsAvailableInTouch/ApexPageInfo_IsAvailableInTouch',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ApexPageInfo_IsShowHeader/ApexPageInfo_IsShowHeader',type:'sap.ui.model.type.String'}" linkText="{i18n>ApexPageInfo.ApexPageInfo_IsShowHeader/ApexPageInfo_IsShowHeader}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ApexPageInfo_MasterLabel/ApexPageInfo_MasterLabel',type:'sap.ui.model.type.String'}" linkText="{i18n>ApexPageInfo.ApexPageInfo_MasterLabel/ApexPageInfo_MasterLabel}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ApexPageInfo_Name/ApexPageInfo_Name',type:'sap.ui.model.type.String'}" linkText="{i18n>ApexPageInfo.ApexPageInfo_Name/ApexPageInfo_Name}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ApexPageInfo_NameSpacePrefix/ApexPageInfo_NameSpacePrefix',type:'sap.ui.model.type.String'}" linkText="{i18n>ApexPageInfo.ApexPageInfo_NameSpacePrefix/ApexPageInfo_NameSpacePrefix}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>