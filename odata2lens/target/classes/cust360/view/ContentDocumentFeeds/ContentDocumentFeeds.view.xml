<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>ContentDocumentFeeds.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/ContentDocumentFeed' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>ContentDocumentFeeds.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContentDocumentFeeds.ContentDocumentFeed_Body}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContentDocumentFeeds.ContentDocumentFeed_CommentCount}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContentDocumentFeeds.ContentDocumentFeed_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContentDocumentFeeds.ContentDocumentFeed_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContentDocumentFeeds.ContentDocumentFeed_IsDeleted}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContentDocumentFeeds.ContentDocumentFeed_IsRichText}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContentDocumentFeeds.ContentDocumentFeed_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContentDocumentFeeds.ContentDocumentFeed_LikeCount}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContentDocumentFeeds.ContentDocumentFeed_LinkUrl}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContentDocumentFeeds.ContentDocumentFeed_SystemModstamp}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContentDocumentFeeds.ContentDocumentFeed_Title}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContentDocumentFeeds.ContentDocumentFeed_Type}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="ContentDocumentFeed"/>
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ContentDocumentFeed_Body/ContentDocumentFeed_Body',type:'sap.ui.model.type.String'}" linkText="{i18n>ContentDocumentFeed.ContentDocumentFeed_Body/ContentDocumentFeed_Body}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>ContentDocumentFeed_CommentCount/ContentDocumentFeed_CommentCount',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>ContentDocumentFeed_CreatedDate/ContentDocumentFeed_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ContentDocumentFeed_Id/ContentDocumentFeed_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>ContentDocumentFeed.ContentDocumentFeed_Id/ContentDocumentFeed_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>ContentDocumentFeed_IsDeleted/ContentDocumentFeed_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>ContentDocumentFeed_IsRichText/ContentDocumentFeed_IsRichText',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<Text text="{dataModel>ContentDocumentFeed_LastModifiedDate/ContentDocumentFeed_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>ContentDocumentFeed_LikeCount/ContentDocumentFeed_LikeCount',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ContentDocumentFeed_LinkUrl/ContentDocumentFeed_LinkUrl',type:'sap.ui.model.type.String'}" linkText="{i18n>ContentDocumentFeed.ContentDocumentFeed_LinkUrl/ContentDocumentFeed_LinkUrl}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>ContentDocumentFeed_SystemModstamp/ContentDocumentFeed_SystemModstamp}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ContentDocumentFeed_Title/ContentDocumentFeed_Title',type:'sap.ui.model.type.String'}" linkText="{i18n>ContentDocumentFeed.ContentDocumentFeed_Title/ContentDocumentFeed_Title}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ContentDocumentFeed_Type/ContentDocumentFeed_Type',type:'sap.ui.model.type.String'}" linkText="{i18n>ContentDocumentFeed.ContentDocumentFeed_Type/ContentDocumentFeed_Type}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>