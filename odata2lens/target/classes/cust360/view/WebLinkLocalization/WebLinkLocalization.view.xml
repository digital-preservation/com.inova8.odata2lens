<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>WebLinkLocalization.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>WebLinkLocalization.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="WebLinkLocalization"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="WebLinkLocalization"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>WebLinkLocalization.WebLinkLocalization_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>WebLinkLocalization_CreatedById/sfdc_WebLinkLocalization_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>WebLinkLocalization.WebLinkLocalization_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>WebLinkLocalization_CreatedDate/WebLinkLocalization_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>WebLinkLocalization.WebLinkLocalization_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>WebLinkLocalization_Id/WebLinkLocalization_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>WebLinkLocalization.WebLinkLocalization_Id/WebLinkLocalization_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>WebLinkLocalization.WebLinkLocalization_Language}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>WebLinkLocalization_Language/WebLinkLocalization_Language',type:'sap.ui.model.type.String' }" linkText="{i18n>WebLinkLocalization.WebLinkLocalization_Language/WebLinkLocalization_Language}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>WebLinkLocalization.WebLinkLocalization_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>WebLinkLocalization_LastModifiedById/sfdc_WebLinkLocalization_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>WebLinkLocalization.WebLinkLocalization_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>WebLinkLocalization_LastModifiedDate/WebLinkLocalization_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>WebLinkLocalization.WebLinkLocalization_NamespacePrefix}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>WebLinkLocalization_NamespacePrefix/WebLinkLocalization_NamespacePrefix',type:'sap.ui.model.type.String' }" linkText="{i18n>WebLinkLocalization.WebLinkLocalization_NamespacePrefix/WebLinkLocalization_NamespacePrefix}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>WebLinkLocalization.WebLinkLocalization_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>WebLinkLocalization_SystemModstamp/WebLinkLocalization_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>WebLinkLocalization.WebLinkLocalization_Value}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>WebLinkLocalization_Value/WebLinkLocalization_Value',type:'sap.ui.model.type.String' }" linkText="{i18n>WebLinkLocalization.WebLinkLocalization_Value/WebLinkLocalization_Value}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>WebLinkLocalization.WebLinkLocalization_WebLinkId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>WebLinkLocalization_WebLinkId/sfdc_WebLinkLocalization_WebLinkId" entitySet="WebLink"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>WebLinkLocalization.WebLinkLocalization_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>WebLinkLocalization_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>WebLinkLocalization.WebLinkLocalization_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>WebLinkLocalization_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>WebLinkLocalization.WebLinkLocalization_WebLinkId}" required="false"/> 
						<rdf:ListResource path="dataModel>WebLinkLocalization_WebLinkId" entitySet="WebLink"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>