<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>EnvironmentHubInvitations.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/EnvironmentHubInvitation' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>EnvironmentHubInvitations.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EnvironmentHubInvitations.EnvironmentHubInvitation_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EnvironmentHubInvitations.EnvironmentHubInvitation_EnvironmentHubMemberDescription}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EnvironmentHubInvitations.EnvironmentHubInvitation_FailureReason}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EnvironmentHubInvitations.EnvironmentHubInvitation_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EnvironmentHubInvitations.EnvironmentHubInvitation_InviteeUserName}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EnvironmentHubInvitations.EnvironmentHubInvitation_IsDeleted}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EnvironmentHubInvitations.EnvironmentHubInvitation_IsExpired}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EnvironmentHubInvitations.EnvironmentHubInvitation_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EnvironmentHubInvitations.EnvironmentHubInvitation_Name}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EnvironmentHubInvitations.EnvironmentHubInvitation_ShouldAddRelatedOrgs}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EnvironmentHubInvitations.EnvironmentHubInvitation_ShouldEnableSso}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EnvironmentHubInvitations.EnvironmentHubInvitation_Status}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EnvironmentHubInvitations.EnvironmentHubInvitation_SystemModstamp}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="EnvironmentHubInvitation"/>
							<VBox>					
							<Text text="{dataModel>EnvironmentHubInvitation_CreatedDate/EnvironmentHubInvitation_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>EnvironmentHubInvitation_EnvironmentHubMemberDescription/EnvironmentHubInvitation_EnvironmentHubMemberDescription',type:'sap.ui.model.type.String'}" linkText="{i18n>EnvironmentHubInvitation.EnvironmentHubInvitation_EnvironmentHubMemberDescription/EnvironmentHubInvitation_EnvironmentHubMemberDescription}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>EnvironmentHubInvitation_FailureReason/EnvironmentHubInvitation_FailureReason',type:'sap.ui.model.type.String'}" linkText="{i18n>EnvironmentHubInvitation.EnvironmentHubInvitation_FailureReason/EnvironmentHubInvitation_FailureReason}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>EnvironmentHubInvitation_Id/EnvironmentHubInvitation_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>EnvironmentHubInvitation.EnvironmentHubInvitation_Id/EnvironmentHubInvitation_Id}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>EnvironmentHubInvitation_InviteeUserName/EnvironmentHubInvitation_InviteeUserName',type:'sap.ui.model.type.String'}" linkText="{i18n>EnvironmentHubInvitation.EnvironmentHubInvitation_InviteeUserName/EnvironmentHubInvitation_InviteeUserName}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>EnvironmentHubInvitation_IsDeleted/EnvironmentHubInvitation_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>EnvironmentHubInvitation_IsExpired/EnvironmentHubInvitation_IsExpired',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<Text text="{dataModel>EnvironmentHubInvitation_LastModifiedDate/EnvironmentHubInvitation_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>EnvironmentHubInvitation_Name/EnvironmentHubInvitation_Name',type:'sap.ui.model.type.String'}" linkText="{i18n>EnvironmentHubInvitation.EnvironmentHubInvitation_Name/EnvironmentHubInvitation_Name}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>EnvironmentHubInvitation_ShouldAddRelatedOrgs/EnvironmentHubInvitation_ShouldAddRelatedOrgs',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>EnvironmentHubInvitation_ShouldEnableSso/EnvironmentHubInvitation_ShouldEnableSso',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>EnvironmentHubInvitation_Status/EnvironmentHubInvitation_Status',type:'sap.ui.model.type.String'}" linkText="{i18n>EnvironmentHubInvitation.EnvironmentHubInvitation_Status/EnvironmentHubInvitation_Status}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>EnvironmentHubInvitation_SystemModstamp/EnvironmentHubInvitation_SystemModstamp}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>