<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>Entitlements.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/Entitlement' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>Entitlements.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Entitlements.Entitlement_CasesPerEntitlement}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Entitlements.Entitlement_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Entitlements.Entitlement_EndDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Entitlements.Entitlement_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Entitlements.Entitlement_IsDeleted}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Entitlements.Entitlement_IsPerIncident}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Entitlements.Entitlement_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Entitlements.Entitlement_LastReferencedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Entitlements.Entitlement_LastViewedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Entitlements.Entitlement_Name}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Entitlements.Entitlement_RemainingCases}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Entitlements.Entitlement_StartDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Entitlements.Entitlement_Status}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Entitlements.Entitlement_SystemModstamp}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Entitlements.Entitlement_Type}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="Entitlement"/>
							<VBox>					
							<Text text="{path:'dataModel>Entitlement_CasesPerEntitlement/Entitlement_CasesPerEntitlement',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>Entitlement_CreatedDate/Entitlement_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>Entitlement_EndDate/Entitlement_EndDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Entitlement_Id/Entitlement_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>Entitlement.Entitlement_Id/Entitlement_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>Entitlement_IsDeleted/Entitlement_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>Entitlement_IsPerIncident/Entitlement_IsPerIncident',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<Text text="{dataModel>Entitlement_LastModifiedDate/Entitlement_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>Entitlement_LastReferencedDate/Entitlement_LastReferencedDate}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>Entitlement_LastViewedDate/Entitlement_LastViewedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Entitlement_Name/Entitlement_Name',type:'sap.ui.model.type.String'}" linkText="{i18n>Entitlement.Entitlement_Name/Entitlement_Name}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>Entitlement_RemainingCases/Entitlement_RemainingCases',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>Entitlement_StartDate/Entitlement_StartDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Entitlement_Status/Entitlement_Status',type:'sap.ui.model.type.String'}" linkText="{i18n>Entitlement.Entitlement_Status/Entitlement_Status}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>Entitlement_SystemModstamp/Entitlement_SystemModstamp}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Entitlement_Type/Entitlement_Type',type:'sap.ui.model.type.String'}" linkText="{i18n>Entitlement.Entitlement_Type/Entitlement_Type}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>