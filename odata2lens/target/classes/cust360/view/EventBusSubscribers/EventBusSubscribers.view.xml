<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>EventBusSubscribers.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/EventBusSubscriber' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>EventBusSubscribers.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EventBusSubscribers.EventBusSubscriber_ExternalId}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EventBusSubscribers.EventBusSubscriber_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EventBusSubscribers.EventBusSubscriber_Name}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EventBusSubscribers.EventBusSubscriber_Position}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EventBusSubscribers.EventBusSubscriber_Status}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EventBusSubscribers.EventBusSubscriber_Tip}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EventBusSubscribers.EventBusSubscriber_Topic}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EventBusSubscribers.EventBusSubscriber_Type}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="EventBusSubscriber"/>
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>EventBusSubscriber_ExternalId/EventBusSubscriber_ExternalId',type:'sap.ui.model.type.String'}" linkText="{i18n>EventBusSubscriber.EventBusSubscriber_ExternalId/EventBusSubscriber_ExternalId}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>EventBusSubscriber_Id/EventBusSubscriber_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>EventBusSubscriber.EventBusSubscriber_Id/EventBusSubscriber_Id}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>EventBusSubscriber_Name/EventBusSubscriber_Name',type:'sap.ui.model.type.String'}" linkText="{i18n>EventBusSubscriber.EventBusSubscriber_Name/EventBusSubscriber_Name}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>EventBusSubscriber_Position/EventBusSubscriber_Position',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>EventBusSubscriber_Status/EventBusSubscriber_Status',type:'sap.ui.model.type.String'}" linkText="{i18n>EventBusSubscriber.EventBusSubscriber_Status/EventBusSubscriber_Status}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>EventBusSubscriber_Tip/EventBusSubscriber_Tip',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>EventBusSubscriber_Topic/EventBusSubscriber_Topic',type:'sap.ui.model.type.String'}" linkText="{i18n>EventBusSubscriber.EventBusSubscriber_Topic/EventBusSubscriber_Topic}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>EventBusSubscriber_Type/EventBusSubscriber_Type',type:'sap.ui.model.type.String'}" linkText="{i18n>EventBusSubscriber.EventBusSubscriber_Type/EventBusSubscriber_Type}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>