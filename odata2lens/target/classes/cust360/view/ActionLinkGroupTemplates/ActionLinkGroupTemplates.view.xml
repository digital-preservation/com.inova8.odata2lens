<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>ActionLinkGroupTemplates.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/ActionLinkGroupTemplate' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>ActionLinkGroupTemplates.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ActionLinkGroupTemplates.ActionLinkGroupTemplate_Category}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ActionLinkGroupTemplates.ActionLinkGroupTemplate_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ActionLinkGroupTemplates.ActionLinkGroupTemplate_DeveloperName}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ActionLinkGroupTemplates.ActionLinkGroupTemplate_ExecutionsAllowed}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ActionLinkGroupTemplates.ActionLinkGroupTemplate_HoursUntilExpiration}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ActionLinkGroupTemplates.ActionLinkGroupTemplate_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ActionLinkGroupTemplates.ActionLinkGroupTemplate_IsDeleted}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ActionLinkGroupTemplates.ActionLinkGroupTemplate_IsPublished}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ActionLinkGroupTemplates.ActionLinkGroupTemplate_Language}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ActionLinkGroupTemplates.ActionLinkGroupTemplate_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ActionLinkGroupTemplates.ActionLinkGroupTemplate_MasterLabel}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ActionLinkGroupTemplates.ActionLinkGroupTemplate_NamespacePrefix}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ActionLinkGroupTemplates.ActionLinkGroupTemplate_SystemModstamp}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="ActionLinkGroupTemplate"/>
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ActionLinkGroupTemplate_Category/ActionLinkGroupTemplate_Category',type:'sap.ui.model.type.String'}" linkText="{i18n>ActionLinkGroupTemplate.ActionLinkGroupTemplate_Category/ActionLinkGroupTemplate_Category}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>ActionLinkGroupTemplate_CreatedDate/ActionLinkGroupTemplate_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ActionLinkGroupTemplate_DeveloperName/ActionLinkGroupTemplate_DeveloperName',type:'sap.ui.model.type.String'}" linkText="{i18n>ActionLinkGroupTemplate.ActionLinkGroupTemplate_DeveloperName/ActionLinkGroupTemplate_DeveloperName}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ActionLinkGroupTemplate_ExecutionsAllowed/ActionLinkGroupTemplate_ExecutionsAllowed',type:'sap.ui.model.type.String'}" linkText="{i18n>ActionLinkGroupTemplate.ActionLinkGroupTemplate_ExecutionsAllowed/ActionLinkGroupTemplate_ExecutionsAllowed}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>ActionLinkGroupTemplate_HoursUntilExpiration/ActionLinkGroupTemplate_HoursUntilExpiration',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ActionLinkGroupTemplate_Id/ActionLinkGroupTemplate_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>ActionLinkGroupTemplate.ActionLinkGroupTemplate_Id/ActionLinkGroupTemplate_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>ActionLinkGroupTemplate_IsDeleted/ActionLinkGroupTemplate_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>ActionLinkGroupTemplate_IsPublished/ActionLinkGroupTemplate_IsPublished',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ActionLinkGroupTemplate_Language/ActionLinkGroupTemplate_Language',type:'sap.ui.model.type.String'}" linkText="{i18n>ActionLinkGroupTemplate.ActionLinkGroupTemplate_Language/ActionLinkGroupTemplate_Language}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>ActionLinkGroupTemplate_LastModifiedDate/ActionLinkGroupTemplate_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ActionLinkGroupTemplate_MasterLabel/ActionLinkGroupTemplate_MasterLabel',type:'sap.ui.model.type.String'}" linkText="{i18n>ActionLinkGroupTemplate.ActionLinkGroupTemplate_MasterLabel/ActionLinkGroupTemplate_MasterLabel}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ActionLinkGroupTemplate_NamespacePrefix/ActionLinkGroupTemplate_NamespacePrefix',type:'sap.ui.model.type.String'}" linkText="{i18n>ActionLinkGroupTemplate.ActionLinkGroupTemplate_NamespacePrefix/ActionLinkGroupTemplate_NamespacePrefix}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>ActionLinkGroupTemplate_SystemModstamp/ActionLinkGroupTemplate_SystemModstamp}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>