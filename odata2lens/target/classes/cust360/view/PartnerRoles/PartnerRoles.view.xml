<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>PartnerRoles.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/PartnerRole' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>PartnerRoles.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>PartnerRoles.PartnerRole_ApiName}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>PartnerRoles.PartnerRole_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>PartnerRoles.PartnerRole_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>PartnerRoles.PartnerRole_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>PartnerRoles.PartnerRole_MasterLabel}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>PartnerRoles.PartnerRole_ReverseRole}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>PartnerRoles.PartnerRole_SortOrder}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>PartnerRoles.PartnerRole_SystemModstamp}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="PartnerRole"/>
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>PartnerRole_ApiName/PartnerRole_ApiName',type:'sap.ui.model.type.String'}" linkText="{i18n>PartnerRole.PartnerRole_ApiName/PartnerRole_ApiName}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>PartnerRole_CreatedDate/PartnerRole_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>PartnerRole_Id/PartnerRole_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>PartnerRole.PartnerRole_Id/PartnerRole_Id}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>PartnerRole_LastModifiedDate/PartnerRole_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>PartnerRole_MasterLabel/PartnerRole_MasterLabel',type:'sap.ui.model.type.String'}" linkText="{i18n>PartnerRole.PartnerRole_MasterLabel/PartnerRole_MasterLabel}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>PartnerRole_ReverseRole/PartnerRole_ReverseRole',type:'sap.ui.model.type.String'}" linkText="{i18n>PartnerRole.PartnerRole_ReverseRole/PartnerRole_ReverseRole}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>PartnerRole_SortOrder/PartnerRole_SortOrder',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>PartnerRole_SystemModstamp/PartnerRole_SystemModstamp}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>