<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>ContentDistributions.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/ContentDistribution' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>ContentDistributions.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContentDistributions.ContentDistribution_ContentDownloadUrl}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContentDistributions.ContentDistribution_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContentDistributions.ContentDistribution_DistributionPublicUrl}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContentDistributions.ContentDistribution_ExpiryDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContentDistributions.ContentDistribution_FirstViewDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContentDistributions.ContentDistribution_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContentDistributions.ContentDistribution_IsDeleted}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContentDistributions.ContentDistribution_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContentDistributions.ContentDistribution_LastViewDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContentDistributions.ContentDistribution_Name}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContentDistributions.ContentDistribution_Password}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContentDistributions.ContentDistribution_PdfDownloadUrl}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContentDistributions.ContentDistribution_PreferencesAllowOriginalDownload}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContentDistributions.ContentDistribution_PreferencesAllowPDFDownload}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContentDistributions.ContentDistribution_PreferencesAllowViewInBrowser}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContentDistributions.ContentDistribution_PreferencesExpires}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContentDistributions.ContentDistribution_PreferencesLinkLatestVersion}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContentDistributions.ContentDistribution_PreferencesNotifyOnVisit}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContentDistributions.ContentDistribution_PreferencesNotifyRndtnComplete}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContentDistributions.ContentDistribution_PreferencesPasswordRequired}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContentDistributions.ContentDistribution_SystemModstamp}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContentDistributions.ContentDistribution_ViewCount}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="ContentDistribution"/>
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ContentDistribution_ContentDownloadUrl/ContentDistribution_ContentDownloadUrl',type:'sap.ui.model.type.String'}" linkText="{i18n>ContentDistribution.ContentDistribution_ContentDownloadUrl/ContentDistribution_ContentDownloadUrl}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>ContentDistribution_CreatedDate/ContentDistribution_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ContentDistribution_DistributionPublicUrl/ContentDistribution_DistributionPublicUrl',type:'sap.ui.model.type.String'}" linkText="{i18n>ContentDistribution.ContentDistribution_DistributionPublicUrl/ContentDistribution_DistributionPublicUrl}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>ContentDistribution_ExpiryDate/ContentDistribution_ExpiryDate}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>ContentDistribution_FirstViewDate/ContentDistribution_FirstViewDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ContentDistribution_Id/ContentDistribution_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>ContentDistribution.ContentDistribution_Id/ContentDistribution_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>ContentDistribution_IsDeleted/ContentDistribution_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<Text text="{dataModel>ContentDistribution_LastModifiedDate/ContentDistribution_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>ContentDistribution_LastViewDate/ContentDistribution_LastViewDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ContentDistribution_Name/ContentDistribution_Name',type:'sap.ui.model.type.String'}" linkText="{i18n>ContentDistribution.ContentDistribution_Name/ContentDistribution_Name}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ContentDistribution_Password/ContentDistribution_Password',type:'sap.ui.model.type.String'}" linkText="{i18n>ContentDistribution.ContentDistribution_Password/ContentDistribution_Password}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ContentDistribution_PdfDownloadUrl/ContentDistribution_PdfDownloadUrl',type:'sap.ui.model.type.String'}" linkText="{i18n>ContentDistribution.ContentDistribution_PdfDownloadUrl/ContentDistribution_PdfDownloadUrl}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>ContentDistribution_PreferencesAllowOriginalDownload/ContentDistribution_PreferencesAllowOriginalDownload',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>ContentDistribution_PreferencesAllowPDFDownload/ContentDistribution_PreferencesAllowPDFDownload',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>ContentDistribution_PreferencesAllowViewInBrowser/ContentDistribution_PreferencesAllowViewInBrowser',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>ContentDistribution_PreferencesExpires/ContentDistribution_PreferencesExpires',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>ContentDistribution_PreferencesLinkLatestVersion/ContentDistribution_PreferencesLinkLatestVersion',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>ContentDistribution_PreferencesNotifyOnVisit/ContentDistribution_PreferencesNotifyOnVisit',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>ContentDistribution_PreferencesNotifyRndtnComplete/ContentDistribution_PreferencesNotifyRndtnComplete',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>ContentDistribution_PreferencesPasswordRequired/ContentDistribution_PreferencesPasswordRequired',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<Text text="{dataModel>ContentDistribution_SystemModstamp/ContentDistribution_SystemModstamp}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>ContentDistribution_ViewCount/ContentDistribution_ViewCount',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>