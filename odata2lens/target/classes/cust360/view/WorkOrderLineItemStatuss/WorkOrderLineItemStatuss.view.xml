					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>WorkOrderLineItemStatuss.WorkOrderLineItemStatus_SystemModstamp}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="WorkOrderLineItemStatus"/>
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>WorkOrderLineItemStatus_ApiName/WorkOrderLineItemStatus_ApiName',type:'sap.ui.model.type.String'}" linkText="{i18n>WorkOrderLineItemStatus.WorkOrderLineItemStatus_ApiName/WorkOrderLineItemStatus_ApiName}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>WorkOrderLineItemStatus_CreatedDate/WorkOrderLineItemStatus_CreatedDate}"/>
