				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="ProcessInstanceWorkitem"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ProcessInstanceWorkitem.ProcessInstanceWorkitem_ActorId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ProcessInstanceWorkitem_ActorId/sfdc_ProcessInstanceWorkitem_ActorId" entitySet="Group"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ProcessInstanceWorkitem.ProcessInstanceWorkitem_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ProcessInstanceWorkitem_CreatedById/sfdc_ProcessInstanceWorkitem_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ProcessInstanceWorkitem.ProcessInstanceWorkitem_CreatedDate}" required="false"/> 
						<Text text=""/>
