					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ProcessInstanceWorkitem.ProcessInstanceWorkitem_ElapsedTimeInDays}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>ProcessInstanceWorkitem_ElapsedTimeInDays/ProcessInstanceWorkitem_ElapsedTimeInDays',type:'sap.ui.model.type.Float', formatOptions:{'style':'short','maxFractionDigits':2} }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ProcessInstanceWorkitem.ProcessInstanceWorkitem_ElapsedTimeInHours}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>ProcessInstanceWorkitem_ElapsedTimeInHours/ProcessInstanceWorkitem_ElapsedTimeInHours',type:'sap.ui.model.type.Float', formatOptions:{'style':'short','maxFractionDigits':2} }"/>
