						<Label text="{i18n>ProcessInstanceWorkitem.ProcessInstanceWorkitem_ProcessInstanceId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ProcessInstanceWorkitem_ProcessInstanceId/sfdc_ProcessInstanceWorkitem_ProcessInstanceId" entitySet="ProcessInstance"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ProcessInstanceWorkitem.ProcessInstanceWorkitem_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>ProcessInstanceWorkitem_SystemModstamp/ProcessInstanceWorkitem_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ProcessInstanceWorkitem.ProcessInstanceWorkitem_ActorId}" required="false"/> 
						<rdf:ListResource path="dataModel>ProcessInstanceWorkitem_ActorId" entitySet="Group"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ProcessInstanceWorkitem.ProcessInstanceWorkitem_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>ProcessInstanceWorkitem_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ProcessInstanceWorkitem.ProcessInstanceWorkitem_OriginalActorId}" required="false"/> 
						<rdf:ListResource path="dataModel>ProcessInstanceWorkitem_OriginalActorId" entitySet="Group"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ProcessInstanceWorkitem.ProcessInstanceWorkitem_ProcessInstanceId}" required="false"/> 
						<rdf:ListResource path="dataModel>ProcessInstanceWorkitem_ProcessInstanceId" entitySet="ProcessInstance"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>