				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ProcessInstanceWorkitem.ProcessInstanceWorkitem_ElapsedTimeInMinutes}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>ProcessInstanceWorkitem_ElapsedTimeInMinutes/ProcessInstanceWorkitem_ElapsedTimeInMinutes',type:'sap.ui.model.type.Float', formatOptions:{'style':'short','maxFractionDigits':2} }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ProcessInstanceWorkitem.ProcessInstanceWorkitem_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ProcessInstanceWorkitem_Id/ProcessInstanceWorkitem_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>ProcessInstanceWorkitem.ProcessInstanceWorkitem_Id/ProcessInstanceWorkitem_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ProcessInstanceWorkitem.ProcessInstanceWorkitem_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>ProcessInstanceWorkitem_IsDeleted/ProcessInstanceWorkitem_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ProcessInstanceWorkitem.ProcessInstanceWorkitem_OriginalActorId}" required="false"/> 
