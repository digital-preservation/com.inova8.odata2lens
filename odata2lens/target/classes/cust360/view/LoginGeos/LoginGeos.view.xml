<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>LoginGeos.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/LoginGeo' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>LoginGeos.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>LoginGeos.LoginGeo_City}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>LoginGeos.LoginGeo_Country}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>LoginGeos.LoginGeo_CountryIso}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>LoginGeos.LoginGeo_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>LoginGeos.LoginGeo_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>LoginGeos.LoginGeo_IsDeleted}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>LoginGeos.LoginGeo_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>LoginGeos.LoginGeo_Latitude}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>LoginGeos.LoginGeo_LoginTime}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>LoginGeos.LoginGeo_Longitude}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>LoginGeos.LoginGeo_PostalCode}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>LoginGeos.LoginGeo_Subdivision}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>LoginGeos.LoginGeo_SystemModstamp}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="LoginGeo"/>
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>LoginGeo_City/LoginGeo_City',type:'sap.ui.model.type.String'}" linkText="{i18n>LoginGeo.LoginGeo_City/LoginGeo_City}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>LoginGeo_Country/LoginGeo_Country',type:'sap.ui.model.type.String'}" linkText="{i18n>LoginGeo.LoginGeo_Country/LoginGeo_Country}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>LoginGeo_CountryIso/LoginGeo_CountryIso',type:'sap.ui.model.type.String'}" linkText="{i18n>LoginGeo.LoginGeo_CountryIso/LoginGeo_CountryIso}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>LoginGeo_CreatedDate/LoginGeo_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>LoginGeo_Id/LoginGeo_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>LoginGeo.LoginGeo_Id/LoginGeo_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>LoginGeo_IsDeleted/LoginGeo_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<Text text="{dataModel>LoginGeo_LastModifiedDate/LoginGeo_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>LoginGeo_Latitude/LoginGeo_Latitude',type:'sap.ui.model.type.Float', formatOptions:{'style':'short','maxFractionDigits':2} }"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>LoginGeo_LoginTime/LoginGeo_LoginTime}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>LoginGeo_Longitude/LoginGeo_Longitude',type:'sap.ui.model.type.Float', formatOptions:{'style':'short','maxFractionDigits':2} }"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>LoginGeo_PostalCode/LoginGeo_PostalCode',type:'sap.ui.model.type.String'}" linkText="{i18n>LoginGeo.LoginGeo_PostalCode/LoginGeo_PostalCode}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>LoginGeo_Subdivision/LoginGeo_Subdivision',type:'sap.ui.model.type.String'}" linkText="{i18n>LoginGeo.LoginGeo_Subdivision/LoginGeo_Subdivision}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>LoginGeo_SystemModstamp/LoginGeo_SystemModstamp}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>