<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>Publisher.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>Publisher.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="Publisher"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="Publisher"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Publisher.EntityDefinition_PublisherId_Inverse}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>EntityDefinition_PublisherId_Inverse/sfdc_EntityDefinition_PublisherId_Inverse" entitySet="EntityDefinition"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Publisher.FieldDefinition_PublisherId_Inverse}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>FieldDefinition_PublisherId_Inverse/sfdc_FieldDefinition_PublisherId_Inverse" entitySet="FieldDefinition"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Publisher.Publisher_DurableId}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>Publisher_DurableId/Publisher_DurableId',type:'sap.ui.model.type.String' }" linkText="{i18n>Publisher.Publisher_DurableId/Publisher_DurableId}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Publisher.Publisher_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>Publisher_Id/Publisher_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>Publisher.Publisher_Id/Publisher_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Publisher.Publisher_IsSalesforce}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>Publisher_IsSalesforce/Publisher_IsSalesforce',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Publisher.Publisher_MajorVersion}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>Publisher_MajorVersion/Publisher_MajorVersion',type:'sap.ui.model.type.Integer' }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Publisher.Publisher_MinorVersion}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>Publisher_MinorVersion/Publisher_MinorVersion',type:'sap.ui.model.type.Integer' }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Publisher.Publisher_Name}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>Publisher_Name/Publisher_Name',type:'sap.ui.model.type.String' }" linkText="{i18n>Publisher.Publisher_Name/Publisher_Name}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Publisher.Publisher_NamespacePrefix}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>Publisher_NamespacePrefix/Publisher_NamespacePrefix',type:'sap.ui.model.type.String' }" linkText="{i18n>Publisher.Publisher_NamespacePrefix/Publisher_NamespacePrefix}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Publisher.EntityDefinition_PublisherId_Inverse}" required="false"/> 
						<rdf:ListResource path="dataModel>EntityDefinition_PublisherId_Inverse" entitySet="EntityDefinition"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Publisher.FieldDefinition_PublisherId_Inverse}" required="false"/> 
						<rdf:ListResource path="dataModel>FieldDefinition_PublisherId_Inverse" entitySet="FieldDefinition"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>