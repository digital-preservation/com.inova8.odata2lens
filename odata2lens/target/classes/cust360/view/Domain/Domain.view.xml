<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>Domain.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>Domain.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="Domain"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="Domain"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Domain.DomainSite_DomainId_Inverse}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>DomainSite_DomainId_Inverse/sfdc_DomainSite_DomainId_Inverse" entitySet="DomainSite"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Domain.Domain_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>Domain_CreatedById/sfdc_Domain_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Domain.Domain_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>Domain_CreatedDate/Domain_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Domain.Domain_Domain}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>Domain_Domain/Domain_Domain',type:'sap.ui.model.type.String' }" linkText="{i18n>Domain.Domain_Domain/Domain_Domain}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Domain.Domain_DomainType}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>Domain_DomainType/Domain_DomainType',type:'sap.ui.model.type.String' }" linkText="{i18n>Domain.Domain_DomainType/Domain_DomainType}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Domain.Domain_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>Domain_Id/Domain_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>Domain.Domain_Id/Domain_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Domain.Domain_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>Domain_LastModifiedById/sfdc_Domain_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Domain.Domain_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>Domain_LastModifiedDate/Domain_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Domain.Domain_OptionsExternalHttps}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>Domain_OptionsExternalHttps/Domain_OptionsExternalHttps',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Domain.Domain_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>Domain_SystemModstamp/Domain_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Domain.DomainSite_DomainId_Inverse}" required="false"/> 
						<rdf:ListResource path="dataModel>DomainSite_DomainId_Inverse" entitySet="DomainSite"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Domain.Domain_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>Domain_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Domain.Domain_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>Domain_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>