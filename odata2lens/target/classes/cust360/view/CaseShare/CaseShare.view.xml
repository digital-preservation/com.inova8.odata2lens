<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>CaseShare.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>CaseShare.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="CaseShare"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="CaseShare"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CaseShare.CaseShare_CaseAccessLevel}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>CaseShare_CaseAccessLevel/CaseShare_CaseAccessLevel',type:'sap.ui.model.type.String' }" linkText="{i18n>CaseShare.CaseShare_CaseAccessLevel/CaseShare_CaseAccessLevel}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CaseShare.CaseShare_CaseId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>CaseShare_CaseId/sfdc_CaseShare_CaseId" entitySet="Case"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CaseShare.CaseShare_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>CaseShare_Id/CaseShare_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>CaseShare.CaseShare_Id/CaseShare_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CaseShare.CaseShare_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>CaseShare_IsDeleted/CaseShare_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CaseShare.CaseShare_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>CaseShare_LastModifiedById/sfdc_CaseShare_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CaseShare.CaseShare_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>CaseShare_LastModifiedDate/CaseShare_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CaseShare.CaseShare_RowCause}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>CaseShare_RowCause/CaseShare_RowCause',type:'sap.ui.model.type.String' }" linkText="{i18n>CaseShare.CaseShare_RowCause/CaseShare_RowCause}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CaseShare.CaseShare_UserOrGroupId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>CaseShare_UserOrGroupId/sfdc_CaseShare_UserOrGroupId" entitySet="Group"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CaseShare.CaseShare_CaseId}" required="false"/> 
						<rdf:ListResource path="dataModel>CaseShare_CaseId" entitySet="Case"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CaseShare.CaseShare_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>CaseShare_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CaseShare.CaseShare_UserOrGroupId}" required="false"/> 
						<rdf:ListResource path="dataModel>CaseShare_UserOrGroupId" entitySet="Group"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>