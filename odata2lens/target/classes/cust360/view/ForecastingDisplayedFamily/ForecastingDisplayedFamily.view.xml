<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>ForecastingDisplayedFamily.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>ForecastingDisplayedFamily.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="ForecastingDisplayedFamily"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="ForecastingDisplayedFamily"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ForecastingDisplayedFamily.ForecastingDisplayedFamily_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ForecastingDisplayedFamily_CreatedById/sfdc_ForecastingDisplayedFamily_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ForecastingDisplayedFamily.ForecastingDisplayedFamily_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>ForecastingDisplayedFamily_CreatedDate/ForecastingDisplayedFamily_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ForecastingDisplayedFamily.ForecastingDisplayedFamily_DisplayPosition}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>ForecastingDisplayedFamily_DisplayPosition/ForecastingDisplayedFamily_DisplayPosition',type:'sap.ui.model.type.Integer' }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ForecastingDisplayedFamily.ForecastingDisplayedFamily_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ForecastingDisplayedFamily_Id/ForecastingDisplayedFamily_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>ForecastingDisplayedFamily.ForecastingDisplayedFamily_Id/ForecastingDisplayedFamily_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ForecastingDisplayedFamily.ForecastingDisplayedFamily_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>ForecastingDisplayedFamily_IsDeleted/ForecastingDisplayedFamily_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ForecastingDisplayedFamily.ForecastingDisplayedFamily_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ForecastingDisplayedFamily_LastModifiedById/sfdc_ForecastingDisplayedFamily_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ForecastingDisplayedFamily.ForecastingDisplayedFamily_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>ForecastingDisplayedFamily_LastModifiedDate/ForecastingDisplayedFamily_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ForecastingDisplayedFamily.ForecastingDisplayedFamily_ProductFamily}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ForecastingDisplayedFamily_ProductFamily/ForecastingDisplayedFamily_ProductFamily',type:'sap.ui.model.type.String' }" linkText="{i18n>ForecastingDisplayedFamily.ForecastingDisplayedFamily_ProductFamily/ForecastingDisplayedFamily_ProductFamily}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ForecastingDisplayedFamily.ForecastingDisplayedFamily_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>ForecastingDisplayedFamily_SystemModstamp/ForecastingDisplayedFamily_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ForecastingDisplayedFamily.ForecastingDisplayedFamily_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>ForecastingDisplayedFamily_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ForecastingDisplayedFamily.ForecastingDisplayedFamily_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>ForecastingDisplayedFamily_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>