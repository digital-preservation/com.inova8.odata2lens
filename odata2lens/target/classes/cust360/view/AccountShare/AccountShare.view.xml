<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>AccountShare.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>AccountShare.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="AccountShare"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="AccountShare"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AccountShare.AccountShare_AccountAccessLevel}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>AccountShare_AccountAccessLevel/AccountShare_AccountAccessLevel',type:'sap.ui.model.type.String' }" linkText="{i18n>AccountShare.AccountShare_AccountAccessLevel/AccountShare_AccountAccessLevel}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AccountShare.AccountShare_AccountId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>AccountShare_AccountId/sfdc_AccountShare_AccountId" entitySet="Account"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AccountShare.AccountShare_CaseAccessLevel}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>AccountShare_CaseAccessLevel/AccountShare_CaseAccessLevel',type:'sap.ui.model.type.String' }" linkText="{i18n>AccountShare.AccountShare_CaseAccessLevel/AccountShare_CaseAccessLevel}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AccountShare.AccountShare_ContactAccessLevel}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>AccountShare_ContactAccessLevel/AccountShare_ContactAccessLevel',type:'sap.ui.model.type.String' }" linkText="{i18n>AccountShare.AccountShare_ContactAccessLevel/AccountShare_ContactAccessLevel}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AccountShare.AccountShare_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>AccountShare_Id/AccountShare_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>AccountShare.AccountShare_Id/AccountShare_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AccountShare.AccountShare_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>AccountShare_IsDeleted/AccountShare_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AccountShare.AccountShare_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>AccountShare_LastModifiedById/sfdc_AccountShare_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AccountShare.AccountShare_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>AccountShare_LastModifiedDate/AccountShare_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AccountShare.AccountShare_OpportunityAccessLevel}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>AccountShare_OpportunityAccessLevel/AccountShare_OpportunityAccessLevel',type:'sap.ui.model.type.String' }" linkText="{i18n>AccountShare.AccountShare_OpportunityAccessLevel/AccountShare_OpportunityAccessLevel}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AccountShare.AccountShare_RowCause}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>AccountShare_RowCause/AccountShare_RowCause',type:'sap.ui.model.type.String' }" linkText="{i18n>AccountShare.AccountShare_RowCause/AccountShare_RowCause}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AccountShare.AccountShare_UserOrGroupId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>AccountShare_UserOrGroupId/sfdc_AccountShare_UserOrGroupId" entitySet="Group"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AccountShare.AccountShare_AccountId}" required="false"/> 
						<rdf:ListResource path="dataModel>AccountShare_AccountId" entitySet="Account"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AccountShare.AccountShare_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>AccountShare_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AccountShare.AccountShare_UserOrGroupId}" required="false"/> 
						<rdf:ListResource path="dataModel>AccountShare_UserOrGroupId" entitySet="Group"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>