<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>EmbeddedServiceDetail.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>EmbeddedServiceDetail.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="EmbeddedServiceDetail"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="EmbeddedServiceDetail"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EmbeddedServiceDetail.EmbeddedServiceDetail_ContrastPrimaryColor}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>EmbeddedServiceDetail_ContrastPrimaryColor/EmbeddedServiceDetail_ContrastPrimaryColor',type:'sap.ui.model.type.String' }" linkText="{i18n>EmbeddedServiceDetail.EmbeddedServiceDetail_ContrastPrimaryColor/EmbeddedServiceDetail_ContrastPrimaryColor}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EmbeddedServiceDetail.EmbeddedServiceDetail_DurableId}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>EmbeddedServiceDetail_DurableId/EmbeddedServiceDetail_DurableId',type:'sap.ui.model.type.String' }" linkText="{i18n>EmbeddedServiceDetail.EmbeddedServiceDetail_DurableId/EmbeddedServiceDetail_DurableId}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EmbeddedServiceDetail.EmbeddedServiceDetail_FlowDeveloperName}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>EmbeddedServiceDetail_FlowDeveloperName/EmbeddedServiceDetail_FlowDeveloperName',type:'sap.ui.model.type.String' }" linkText="{i18n>EmbeddedServiceDetail.EmbeddedServiceDetail_FlowDeveloperName/EmbeddedServiceDetail_FlowDeveloperName}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EmbeddedServiceDetail.EmbeddedServiceDetail_Font}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>EmbeddedServiceDetail_Font/EmbeddedServiceDetail_Font',type:'sap.ui.model.type.String' }" linkText="{i18n>EmbeddedServiceDetail.EmbeddedServiceDetail_Font/EmbeddedServiceDetail_Font}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EmbeddedServiceDetail.EmbeddedServiceDetail_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>EmbeddedServiceDetail_Id/EmbeddedServiceDetail_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>EmbeddedServiceDetail.EmbeddedServiceDetail_Id/EmbeddedServiceDetail_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EmbeddedServiceDetail.EmbeddedServiceDetail_IsPrechatEnabled}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>EmbeddedServiceDetail_IsPrechatEnabled/EmbeddedServiceDetail_IsPrechatEnabled',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EmbeddedServiceDetail.EmbeddedServiceDetail_NavBarColor}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>EmbeddedServiceDetail_NavBarColor/EmbeddedServiceDetail_NavBarColor',type:'sap.ui.model.type.String' }" linkText="{i18n>EmbeddedServiceDetail.EmbeddedServiceDetail_NavBarColor/EmbeddedServiceDetail_NavBarColor}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EmbeddedServiceDetail.EmbeddedServiceDetail_PrimaryColor}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>EmbeddedServiceDetail_PrimaryColor/EmbeddedServiceDetail_PrimaryColor',type:'sap.ui.model.type.String' }" linkText="{i18n>EmbeddedServiceDetail.EmbeddedServiceDetail_PrimaryColor/EmbeddedServiceDetail_PrimaryColor}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EmbeddedServiceDetail.EmbeddedServiceDetail_SecondaryColor}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>EmbeddedServiceDetail_SecondaryColor/EmbeddedServiceDetail_SecondaryColor',type:'sap.ui.model.type.String' }" linkText="{i18n>EmbeddedServiceDetail.EmbeddedServiceDetail_SecondaryColor/EmbeddedServiceDetail_SecondaryColor}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EmbeddedServiceDetail.EmbeddedServiceDetail_Site}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>EmbeddedServiceDetail_Site/EmbeddedServiceDetail_Site',type:'sap.ui.model.type.String' }" linkText="{i18n>EmbeddedServiceDetail.EmbeddedServiceDetail_Site/EmbeddedServiceDetail_Site}"/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>