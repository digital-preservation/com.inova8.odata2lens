<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>EmailStatus.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>EmailStatus.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="EmailStatus"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="EmailStatus"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EmailStatus.EmailStatus_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>EmailStatus_CreatedById/sfdc_EmailStatus_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EmailStatus.EmailStatus_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>EmailStatus_CreatedDate/EmailStatus_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EmailStatus.EmailStatus_EmailTemplateName}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>EmailStatus_EmailTemplateName/EmailStatus_EmailTemplateName',type:'sap.ui.model.type.String' }" linkText="{i18n>EmailStatus.EmailStatus_EmailTemplateName/EmailStatus_EmailTemplateName}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EmailStatus.EmailStatus_FirstOpenDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>EmailStatus_FirstOpenDate/EmailStatus_FirstOpenDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EmailStatus.EmailStatus_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>EmailStatus_Id/EmailStatus_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>EmailStatus.EmailStatus_Id/EmailStatus_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EmailStatus.EmailStatus_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>EmailStatus_LastModifiedById/sfdc_EmailStatus_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EmailStatus.EmailStatus_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>EmailStatus_LastModifiedDate/EmailStatus_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EmailStatus.EmailStatus_LastOpenDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>EmailStatus_LastOpenDate/EmailStatus_LastOpenDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EmailStatus.EmailStatus_TaskId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>EmailStatus_TaskId/sfdc_EmailStatus_TaskId" entitySet="Task"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EmailStatus.EmailStatus_TimesOpened}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>EmailStatus_TimesOpened/EmailStatus_TimesOpened',type:'sap.ui.model.type.Integer' }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EmailStatus.EmailStatus_WhoId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>EmailStatus_WhoId/sfdc_EmailStatus_WhoId" entitySet="Contact"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EmailStatus.EmailStatus_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>EmailStatus_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EmailStatus.EmailStatus_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>EmailStatus_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EmailStatus.EmailStatus_TaskId}" required="false"/> 
						<rdf:ListResource path="dataModel>EmailStatus_TaskId" entitySet="Task"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EmailStatus.EmailStatus_WhoId}" required="false"/> 
						<rdf:ListResource path="dataModel>EmailStatus_WhoId" entitySet="Contact"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>