<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>ForecastingTypes.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/ForecastingType' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>ForecastingTypes.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ForecastingTypes.ForecastingType_CanDisplayQuotas}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ForecastingTypes.ForecastingType_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ForecastingTypes.ForecastingType_DeveloperName}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ForecastingTypes.ForecastingType_HasProductFamily}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ForecastingTypes.ForecastingType_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ForecastingTypes.ForecastingType_IsActive}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ForecastingTypes.ForecastingType_IsAmount}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ForecastingTypes.ForecastingType_IsDeleted}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ForecastingTypes.ForecastingType_IsQuantity}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ForecastingTypes.ForecastingType_Language}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ForecastingTypes.ForecastingType_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ForecastingTypes.ForecastingType_MasterLabel}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ForecastingTypes.ForecastingType_RoleType}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ForecastingTypes.ForecastingType_SystemModstamp}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="ForecastingType"/>
							<VBox>					
								<Text text="{path:'dataModel>ForecastingType_CanDisplayQuotas/ForecastingType_CanDisplayQuotas',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<Text text="{dataModel>ForecastingType_CreatedDate/ForecastingType_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ForecastingType_DeveloperName/ForecastingType_DeveloperName',type:'sap.ui.model.type.String'}" linkText="{i18n>ForecastingType.ForecastingType_DeveloperName/ForecastingType_DeveloperName}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>ForecastingType_HasProductFamily/ForecastingType_HasProductFamily',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ForecastingType_Id/ForecastingType_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>ForecastingType.ForecastingType_Id/ForecastingType_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>ForecastingType_IsActive/ForecastingType_IsActive',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>ForecastingType_IsAmount/ForecastingType_IsAmount',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>ForecastingType_IsDeleted/ForecastingType_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>ForecastingType_IsQuantity/ForecastingType_IsQuantity',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ForecastingType_Language/ForecastingType_Language',type:'sap.ui.model.type.String'}" linkText="{i18n>ForecastingType.ForecastingType_Language/ForecastingType_Language}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>ForecastingType_LastModifiedDate/ForecastingType_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ForecastingType_MasterLabel/ForecastingType_MasterLabel',type:'sap.ui.model.type.String'}" linkText="{i18n>ForecastingType.ForecastingType_MasterLabel/ForecastingType_MasterLabel}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ForecastingType_RoleType/ForecastingType_RoleType',type:'sap.ui.model.type.String'}" linkText="{i18n>ForecastingType.ForecastingType_RoleType/ForecastingType_RoleType}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>ForecastingType_SystemModstamp/ForecastingType_SystemModstamp}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>