<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>DatacloudAddresss.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/DatacloudAddress' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>DatacloudAddresss.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>DatacloudAddresss.DatacloudAddress_AddressLine1}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>DatacloudAddresss.DatacloudAddress_AddressLine2}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>DatacloudAddresss.DatacloudAddress_City}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>DatacloudAddresss.DatacloudAddress_Country}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>DatacloudAddresss.DatacloudAddress_ExternalId}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>DatacloudAddresss.DatacloudAddress_GeoAccuracyCode}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>DatacloudAddresss.DatacloudAddress_GeoAccuracyNum}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>DatacloudAddresss.DatacloudAddress_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>DatacloudAddresss.DatacloudAddress_Latitude}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>DatacloudAddresss.DatacloudAddress_Longitude}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>DatacloudAddresss.DatacloudAddress_PostalCode}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>DatacloudAddresss.DatacloudAddress_State}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="DatacloudAddress"/>
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>DatacloudAddress_AddressLine1/DatacloudAddress_AddressLine1',type:'sap.ui.model.type.String'}" linkText="{i18n>DatacloudAddress.DatacloudAddress_AddressLine1/DatacloudAddress_AddressLine1}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>DatacloudAddress_AddressLine2/DatacloudAddress_AddressLine2',type:'sap.ui.model.type.String'}" linkText="{i18n>DatacloudAddress.DatacloudAddress_AddressLine2/DatacloudAddress_AddressLine2}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>DatacloudAddress_City/DatacloudAddress_City',type:'sap.ui.model.type.String'}" linkText="{i18n>DatacloudAddress.DatacloudAddress_City/DatacloudAddress_City}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>DatacloudAddress_Country/DatacloudAddress_Country',type:'sap.ui.model.type.String'}" linkText="{i18n>DatacloudAddress.DatacloudAddress_Country/DatacloudAddress_Country}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>DatacloudAddress_ExternalId/DatacloudAddress_ExternalId',type:'sap.ui.model.type.String'}" linkText="{i18n>DatacloudAddress.DatacloudAddress_ExternalId/DatacloudAddress_ExternalId}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>DatacloudAddress_GeoAccuracyCode/DatacloudAddress_GeoAccuracyCode',type:'sap.ui.model.type.String'}" linkText="{i18n>DatacloudAddress.DatacloudAddress_GeoAccuracyCode/DatacloudAddress_GeoAccuracyCode}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>DatacloudAddress_GeoAccuracyNum/DatacloudAddress_GeoAccuracyNum',type:'sap.ui.model.type.String'}" linkText="{i18n>DatacloudAddress.DatacloudAddress_GeoAccuracyNum/DatacloudAddress_GeoAccuracyNum}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>DatacloudAddress_Id/DatacloudAddress_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>DatacloudAddress.DatacloudAddress_Id/DatacloudAddress_Id}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>DatacloudAddress_Latitude/DatacloudAddress_Latitude',type:'sap.ui.model.type.String'}" linkText="{i18n>DatacloudAddress.DatacloudAddress_Latitude/DatacloudAddress_Latitude}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>DatacloudAddress_Longitude/DatacloudAddress_Longitude',type:'sap.ui.model.type.String'}" linkText="{i18n>DatacloudAddress.DatacloudAddress_Longitude/DatacloudAddress_Longitude}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>DatacloudAddress_PostalCode/DatacloudAddress_PostalCode',type:'sap.ui.model.type.String'}" linkText="{i18n>DatacloudAddress.DatacloudAddress_PostalCode/DatacloudAddress_PostalCode}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>DatacloudAddress_State/DatacloudAddress_State',type:'sap.ui.model.type.String'}" linkText="{i18n>DatacloudAddress.DatacloudAddress_State/DatacloudAddress_State}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>