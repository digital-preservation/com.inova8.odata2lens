<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>CollaborationInvitations.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/CollaborationInvitation' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>CollaborationInvitations.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CollaborationInvitations.CollaborationInvitation_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CollaborationInvitations.CollaborationInvitation_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CollaborationInvitations.CollaborationInvitation_InvitedUserEmail}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CollaborationInvitations.CollaborationInvitation_InvitedUserEmailNormalized}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CollaborationInvitations.CollaborationInvitation_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CollaborationInvitations.CollaborationInvitation_OptionalMessage}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CollaborationInvitations.CollaborationInvitation_Status}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CollaborationInvitations.CollaborationInvitation_SystemModstamp}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="CollaborationInvitation"/>
							<VBox>					
							<Text text="{dataModel>CollaborationInvitation_CreatedDate/CollaborationInvitation_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>CollaborationInvitation_Id/CollaborationInvitation_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>CollaborationInvitation.CollaborationInvitation_Id/CollaborationInvitation_Id}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>CollaborationInvitation_InvitedUserEmail/CollaborationInvitation_InvitedUserEmail',type:'sap.ui.model.type.String'}" linkText="{i18n>CollaborationInvitation.CollaborationInvitation_InvitedUserEmail/CollaborationInvitation_InvitedUserEmail}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>CollaborationInvitation_InvitedUserEmailNormalized/CollaborationInvitation_InvitedUserEmailNormalized',type:'sap.ui.model.type.String'}" linkText="{i18n>CollaborationInvitation.CollaborationInvitation_InvitedUserEmailNormalized/CollaborationInvitation_InvitedUserEmailNormalized}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>CollaborationInvitation_LastModifiedDate/CollaborationInvitation_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>CollaborationInvitation_OptionalMessage/CollaborationInvitation_OptionalMessage',type:'sap.ui.model.type.String'}" linkText="{i18n>CollaborationInvitation.CollaborationInvitation_OptionalMessage/CollaborationInvitation_OptionalMessage}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>CollaborationInvitation_Status/CollaborationInvitation_Status',type:'sap.ui.model.type.String'}" linkText="{i18n>CollaborationInvitation.CollaborationInvitation_Status/CollaborationInvitation_Status}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>CollaborationInvitation_SystemModstamp/CollaborationInvitation_SystemModstamp}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>