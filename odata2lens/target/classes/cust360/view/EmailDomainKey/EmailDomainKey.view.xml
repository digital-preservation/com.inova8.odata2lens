<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>EmailDomainKey.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>EmailDomainKey.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="EmailDomainKey"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="EmailDomainKey"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EmailDomainKey.EmailDomainKey_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>EmailDomainKey_CreatedById/sfdc_EmailDomainKey_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EmailDomainKey.EmailDomainKey_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>EmailDomainKey_CreatedDate/EmailDomainKey_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EmailDomainKey.EmailDomainKey_Domain}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>EmailDomainKey_Domain/EmailDomainKey_Domain',type:'sap.ui.model.type.String' }" linkText="{i18n>EmailDomainKey.EmailDomainKey_Domain/EmailDomainKey_Domain}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EmailDomainKey.EmailDomainKey_DomainMatch}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>EmailDomainKey_DomainMatch/EmailDomainKey_DomainMatch',type:'sap.ui.model.type.String' }" linkText="{i18n>EmailDomainKey.EmailDomainKey_DomainMatch/EmailDomainKey_DomainMatch}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EmailDomainKey.EmailDomainKey_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>EmailDomainKey_Id/EmailDomainKey_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>EmailDomainKey.EmailDomainKey_Id/EmailDomainKey_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EmailDomainKey.EmailDomainKey_IsActive}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>EmailDomainKey_IsActive/EmailDomainKey_IsActive',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EmailDomainKey.EmailDomainKey_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>EmailDomainKey_IsDeleted/EmailDomainKey_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EmailDomainKey.EmailDomainKey_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>EmailDomainKey_LastModifiedById/sfdc_EmailDomainKey_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EmailDomainKey.EmailDomainKey_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>EmailDomainKey_LastModifiedDate/EmailDomainKey_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EmailDomainKey.EmailDomainKey_PrivateKey}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>EmailDomainKey_PrivateKey/EmailDomainKey_PrivateKey',type:'sap.ui.model.type.String' }" linkText="{i18n>EmailDomainKey.EmailDomainKey_PrivateKey/EmailDomainKey_PrivateKey}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EmailDomainKey.EmailDomainKey_PublicKey}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>EmailDomainKey_PublicKey/EmailDomainKey_PublicKey',type:'sap.ui.model.type.String' }" linkText="{i18n>EmailDomainKey.EmailDomainKey_PublicKey/EmailDomainKey_PublicKey}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EmailDomainKey.EmailDomainKey_Selector}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>EmailDomainKey_Selector/EmailDomainKey_Selector',type:'sap.ui.model.type.String' }" linkText="{i18n>EmailDomainKey.EmailDomainKey_Selector/EmailDomainKey_Selector}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EmailDomainKey.EmailDomainKey_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>EmailDomainKey_SystemModstamp/EmailDomainKey_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EmailDomainKey.EmailDomainKey_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>EmailDomainKey_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EmailDomainKey.EmailDomainKey_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>EmailDomainKey_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>