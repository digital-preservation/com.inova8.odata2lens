<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>ApexLogs.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/ApexLog' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>ApexLogs.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexLogs.ApexLog_Application}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexLogs.ApexLog_DurationMilliseconds}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexLogs.ApexLog_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexLogs.ApexLog_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexLogs.ApexLog_Location}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexLogs.ApexLog_LogLength}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexLogs.ApexLog_Operation}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexLogs.ApexLog_Request}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexLogs.ApexLog_StartTime}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexLogs.ApexLog_Status}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexLogs.ApexLog_SystemModstamp}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="ApexLog"/>
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ApexLog_Application/ApexLog_Application',type:'sap.ui.model.type.String'}" linkText="{i18n>ApexLog.ApexLog_Application/ApexLog_Application}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>ApexLog_DurationMilliseconds/ApexLog_DurationMilliseconds',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ApexLog_Id/ApexLog_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>ApexLog.ApexLog_Id/ApexLog_Id}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>ApexLog_LastModifiedDate/ApexLog_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ApexLog_Location/ApexLog_Location',type:'sap.ui.model.type.String'}" linkText="{i18n>ApexLog.ApexLog_Location/ApexLog_Location}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>ApexLog_LogLength/ApexLog_LogLength',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ApexLog_Operation/ApexLog_Operation',type:'sap.ui.model.type.String'}" linkText="{i18n>ApexLog.ApexLog_Operation/ApexLog_Operation}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ApexLog_Request/ApexLog_Request',type:'sap.ui.model.type.String'}" linkText="{i18n>ApexLog.ApexLog_Request/ApexLog_Request}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>ApexLog_StartTime/ApexLog_StartTime}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ApexLog_Status/ApexLog_Status',type:'sap.ui.model.type.String'}" linkText="{i18n>ApexLog.ApexLog_Status/ApexLog_Status}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>ApexLog_SystemModstamp/ApexLog_SystemModstamp}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>