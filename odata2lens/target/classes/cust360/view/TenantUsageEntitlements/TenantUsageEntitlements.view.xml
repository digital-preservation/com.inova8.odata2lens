							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>TenantUsageEntitlement_IsDeleted/TenantUsageEntitlement_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>TenantUsageEntitlement_IsPersistentResource/TenantUsageEntitlement_IsPersistentResource',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<Text text="{dataModel>TenantUsageEntitlement_LastModifiedDate/TenantUsageEntitlement_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>TenantUsageEntitlement_MasterLabel/TenantUsageEntitlement_MasterLabel',type:'sap.ui.model.type.String'}" linkText="{i18n>TenantUsageEntitlement.TenantUsageEntitlement_MasterLabel/TenantUsageEntitlement_MasterLabel}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>TenantUsageEntitlement_OverageGrace/TenantUsageEntitlement_OverageGrace',type:'sap.ui.model.type.Float', formatOptions:{'style':'short','maxFractionDigits':2} }"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>TenantUsageEntitlement_ResourceGroupKey/TenantUsageEntitlement_ResourceGroupKey',type:'sap.ui.model.type.String'}" linkText="{i18n>TenantUsageEntitlement.TenantUsageEntitlement_ResourceGroupKey/TenantUsageEntitlement_ResourceGroupKey}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>TenantUsageEntitlement_Setting/TenantUsageEntitlement_Setting',type:'sap.ui.model.type.String'}" linkText="{i18n>TenantUsageEntitlement.TenantUsageEntitlement_Setting/TenantUsageEntitlement_Setting}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>TenantUsageEntitlement_StartDate/TenantUsageEntitlement_StartDate}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>TenantUsageEntitlement_SystemModstamp/TenantUsageEntitlement_SystemModstamp}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>TenantUsageEntitlement_UsageDate/TenantUsageEntitlement_UsageDate}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
