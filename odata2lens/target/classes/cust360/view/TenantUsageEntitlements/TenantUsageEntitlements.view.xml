					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>TenantUsageEntitlements.TenantUsageEntitlement_IsPersistentResource}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>TenantUsageEntitlements.TenantUsageEntitlement_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>TenantUsageEntitlements.TenantUsageEntitlement_MasterLabel}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>TenantUsageEntitlements.TenantUsageEntitlement_OverageGrace}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>TenantUsageEntitlements.TenantUsageEntitlement_ResourceGroupKey}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>TenantUsageEntitlements.TenantUsageEntitlement_Setting}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>TenantUsageEntitlements.TenantUsageEntitlement_StartDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>TenantUsageEntitlements.TenantUsageEntitlement_SystemModstamp}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>TenantUsageEntitlements.TenantUsageEntitlement_UsageDate}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="TenantUsageEntitlement"/>
							<VBox>					
							<Text text="{path:'dataModel>TenantUsageEntitlement_AmountUsed/TenantUsageEntitlement_AmountUsed',type:'sap.ui.model.type.Float', formatOptions:{'style':'short','maxFractionDigits':2} }"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>TenantUsageEntitlement_CreatedDate/TenantUsageEntitlement_CreatedDate}"/>
							</VBox>							
							<VBox>					
