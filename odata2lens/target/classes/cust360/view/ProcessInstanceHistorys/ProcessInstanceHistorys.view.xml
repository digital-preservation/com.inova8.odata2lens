<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>ProcessInstanceHistorys.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/ProcessInstanceHistory' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>ProcessInstanceHistorys.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ProcessInstanceHistorys.ProcessInstanceHistory_Comments}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ProcessInstanceHistorys.ProcessInstanceHistory_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ProcessInstanceHistorys.ProcessInstanceHistory_ElapsedTimeInDays}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ProcessInstanceHistorys.ProcessInstanceHistory_ElapsedTimeInHours}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ProcessInstanceHistorys.ProcessInstanceHistory_ElapsedTimeInMinutes}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ProcessInstanceHistorys.ProcessInstanceHistory_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ProcessInstanceHistorys.ProcessInstanceHistory_IsDeleted}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ProcessInstanceHistorys.ProcessInstanceHistory_IsPending}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ProcessInstanceHistorys.ProcessInstanceHistory_RemindersSent}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ProcessInstanceHistorys.ProcessInstanceHistory_StepStatus}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ProcessInstanceHistorys.ProcessInstanceHistory_SystemModstamp}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="ProcessInstanceHistory"/>
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ProcessInstanceHistory_Comments/ProcessInstanceHistory_Comments',type:'sap.ui.model.type.String'}" linkText="{i18n>ProcessInstanceHistory.ProcessInstanceHistory_Comments/ProcessInstanceHistory_Comments}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>ProcessInstanceHistory_CreatedDate/ProcessInstanceHistory_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>ProcessInstanceHistory_ElapsedTimeInDays/ProcessInstanceHistory_ElapsedTimeInDays',type:'sap.ui.model.type.Float', formatOptions:{'style':'short','maxFractionDigits':2} }"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>ProcessInstanceHistory_ElapsedTimeInHours/ProcessInstanceHistory_ElapsedTimeInHours',type:'sap.ui.model.type.Float', formatOptions:{'style':'short','maxFractionDigits':2} }"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>ProcessInstanceHistory_ElapsedTimeInMinutes/ProcessInstanceHistory_ElapsedTimeInMinutes',type:'sap.ui.model.type.Float', formatOptions:{'style':'short','maxFractionDigits':2} }"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ProcessInstanceHistory_Id/ProcessInstanceHistory_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>ProcessInstanceHistory.ProcessInstanceHistory_Id/ProcessInstanceHistory_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>ProcessInstanceHistory_IsDeleted/ProcessInstanceHistory_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>ProcessInstanceHistory_IsPending/ProcessInstanceHistory_IsPending',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>ProcessInstanceHistory_RemindersSent/ProcessInstanceHistory_RemindersSent',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ProcessInstanceHistory_StepStatus/ProcessInstanceHistory_StepStatus',type:'sap.ui.model.type.String'}" linkText="{i18n>ProcessInstanceHistory.ProcessInstanceHistory_StepStatus/ProcessInstanceHistory_StepStatus}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>ProcessInstanceHistory_SystemModstamp/ProcessInstanceHistory_SystemModstamp}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>