<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>OrgLifecycleNotifications.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/OrgLifecycleNotification' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>OrgLifecycleNotifications.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>OrgLifecycleNotifications.OrgLifecycleNotification_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>OrgLifecycleNotifications.OrgLifecycleNotification_LifecycleRequestId}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>OrgLifecycleNotifications.OrgLifecycleNotification_LifecycleRequestType}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>OrgLifecycleNotifications.OrgLifecycleNotification_OrgId}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>OrgLifecycleNotifications.OrgLifecycleNotification_ReplayId}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>OrgLifecycleNotifications.OrgLifecycleNotification_Status}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>OrgLifecycleNotifications.OrgLifecycleNotification_StatusCode}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="OrgLifecycleNotification"/>
							<VBox>					
							<Text text="{dataModel>OrgLifecycleNotification_CreatedDate/OrgLifecycleNotification_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>OrgLifecycleNotification_LifecycleRequestId/OrgLifecycleNotification_LifecycleRequestId',type:'sap.ui.model.type.String'}" linkText="{i18n>OrgLifecycleNotification.OrgLifecycleNotification_LifecycleRequestId/OrgLifecycleNotification_LifecycleRequestId}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>OrgLifecycleNotification_LifecycleRequestType/OrgLifecycleNotification_LifecycleRequestType',type:'sap.ui.model.type.String'}" linkText="{i18n>OrgLifecycleNotification.OrgLifecycleNotification_LifecycleRequestType/OrgLifecycleNotification_LifecycleRequestType}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>OrgLifecycleNotification_OrgId/OrgLifecycleNotification_OrgId',type:'sap.ui.model.type.String'}" linkText="{i18n>OrgLifecycleNotification.OrgLifecycleNotification_OrgId/OrgLifecycleNotification_OrgId}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>OrgLifecycleNotification_ReplayId/OrgLifecycleNotification_ReplayId',type:'sap.ui.model.type.String'}" linkText="{i18n>OrgLifecycleNotification.OrgLifecycleNotification_ReplayId/OrgLifecycleNotification_ReplayId}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>OrgLifecycleNotification_Status/OrgLifecycleNotification_Status',type:'sap.ui.model.type.String'}" linkText="{i18n>OrgLifecycleNotification.OrgLifecycleNotification_Status/OrgLifecycleNotification_Status}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>OrgLifecycleNotification_StatusCode/OrgLifecycleNotification_StatusCode',type:'sap.ui.model.type.String'}" linkText="{i18n>OrgLifecycleNotification.OrgLifecycleNotification_StatusCode/OrgLifecycleNotification_StatusCode}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>