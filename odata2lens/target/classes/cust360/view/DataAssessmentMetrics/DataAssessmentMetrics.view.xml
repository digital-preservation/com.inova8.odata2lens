<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>DataAssessmentMetrics.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/DataAssessmentMetric' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>DataAssessmentMetrics.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>DataAssessmentMetrics.DataAssessmentMetric_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>DataAssessmentMetrics.DataAssessmentMetric_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>DataAssessmentMetrics.DataAssessmentMetric_IsDeleted}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>DataAssessmentMetrics.DataAssessmentMetric_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>DataAssessmentMetrics.DataAssessmentMetric_Name}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>DataAssessmentMetrics.DataAssessmentMetric_NumDuplicates}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>DataAssessmentMetrics.DataAssessmentMetric_NumMatched}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>DataAssessmentMetrics.DataAssessmentMetric_NumMatchedDifferent}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>DataAssessmentMetrics.DataAssessmentMetric_NumProcessed}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>DataAssessmentMetrics.DataAssessmentMetric_NumTotal}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>DataAssessmentMetrics.DataAssessmentMetric_NumUnmatched}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>DataAssessmentMetrics.DataAssessmentMetric_SystemModstamp}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="DataAssessmentMetric"/>
							<VBox>					
							<Text text="{dataModel>DataAssessmentMetric_CreatedDate/DataAssessmentMetric_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>DataAssessmentMetric_Id/DataAssessmentMetric_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>DataAssessmentMetric.DataAssessmentMetric_Id/DataAssessmentMetric_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>DataAssessmentMetric_IsDeleted/DataAssessmentMetric_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<Text text="{dataModel>DataAssessmentMetric_LastModifiedDate/DataAssessmentMetric_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>DataAssessmentMetric_Name/DataAssessmentMetric_Name',type:'sap.ui.model.type.String'}" linkText="{i18n>DataAssessmentMetric.DataAssessmentMetric_Name/DataAssessmentMetric_Name}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>DataAssessmentMetric_NumDuplicates/DataAssessmentMetric_NumDuplicates',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>DataAssessmentMetric_NumMatched/DataAssessmentMetric_NumMatched',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>DataAssessmentMetric_NumMatchedDifferent/DataAssessmentMetric_NumMatchedDifferent',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>DataAssessmentMetric_NumProcessed/DataAssessmentMetric_NumProcessed',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>DataAssessmentMetric_NumTotal/DataAssessmentMetric_NumTotal',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>DataAssessmentMetric_NumUnmatched/DataAssessmentMetric_NumUnmatched',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>DataAssessmentMetric_SystemModstamp/DataAssessmentMetric_SystemModstamp}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>