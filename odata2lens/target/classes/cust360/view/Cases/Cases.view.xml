<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>Cases.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/Case' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>Cases.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Cases.Case_CaseNumber}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Cases.Case_ClosedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Cases.Case_ContactEmail}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Cases.Case_ContactFax}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Cases.Case_ContactMobile}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Cases.Case_ContactPhone}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Cases.Case_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Cases.Case_Description}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Cases.Case_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Cases.Case_IsClosed}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Cases.Case_IsDeleted}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Cases.Case_IsEscalated}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Cases.Case_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Cases.Case_LastReferencedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Cases.Case_LastViewedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Cases.Case_Origin}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Cases.Case_Priority}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Cases.Case_Reason}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Cases.Case_Status}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Cases.Case_Subject}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Cases.Case_SuppliedCompany}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Cases.Case_SuppliedEmail}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Cases.Case_SuppliedName}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Cases.Case_SuppliedPhone}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Cases.Case_SystemModstamp}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Cases.Case_Type}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="Case"/>
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Case_CaseNumber/Case_CaseNumber',type:'sap.ui.model.type.String'}" linkText="{i18n>Case.Case_CaseNumber/Case_CaseNumber}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>Case_ClosedDate/Case_ClosedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Case_ContactEmail/Case_ContactEmail',type:'sap.ui.model.type.String'}" linkText="{i18n>Case.Case_ContactEmail/Case_ContactEmail}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Case_ContactFax/Case_ContactFax',type:'sap.ui.model.type.String'}" linkText="{i18n>Case.Case_ContactFax/Case_ContactFax}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Case_ContactMobile/Case_ContactMobile',type:'sap.ui.model.type.String'}" linkText="{i18n>Case.Case_ContactMobile/Case_ContactMobile}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Case_ContactPhone/Case_ContactPhone',type:'sap.ui.model.type.String'}" linkText="{i18n>Case.Case_ContactPhone/Case_ContactPhone}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>Case_CreatedDate/Case_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Case_Description/Case_Description',type:'sap.ui.model.type.String'}" linkText="{i18n>Case.Case_Description/Case_Description}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Case_Id/Case_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>Case.Case_Id/Case_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>Case_IsClosed/Case_IsClosed',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>Case_IsDeleted/Case_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>Case_IsEscalated/Case_IsEscalated',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<Text text="{dataModel>Case_LastModifiedDate/Case_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>Case_LastReferencedDate/Case_LastReferencedDate}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>Case_LastViewedDate/Case_LastViewedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Case_Origin/Case_Origin',type:'sap.ui.model.type.String'}" linkText="{i18n>Case.Case_Origin/Case_Origin}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Case_Priority/Case_Priority',type:'sap.ui.model.type.String'}" linkText="{i18n>Case.Case_Priority/Case_Priority}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Case_Reason/Case_Reason',type:'sap.ui.model.type.String'}" linkText="{i18n>Case.Case_Reason/Case_Reason}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Case_Status/Case_Status',type:'sap.ui.model.type.String'}" linkText="{i18n>Case.Case_Status/Case_Status}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Case_Subject/Case_Subject',type:'sap.ui.model.type.String'}" linkText="{i18n>Case.Case_Subject/Case_Subject}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Case_SuppliedCompany/Case_SuppliedCompany',type:'sap.ui.model.type.String'}" linkText="{i18n>Case.Case_SuppliedCompany/Case_SuppliedCompany}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Case_SuppliedEmail/Case_SuppliedEmail',type:'sap.ui.model.type.String'}" linkText="{i18n>Case.Case_SuppliedEmail/Case_SuppliedEmail}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Case_SuppliedName/Case_SuppliedName',type:'sap.ui.model.type.String'}" linkText="{i18n>Case.Case_SuppliedName/Case_SuppliedName}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Case_SuppliedPhone/Case_SuppliedPhone',type:'sap.ui.model.type.String'}" linkText="{i18n>Case.Case_SuppliedPhone/Case_SuppliedPhone}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>Case_SystemModstamp/Case_SystemModstamp}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Case_Type/Case_Type',type:'sap.ui.model.type.String'}" linkText="{i18n>Case.Case_Type/Case_Type}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>