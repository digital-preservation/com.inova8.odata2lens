<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>SignupRequestHistorys.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/SignupRequestHistory' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>SignupRequestHistorys.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>SignupRequestHistorys.SignupRequestHistory_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>SignupRequestHistorys.SignupRequestHistory_Field}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>SignupRequestHistorys.SignupRequestHistory_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>SignupRequestHistorys.SignupRequestHistory_IsDeleted}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>SignupRequestHistorys.SignupRequestHistory_NewValue}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>SignupRequestHistorys.SignupRequestHistory_OldValue}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="SignupRequestHistory"/>
							<VBox>					
							<Text text="{dataModel>SignupRequestHistory_CreatedDate/SignupRequestHistory_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>SignupRequestHistory_Field/SignupRequestHistory_Field',type:'sap.ui.model.type.String'}" linkText="{i18n>SignupRequestHistory.SignupRequestHistory_Field/SignupRequestHistory_Field}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>SignupRequestHistory_Id/SignupRequestHistory_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>SignupRequestHistory.SignupRequestHistory_Id/SignupRequestHistory_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>SignupRequestHistory_IsDeleted/SignupRequestHistory_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>SignupRequestHistory_NewValue/SignupRequestHistory_NewValue',type:'sap.ui.model.type.String'}" linkText="{i18n>SignupRequestHistory.SignupRequestHistory_NewValue/SignupRequestHistory_NewValue}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>SignupRequestHistory_OldValue/SignupRequestHistory_OldValue',type:'sap.ui.model.type.String'}" linkText="{i18n>SignupRequestHistory.SignupRequestHistory_OldValue/SignupRequestHistory_OldValue}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>