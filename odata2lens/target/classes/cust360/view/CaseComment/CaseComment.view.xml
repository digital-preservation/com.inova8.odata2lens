<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>CaseComment.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>CaseComment.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="CaseComment"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="CaseComment"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CaseComment.CaseComment_CommentBody}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>CaseComment_CommentBody/CaseComment_CommentBody',type:'sap.ui.model.type.String' }" linkText="{i18n>CaseComment.CaseComment_CommentBody/CaseComment_CommentBody}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CaseComment.CaseComment_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>CaseComment_CreatedById/sfdc_CaseComment_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CaseComment.CaseComment_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>CaseComment_CreatedDate/CaseComment_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CaseComment.CaseComment_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>CaseComment_Id/CaseComment_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>CaseComment.CaseComment_Id/CaseComment_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CaseComment.CaseComment_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>CaseComment_IsDeleted/CaseComment_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CaseComment.CaseComment_IsPublished}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>CaseComment_IsPublished/CaseComment_IsPublished',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CaseComment.CaseComment_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>CaseComment_LastModifiedById/sfdc_CaseComment_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CaseComment.CaseComment_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>CaseComment_LastModifiedDate/CaseComment_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CaseComment.CaseComment_ParentId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>CaseComment_ParentId/sfdc_CaseComment_ParentId" entitySet="Case"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CaseComment.CaseComment_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>CaseComment_SystemModstamp/CaseComment_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CaseComment.CaseComment_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>CaseComment_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CaseComment.CaseComment_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>CaseComment_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CaseComment.CaseComment_ParentId}" required="false"/> 
						<rdf:ListResource path="dataModel>CaseComment_ParentId" entitySet="Case"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>