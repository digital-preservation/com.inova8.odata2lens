<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>EmailServicesAddress.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>EmailServicesAddress.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="EmailServicesAddress"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="EmailServicesAddress"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EmailServicesAddress.EmailServicesAddress_AuthorizedSenders}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>EmailServicesAddress_AuthorizedSenders/EmailServicesAddress_AuthorizedSenders',type:'sap.ui.model.type.String' }" linkText="{i18n>EmailServicesAddress.EmailServicesAddress_AuthorizedSenders/EmailServicesAddress_AuthorizedSenders}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EmailServicesAddress.EmailServicesAddress_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>EmailServicesAddress_CreatedById/sfdc_EmailServicesAddress_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EmailServicesAddress.EmailServicesAddress_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>EmailServicesAddress_CreatedDate/EmailServicesAddress_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EmailServicesAddress.EmailServicesAddress_EmailDomainName}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>EmailServicesAddress_EmailDomainName/EmailServicesAddress_EmailDomainName',type:'sap.ui.model.type.String' }" linkText="{i18n>EmailServicesAddress.EmailServicesAddress_EmailDomainName/EmailServicesAddress_EmailDomainName}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EmailServicesAddress.EmailServicesAddress_FunctionId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>EmailServicesAddress_FunctionId/sfdc_EmailServicesAddress_FunctionId" entitySet="EmailServicesFunction"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EmailServicesAddress.EmailServicesAddress_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>EmailServicesAddress_Id/EmailServicesAddress_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>EmailServicesAddress.EmailServicesAddress_Id/EmailServicesAddress_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EmailServicesAddress.EmailServicesAddress_IsActive}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>EmailServicesAddress_IsActive/EmailServicesAddress_IsActive',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EmailServicesAddress.EmailServicesAddress_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>EmailServicesAddress_LastModifiedById/sfdc_EmailServicesAddress_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EmailServicesAddress.EmailServicesAddress_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>EmailServicesAddress_LastModifiedDate/EmailServicesAddress_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EmailServicesAddress.EmailServicesAddress_LocalPart}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>EmailServicesAddress_LocalPart/EmailServicesAddress_LocalPart',type:'sap.ui.model.type.String' }" linkText="{i18n>EmailServicesAddress.EmailServicesAddress_LocalPart/EmailServicesAddress_LocalPart}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EmailServicesAddress.EmailServicesAddress_RunAsUserId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>EmailServicesAddress_RunAsUserId/sfdc_EmailServicesAddress_RunAsUserId" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EmailServicesAddress.EmailServicesAddress_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>EmailServicesAddress_SystemModstamp/EmailServicesAddress_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EmailServicesAddress.EmailServicesAddress_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>EmailServicesAddress_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EmailServicesAddress.EmailServicesAddress_FunctionId}" required="false"/> 
						<rdf:ListResource path="dataModel>EmailServicesAddress_FunctionId" entitySet="EmailServicesFunction"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EmailServicesAddress.EmailServicesAddress_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>EmailServicesAddress_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EmailServicesAddress.EmailServicesAddress_RunAsUserId}" required="false"/> 
						<rdf:ListResource path="dataModel>EmailServicesAddress_RunAsUserId" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>