<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>ApexTestResults.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/ApexTestResult' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>ApexTestResults.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexTestResults.ApexTestResult_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexTestResults.ApexTestResult_Message}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexTestResults.ApexTestResult_MethodName}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexTestResults.ApexTestResult_Outcome}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexTestResults.ApexTestResult_RunTime}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexTestResults.ApexTestResult_StackTrace}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexTestResults.ApexTestResult_SystemModstamp}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexTestResults.ApexTestResult_TestTimestamp}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="ApexTestResult"/>
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ApexTestResult_Id/ApexTestResult_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>ApexTestResult.ApexTestResult_Id/ApexTestResult_Id}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ApexTestResult_Message/ApexTestResult_Message',type:'sap.ui.model.type.String'}" linkText="{i18n>ApexTestResult.ApexTestResult_Message/ApexTestResult_Message}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ApexTestResult_MethodName/ApexTestResult_MethodName',type:'sap.ui.model.type.String'}" linkText="{i18n>ApexTestResult.ApexTestResult_MethodName/ApexTestResult_MethodName}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ApexTestResult_Outcome/ApexTestResult_Outcome',type:'sap.ui.model.type.String'}" linkText="{i18n>ApexTestResult.ApexTestResult_Outcome/ApexTestResult_Outcome}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>ApexTestResult_RunTime/ApexTestResult_RunTime',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ApexTestResult_StackTrace/ApexTestResult_StackTrace',type:'sap.ui.model.type.String'}" linkText="{i18n>ApexTestResult.ApexTestResult_StackTrace/ApexTestResult_StackTrace}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>ApexTestResult_SystemModstamp/ApexTestResult_SystemModstamp}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>ApexTestResult_TestTimestamp/ApexTestResult_TestTimestamp}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>