<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>PackageLicense.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>PackageLicense.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="PackageLicense"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="PackageLicense"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>PackageLicense.PackageLicense_AllowedLicenses}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>PackageLicense_AllowedLicenses/PackageLicense_AllowedLicenses',type:'sap.ui.model.type.Integer' }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>PackageLicense.PackageLicense_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>PackageLicense_CreatedDate/PackageLicense_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>PackageLicense.PackageLicense_ExpirationDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>PackageLicense_ExpirationDate/PackageLicense_ExpirationDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>PackageLicense.PackageLicense_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>PackageLicense_Id/PackageLicense_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>PackageLicense.PackageLicense_Id/PackageLicense_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>PackageLicense.PackageLicense_IsProvisioned}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>PackageLicense_IsProvisioned/PackageLicense_IsProvisioned',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>PackageLicense.PackageLicense_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>PackageLicense_LastModifiedDate/PackageLicense_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>PackageLicense.PackageLicense_NamespacePrefix}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>PackageLicense_NamespacePrefix/PackageLicense_NamespacePrefix',type:'sap.ui.model.type.String' }" linkText="{i18n>PackageLicense.PackageLicense_NamespacePrefix/PackageLicense_NamespacePrefix}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>PackageLicense.PackageLicense_Status}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>PackageLicense_Status/PackageLicense_Status',type:'sap.ui.model.type.String' }" linkText="{i18n>PackageLicense.PackageLicense_Status/PackageLicense_Status}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>PackageLicense.PackageLicense_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>PackageLicense_SystemModstamp/PackageLicense_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>PackageLicense.PackageLicense_UsedLicenses}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>PackageLicense_UsedLicenses/PackageLicense_UsedLicenses',type:'sap.ui.model.type.Integer' }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>PackageLicense.UserPackageLicense_PackageLicenseId_Inverse}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>UserPackageLicense_PackageLicenseId_Inverse/sfdc_UserPackageLicense_PackageLicenseId_Inverse" entitySet="UserPackageLicense"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>PackageLicense.UserPackageLicense_PackageLicenseId_Inverse}" required="false"/> 
						<rdf:ListResource path="dataModel>UserPackageLicense_PackageLicenseId_Inverse" entitySet="UserPackageLicense"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>