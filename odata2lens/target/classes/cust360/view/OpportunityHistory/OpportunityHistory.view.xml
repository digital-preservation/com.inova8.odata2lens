<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>OpportunityHistory.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>OpportunityHistory.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="OpportunityHistory"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="OpportunityHistory"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OpportunityHistory.OpportunityHistory_Amount}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>OpportunityHistory_Amount/OpportunityHistory_Amount',type:'sap.ui.model.type.Float', formatOptions:{'style':'short','maxFractionDigits':2} }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OpportunityHistory.OpportunityHistory_CloseDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>OpportunityHistory_CloseDate/OpportunityHistory_CloseDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OpportunityHistory.OpportunityHistory_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>OpportunityHistory_CreatedById/sfdc_OpportunityHistory_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OpportunityHistory.OpportunityHistory_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>OpportunityHistory_CreatedDate/OpportunityHistory_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OpportunityHistory.OpportunityHistory_ExpectedRevenue}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>OpportunityHistory_ExpectedRevenue/OpportunityHistory_ExpectedRevenue',type:'sap.ui.model.type.Float', formatOptions:{'style':'short','maxFractionDigits':2} }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OpportunityHistory.OpportunityHistory_ForecastCategory}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>OpportunityHistory_ForecastCategory/OpportunityHistory_ForecastCategory',type:'sap.ui.model.type.String' }" linkText="{i18n>OpportunityHistory.OpportunityHistory_ForecastCategory/OpportunityHistory_ForecastCategory}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OpportunityHistory.OpportunityHistory_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>OpportunityHistory_Id/OpportunityHistory_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>OpportunityHistory.OpportunityHistory_Id/OpportunityHistory_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OpportunityHistory.OpportunityHistory_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>OpportunityHistory_IsDeleted/OpportunityHistory_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OpportunityHistory.OpportunityHistory_OpportunityId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>OpportunityHistory_OpportunityId/sfdc_OpportunityHistory_OpportunityId" entitySet="Opportunity"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OpportunityHistory.OpportunityHistory_Probability}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>OpportunityHistory_Probability/OpportunityHistory_Probability',type:'sap.ui.model.type.Float', formatOptions:{'style':'short','maxFractionDigits':2} }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OpportunityHistory.OpportunityHistory_StageName}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>OpportunityHistory_StageName/OpportunityHistory_StageName',type:'sap.ui.model.type.String' }" linkText="{i18n>OpportunityHistory.OpportunityHistory_StageName/OpportunityHistory_StageName}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OpportunityHistory.OpportunityHistory_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>OpportunityHistory_SystemModstamp/OpportunityHistory_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OpportunityHistory.OpportunityHistory_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>OpportunityHistory_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OpportunityHistory.OpportunityHistory_OpportunityId}" required="false"/> 
						<rdf:ListResource path="dataModel>OpportunityHistory_OpportunityId" entitySet="Opportunity"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>