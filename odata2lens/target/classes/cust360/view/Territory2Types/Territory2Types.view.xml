<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>Territory2Types.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/Territory2Type' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>Territory2Types.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Territory2Types.Territory2Type_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Territory2Types.Territory2Type_Description}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Territory2Types.Territory2Type_DeveloperName}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Territory2Types.Territory2Type_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Territory2Types.Territory2Type_IsDeleted}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Territory2Types.Territory2Type_Language}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Territory2Types.Territory2Type_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Territory2Types.Territory2Type_MasterLabel}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Territory2Types.Territory2Type_Priority}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Territory2Types.Territory2Type_SystemModstamp}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="Territory2Type"/>
							<VBox>					
							<Text text="{dataModel>Territory2Type_CreatedDate/Territory2Type_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Territory2Type_Description/Territory2Type_Description',type:'sap.ui.model.type.String'}" linkText="{i18n>Territory2Type.Territory2Type_Description/Territory2Type_Description}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Territory2Type_DeveloperName/Territory2Type_DeveloperName',type:'sap.ui.model.type.String'}" linkText="{i18n>Territory2Type.Territory2Type_DeveloperName/Territory2Type_DeveloperName}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Territory2Type_Id/Territory2Type_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>Territory2Type.Territory2Type_Id/Territory2Type_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>Territory2Type_IsDeleted/Territory2Type_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Territory2Type_Language/Territory2Type_Language',type:'sap.ui.model.type.String'}" linkText="{i18n>Territory2Type.Territory2Type_Language/Territory2Type_Language}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>Territory2Type_LastModifiedDate/Territory2Type_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Territory2Type_MasterLabel/Territory2Type_MasterLabel',type:'sap.ui.model.type.String'}" linkText="{i18n>Territory2Type.Territory2Type_MasterLabel/Territory2Type_MasterLabel}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>Territory2Type_Priority/Territory2Type_Priority',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>Territory2Type_SystemModstamp/Territory2Type_SystemModstamp}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>