<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>ContractLineItems.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/ContractLineItem' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>ContractLineItems.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContractLineItems.ContractLineItem_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContractLineItems.ContractLineItem_Description}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContractLineItems.ContractLineItem_Discount}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContractLineItems.ContractLineItem_EndDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContractLineItems.ContractLineItem_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContractLineItems.ContractLineItem_IsDeleted}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContractLineItems.ContractLineItem_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContractLineItems.ContractLineItem_LastReferencedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContractLineItems.ContractLineItem_LastViewedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContractLineItems.ContractLineItem_LineItemNumber}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContractLineItems.ContractLineItem_ListPrice}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContractLineItems.ContractLineItem_Quantity}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContractLineItems.ContractLineItem_StartDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContractLineItems.ContractLineItem_Status}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContractLineItems.ContractLineItem_Subtotal}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContractLineItems.ContractLineItem_SystemModstamp}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContractLineItems.ContractLineItem_TotalPrice}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContractLineItems.ContractLineItem_UnitPrice}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="ContractLineItem"/>
							<VBox>					
							<Text text="{dataModel>ContractLineItem_CreatedDate/ContractLineItem_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ContractLineItem_Description/ContractLineItem_Description',type:'sap.ui.model.type.String'}" linkText="{i18n>ContractLineItem.ContractLineItem_Description/ContractLineItem_Description}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>ContractLineItem_Discount/ContractLineItem_Discount',type:'sap.ui.model.type.Float', formatOptions:{'style':'short','maxFractionDigits':2} }"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>ContractLineItem_EndDate/ContractLineItem_EndDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ContractLineItem_Id/ContractLineItem_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>ContractLineItem.ContractLineItem_Id/ContractLineItem_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>ContractLineItem_IsDeleted/ContractLineItem_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<Text text="{dataModel>ContractLineItem_LastModifiedDate/ContractLineItem_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>ContractLineItem_LastReferencedDate/ContractLineItem_LastReferencedDate}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>ContractLineItem_LastViewedDate/ContractLineItem_LastViewedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ContractLineItem_LineItemNumber/ContractLineItem_LineItemNumber',type:'sap.ui.model.type.String'}" linkText="{i18n>ContractLineItem.ContractLineItem_LineItemNumber/ContractLineItem_LineItemNumber}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>ContractLineItem_ListPrice/ContractLineItem_ListPrice',type:'sap.ui.model.type.Float', formatOptions:{'style':'short','maxFractionDigits':2} }"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>ContractLineItem_Quantity/ContractLineItem_Quantity',type:'sap.ui.model.type.Float', formatOptions:{'style':'short','maxFractionDigits':2} }"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>ContractLineItem_StartDate/ContractLineItem_StartDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ContractLineItem_Status/ContractLineItem_Status',type:'sap.ui.model.type.String'}" linkText="{i18n>ContractLineItem.ContractLineItem_Status/ContractLineItem_Status}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>ContractLineItem_Subtotal/ContractLineItem_Subtotal',type:'sap.ui.model.type.Float', formatOptions:{'style':'short','maxFractionDigits':2} }"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>ContractLineItem_SystemModstamp/ContractLineItem_SystemModstamp}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>ContractLineItem_TotalPrice/ContractLineItem_TotalPrice',type:'sap.ui.model.type.Float', formatOptions:{'style':'short','maxFractionDigits':2} }"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>ContractLineItem_UnitPrice/ContractLineItem_UnitPrice',type:'sap.ui.model.type.Float', formatOptions:{'style':'short','maxFractionDigits':2} }"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>