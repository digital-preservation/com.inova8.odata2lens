<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>EntitySubscription.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>EntitySubscription.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="EntitySubscription"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="EntitySubscription"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EntitySubscription.EntitySubscription_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>EntitySubscription_CreatedById/sfdc_EntitySubscription_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EntitySubscription.EntitySubscription_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>EntitySubscription_CreatedDate/EntitySubscription_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EntitySubscription.EntitySubscription_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>EntitySubscription_Id/EntitySubscription_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>EntitySubscription.EntitySubscription_Id/EntitySubscription_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EntitySubscription.EntitySubscription_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>EntitySubscription_IsDeleted/EntitySubscription_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EntitySubscription.EntitySubscription_ParentId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>EntitySubscription_ParentId/sfdc_EntitySubscription_ParentId" entitySet="Account"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EntitySubscription.EntitySubscription_SubscriberId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>EntitySubscription_SubscriberId/sfdc_EntitySubscription_SubscriberId" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EntitySubscription.EntitySubscription_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>EntitySubscription_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EntitySubscription.EntitySubscription_ParentId}" required="false"/> 
						<rdf:ListResource path="dataModel>EntitySubscription_ParentId" entitySet="Account"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EntitySubscription.EntitySubscription_SubscriberId}" required="false"/> 
						<rdf:ListResource path="dataModel>EntitySubscription_SubscriberId" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>