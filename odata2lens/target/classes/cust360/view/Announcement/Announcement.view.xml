<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>Announcement.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>Announcement.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="Announcement"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="Announcement"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Announcement.Announcement_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>Announcement_CreatedById/sfdc_Announcement_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Announcement.Announcement_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>Announcement_CreatedDate/Announcement_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Announcement.Announcement_ExpirationDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>Announcement_ExpirationDate/Announcement_ExpirationDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Announcement.Announcement_FeedItemId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>Announcement_FeedItemId/sfdc_Announcement_FeedItemId" entitySet="FeedItem"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Announcement.Announcement_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>Announcement_Id/Announcement_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>Announcement.Announcement_Id/Announcement_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Announcement.Announcement_IsArchived}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>Announcement_IsArchived/Announcement_IsArchived',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Announcement.Announcement_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>Announcement_IsDeleted/Announcement_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Announcement.Announcement_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>Announcement_LastModifiedById/sfdc_Announcement_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Announcement.Announcement_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>Announcement_LastModifiedDate/Announcement_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Announcement.Announcement_ParentId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>Announcement_ParentId/sfdc_Announcement_ParentId" entitySet="CollaborationGroup"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Announcement.Announcement_SendEmails}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>Announcement_SendEmails/Announcement_SendEmails',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Announcement.Announcement_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>Announcement_SystemModstamp/Announcement_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Announcement.CollaborationGroup_AnnouncementId_Inverse}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>CollaborationGroup_AnnouncementId_Inverse/sfdc_CollaborationGroup_AnnouncementId_Inverse" entitySet="CollaborationGroup"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Announcement.Announcement_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>Announcement_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Announcement.Announcement_FeedItemId}" required="false"/> 
						<rdf:ListResource path="dataModel>Announcement_FeedItemId" entitySet="FeedItem"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Announcement.Announcement_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>Announcement_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Announcement.Announcement_ParentId}" required="false"/> 
						<rdf:ListResource path="dataModel>Announcement_ParentId" entitySet="CollaborationGroup"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Announcement.CollaborationGroup_AnnouncementId_Inverse}" required="false"/> 
						<rdf:ListResource path="dataModel>CollaborationGroup_AnnouncementId_Inverse" entitySet="CollaborationGroup"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>