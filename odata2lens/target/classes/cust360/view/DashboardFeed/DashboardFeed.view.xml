<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>DashboardFeed.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>DashboardFeed.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="DashboardFeed"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="DashboardFeed"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>DashboardFeed.DashboardFeed_Body}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>DashboardFeed_Body/DashboardFeed_Body',type:'sap.ui.model.type.String' }" linkText="{i18n>DashboardFeed.DashboardFeed_Body/DashboardFeed_Body}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>DashboardFeed.DashboardFeed_CommentCount}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>DashboardFeed_CommentCount/DashboardFeed_CommentCount',type:'sap.ui.model.type.Integer' }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>DashboardFeed.DashboardFeed_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>DashboardFeed_CreatedById/sfdc_DashboardFeed_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>DashboardFeed.DashboardFeed_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>DashboardFeed_CreatedDate/DashboardFeed_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>DashboardFeed.DashboardFeed_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>DashboardFeed_Id/DashboardFeed_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>DashboardFeed.DashboardFeed_Id/DashboardFeed_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>DashboardFeed.DashboardFeed_InsertedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>DashboardFeed_InsertedById/sfdc_DashboardFeed_InsertedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>DashboardFeed.DashboardFeed_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>DashboardFeed_IsDeleted/DashboardFeed_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>DashboardFeed.DashboardFeed_IsRichText}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>DashboardFeed_IsRichText/DashboardFeed_IsRichText',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>DashboardFeed.DashboardFeed_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>DashboardFeed_LastModifiedDate/DashboardFeed_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>DashboardFeed.DashboardFeed_LikeCount}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>DashboardFeed_LikeCount/DashboardFeed_LikeCount',type:'sap.ui.model.type.Integer' }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>DashboardFeed.DashboardFeed_LinkUrl}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>DashboardFeed_LinkUrl/DashboardFeed_LinkUrl',type:'sap.ui.model.type.String' }" linkText="{i18n>DashboardFeed.DashboardFeed_LinkUrl/DashboardFeed_LinkUrl}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>DashboardFeed.DashboardFeed_ParentId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>DashboardFeed_ParentId/sfdc_DashboardFeed_ParentId" entitySet="Dashboard"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>DashboardFeed.DashboardFeed_RelatedRecordId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>DashboardFeed_RelatedRecordId/sfdc_DashboardFeed_RelatedRecordId" entitySet="ContentVersion"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>DashboardFeed.DashboardFeed_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>DashboardFeed_SystemModstamp/DashboardFeed_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>DashboardFeed.DashboardFeed_Title}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>DashboardFeed_Title/DashboardFeed_Title',type:'sap.ui.model.type.String' }" linkText="{i18n>DashboardFeed.DashboardFeed_Title/DashboardFeed_Title}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>DashboardFeed.DashboardFeed_Type}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>DashboardFeed_Type/DashboardFeed_Type',type:'sap.ui.model.type.String' }" linkText="{i18n>DashboardFeed.DashboardFeed_Type/DashboardFeed_Type}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>DashboardFeed.DashboardFeed_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>DashboardFeed_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>DashboardFeed.DashboardFeed_InsertedById}" required="false"/> 
						<rdf:ListResource path="dataModel>DashboardFeed_InsertedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>DashboardFeed.DashboardFeed_ParentId}" required="false"/> 
						<rdf:ListResource path="dataModel>DashboardFeed_ParentId" entitySet="Dashboard"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>DashboardFeed.DashboardFeed_RelatedRecordId}" required="false"/> 
						<rdf:ListResource path="dataModel>DashboardFeed_RelatedRecordId" entitySet="ContentVersion"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>