<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>AuraDefinition.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>AuraDefinition.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="AuraDefinition"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="AuraDefinition"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AuraDefinition.AuraDefinition_AuraDefinitionBundleId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>AuraDefinition_AuraDefinitionBundleId/sfdc_AuraDefinition_AuraDefinitionBundleId" entitySet="AuraDefinitionBundle"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AuraDefinition.AuraDefinition_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>AuraDefinition_CreatedById/sfdc_AuraDefinition_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AuraDefinition.AuraDefinition_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>AuraDefinition_CreatedDate/AuraDefinition_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AuraDefinition.AuraDefinition_DefType}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>AuraDefinition_DefType/AuraDefinition_DefType',type:'sap.ui.model.type.String' }" linkText="{i18n>AuraDefinition.AuraDefinition_DefType/AuraDefinition_DefType}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AuraDefinition.AuraDefinition_Format}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>AuraDefinition_Format/AuraDefinition_Format',type:'sap.ui.model.type.String' }" linkText="{i18n>AuraDefinition.AuraDefinition_Format/AuraDefinition_Format}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AuraDefinition.AuraDefinition_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>AuraDefinition_Id/AuraDefinition_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>AuraDefinition.AuraDefinition_Id/AuraDefinition_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AuraDefinition.AuraDefinition_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>AuraDefinition_IsDeleted/AuraDefinition_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AuraDefinition.AuraDefinition_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>AuraDefinition_LastModifiedById/sfdc_AuraDefinition_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AuraDefinition.AuraDefinition_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>AuraDefinition_LastModifiedDate/AuraDefinition_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AuraDefinition.AuraDefinition_Source}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>AuraDefinition_Source/AuraDefinition_Source',type:'sap.ui.model.type.String' }" linkText="{i18n>AuraDefinition.AuraDefinition_Source/AuraDefinition_Source}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AuraDefinition.AuraDefinition_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>AuraDefinition_SystemModstamp/AuraDefinition_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AuraDefinition.AuraDefinition_AuraDefinitionBundleId}" required="false"/> 
						<rdf:ListResource path="dataModel>AuraDefinition_AuraDefinitionBundleId" entitySet="AuraDefinitionBundle"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AuraDefinition.AuraDefinition_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>AuraDefinition_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AuraDefinition.AuraDefinition_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>AuraDefinition_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>