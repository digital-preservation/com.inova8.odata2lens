<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>ForecastingCategoryMapping.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>ForecastingCategoryMapping.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="ForecastingCategoryMapping"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="ForecastingCategoryMapping"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ForecastingCategoryMapping.ForecastingCategoryMapping_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ForecastingCategoryMapping_CreatedById/sfdc_ForecastingCategoryMapping_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ForecastingCategoryMapping.ForecastingCategoryMapping_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>ForecastingCategoryMapping_CreatedDate/ForecastingCategoryMapping_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ForecastingCategoryMapping.ForecastingCategoryMapping_ForecastingItemCategory}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ForecastingCategoryMapping_ForecastingItemCategory/ForecastingCategoryMapping_ForecastingItemCategory',type:'sap.ui.model.type.String' }" linkText="{i18n>ForecastingCategoryMapping.ForecastingCategoryMapping_ForecastingItemCategory/ForecastingCategoryMapping_ForecastingItemCategory}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ForecastingCategoryMapping.ForecastingCategoryMapping_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ForecastingCategoryMapping_Id/ForecastingCategoryMapping_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>ForecastingCategoryMapping.ForecastingCategoryMapping_Id/ForecastingCategoryMapping_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ForecastingCategoryMapping.ForecastingCategoryMapping_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>ForecastingCategoryMapping_IsDeleted/ForecastingCategoryMapping_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ForecastingCategoryMapping.ForecastingCategoryMapping_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ForecastingCategoryMapping_LastModifiedById/sfdc_ForecastingCategoryMapping_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ForecastingCategoryMapping.ForecastingCategoryMapping_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>ForecastingCategoryMapping_LastModifiedDate/ForecastingCategoryMapping_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ForecastingCategoryMapping.ForecastingCategoryMapping_SourceCategory}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ForecastingCategoryMapping_SourceCategory/ForecastingCategoryMapping_SourceCategory',type:'sap.ui.model.type.String' }" linkText="{i18n>ForecastingCategoryMapping.ForecastingCategoryMapping_SourceCategory/ForecastingCategoryMapping_SourceCategory}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ForecastingCategoryMapping.ForecastingCategoryMapping_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>ForecastingCategoryMapping_SystemModstamp/ForecastingCategoryMapping_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ForecastingCategoryMapping.ForecastingCategoryMapping_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>ForecastingCategoryMapping_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ForecastingCategoryMapping.ForecastingCategoryMapping_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>ForecastingCategoryMapping_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>