<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>MacroShare.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>MacroShare.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="MacroShare"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="MacroShare"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>MacroShare.MacroShare_AccessLevel}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>MacroShare_AccessLevel/MacroShare_AccessLevel',type:'sap.ui.model.type.String' }" linkText="{i18n>MacroShare.MacroShare_AccessLevel/MacroShare_AccessLevel}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>MacroShare.MacroShare_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>MacroShare_Id/MacroShare_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>MacroShare.MacroShare_Id/MacroShare_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>MacroShare.MacroShare_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>MacroShare_IsDeleted/MacroShare_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>MacroShare.MacroShare_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>MacroShare_LastModifiedById/sfdc_MacroShare_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>MacroShare.MacroShare_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>MacroShare_LastModifiedDate/MacroShare_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>MacroShare.MacroShare_ParentId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>MacroShare_ParentId/sfdc_MacroShare_ParentId" entitySet="Macro"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>MacroShare.MacroShare_RowCause}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>MacroShare_RowCause/MacroShare_RowCause',type:'sap.ui.model.type.String' }" linkText="{i18n>MacroShare.MacroShare_RowCause/MacroShare_RowCause}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>MacroShare.MacroShare_UserOrGroupId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>MacroShare_UserOrGroupId/sfdc_MacroShare_UserOrGroupId" entitySet="Group"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>MacroShare.MacroShare_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>MacroShare_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>MacroShare.MacroShare_ParentId}" required="false"/> 
						<rdf:ListResource path="dataModel>MacroShare_ParentId" entitySet="Macro"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>MacroShare.MacroShare_UserOrGroupId}" required="false"/> 
						<rdf:ListResource path="dataModel>MacroShare_UserOrGroupId" entitySet="Group"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>