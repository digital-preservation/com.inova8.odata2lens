<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>UserLicense.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>UserLicense.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="UserLicense"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="UserLicense"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserLicense.CustomObjectUserLicenseMetrics_UserLicenseId_Inverse}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>CustomObjectUserLicenseMetrics_UserLicenseId_Inverse/sfdc_CustomObjectUserLicenseMetrics_UserLicenseId_Inverse" entitySet="CustomObjectUserLicenseMetrics"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserLicense.Profile_UserLicenseId_Inverse}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>Profile_UserLicenseId_Inverse/sfdc_Profile_UserLicenseId_Inverse" entitySet="Profile"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserLicense.UserLicense_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>UserLicense_CreatedDate/UserLicense_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserLicense.UserLicense_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>UserLicense_Id/UserLicense_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>UserLicense.UserLicense_Id/UserLicense_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserLicense.UserLicense_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>UserLicense_LastModifiedDate/UserLicense_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserLicense.UserLicense_LicenseDefinitionKey}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>UserLicense_LicenseDefinitionKey/UserLicense_LicenseDefinitionKey',type:'sap.ui.model.type.String' }" linkText="{i18n>UserLicense.UserLicense_LicenseDefinitionKey/UserLicense_LicenseDefinitionKey}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserLicense.UserLicense_MasterLabel}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>UserLicense_MasterLabel/UserLicense_MasterLabel',type:'sap.ui.model.type.String' }" linkText="{i18n>UserLicense.UserLicense_MasterLabel/UserLicense_MasterLabel}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserLicense.UserLicense_Name}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>UserLicense_Name/UserLicense_Name',type:'sap.ui.model.type.String' }" linkText="{i18n>UserLicense.UserLicense_Name/UserLicense_Name}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserLicense.UserLicense_Status}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>UserLicense_Status/UserLicense_Status',type:'sap.ui.model.type.String' }" linkText="{i18n>UserLicense.UserLicense_Status/UserLicense_Status}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserLicense.UserLicense_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>UserLicense_SystemModstamp/UserLicense_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserLicense.UserLicense_TotalLicenses}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>UserLicense_TotalLicenses/UserLicense_TotalLicenses',type:'sap.ui.model.type.Integer' }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserLicense.UserLicense_UsedLicenses}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>UserLicense_UsedLicenses/UserLicense_UsedLicenses',type:'sap.ui.model.type.Integer' }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserLicense.UserLicense_UsedLicensesLastUpdated}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>UserLicense_UsedLicensesLastUpdated/UserLicense_UsedLicensesLastUpdated}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserLicense.CustomObjectUserLicenseMetrics_UserLicenseId_Inverse}" required="false"/> 
						<rdf:ListResource path="dataModel>CustomObjectUserLicenseMetrics_UserLicenseId_Inverse" entitySet="CustomObjectUserLicenseMetrics"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserLicense.Profile_UserLicenseId_Inverse}" required="false"/> 
						<rdf:ListResource path="dataModel>Profile_UserLicenseId_Inverse" entitySet="Profile"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>