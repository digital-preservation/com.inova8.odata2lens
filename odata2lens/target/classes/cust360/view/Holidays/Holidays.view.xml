<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>Holidays.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/Holiday' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>Holidays.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Holidays.Holiday_ActivityDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Holidays.Holiday_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Holidays.Holiday_Description}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Holidays.Holiday_EndTimeInMinutes}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Holidays.Holiday_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Holidays.Holiday_IsAllDay}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Holidays.Holiday_IsRecurrence}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Holidays.Holiday_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Holidays.Holiday_Name}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Holidays.Holiday_RecurrenceDayOfMonth}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Holidays.Holiday_RecurrenceDayOfWeekMask}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Holidays.Holiday_RecurrenceEndDateOnly}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Holidays.Holiday_RecurrenceInstance}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Holidays.Holiday_RecurrenceInterval}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Holidays.Holiday_RecurrenceMonthOfYear}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Holidays.Holiday_RecurrenceStartDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Holidays.Holiday_RecurrenceType}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Holidays.Holiday_StartTimeInMinutes}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Holidays.Holiday_SystemModstamp}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="Holiday"/>
							<VBox>					
							<Text text="{dataModel>Holiday_ActivityDate/Holiday_ActivityDate}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>Holiday_CreatedDate/Holiday_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Holiday_Description/Holiday_Description',type:'sap.ui.model.type.String'}" linkText="{i18n>Holiday.Holiday_Description/Holiday_Description}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>Holiday_EndTimeInMinutes/Holiday_EndTimeInMinutes',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Holiday_Id/Holiday_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>Holiday.Holiday_Id/Holiday_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>Holiday_IsAllDay/Holiday_IsAllDay',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>Holiday_IsRecurrence/Holiday_IsRecurrence',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<Text text="{dataModel>Holiday_LastModifiedDate/Holiday_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Holiday_Name/Holiday_Name',type:'sap.ui.model.type.String'}" linkText="{i18n>Holiday.Holiday_Name/Holiday_Name}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>Holiday_RecurrenceDayOfMonth/Holiday_RecurrenceDayOfMonth',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>Holiday_RecurrenceDayOfWeekMask/Holiday_RecurrenceDayOfWeekMask',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>Holiday_RecurrenceEndDateOnly/Holiday_RecurrenceEndDateOnly}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Holiday_RecurrenceInstance/Holiday_RecurrenceInstance',type:'sap.ui.model.type.String'}" linkText="{i18n>Holiday.Holiday_RecurrenceInstance/Holiday_RecurrenceInstance}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>Holiday_RecurrenceInterval/Holiday_RecurrenceInterval',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Holiday_RecurrenceMonthOfYear/Holiday_RecurrenceMonthOfYear',type:'sap.ui.model.type.String'}" linkText="{i18n>Holiday.Holiday_RecurrenceMonthOfYear/Holiday_RecurrenceMonthOfYear}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>Holiday_RecurrenceStartDate/Holiday_RecurrenceStartDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Holiday_RecurrenceType/Holiday_RecurrenceType',type:'sap.ui.model.type.String'}" linkText="{i18n>Holiday.Holiday_RecurrenceType/Holiday_RecurrenceType}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>Holiday_StartTimeInMinutes/Holiday_StartTimeInMinutes',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>Holiday_SystemModstamp/Holiday_SystemModstamp}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>