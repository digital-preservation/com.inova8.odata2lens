<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>PartnerRole.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>PartnerRole.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="PartnerRole"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="PartnerRole"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>PartnerRole.PartnerRole_ApiName}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>PartnerRole_ApiName/PartnerRole_ApiName',type:'sap.ui.model.type.String' }" linkText="{i18n>PartnerRole.PartnerRole_ApiName/PartnerRole_ApiName}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>PartnerRole.PartnerRole_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>PartnerRole_CreatedById/sfdc_PartnerRole_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>PartnerRole.PartnerRole_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>PartnerRole_CreatedDate/PartnerRole_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>PartnerRole.PartnerRole_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>PartnerRole_Id/PartnerRole_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>PartnerRole.PartnerRole_Id/PartnerRole_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>PartnerRole.PartnerRole_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>PartnerRole_LastModifiedById/sfdc_PartnerRole_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>PartnerRole.PartnerRole_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>PartnerRole_LastModifiedDate/PartnerRole_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>PartnerRole.PartnerRole_MasterLabel}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>PartnerRole_MasterLabel/PartnerRole_MasterLabel',type:'sap.ui.model.type.String' }" linkText="{i18n>PartnerRole.PartnerRole_MasterLabel/PartnerRole_MasterLabel}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>PartnerRole.PartnerRole_ReverseRole}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>PartnerRole_ReverseRole/PartnerRole_ReverseRole',type:'sap.ui.model.type.String' }" linkText="{i18n>PartnerRole.PartnerRole_ReverseRole/PartnerRole_ReverseRole}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>PartnerRole.PartnerRole_SortOrder}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>PartnerRole_SortOrder/PartnerRole_SortOrder',type:'sap.ui.model.type.Integer' }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>PartnerRole.PartnerRole_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>PartnerRole_SystemModstamp/PartnerRole_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>PartnerRole.PartnerRole_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>PartnerRole_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>PartnerRole.PartnerRole_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>PartnerRole_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>