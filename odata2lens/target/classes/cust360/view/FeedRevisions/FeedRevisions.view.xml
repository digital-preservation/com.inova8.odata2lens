<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>FeedRevisions.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/FeedRevision' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>FeedRevisions.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>FeedRevisions.FeedRevision_Action}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>FeedRevisions.FeedRevision_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>FeedRevisions.FeedRevision_EditedAttribute}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>FeedRevisions.FeedRevision_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>FeedRevisions.FeedRevision_IsDeleted}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>FeedRevisions.FeedRevision_IsValueRichText}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>FeedRevisions.FeedRevision_Revision}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>FeedRevisions.FeedRevision_SystemModstamp}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>FeedRevisions.FeedRevision_Value}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="FeedRevision"/>
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>FeedRevision_Action/FeedRevision_Action',type:'sap.ui.model.type.String'}" linkText="{i18n>FeedRevision.FeedRevision_Action/FeedRevision_Action}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>FeedRevision_CreatedDate/FeedRevision_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>FeedRevision_EditedAttribute/FeedRevision_EditedAttribute',type:'sap.ui.model.type.String'}" linkText="{i18n>FeedRevision.FeedRevision_EditedAttribute/FeedRevision_EditedAttribute}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>FeedRevision_Id/FeedRevision_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>FeedRevision.FeedRevision_Id/FeedRevision_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>FeedRevision_IsDeleted/FeedRevision_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>FeedRevision_IsValueRichText/FeedRevision_IsValueRichText',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>FeedRevision_Revision/FeedRevision_Revision',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>FeedRevision_SystemModstamp/FeedRevision_SystemModstamp}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>FeedRevision_Value/FeedRevision_Value',type:'sap.ui.model.type.String'}" linkText="{i18n>FeedRevision.FeedRevision_Value/FeedRevision_Value}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>