<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>SearchLayout.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>SearchLayout.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="SearchLayout"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="SearchLayout"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SearchLayout.SearchLayout_DurableId}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>SearchLayout_DurableId/SearchLayout_DurableId',type:'sap.ui.model.type.String' }" linkText="{i18n>SearchLayout.SearchLayout_DurableId/SearchLayout_DurableId}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SearchLayout.SearchLayout_EntityDefinitionId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>SearchLayout_EntityDefinitionId/sfdc_SearchLayout_EntityDefinitionId" entitySet="EntityDefinition"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SearchLayout.SearchLayout_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>SearchLayout_Id/SearchLayout_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>SearchLayout.SearchLayout_Id/SearchLayout_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SearchLayout.SearchLayout_Label}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>SearchLayout_Label/SearchLayout_Label',type:'sap.ui.model.type.String' }" linkText="{i18n>SearchLayout.SearchLayout_Label/SearchLayout_Label}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SearchLayout.SearchLayout_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>SearchLayout_LastModifiedById/sfdc_SearchLayout_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SearchLayout.SearchLayout_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>SearchLayout_LastModifiedDate/SearchLayout_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SearchLayout.SearchLayout_LayoutType}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>SearchLayout_LayoutType/SearchLayout_LayoutType',type:'sap.ui.model.type.String' }" linkText="{i18n>SearchLayout.SearchLayout_LayoutType/SearchLayout_LayoutType}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SearchLayout.SearchLayout_EntityDefinitionId}" required="false"/> 
						<rdf:ListResource path="dataModel>SearchLayout_EntityDefinitionId" entitySet="EntityDefinition"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SearchLayout.SearchLayout_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>SearchLayout_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>