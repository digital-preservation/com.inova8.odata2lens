<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>UserProvAccounts.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/UserProvAccount' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>UserProvAccounts.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserProvAccounts.UserProvAccount_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserProvAccounts.UserProvAccount_DeletedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserProvAccounts.UserProvAccount_ExternalEmail}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserProvAccounts.UserProvAccount_ExternalFirstName}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserProvAccounts.UserProvAccount_ExternalLastName}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserProvAccounts.UserProvAccount_ExternalUserId}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserProvAccounts.UserProvAccount_ExternalUsername}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserProvAccounts.UserProvAccount_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserProvAccounts.UserProvAccount_IsDeleted}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserProvAccounts.UserProvAccount_IsKnownLink}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserProvAccounts.UserProvAccount_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserProvAccounts.UserProvAccount_LinkState}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserProvAccounts.UserProvAccount_Name}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserProvAccounts.UserProvAccount_Status}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserProvAccounts.UserProvAccount_SystemModstamp}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="UserProvAccount"/>
							<VBox>					
							<Text text="{dataModel>UserProvAccount_CreatedDate/UserProvAccount_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>UserProvAccount_DeletedDate/UserProvAccount_DeletedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>UserProvAccount_ExternalEmail/UserProvAccount_ExternalEmail',type:'sap.ui.model.type.String'}" linkText="{i18n>UserProvAccount.UserProvAccount_ExternalEmail/UserProvAccount_ExternalEmail}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>UserProvAccount_ExternalFirstName/UserProvAccount_ExternalFirstName',type:'sap.ui.model.type.String'}" linkText="{i18n>UserProvAccount.UserProvAccount_ExternalFirstName/UserProvAccount_ExternalFirstName}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>UserProvAccount_ExternalLastName/UserProvAccount_ExternalLastName',type:'sap.ui.model.type.String'}" linkText="{i18n>UserProvAccount.UserProvAccount_ExternalLastName/UserProvAccount_ExternalLastName}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>UserProvAccount_ExternalUserId/UserProvAccount_ExternalUserId',type:'sap.ui.model.type.String'}" linkText="{i18n>UserProvAccount.UserProvAccount_ExternalUserId/UserProvAccount_ExternalUserId}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>UserProvAccount_ExternalUsername/UserProvAccount_ExternalUsername',type:'sap.ui.model.type.String'}" linkText="{i18n>UserProvAccount.UserProvAccount_ExternalUsername/UserProvAccount_ExternalUsername}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>UserProvAccount_Id/UserProvAccount_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>UserProvAccount.UserProvAccount_Id/UserProvAccount_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>UserProvAccount_IsDeleted/UserProvAccount_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>UserProvAccount_IsKnownLink/UserProvAccount_IsKnownLink',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<Text text="{dataModel>UserProvAccount_LastModifiedDate/UserProvAccount_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>UserProvAccount_LinkState/UserProvAccount_LinkState',type:'sap.ui.model.type.String'}" linkText="{i18n>UserProvAccount.UserProvAccount_LinkState/UserProvAccount_LinkState}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>UserProvAccount_Name/UserProvAccount_Name',type:'sap.ui.model.type.String'}" linkText="{i18n>UserProvAccount.UserProvAccount_Name/UserProvAccount_Name}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>UserProvAccount_Status/UserProvAccount_Status',type:'sap.ui.model.type.String'}" linkText="{i18n>UserProvAccount.UserProvAccount_Status/UserProvAccount_Status}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>UserProvAccount_SystemModstamp/UserProvAccount_SystemModstamp}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>