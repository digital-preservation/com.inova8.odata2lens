<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>ForecastingTypeToCategorys.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/ForecastingTypeToCategory' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>ForecastingTypeToCategorys.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ForecastingTypeToCategorys.ForecastingTypeToCategory_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ForecastingTypeToCategorys.ForecastingTypeToCategory_DisplayPosition}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ForecastingTypeToCategorys.ForecastingTypeToCategory_ForecastingItemCategory}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ForecastingTypeToCategorys.ForecastingTypeToCategory_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ForecastingTypeToCategorys.ForecastingTypeToCategory_IsAdjustable}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ForecastingTypeToCategorys.ForecastingTypeToCategory_IsDeleted}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ForecastingTypeToCategorys.ForecastingTypeToCategory_IsOwnerAdjustable}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ForecastingTypeToCategorys.ForecastingTypeToCategory_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ForecastingTypeToCategorys.ForecastingTypeToCategory_SystemModstamp}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="ForecastingTypeToCategory"/>
							<VBox>					
							<Text text="{dataModel>ForecastingTypeToCategory_CreatedDate/ForecastingTypeToCategory_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>ForecastingTypeToCategory_DisplayPosition/ForecastingTypeToCategory_DisplayPosition',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ForecastingTypeToCategory_ForecastingItemCategory/ForecastingTypeToCategory_ForecastingItemCategory',type:'sap.ui.model.type.String'}" linkText="{i18n>ForecastingTypeToCategory.ForecastingTypeToCategory_ForecastingItemCategory/ForecastingTypeToCategory_ForecastingItemCategory}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ForecastingTypeToCategory_Id/ForecastingTypeToCategory_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>ForecastingTypeToCategory.ForecastingTypeToCategory_Id/ForecastingTypeToCategory_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>ForecastingTypeToCategory_IsAdjustable/ForecastingTypeToCategory_IsAdjustable',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>ForecastingTypeToCategory_IsDeleted/ForecastingTypeToCategory_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>ForecastingTypeToCategory_IsOwnerAdjustable/ForecastingTypeToCategory_IsOwnerAdjustable',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<Text text="{dataModel>ForecastingTypeToCategory_LastModifiedDate/ForecastingTypeToCategory_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>ForecastingTypeToCategory_SystemModstamp/ForecastingTypeToCategory_SystemModstamp}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>