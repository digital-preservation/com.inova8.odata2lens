<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>Names.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/Name' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>Names.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Names.Name_Alias}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Names.Name_Email}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Names.Name_FirstName}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Names.Name_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Names.Name_IsActive}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Names.Name_LastName}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Names.Name_LastReferencedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Names.Name_LastViewedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Names.Name_Name}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Names.Name_NameOrAlias}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Names.Name_Phone}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Names.Name_Title}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Names.Name_Type}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Names.Name_Username}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="Name"/>
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Name_Alias/Name_Alias',type:'sap.ui.model.type.String'}" linkText="{i18n>Name.Name_Alias/Name_Alias}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Name_Email/Name_Email',type:'sap.ui.model.type.String'}" linkText="{i18n>Name.Name_Email/Name_Email}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Name_FirstName/Name_FirstName',type:'sap.ui.model.type.String'}" linkText="{i18n>Name.Name_FirstName/Name_FirstName}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Name_Id/Name_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>Name.Name_Id/Name_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>Name_IsActive/Name_IsActive',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Name_LastName/Name_LastName',type:'sap.ui.model.type.String'}" linkText="{i18n>Name.Name_LastName/Name_LastName}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>Name_LastReferencedDate/Name_LastReferencedDate}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>Name_LastViewedDate/Name_LastViewedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Name_Name/Name_Name',type:'sap.ui.model.type.String'}" linkText="{i18n>Name.Name_Name/Name_Name}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Name_NameOrAlias/Name_NameOrAlias',type:'sap.ui.model.type.String'}" linkText="{i18n>Name.Name_NameOrAlias/Name_NameOrAlias}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Name_Phone/Name_Phone',type:'sap.ui.model.type.String'}" linkText="{i18n>Name.Name_Phone/Name_Phone}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Name_Title/Name_Title',type:'sap.ui.model.type.String'}" linkText="{i18n>Name.Name_Title/Name_Title}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Name_Type/Name_Type',type:'sap.ui.model.type.String'}" linkText="{i18n>Name.Name_Type/Name_Type}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Name_Username/Name_Username',type:'sap.ui.model.type.String'}" linkText="{i18n>Name.Name_Username/Name_Username}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>