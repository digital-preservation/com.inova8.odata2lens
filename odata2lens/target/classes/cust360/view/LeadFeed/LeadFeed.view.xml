<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>LeadFeed.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>LeadFeed.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="LeadFeed"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="LeadFeed"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>LeadFeed.LeadFeed_Body}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>LeadFeed_Body/LeadFeed_Body',type:'sap.ui.model.type.String' }" linkText="{i18n>LeadFeed.LeadFeed_Body/LeadFeed_Body}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>LeadFeed.LeadFeed_CommentCount}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>LeadFeed_CommentCount/LeadFeed_CommentCount',type:'sap.ui.model.type.Integer' }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>LeadFeed.LeadFeed_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>LeadFeed_CreatedById/sfdc_LeadFeed_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>LeadFeed.LeadFeed_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>LeadFeed_CreatedDate/LeadFeed_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>LeadFeed.LeadFeed_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>LeadFeed_Id/LeadFeed_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>LeadFeed.LeadFeed_Id/LeadFeed_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>LeadFeed.LeadFeed_InsertedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>LeadFeed_InsertedById/sfdc_LeadFeed_InsertedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>LeadFeed.LeadFeed_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>LeadFeed_IsDeleted/LeadFeed_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>LeadFeed.LeadFeed_IsRichText}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>LeadFeed_IsRichText/LeadFeed_IsRichText',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>LeadFeed.LeadFeed_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>LeadFeed_LastModifiedDate/LeadFeed_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>LeadFeed.LeadFeed_LikeCount}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>LeadFeed_LikeCount/LeadFeed_LikeCount',type:'sap.ui.model.type.Integer' }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>LeadFeed.LeadFeed_LinkUrl}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>LeadFeed_LinkUrl/LeadFeed_LinkUrl',type:'sap.ui.model.type.String' }" linkText="{i18n>LeadFeed.LeadFeed_LinkUrl/LeadFeed_LinkUrl}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>LeadFeed.LeadFeed_ParentId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>LeadFeed_ParentId/sfdc_LeadFeed_ParentId" entitySet="Lead"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>LeadFeed.LeadFeed_RelatedRecordId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>LeadFeed_RelatedRecordId/sfdc_LeadFeed_RelatedRecordId" entitySet="ContentVersion"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>LeadFeed.LeadFeed_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>LeadFeed_SystemModstamp/LeadFeed_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>LeadFeed.LeadFeed_Title}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>LeadFeed_Title/LeadFeed_Title',type:'sap.ui.model.type.String' }" linkText="{i18n>LeadFeed.LeadFeed_Title/LeadFeed_Title}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>LeadFeed.LeadFeed_Type}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>LeadFeed_Type/LeadFeed_Type',type:'sap.ui.model.type.String' }" linkText="{i18n>LeadFeed.LeadFeed_Type/LeadFeed_Type}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>LeadFeed.LeadFeed_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>LeadFeed_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>LeadFeed.LeadFeed_InsertedById}" required="false"/> 
						<rdf:ListResource path="dataModel>LeadFeed_InsertedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>LeadFeed.LeadFeed_ParentId}" required="false"/> 
						<rdf:ListResource path="dataModel>LeadFeed_ParentId" entitySet="Lead"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>LeadFeed.LeadFeed_RelatedRecordId}" required="false"/> 
						<rdf:ListResource path="dataModel>LeadFeed_RelatedRecordId" entitySet="ContentVersion"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>