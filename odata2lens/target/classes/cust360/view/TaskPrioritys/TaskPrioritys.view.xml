<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>TaskPrioritys.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/TaskPriority' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>TaskPrioritys.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>TaskPrioritys.TaskPriority_ApiName}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>TaskPrioritys.TaskPriority_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>TaskPrioritys.TaskPriority_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>TaskPrioritys.TaskPriority_IsDefault}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>TaskPrioritys.TaskPriority_IsHighPriority}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>TaskPrioritys.TaskPriority_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>TaskPrioritys.TaskPriority_MasterLabel}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>TaskPrioritys.TaskPriority_SortOrder}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>TaskPrioritys.TaskPriority_SystemModstamp}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="TaskPriority"/>
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>TaskPriority_ApiName/TaskPriority_ApiName',type:'sap.ui.model.type.String'}" linkText="{i18n>TaskPriority.TaskPriority_ApiName/TaskPriority_ApiName}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>TaskPriority_CreatedDate/TaskPriority_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>TaskPriority_Id/TaskPriority_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>TaskPriority.TaskPriority_Id/TaskPriority_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>TaskPriority_IsDefault/TaskPriority_IsDefault',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>TaskPriority_IsHighPriority/TaskPriority_IsHighPriority',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<Text text="{dataModel>TaskPriority_LastModifiedDate/TaskPriority_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>TaskPriority_MasterLabel/TaskPriority_MasterLabel',type:'sap.ui.model.type.String'}" linkText="{i18n>TaskPriority.TaskPriority_MasterLabel/TaskPriority_MasterLabel}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>TaskPriority_SortOrder/TaskPriority_SortOrder',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>TaskPriority_SystemModstamp/TaskPriority_SystemModstamp}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>