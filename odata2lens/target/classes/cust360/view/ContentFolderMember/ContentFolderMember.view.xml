<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>ContentFolderMember.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>ContentFolderMember.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="ContentFolderMember"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="ContentFolderMember"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentFolderMember.ContentFolderMember_ChildRecordId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ContentFolderMember_ChildRecordId/sfdc_ContentFolderMember_ChildRecordId" entitySet="ContentDocument"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentFolderMember.ContentFolderMember_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ContentFolderMember_CreatedById/sfdc_ContentFolderMember_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentFolderMember.ContentFolderMember_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>ContentFolderMember_CreatedDate/ContentFolderMember_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentFolderMember.ContentFolderMember_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ContentFolderMember_Id/ContentFolderMember_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>ContentFolderMember.ContentFolderMember_Id/ContentFolderMember_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentFolderMember.ContentFolderMember_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>ContentFolderMember_IsDeleted/ContentFolderMember_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentFolderMember.ContentFolderMember_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ContentFolderMember_LastModifiedById/sfdc_ContentFolderMember_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentFolderMember.ContentFolderMember_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>ContentFolderMember_LastModifiedDate/ContentFolderMember_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentFolderMember.ContentFolderMember_ParentContentFolderId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ContentFolderMember_ParentContentFolderId/sfdc_ContentFolderMember_ParentContentFolderId" entitySet="ContentFolder"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentFolderMember.ContentFolderMember_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>ContentFolderMember_SystemModstamp/ContentFolderMember_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentFolderMember.ContentFolderMember_ChildRecordId}" required="false"/> 
						<rdf:ListResource path="dataModel>ContentFolderMember_ChildRecordId" entitySet="ContentDocument"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentFolderMember.ContentFolderMember_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>ContentFolderMember_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentFolderMember.ContentFolderMember_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>ContentFolderMember_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentFolderMember.ContentFolderMember_ParentContentFolderId}" required="false"/> 
						<rdf:ListResource path="dataModel>ContentFolderMember_ParentContentFolderId" entitySet="ContentFolder"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>