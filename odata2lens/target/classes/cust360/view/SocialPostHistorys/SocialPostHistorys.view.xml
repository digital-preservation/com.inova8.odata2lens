<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>SocialPostHistorys.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/SocialPostHistory' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>SocialPostHistorys.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>SocialPostHistorys.SocialPostHistory_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>SocialPostHistorys.SocialPostHistory_Field}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>SocialPostHistorys.SocialPostHistory_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>SocialPostHistorys.SocialPostHistory_IsDeleted}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>SocialPostHistorys.SocialPostHistory_NewValue}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>SocialPostHistorys.SocialPostHistory_OldValue}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="SocialPostHistory"/>
							<VBox>					
							<Text text="{dataModel>SocialPostHistory_CreatedDate/SocialPostHistory_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>SocialPostHistory_Field/SocialPostHistory_Field',type:'sap.ui.model.type.String'}" linkText="{i18n>SocialPostHistory.SocialPostHistory_Field/SocialPostHistory_Field}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>SocialPostHistory_Id/SocialPostHistory_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>SocialPostHistory.SocialPostHistory_Id/SocialPostHistory_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>SocialPostHistory_IsDeleted/SocialPostHistory_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>SocialPostHistory_NewValue/SocialPostHistory_NewValue',type:'sap.ui.model.type.String'}" linkText="{i18n>SocialPostHistory.SocialPostHistory_NewValue/SocialPostHistory_NewValue}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>SocialPostHistory_OldValue/SocialPostHistory_OldValue',type:'sap.ui.model.type.String'}" linkText="{i18n>SocialPostHistory.SocialPostHistory_OldValue/SocialPostHistory_OldValue}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>