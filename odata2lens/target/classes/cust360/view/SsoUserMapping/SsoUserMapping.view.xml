<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>SsoUserMapping.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>SsoUserMapping.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="SsoUserMapping"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="SsoUserMapping"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SsoUserMapping.SsoUserMapping_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>SsoUserMapping_CreatedById/sfdc_SsoUserMapping_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SsoUserMapping.SsoUserMapping_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>SsoUserMapping_CreatedDate/SsoUserMapping_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SsoUserMapping.SsoUserMapping_HubUserId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>SsoUserMapping_HubUserId/sfdc_SsoUserMapping_HubUserId" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SsoUserMapping.SsoUserMapping_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>SsoUserMapping_Id/SsoUserMapping_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>SsoUserMapping.SsoUserMapping_Id/SsoUserMapping_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SsoUserMapping.SsoUserMapping_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>SsoUserMapping_IsDeleted/SsoUserMapping_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SsoUserMapping.SsoUserMapping_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>SsoUserMapping_LastModifiedById/sfdc_SsoUserMapping_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SsoUserMapping.SsoUserMapping_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>SsoUserMapping_LastModifiedDate/SsoUserMapping_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SsoUserMapping.SsoUserMapping_MemberUserId}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>SsoUserMapping_MemberUserId/SsoUserMapping_MemberUserId',type:'sap.ui.model.type.String' }" linkText="{i18n>SsoUserMapping.SsoUserMapping_MemberUserId/SsoUserMapping_MemberUserId}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SsoUserMapping.SsoUserMapping_MemberUserName}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>SsoUserMapping_MemberUserName/SsoUserMapping_MemberUserName',type:'sap.ui.model.type.String' }" linkText="{i18n>SsoUserMapping.SsoUserMapping_MemberUserName/SsoUserMapping_MemberUserName}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SsoUserMapping.SsoUserMapping_Name}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>SsoUserMapping_Name/SsoUserMapping_Name',type:'sap.ui.model.type.String' }" linkText="{i18n>SsoUserMapping.SsoUserMapping_Name/SsoUserMapping_Name}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SsoUserMapping.SsoUserMapping_ParentId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>SsoUserMapping_ParentId/sfdc_SsoUserMapping_ParentId" entitySet="EnvironmentHubMember"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SsoUserMapping.SsoUserMapping_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>SsoUserMapping_SystemModstamp/SsoUserMapping_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SsoUserMapping.SsoUserMapping_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>SsoUserMapping_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SsoUserMapping.SsoUserMapping_HubUserId}" required="false"/> 
						<rdf:ListResource path="dataModel>SsoUserMapping_HubUserId" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SsoUserMapping.SsoUserMapping_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>SsoUserMapping_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SsoUserMapping.SsoUserMapping_ParentId}" required="false"/> 
						<rdf:ListResource path="dataModel>SsoUserMapping_ParentId" entitySet="EnvironmentHubMember"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>