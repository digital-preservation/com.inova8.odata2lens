<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>EventLogFiles.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/EventLogFile' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>EventLogFiles.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EventLogFiles.EventLogFile_ApiVersion}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EventLogFiles.EventLogFile_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EventLogFiles.EventLogFile_EventType}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EventLogFiles.EventLogFile_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EventLogFiles.EventLogFile_IsDeleted}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EventLogFiles.EventLogFile_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EventLogFiles.EventLogFile_LogDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EventLogFiles.EventLogFile_LogFile}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EventLogFiles.EventLogFile_LogFileContentType}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EventLogFiles.EventLogFile_LogFileFieldNames}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EventLogFiles.EventLogFile_LogFileFieldTypes}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EventLogFiles.EventLogFile_LogFileLength}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EventLogFiles.EventLogFile_SystemModstamp}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="EventLogFile"/>
							<VBox>					
							<Text text="{path:'dataModel>EventLogFile_ApiVersion/EventLogFile_ApiVersion',type:'sap.ui.model.type.Float', formatOptions:{'style':'short','maxFractionDigits':2} }"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>EventLogFile_CreatedDate/EventLogFile_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>EventLogFile_EventType/EventLogFile_EventType',type:'sap.ui.model.type.String'}" linkText="{i18n>EventLogFile.EventLogFile_EventType/EventLogFile_EventType}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>EventLogFile_Id/EventLogFile_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>EventLogFile.EventLogFile_Id/EventLogFile_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>EventLogFile_IsDeleted/EventLogFile_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<Text text="{dataModel>EventLogFile_LastModifiedDate/EventLogFile_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>EventLogFile_LogDate/EventLogFile_LogDate}"/>
							</VBox>							
							<VBox>					
								<Text text="{dataModel>EventLogFile_LogFile/EventLogFile_LogFile}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>EventLogFile_LogFileContentType/EventLogFile_LogFileContentType',type:'sap.ui.model.type.String'}" linkText="{i18n>EventLogFile.EventLogFile_LogFileContentType/EventLogFile_LogFileContentType}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>EventLogFile_LogFileFieldNames/EventLogFile_LogFileFieldNames',type:'sap.ui.model.type.String'}" linkText="{i18n>EventLogFile.EventLogFile_LogFileFieldNames/EventLogFile_LogFileFieldNames}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>EventLogFile_LogFileFieldTypes/EventLogFile_LogFileFieldTypes',type:'sap.ui.model.type.String'}" linkText="{i18n>EventLogFile.EventLogFile_LogFileFieldTypes/EventLogFile_LogFileFieldTypes}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>EventLogFile_LogFileLength/EventLogFile_LogFileLength',type:'sap.ui.model.type.Float', formatOptions:{'style':'short','maxFractionDigits':2} }"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>EventLogFile_SystemModstamp/EventLogFile_SystemModstamp}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>