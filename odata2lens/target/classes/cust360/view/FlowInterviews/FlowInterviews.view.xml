<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>FlowInterviews.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/FlowInterview' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>FlowInterviews.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>FlowInterviews.FlowInterview_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>FlowInterviews.FlowInterview_CurrentElement}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>FlowInterviews.FlowInterview_Guid}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>FlowInterviews.FlowInterview_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>FlowInterviews.FlowInterview_InterviewLabel}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>FlowInterviews.FlowInterview_IsDeleted}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>FlowInterviews.FlowInterview_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>FlowInterviews.FlowInterview_Name}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>FlowInterviews.FlowInterview_PauseLabel}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>FlowInterviews.FlowInterview_SystemModstamp}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="FlowInterview"/>
							<VBox>					
							<Text text="{dataModel>FlowInterview_CreatedDate/FlowInterview_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>FlowInterview_CurrentElement/FlowInterview_CurrentElement',type:'sap.ui.model.type.String'}" linkText="{i18n>FlowInterview.FlowInterview_CurrentElement/FlowInterview_CurrentElement}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>FlowInterview_Guid/FlowInterview_Guid',type:'sap.ui.model.type.String'}" linkText="{i18n>FlowInterview.FlowInterview_Guid/FlowInterview_Guid}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>FlowInterview_Id/FlowInterview_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>FlowInterview.FlowInterview_Id/FlowInterview_Id}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>FlowInterview_InterviewLabel/FlowInterview_InterviewLabel',type:'sap.ui.model.type.String'}" linkText="{i18n>FlowInterview.FlowInterview_InterviewLabel/FlowInterview_InterviewLabel}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>FlowInterview_IsDeleted/FlowInterview_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<Text text="{dataModel>FlowInterview_LastModifiedDate/FlowInterview_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>FlowInterview_Name/FlowInterview_Name',type:'sap.ui.model.type.String'}" linkText="{i18n>FlowInterview.FlowInterview_Name/FlowInterview_Name}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>FlowInterview_PauseLabel/FlowInterview_PauseLabel',type:'sap.ui.model.type.String'}" linkText="{i18n>FlowInterview.FlowInterview_PauseLabel/FlowInterview_PauseLabel}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>FlowInterview_SystemModstamp/FlowInterview_SystemModstamp}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>