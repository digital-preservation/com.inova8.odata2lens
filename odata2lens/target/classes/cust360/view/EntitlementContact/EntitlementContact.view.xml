<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>EntitlementContact.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>EntitlementContact.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="EntitlementContact"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="EntitlementContact"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EntitlementContact.EntitlementContact_ContactId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>EntitlementContact_ContactId/sfdc_EntitlementContact_ContactId" entitySet="Contact"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EntitlementContact.EntitlementContact_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>EntitlementContact_CreatedById/sfdc_EntitlementContact_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EntitlementContact.EntitlementContact_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>EntitlementContact_CreatedDate/EntitlementContact_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EntitlementContact.EntitlementContact_EntitlementId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>EntitlementContact_EntitlementId/sfdc_EntitlementContact_EntitlementId" entitySet="Entitlement"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EntitlementContact.EntitlementContact_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>EntitlementContact_Id/EntitlementContact_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>EntitlementContact.EntitlementContact_Id/EntitlementContact_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EntitlementContact.EntitlementContact_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>EntitlementContact_IsDeleted/EntitlementContact_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EntitlementContact.EntitlementContact_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>EntitlementContact_LastModifiedById/sfdc_EntitlementContact_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EntitlementContact.EntitlementContact_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>EntitlementContact_LastModifiedDate/EntitlementContact_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EntitlementContact.EntitlementContact_Name}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>EntitlementContact_Name/EntitlementContact_Name',type:'sap.ui.model.type.String' }" linkText="{i18n>EntitlementContact.EntitlementContact_Name/EntitlementContact_Name}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EntitlementContact.EntitlementContact_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>EntitlementContact_SystemModstamp/EntitlementContact_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EntitlementContact.EntitlementContact_ContactId}" required="false"/> 
						<rdf:ListResource path="dataModel>EntitlementContact_ContactId" entitySet="Contact"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EntitlementContact.EntitlementContact_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>EntitlementContact_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EntitlementContact.EntitlementContact_EntitlementId}" required="false"/> 
						<rdf:ListResource path="dataModel>EntitlementContact_EntitlementId" entitySet="Entitlement"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EntitlementContact.EntitlementContact_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>EntitlementContact_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>