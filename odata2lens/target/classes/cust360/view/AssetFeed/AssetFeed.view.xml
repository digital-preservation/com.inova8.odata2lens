<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>AssetFeed.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>AssetFeed.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="AssetFeed"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="AssetFeed"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AssetFeed.AssetFeed_Body}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>AssetFeed_Body/AssetFeed_Body',type:'sap.ui.model.type.String' }" linkText="{i18n>AssetFeed.AssetFeed_Body/AssetFeed_Body}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AssetFeed.AssetFeed_CommentCount}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>AssetFeed_CommentCount/AssetFeed_CommentCount',type:'sap.ui.model.type.Integer' }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AssetFeed.AssetFeed_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>AssetFeed_CreatedById/sfdc_AssetFeed_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AssetFeed.AssetFeed_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>AssetFeed_CreatedDate/AssetFeed_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AssetFeed.AssetFeed_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>AssetFeed_Id/AssetFeed_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>AssetFeed.AssetFeed_Id/AssetFeed_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AssetFeed.AssetFeed_InsertedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>AssetFeed_InsertedById/sfdc_AssetFeed_InsertedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AssetFeed.AssetFeed_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>AssetFeed_IsDeleted/AssetFeed_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AssetFeed.AssetFeed_IsRichText}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>AssetFeed_IsRichText/AssetFeed_IsRichText',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AssetFeed.AssetFeed_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>AssetFeed_LastModifiedDate/AssetFeed_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AssetFeed.AssetFeed_LikeCount}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>AssetFeed_LikeCount/AssetFeed_LikeCount',type:'sap.ui.model.type.Integer' }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AssetFeed.AssetFeed_LinkUrl}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>AssetFeed_LinkUrl/AssetFeed_LinkUrl',type:'sap.ui.model.type.String' }" linkText="{i18n>AssetFeed.AssetFeed_LinkUrl/AssetFeed_LinkUrl}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AssetFeed.AssetFeed_ParentId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>AssetFeed_ParentId/sfdc_AssetFeed_ParentId" entitySet="Asset"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AssetFeed.AssetFeed_RelatedRecordId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>AssetFeed_RelatedRecordId/sfdc_AssetFeed_RelatedRecordId" entitySet="ContentVersion"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AssetFeed.AssetFeed_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>AssetFeed_SystemModstamp/AssetFeed_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AssetFeed.AssetFeed_Title}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>AssetFeed_Title/AssetFeed_Title',type:'sap.ui.model.type.String' }" linkText="{i18n>AssetFeed.AssetFeed_Title/AssetFeed_Title}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AssetFeed.AssetFeed_Type}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>AssetFeed_Type/AssetFeed_Type',type:'sap.ui.model.type.String' }" linkText="{i18n>AssetFeed.AssetFeed_Type/AssetFeed_Type}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AssetFeed.AssetFeed_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>AssetFeed_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AssetFeed.AssetFeed_InsertedById}" required="false"/> 
						<rdf:ListResource path="dataModel>AssetFeed_InsertedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AssetFeed.AssetFeed_ParentId}" required="false"/> 
						<rdf:ListResource path="dataModel>AssetFeed_ParentId" entitySet="Asset"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AssetFeed.AssetFeed_RelatedRecordId}" required="false"/> 
						<rdf:ListResource path="dataModel>AssetFeed_RelatedRecordId" entitySet="ContentVersion"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>