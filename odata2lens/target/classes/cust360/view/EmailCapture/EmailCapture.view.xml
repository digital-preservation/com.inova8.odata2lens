<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>EmailCapture.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>EmailCapture.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="EmailCapture"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="EmailCapture"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EmailCapture.EmailCapture_CaptureDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>EmailCapture_CaptureDate/EmailCapture_CaptureDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EmailCapture.EmailCapture_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>EmailCapture_CreatedById/sfdc_EmailCapture_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EmailCapture.EmailCapture_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>EmailCapture_CreatedDate/EmailCapture_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EmailCapture.EmailCapture_FromPattern}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>EmailCapture_FromPattern/EmailCapture_FromPattern',type:'sap.ui.model.type.String' }" linkText="{i18n>EmailCapture.EmailCapture_FromPattern/EmailCapture_FromPattern}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EmailCapture.EmailCapture_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>EmailCapture_Id/EmailCapture_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>EmailCapture.EmailCapture_Id/EmailCapture_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EmailCapture.EmailCapture_IsActive}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>EmailCapture_IsActive/EmailCapture_IsActive',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EmailCapture.EmailCapture_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>EmailCapture_IsDeleted/EmailCapture_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EmailCapture.EmailCapture_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>EmailCapture_LastModifiedById/sfdc_EmailCapture_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EmailCapture.EmailCapture_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>EmailCapture_LastModifiedDate/EmailCapture_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EmailCapture.EmailCapture_RawMessage}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>EmailCapture_RawMessage/EmailCapture_RawMessage}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EmailCapture.EmailCapture_RawMessageLength}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>EmailCapture_RawMessageLength/EmailCapture_RawMessageLength',type:'sap.ui.model.type.Integer' }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EmailCapture.EmailCapture_Recipient}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>EmailCapture_Recipient/EmailCapture_Recipient',type:'sap.ui.model.type.String' }" linkText="{i18n>EmailCapture.EmailCapture_Recipient/EmailCapture_Recipient}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EmailCapture.EmailCapture_Sender}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>EmailCapture_Sender/EmailCapture_Sender',type:'sap.ui.model.type.String' }" linkText="{i18n>EmailCapture.EmailCapture_Sender/EmailCapture_Sender}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EmailCapture.EmailCapture_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>EmailCapture_SystemModstamp/EmailCapture_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EmailCapture.EmailCapture_ToPattern}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>EmailCapture_ToPattern/EmailCapture_ToPattern',type:'sap.ui.model.type.String' }" linkText="{i18n>EmailCapture.EmailCapture_ToPattern/EmailCapture_ToPattern}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EmailCapture.EmailCapture_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>EmailCapture_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EmailCapture.EmailCapture_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>EmailCapture_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>