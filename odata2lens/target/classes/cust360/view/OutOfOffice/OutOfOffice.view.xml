<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>OutOfOffice.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>OutOfOffice.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="OutOfOffice"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="OutOfOffice"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OutOfOffice.OutOfOffice_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>OutOfOffice_CreatedById/sfdc_OutOfOffice_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OutOfOffice.OutOfOffice_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>OutOfOffice_CreatedDate/OutOfOffice_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OutOfOffice.OutOfOffice_EndDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>OutOfOffice_EndDate/OutOfOffice_EndDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OutOfOffice.OutOfOffice_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>OutOfOffice_Id/OutOfOffice_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>OutOfOffice.OutOfOffice_Id/OutOfOffice_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OutOfOffice.OutOfOffice_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>OutOfOffice_IsDeleted/OutOfOffice_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OutOfOffice.OutOfOffice_IsEnabled}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>OutOfOffice_IsEnabled/OutOfOffice_IsEnabled',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OutOfOffice.OutOfOffice_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>OutOfOffice_LastModifiedById/sfdc_OutOfOffice_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OutOfOffice.OutOfOffice_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>OutOfOffice_LastModifiedDate/OutOfOffice_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OutOfOffice.OutOfOffice_Message}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>OutOfOffice_Message/OutOfOffice_Message',type:'sap.ui.model.type.String' }" linkText="{i18n>OutOfOffice.OutOfOffice_Message/OutOfOffice_Message}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OutOfOffice.OutOfOffice_StartDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>OutOfOffice_StartDate/OutOfOffice_StartDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OutOfOffice.OutOfOffice_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>OutOfOffice_SystemModstamp/OutOfOffice_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OutOfOffice.OutOfOffice_UserId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>OutOfOffice_UserId/sfdc_OutOfOffice_UserId" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OutOfOffice.OutOfOffice_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>OutOfOffice_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OutOfOffice.OutOfOffice_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>OutOfOffice_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OutOfOffice.OutOfOffice_UserId}" required="false"/> 
						<rdf:ListResource path="dataModel>OutOfOffice_UserId" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>