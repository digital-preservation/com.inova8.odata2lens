<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>MacroInstruction.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>MacroInstruction.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="MacroInstruction"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="MacroInstruction"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>MacroInstruction.MacroInstruction_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>MacroInstruction_CreatedById/sfdc_MacroInstruction_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>MacroInstruction.MacroInstruction_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>MacroInstruction_CreatedDate/MacroInstruction_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>MacroInstruction.MacroInstruction_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>MacroInstruction_Id/MacroInstruction_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>MacroInstruction.MacroInstruction_Id/MacroInstruction_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>MacroInstruction.MacroInstruction_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>MacroInstruction_IsDeleted/MacroInstruction_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>MacroInstruction.MacroInstruction_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>MacroInstruction_LastModifiedById/sfdc_MacroInstruction_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>MacroInstruction.MacroInstruction_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>MacroInstruction_LastModifiedDate/MacroInstruction_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>MacroInstruction.MacroInstruction_MacroId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>MacroInstruction_MacroId/sfdc_MacroInstruction_MacroId" entitySet="Macro"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>MacroInstruction.MacroInstruction_Name}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>MacroInstruction_Name/MacroInstruction_Name',type:'sap.ui.model.type.String' }" linkText="{i18n>MacroInstruction.MacroInstruction_Name/MacroInstruction_Name}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>MacroInstruction.MacroInstruction_Operation}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>MacroInstruction_Operation/MacroInstruction_Operation',type:'sap.ui.model.type.String' }" linkText="{i18n>MacroInstruction.MacroInstruction_Operation/MacroInstruction_Operation}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>MacroInstruction.MacroInstruction_SortOrder}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>MacroInstruction_SortOrder/MacroInstruction_SortOrder',type:'sap.ui.model.type.Integer' }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>MacroInstruction.MacroInstruction_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>MacroInstruction_SystemModstamp/MacroInstruction_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>MacroInstruction.MacroInstruction_Target}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>MacroInstruction_Target/MacroInstruction_Target',type:'sap.ui.model.type.String' }" linkText="{i18n>MacroInstruction.MacroInstruction_Target/MacroInstruction_Target}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>MacroInstruction.MacroInstruction_Value}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>MacroInstruction_Value/MacroInstruction_Value',type:'sap.ui.model.type.String' }" linkText="{i18n>MacroInstruction.MacroInstruction_Value/MacroInstruction_Value}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>MacroInstruction.MacroInstruction_ValueRecord}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>MacroInstruction_ValueRecord/MacroInstruction_ValueRecord',type:'sap.ui.model.type.String' }" linkText="{i18n>MacroInstruction.MacroInstruction_ValueRecord/MacroInstruction_ValueRecord}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>MacroInstruction.MacroInstruction_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>MacroInstruction_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>MacroInstruction.MacroInstruction_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>MacroInstruction_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>MacroInstruction.MacroInstruction_MacroId}" required="false"/> 
						<rdf:ListResource path="dataModel>MacroInstruction_MacroId" entitySet="Macro"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>