					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CaseFeed.CaseFeed_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>CaseFeed_Id/CaseFeed_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>CaseFeed.CaseFeed_Id/CaseFeed_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CaseFeed.CaseFeed_InsertedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>CaseFeed_InsertedById/sfdc_CaseFeed_InsertedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CaseFeed.CaseFeed_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>CaseFeed_IsDeleted/CaseFeed_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CaseFeed.CaseFeed_IsRichText}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>CaseFeed_IsRichText/CaseFeed_IsRichText',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CaseFeed.CaseFeed_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>CaseFeed_LastModifiedDate/CaseFeed_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CaseFeed.CaseFeed_LikeCount}" required="false"/> 
						<Text text=""/>
