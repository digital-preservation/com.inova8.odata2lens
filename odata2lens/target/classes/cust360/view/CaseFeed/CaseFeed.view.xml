						<rdf:ListResource path="dataModel>CaseFeed_ParentId" entitySet="Case"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CaseFeed.CaseFeed_RelatedRecordId}" required="false"/> 
						<rdf:ListResource path="dataModel>CaseFeed_RelatedRecordId" entitySet="ContentVersion"/>
