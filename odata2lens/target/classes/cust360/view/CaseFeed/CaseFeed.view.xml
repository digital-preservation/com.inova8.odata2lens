					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CaseFeed.CaseFeed_Type}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>CaseFeed_Type/CaseFeed_Type',type:'sap.ui.model.type.String' }" linkText="{i18n>CaseFeed.CaseFeed_Type/CaseFeed_Type}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CaseFeed.CaseFeed_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>CaseFeed_CreatedById" entitySet="User"/>
						<Text text=""/>
