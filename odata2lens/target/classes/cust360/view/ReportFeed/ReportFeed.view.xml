<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>ReportFeed.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>ReportFeed.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="ReportFeed"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="ReportFeed"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ReportFeed.ReportFeed_Body}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ReportFeed_Body/ReportFeed_Body',type:'sap.ui.model.type.String' }" linkText="{i18n>ReportFeed.ReportFeed_Body/ReportFeed_Body}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ReportFeed.ReportFeed_CommentCount}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>ReportFeed_CommentCount/ReportFeed_CommentCount',type:'sap.ui.model.type.Integer' }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ReportFeed.ReportFeed_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ReportFeed_CreatedById/sfdc_ReportFeed_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ReportFeed.ReportFeed_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>ReportFeed_CreatedDate/ReportFeed_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ReportFeed.ReportFeed_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ReportFeed_Id/ReportFeed_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>ReportFeed.ReportFeed_Id/ReportFeed_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ReportFeed.ReportFeed_InsertedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ReportFeed_InsertedById/sfdc_ReportFeed_InsertedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ReportFeed.ReportFeed_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>ReportFeed_IsDeleted/ReportFeed_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ReportFeed.ReportFeed_IsRichText}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>ReportFeed_IsRichText/ReportFeed_IsRichText',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ReportFeed.ReportFeed_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>ReportFeed_LastModifiedDate/ReportFeed_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ReportFeed.ReportFeed_LikeCount}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>ReportFeed_LikeCount/ReportFeed_LikeCount',type:'sap.ui.model.type.Integer' }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ReportFeed.ReportFeed_LinkUrl}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ReportFeed_LinkUrl/ReportFeed_LinkUrl',type:'sap.ui.model.type.String' }" linkText="{i18n>ReportFeed.ReportFeed_LinkUrl/ReportFeed_LinkUrl}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ReportFeed.ReportFeed_ParentId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ReportFeed_ParentId/sfdc_ReportFeed_ParentId" entitySet="Report"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ReportFeed.ReportFeed_RelatedRecordId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ReportFeed_RelatedRecordId/sfdc_ReportFeed_RelatedRecordId" entitySet="ContentVersion"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ReportFeed.ReportFeed_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>ReportFeed_SystemModstamp/ReportFeed_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ReportFeed.ReportFeed_Title}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ReportFeed_Title/ReportFeed_Title',type:'sap.ui.model.type.String' }" linkText="{i18n>ReportFeed.ReportFeed_Title/ReportFeed_Title}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ReportFeed.ReportFeed_Type}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ReportFeed_Type/ReportFeed_Type',type:'sap.ui.model.type.String' }" linkText="{i18n>ReportFeed.ReportFeed_Type/ReportFeed_Type}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ReportFeed.ReportFeed_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>ReportFeed_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ReportFeed.ReportFeed_InsertedById}" required="false"/> 
						<rdf:ListResource path="dataModel>ReportFeed_InsertedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ReportFeed.ReportFeed_ParentId}" required="false"/> 
						<rdf:ListResource path="dataModel>ReportFeed_ParentId" entitySet="Report"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ReportFeed.ReportFeed_RelatedRecordId}" required="false"/> 
						<rdf:ListResource path="dataModel>ReportFeed_RelatedRecordId" entitySet="ContentVersion"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>