					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>RecentlyViewed.RecentlyViewed_IsActive}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>RecentlyViewed_IsActive/RecentlyViewed_IsActive',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>RecentlyViewed.RecentlyViewed_Language}" required="false"/> 
