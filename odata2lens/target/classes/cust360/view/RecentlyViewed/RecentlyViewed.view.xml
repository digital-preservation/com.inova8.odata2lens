						<rdf:TextLink text="{path:'dataModel>RecentlyViewed_Name/RecentlyViewed_Name',type:'sap.ui.model.type.String' }" linkText="{i18n>RecentlyViewed.RecentlyViewed_Name/RecentlyViewed_Name}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>RecentlyViewed.RecentlyViewed_NameOrAlias}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>RecentlyViewed_NameOrAlias/RecentlyViewed_NameOrAlias',type:'sap.ui.model.type.String' }" linkText="{i18n>RecentlyViewed.RecentlyViewed_NameOrAlias/RecentlyViewed_NameOrAlias}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
