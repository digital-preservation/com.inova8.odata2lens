						<Label text="{i18n>RecentlyViewed.RecentlyViewed_UserRoleId}" required="false"/> 
						<rdf:ListResource path="dataModel>RecentlyViewed_UserRoleId" entitySet="UserRole"/>
						<Text text=""/>
