<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>SecurityCustomBaseline.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>SecurityCustomBaseline.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="SecurityCustomBaseline"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="SecurityCustomBaseline"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SecurityCustomBaseline.SecurityCustomBaseline_Baseline}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>SecurityCustomBaseline_Baseline/SecurityCustomBaseline_Baseline',type:'sap.ui.model.type.String' }" linkText="{i18n>SecurityCustomBaseline.SecurityCustomBaseline_Baseline/SecurityCustomBaseline_Baseline}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SecurityCustomBaseline.SecurityCustomBaseline_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>SecurityCustomBaseline_CreatedById/sfdc_SecurityCustomBaseline_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SecurityCustomBaseline.SecurityCustomBaseline_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>SecurityCustomBaseline_CreatedDate/SecurityCustomBaseline_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SecurityCustomBaseline.SecurityCustomBaseline_DeveloperName}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>SecurityCustomBaseline_DeveloperName/SecurityCustomBaseline_DeveloperName',type:'sap.ui.model.type.String' }" linkText="{i18n>SecurityCustomBaseline.SecurityCustomBaseline_DeveloperName/SecurityCustomBaseline_DeveloperName}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SecurityCustomBaseline.SecurityCustomBaseline_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>SecurityCustomBaseline_Id/SecurityCustomBaseline_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>SecurityCustomBaseline.SecurityCustomBaseline_Id/SecurityCustomBaseline_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SecurityCustomBaseline.SecurityCustomBaseline_IsDefault}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>SecurityCustomBaseline_IsDefault/SecurityCustomBaseline_IsDefault',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SecurityCustomBaseline.SecurityCustomBaseline_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>SecurityCustomBaseline_IsDeleted/SecurityCustomBaseline_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SecurityCustomBaseline.SecurityCustomBaseline_Language}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>SecurityCustomBaseline_Language/SecurityCustomBaseline_Language',type:'sap.ui.model.type.String' }" linkText="{i18n>SecurityCustomBaseline.SecurityCustomBaseline_Language/SecurityCustomBaseline_Language}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SecurityCustomBaseline.SecurityCustomBaseline_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>SecurityCustomBaseline_LastModifiedById/sfdc_SecurityCustomBaseline_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SecurityCustomBaseline.SecurityCustomBaseline_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>SecurityCustomBaseline_LastModifiedDate/SecurityCustomBaseline_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SecurityCustomBaseline.SecurityCustomBaseline_MasterLabel}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>SecurityCustomBaseline_MasterLabel/SecurityCustomBaseline_MasterLabel',type:'sap.ui.model.type.String' }" linkText="{i18n>SecurityCustomBaseline.SecurityCustomBaseline_MasterLabel/SecurityCustomBaseline_MasterLabel}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SecurityCustomBaseline.SecurityCustomBaseline_NamespacePrefix}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>SecurityCustomBaseline_NamespacePrefix/SecurityCustomBaseline_NamespacePrefix',type:'sap.ui.model.type.String' }" linkText="{i18n>SecurityCustomBaseline.SecurityCustomBaseline_NamespacePrefix/SecurityCustomBaseline_NamespacePrefix}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SecurityCustomBaseline.SecurityCustomBaseline_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>SecurityCustomBaseline_SystemModstamp/SecurityCustomBaseline_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SecurityCustomBaseline.SecurityCustomBaseline_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>SecurityCustomBaseline_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SecurityCustomBaseline.SecurityCustomBaseline_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>SecurityCustomBaseline_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>