<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>SocialPostFeeds.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/SocialPostFeed' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>SocialPostFeeds.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>SocialPostFeeds.SocialPostFeed_Body}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>SocialPostFeeds.SocialPostFeed_CommentCount}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>SocialPostFeeds.SocialPostFeed_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>SocialPostFeeds.SocialPostFeed_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>SocialPostFeeds.SocialPostFeed_IsDeleted}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>SocialPostFeeds.SocialPostFeed_IsRichText}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>SocialPostFeeds.SocialPostFeed_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>SocialPostFeeds.SocialPostFeed_LikeCount}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>SocialPostFeeds.SocialPostFeed_LinkUrl}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>SocialPostFeeds.SocialPostFeed_SystemModstamp}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>SocialPostFeeds.SocialPostFeed_Title}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>SocialPostFeeds.SocialPostFeed_Type}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="SocialPostFeed"/>
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>SocialPostFeed_Body/SocialPostFeed_Body',type:'sap.ui.model.type.String'}" linkText="{i18n>SocialPostFeed.SocialPostFeed_Body/SocialPostFeed_Body}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>SocialPostFeed_CommentCount/SocialPostFeed_CommentCount',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>SocialPostFeed_CreatedDate/SocialPostFeed_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>SocialPostFeed_Id/SocialPostFeed_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>SocialPostFeed.SocialPostFeed_Id/SocialPostFeed_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>SocialPostFeed_IsDeleted/SocialPostFeed_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>SocialPostFeed_IsRichText/SocialPostFeed_IsRichText',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<Text text="{dataModel>SocialPostFeed_LastModifiedDate/SocialPostFeed_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>SocialPostFeed_LikeCount/SocialPostFeed_LikeCount',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>SocialPostFeed_LinkUrl/SocialPostFeed_LinkUrl',type:'sap.ui.model.type.String'}" linkText="{i18n>SocialPostFeed.SocialPostFeed_LinkUrl/SocialPostFeed_LinkUrl}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>SocialPostFeed_SystemModstamp/SocialPostFeed_SystemModstamp}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>SocialPostFeed_Title/SocialPostFeed_Title',type:'sap.ui.model.type.String'}" linkText="{i18n>SocialPostFeed.SocialPostFeed_Title/SocialPostFeed_Title}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>SocialPostFeed_Type/SocialPostFeed_Type',type:'sap.ui.model.type.String'}" linkText="{i18n>SocialPostFeed.SocialPostFeed_Type/SocialPostFeed_Type}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>