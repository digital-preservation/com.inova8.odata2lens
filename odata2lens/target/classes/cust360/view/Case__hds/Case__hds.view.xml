<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>Case__hds.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/Case__hd' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>Case__hds.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Case__hds.Case__hd_ClosedDate__hpr}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Case__hds.Case__hd_ClosedDate__hst}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Case__hds.Case__hd_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Case__hds.Case__hd_CreatedDate__hpr}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Case__hds.Case__hd_CreatedDate__hst}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Case__hds.Case__hd_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Case__hds.Case__hd_IsDeleted}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Case__hds.Case__hd_Priority__hpr}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Case__hds.Case__hd_Priority__hst}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Case__hds.Case__hd_Reason__hpr}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Case__hds.Case__hd_Reason__hst}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Case__hds.Case__hd_Status__hpr}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Case__hds.Case__hd_Status__hst}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Case__hds.Case__hd_SystemModstamp}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Case__hds.Case__hd_Type__hpr}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Case__hds.Case__hd_Type__hst}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Case__hds.Case__hd_ValidFromDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Case__hds.Case__hd_ValidToDate}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="Case__hd"/>
							<VBox>					
							<Text text="{dataModel>Case__hd_ClosedDate__hpr/Case__hd_ClosedDate__hpr}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>Case__hd_ClosedDate__hst/Case__hd_ClosedDate__hst}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>Case__hd_CreatedDate/Case__hd_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>Case__hd_CreatedDate__hpr/Case__hd_CreatedDate__hpr}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>Case__hd_CreatedDate__hst/Case__hd_CreatedDate__hst}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Case__hd_Id/Case__hd_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>Case__hd.Case__hd_Id/Case__hd_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>Case__hd_IsDeleted/Case__hd_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Case__hd_Priority__hpr/Case__hd_Priority__hpr',type:'sap.ui.model.type.String'}" linkText="{i18n>Case__hd.Case__hd_Priority__hpr/Case__hd_Priority__hpr}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Case__hd_Priority__hst/Case__hd_Priority__hst',type:'sap.ui.model.type.String'}" linkText="{i18n>Case__hd.Case__hd_Priority__hst/Case__hd_Priority__hst}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Case__hd_Reason__hpr/Case__hd_Reason__hpr',type:'sap.ui.model.type.String'}" linkText="{i18n>Case__hd.Case__hd_Reason__hpr/Case__hd_Reason__hpr}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Case__hd_Reason__hst/Case__hd_Reason__hst',type:'sap.ui.model.type.String'}" linkText="{i18n>Case__hd.Case__hd_Reason__hst/Case__hd_Reason__hst}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Case__hd_Status__hpr/Case__hd_Status__hpr',type:'sap.ui.model.type.String'}" linkText="{i18n>Case__hd.Case__hd_Status__hpr/Case__hd_Status__hpr}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Case__hd_Status__hst/Case__hd_Status__hst',type:'sap.ui.model.type.String'}" linkText="{i18n>Case__hd.Case__hd_Status__hst/Case__hd_Status__hst}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>Case__hd_SystemModstamp/Case__hd_SystemModstamp}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Case__hd_Type__hpr/Case__hd_Type__hpr',type:'sap.ui.model.type.String'}" linkText="{i18n>Case__hd.Case__hd_Type__hpr/Case__hd_Type__hpr}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Case__hd_Type__hst/Case__hd_Type__hst',type:'sap.ui.model.type.String'}" linkText="{i18n>Case__hd.Case__hd_Type__hst/Case__hd_Type__hst}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>Case__hd_ValidFromDate/Case__hd_ValidFromDate}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>Case__hd_ValidToDate/Case__hd_ValidToDate}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>