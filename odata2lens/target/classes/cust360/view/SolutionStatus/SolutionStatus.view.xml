<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>SolutionStatus.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>SolutionStatus.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="SolutionStatus"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="SolutionStatus"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SolutionStatus.SolutionStatus_ApiName}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>SolutionStatus_ApiName/SolutionStatus_ApiName',type:'sap.ui.model.type.String' }" linkText="{i18n>SolutionStatus.SolutionStatus_ApiName/SolutionStatus_ApiName}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SolutionStatus.SolutionStatus_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>SolutionStatus_CreatedById/sfdc_SolutionStatus_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SolutionStatus.SolutionStatus_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>SolutionStatus_CreatedDate/SolutionStatus_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SolutionStatus.SolutionStatus_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>SolutionStatus_Id/SolutionStatus_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>SolutionStatus.SolutionStatus_Id/SolutionStatus_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SolutionStatus.SolutionStatus_IsDefault}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>SolutionStatus_IsDefault/SolutionStatus_IsDefault',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SolutionStatus.SolutionStatus_IsReviewed}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>SolutionStatus_IsReviewed/SolutionStatus_IsReviewed',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SolutionStatus.SolutionStatus_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>SolutionStatus_LastModifiedById/sfdc_SolutionStatus_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SolutionStatus.SolutionStatus_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>SolutionStatus_LastModifiedDate/SolutionStatus_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SolutionStatus.SolutionStatus_MasterLabel}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>SolutionStatus_MasterLabel/SolutionStatus_MasterLabel',type:'sap.ui.model.type.String' }" linkText="{i18n>SolutionStatus.SolutionStatus_MasterLabel/SolutionStatus_MasterLabel}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SolutionStatus.SolutionStatus_SortOrder}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>SolutionStatus_SortOrder/SolutionStatus_SortOrder',type:'sap.ui.model.type.Integer' }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SolutionStatus.SolutionStatus_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>SolutionStatus_SystemModstamp/SolutionStatus_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SolutionStatus.SolutionStatus_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>SolutionStatus_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SolutionStatus.SolutionStatus_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>SolutionStatus_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>