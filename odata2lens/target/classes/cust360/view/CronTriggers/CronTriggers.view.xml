<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>CronTriggers.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/CronTrigger' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>CronTriggers.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CronTriggers.CronTrigger_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CronTriggers.CronTrigger_CronExpression}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CronTriggers.CronTrigger_EndTime}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CronTriggers.CronTrigger_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CronTriggers.CronTrigger_NextFireTime}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CronTriggers.CronTrigger_PreviousFireTime}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CronTriggers.CronTrigger_StartTime}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CronTriggers.CronTrigger_State}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CronTriggers.CronTrigger_TimeZoneSidKey}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CronTriggers.CronTrigger_TimesTriggered}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="CronTrigger"/>
							<VBox>					
							<Text text="{dataModel>CronTrigger_CreatedDate/CronTrigger_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>CronTrigger_CronExpression/CronTrigger_CronExpression',type:'sap.ui.model.type.String'}" linkText="{i18n>CronTrigger.CronTrigger_CronExpression/CronTrigger_CronExpression}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>CronTrigger_EndTime/CronTrigger_EndTime}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>CronTrigger_Id/CronTrigger_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>CronTrigger.CronTrigger_Id/CronTrigger_Id}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>CronTrigger_NextFireTime/CronTrigger_NextFireTime}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>CronTrigger_PreviousFireTime/CronTrigger_PreviousFireTime}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>CronTrigger_StartTime/CronTrigger_StartTime}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>CronTrigger_State/CronTrigger_State',type:'sap.ui.model.type.String'}" linkText="{i18n>CronTrigger.CronTrigger_State/CronTrigger_State}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>CronTrigger_TimeZoneSidKey/CronTrigger_TimeZoneSidKey',type:'sap.ui.model.type.String'}" linkText="{i18n>CronTrigger.CronTrigger_TimeZoneSidKey/CronTrigger_TimeZoneSidKey}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>CronTrigger_TimesTriggered/CronTrigger_TimesTriggered',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>