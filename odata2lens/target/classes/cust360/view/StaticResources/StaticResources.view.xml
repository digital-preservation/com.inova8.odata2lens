<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>StaticResources.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/StaticResource' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>StaticResources.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>StaticResources.StaticResource_Body}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>StaticResources.StaticResource_BodyLength}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>StaticResources.StaticResource_CacheControl}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>StaticResources.StaticResource_ContentType}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>StaticResources.StaticResource_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>StaticResources.StaticResource_Description}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>StaticResources.StaticResource_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>StaticResources.StaticResource_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>StaticResources.StaticResource_Name}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>StaticResources.StaticResource_NamespacePrefix}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>StaticResources.StaticResource_SystemModstamp}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="StaticResource"/>
							<VBox>					
								<Text text="{dataModel>StaticResource_Body/StaticResource_Body}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>StaticResource_BodyLength/StaticResource_BodyLength',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>StaticResource_CacheControl/StaticResource_CacheControl',type:'sap.ui.model.type.String'}" linkText="{i18n>StaticResource.StaticResource_CacheControl/StaticResource_CacheControl}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>StaticResource_ContentType/StaticResource_ContentType',type:'sap.ui.model.type.String'}" linkText="{i18n>StaticResource.StaticResource_ContentType/StaticResource_ContentType}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>StaticResource_CreatedDate/StaticResource_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>StaticResource_Description/StaticResource_Description',type:'sap.ui.model.type.String'}" linkText="{i18n>StaticResource.StaticResource_Description/StaticResource_Description}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>StaticResource_Id/StaticResource_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>StaticResource.StaticResource_Id/StaticResource_Id}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>StaticResource_LastModifiedDate/StaticResource_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>StaticResource_Name/StaticResource_Name',type:'sap.ui.model.type.String'}" linkText="{i18n>StaticResource.StaticResource_Name/StaticResource_Name}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>StaticResource_NamespacePrefix/StaticResource_NamespacePrefix',type:'sap.ui.model.type.String'}" linkText="{i18n>StaticResource.StaticResource_NamespacePrefix/StaticResource_NamespacePrefix}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>StaticResource_SystemModstamp/StaticResource_SystemModstamp}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>