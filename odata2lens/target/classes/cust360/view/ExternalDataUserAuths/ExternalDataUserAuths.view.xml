<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>ExternalDataUserAuths.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/ExternalDataUserAuth' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>ExternalDataUserAuths.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ExternalDataUserAuths.ExternalDataUserAuth_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ExternalDataUserAuths.ExternalDataUserAuth_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ExternalDataUserAuths.ExternalDataUserAuth_IsDeleted}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ExternalDataUserAuths.ExternalDataUserAuth_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ExternalDataUserAuths.ExternalDataUserAuth_Password}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ExternalDataUserAuths.ExternalDataUserAuth_Protocol}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ExternalDataUserAuths.ExternalDataUserAuth_SystemModstamp}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ExternalDataUserAuths.ExternalDataUserAuth_Username}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="ExternalDataUserAuth"/>
							<VBox>					
							<Text text="{dataModel>ExternalDataUserAuth_CreatedDate/ExternalDataUserAuth_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ExternalDataUserAuth_Id/ExternalDataUserAuth_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>ExternalDataUserAuth.ExternalDataUserAuth_Id/ExternalDataUserAuth_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>ExternalDataUserAuth_IsDeleted/ExternalDataUserAuth_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<Text text="{dataModel>ExternalDataUserAuth_LastModifiedDate/ExternalDataUserAuth_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ExternalDataUserAuth_Password/ExternalDataUserAuth_Password',type:'sap.ui.model.type.String'}" linkText="{i18n>ExternalDataUserAuth.ExternalDataUserAuth_Password/ExternalDataUserAuth_Password}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ExternalDataUserAuth_Protocol/ExternalDataUserAuth_Protocol',type:'sap.ui.model.type.String'}" linkText="{i18n>ExternalDataUserAuth.ExternalDataUserAuth_Protocol/ExternalDataUserAuth_Protocol}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>ExternalDataUserAuth_SystemModstamp/ExternalDataUserAuth_SystemModstamp}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ExternalDataUserAuth_Username/ExternalDataUserAuth_Username',type:'sap.ui.model.type.String'}" linkText="{i18n>ExternalDataUserAuth.ExternalDataUserAuth_Username/ExternalDataUserAuth_Username}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>