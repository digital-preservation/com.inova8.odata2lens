<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>IdpEventLog.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>IdpEventLog.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="IdpEventLog"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="IdpEventLog"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>IdpEventLog.IdpEventLog_AppId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>IdpEventLog_AppId/sfdc_IdpEventLog_AppId" entitySet="ConnectedApplication"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>IdpEventLog.IdpEventLog_AuthSessionId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>IdpEventLog_AuthSessionId/sfdc_IdpEventLog_AuthSessionId" entitySet="AuthSession"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>IdpEventLog.IdpEventLog_ErrorCode}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>IdpEventLog_ErrorCode/IdpEventLog_ErrorCode',type:'sap.ui.model.type.String' }" linkText="{i18n>IdpEventLog.IdpEventLog_ErrorCode/IdpEventLog_ErrorCode}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>IdpEventLog.IdpEventLog_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>IdpEventLog_Id/IdpEventLog_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>IdpEventLog.IdpEventLog_Id/IdpEventLog_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>IdpEventLog.IdpEventLog_IdentityUsed}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>IdpEventLog_IdentityUsed/IdpEventLog_IdentityUsed',type:'sap.ui.model.type.String' }" linkText="{i18n>IdpEventLog.IdpEventLog_IdentityUsed/IdpEventLog_IdentityUsed}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>IdpEventLog.IdpEventLog_InitiatedBy}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>IdpEventLog_InitiatedBy/IdpEventLog_InitiatedBy',type:'sap.ui.model.type.String' }" linkText="{i18n>IdpEventLog.IdpEventLog_InitiatedBy/IdpEventLog_InitiatedBy}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>IdpEventLog.IdpEventLog_OptionsHasLogoutUrl}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>IdpEventLog_OptionsHasLogoutUrl/IdpEventLog_OptionsHasLogoutUrl',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>IdpEventLog.IdpEventLog_SamlEntityUrl}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>IdpEventLog_SamlEntityUrl/IdpEventLog_SamlEntityUrl',type:'sap.ui.model.type.String' }" linkText="{i18n>IdpEventLog.IdpEventLog_SamlEntityUrl/IdpEventLog_SamlEntityUrl}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>IdpEventLog.IdpEventLog_ServiceProviderId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>IdpEventLog_ServiceProviderId/sfdc_IdpEventLog_ServiceProviderId" entitySet="ServiceProvider"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>IdpEventLog.IdpEventLog_SsoType}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>IdpEventLog_SsoType/IdpEventLog_SsoType',type:'sap.ui.model.type.String' }" linkText="{i18n>IdpEventLog.IdpEventLog_SsoType/IdpEventLog_SsoType}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>IdpEventLog.IdpEventLog_Timestamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>IdpEventLog_Timestamp/IdpEventLog_Timestamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>IdpEventLog.IdpEventLog_UserId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>IdpEventLog_UserId/sfdc_IdpEventLog_UserId" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>IdpEventLog.IdpEventLog_AppId}" required="false"/> 
						<rdf:ListResource path="dataModel>IdpEventLog_AppId" entitySet="ConnectedApplication"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>IdpEventLog.IdpEventLog_AuthSessionId}" required="false"/> 
						<rdf:ListResource path="dataModel>IdpEventLog_AuthSessionId" entitySet="AuthSession"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>IdpEventLog.IdpEventLog_ServiceProviderId}" required="false"/> 
						<rdf:ListResource path="dataModel>IdpEventLog_ServiceProviderId" entitySet="ServiceProvider"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>IdpEventLog.IdpEventLog_UserId}" required="false"/> 
						<rdf:ListResource path="dataModel>IdpEventLog_UserId" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>