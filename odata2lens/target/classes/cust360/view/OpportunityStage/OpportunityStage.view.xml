<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>OpportunityStage.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>OpportunityStage.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="OpportunityStage"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="OpportunityStage"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OpportunityStage.OpportunityStage_ApiName}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>OpportunityStage_ApiName/OpportunityStage_ApiName',type:'sap.ui.model.type.String' }" linkText="{i18n>OpportunityStage.OpportunityStage_ApiName/OpportunityStage_ApiName}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OpportunityStage.OpportunityStage_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>OpportunityStage_CreatedById/sfdc_OpportunityStage_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OpportunityStage.OpportunityStage_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>OpportunityStage_CreatedDate/OpportunityStage_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OpportunityStage.OpportunityStage_DefaultProbability}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>OpportunityStage_DefaultProbability/OpportunityStage_DefaultProbability',type:'sap.ui.model.type.Float', formatOptions:{'style':'short','maxFractionDigits':2} }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OpportunityStage.OpportunityStage_Description}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>OpportunityStage_Description/OpportunityStage_Description',type:'sap.ui.model.type.String' }" linkText="{i18n>OpportunityStage.OpportunityStage_Description/OpportunityStage_Description}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OpportunityStage.OpportunityStage_ForecastCategory}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>OpportunityStage_ForecastCategory/OpportunityStage_ForecastCategory',type:'sap.ui.model.type.String' }" linkText="{i18n>OpportunityStage.OpportunityStage_ForecastCategory/OpportunityStage_ForecastCategory}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OpportunityStage.OpportunityStage_ForecastCategoryName}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>OpportunityStage_ForecastCategoryName/OpportunityStage_ForecastCategoryName',type:'sap.ui.model.type.String' }" linkText="{i18n>OpportunityStage.OpportunityStage_ForecastCategoryName/OpportunityStage_ForecastCategoryName}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OpportunityStage.OpportunityStage_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>OpportunityStage_Id/OpportunityStage_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>OpportunityStage.OpportunityStage_Id/OpportunityStage_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OpportunityStage.OpportunityStage_IsActive}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>OpportunityStage_IsActive/OpportunityStage_IsActive',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OpportunityStage.OpportunityStage_IsClosed}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>OpportunityStage_IsClosed/OpportunityStage_IsClosed',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OpportunityStage.OpportunityStage_IsWon}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>OpportunityStage_IsWon/OpportunityStage_IsWon',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OpportunityStage.OpportunityStage_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>OpportunityStage_LastModifiedById/sfdc_OpportunityStage_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OpportunityStage.OpportunityStage_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>OpportunityStage_LastModifiedDate/OpportunityStage_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OpportunityStage.OpportunityStage_MasterLabel}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>OpportunityStage_MasterLabel/OpportunityStage_MasterLabel',type:'sap.ui.model.type.String' }" linkText="{i18n>OpportunityStage.OpportunityStage_MasterLabel/OpportunityStage_MasterLabel}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OpportunityStage.OpportunityStage_SortOrder}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>OpportunityStage_SortOrder/OpportunityStage_SortOrder',type:'sap.ui.model.type.Integer' }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OpportunityStage.OpportunityStage_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>OpportunityStage_SystemModstamp/OpportunityStage_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OpportunityStage.OpportunityStage_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>OpportunityStage_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>OpportunityStage.OpportunityStage_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>OpportunityStage_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>