<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>EmailCaptures.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/EmailCapture' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>EmailCaptures.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EmailCaptures.EmailCapture_CaptureDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EmailCaptures.EmailCapture_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EmailCaptures.EmailCapture_FromPattern}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EmailCaptures.EmailCapture_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EmailCaptures.EmailCapture_IsActive}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EmailCaptures.EmailCapture_IsDeleted}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EmailCaptures.EmailCapture_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EmailCaptures.EmailCapture_RawMessage}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EmailCaptures.EmailCapture_RawMessageLength}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EmailCaptures.EmailCapture_Recipient}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EmailCaptures.EmailCapture_Sender}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EmailCaptures.EmailCapture_SystemModstamp}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>EmailCaptures.EmailCapture_ToPattern}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="EmailCapture"/>
							<VBox>					
							<Text text="{dataModel>EmailCapture_CaptureDate/EmailCapture_CaptureDate}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>EmailCapture_CreatedDate/EmailCapture_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>EmailCapture_FromPattern/EmailCapture_FromPattern',type:'sap.ui.model.type.String'}" linkText="{i18n>EmailCapture.EmailCapture_FromPattern/EmailCapture_FromPattern}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>EmailCapture_Id/EmailCapture_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>EmailCapture.EmailCapture_Id/EmailCapture_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>EmailCapture_IsActive/EmailCapture_IsActive',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>EmailCapture_IsDeleted/EmailCapture_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<Text text="{dataModel>EmailCapture_LastModifiedDate/EmailCapture_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
								<Text text="{dataModel>EmailCapture_RawMessage/EmailCapture_RawMessage}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>EmailCapture_RawMessageLength/EmailCapture_RawMessageLength',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>EmailCapture_Recipient/EmailCapture_Recipient',type:'sap.ui.model.type.String'}" linkText="{i18n>EmailCapture.EmailCapture_Recipient/EmailCapture_Recipient}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>EmailCapture_Sender/EmailCapture_Sender',type:'sap.ui.model.type.String'}" linkText="{i18n>EmailCapture.EmailCapture_Sender/EmailCapture_Sender}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>EmailCapture_SystemModstamp/EmailCapture_SystemModstamp}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>EmailCapture_ToPattern/EmailCapture_ToPattern',type:'sap.ui.model.type.String'}" linkText="{i18n>EmailCapture.EmailCapture_ToPattern/EmailCapture_ToPattern}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>