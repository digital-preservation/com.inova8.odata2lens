<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>ClientBrowser.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>ClientBrowser.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="ClientBrowser"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="ClientBrowser"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ClientBrowser.ClientBrowser_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>ClientBrowser_CreatedDate/ClientBrowser_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ClientBrowser.ClientBrowser_FullUserAgent}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ClientBrowser_FullUserAgent/ClientBrowser_FullUserAgent',type:'sap.ui.model.type.String' }" linkText="{i18n>ClientBrowser.ClientBrowser_FullUserAgent/ClientBrowser_FullUserAgent}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ClientBrowser.ClientBrowser_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ClientBrowser_Id/ClientBrowser_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>ClientBrowser.ClientBrowser_Id/ClientBrowser_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ClientBrowser.ClientBrowser_LastUpdate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>ClientBrowser_LastUpdate/ClientBrowser_LastUpdate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ClientBrowser.ClientBrowser_ProxyInfo}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ClientBrowser_ProxyInfo/ClientBrowser_ProxyInfo',type:'sap.ui.model.type.String' }" linkText="{i18n>ClientBrowser.ClientBrowser_ProxyInfo/ClientBrowser_ProxyInfo}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ClientBrowser.ClientBrowser_UsersId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ClientBrowser_UsersId/sfdc_ClientBrowser_UsersId" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ClientBrowser.ClientBrowser_UsersId}" required="false"/> 
						<rdf:ListResource path="dataModel>ClientBrowser_UsersId" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>