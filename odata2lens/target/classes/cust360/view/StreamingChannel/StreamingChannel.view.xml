						<Text text=""/>
						<Text text="{dataModel>StreamingChannel_SystemModstamp/StreamingChannel_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>StreamingChannel.StreamingChannelShare_ParentId_Inverse}" required="false"/> 
						<rdf:ListResource path="dataModel>StreamingChannelShare_ParentId_Inverse" entitySet="StreamingChannelShare"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>StreamingChannel.StreamingChannel_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>StreamingChannel_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>StreamingChannel.StreamingChannel_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>StreamingChannel_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>StreamingChannel.StreamingChannel_OwnerId}" required="false"/> 
