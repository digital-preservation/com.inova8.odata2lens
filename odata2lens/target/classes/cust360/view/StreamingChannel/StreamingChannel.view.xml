						<Text text=""/>
						<Text text="{dataModel>StreamingChannel_LastViewedDate/StreamingChannel_LastViewedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>StreamingChannel.StreamingChannel_Name}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>StreamingChannel_Name/StreamingChannel_Name',type:'sap.ui.model.type.String' }" linkText="{i18n>StreamingChannel.StreamingChannel_Name/StreamingChannel_Name}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>StreamingChannel.StreamingChannel_OwnerId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>StreamingChannel_OwnerId/sfdc_StreamingChannel_OwnerId" entitySet="Group"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
