					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>StreamingChannel.StreamingChannel_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>StreamingChannel_Id/StreamingChannel_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>StreamingChannel.StreamingChannel_Id/StreamingChannel_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>StreamingChannel.StreamingChannel_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>StreamingChannel_IsDeleted/StreamingChannel_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>StreamingChannel.StreamingChannel_IsDynamic}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>StreamingChannel_IsDynamic/StreamingChannel_IsDynamic',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>StreamingChannel.StreamingChannel_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>StreamingChannel_LastModifiedById/sfdc_StreamingChannel_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
