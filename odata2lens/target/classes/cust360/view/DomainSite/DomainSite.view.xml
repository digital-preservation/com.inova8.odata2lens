<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>DomainSite.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>DomainSite.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="DomainSite"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="DomainSite"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>DomainSite.DomainSite_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>DomainSite_CreatedById/sfdc_DomainSite_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>DomainSite.DomainSite_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>DomainSite_CreatedDate/DomainSite_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>DomainSite.DomainSite_DomainId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>DomainSite_DomainId/sfdc_DomainSite_DomainId" entitySet="Domain"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>DomainSite.DomainSite_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>DomainSite_Id/DomainSite_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>DomainSite.DomainSite_Id/DomainSite_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>DomainSite.DomainSite_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>DomainSite_LastModifiedById/sfdc_DomainSite_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>DomainSite.DomainSite_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>DomainSite_LastModifiedDate/DomainSite_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>DomainSite.DomainSite_PathPrefix}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>DomainSite_PathPrefix/DomainSite_PathPrefix',type:'sap.ui.model.type.String' }" linkText="{i18n>DomainSite.DomainSite_PathPrefix/DomainSite_PathPrefix}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>DomainSite.DomainSite_SiteId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>DomainSite_SiteId/sfdc_DomainSite_SiteId" entitySet="Site"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>DomainSite.DomainSite_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>DomainSite_SystemModstamp/DomainSite_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>DomainSite.DomainSite_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>DomainSite_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>DomainSite.DomainSite_DomainId}" required="false"/> 
						<rdf:ListResource path="dataModel>DomainSite_DomainId" entitySet="Domain"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>DomainSite.DomainSite_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>DomainSite_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>DomainSite.DomainSite_SiteId}" required="false"/> 
						<rdf:ListResource path="dataModel>DomainSite_SiteId" entitySet="Site"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>