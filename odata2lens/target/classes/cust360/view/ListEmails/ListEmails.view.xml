<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>ListEmails.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/ListEmail' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>ListEmails.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ListEmails.ListEmail_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ListEmails.ListEmail_FromAddress}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ListEmails.ListEmail_FromName}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ListEmails.ListEmail_HasAttachment}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ListEmails.ListEmail_HtmlBody}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ListEmails.ListEmail_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ListEmails.ListEmail_IsDeleted}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ListEmails.ListEmail_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ListEmails.ListEmail_LastReferencedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ListEmails.ListEmail_LastViewedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ListEmails.ListEmail_Name}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ListEmails.ListEmail_ScheduledDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ListEmails.ListEmail_Status}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ListEmails.ListEmail_Subject}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ListEmails.ListEmail_SystemModstamp}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ListEmails.ListEmail_TextBody}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ListEmails.ListEmail_TotalSent}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="ListEmail"/>
							<VBox>					
							<Text text="{dataModel>ListEmail_CreatedDate/ListEmail_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ListEmail_FromAddress/ListEmail_FromAddress',type:'sap.ui.model.type.String'}" linkText="{i18n>ListEmail.ListEmail_FromAddress/ListEmail_FromAddress}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ListEmail_FromName/ListEmail_FromName',type:'sap.ui.model.type.String'}" linkText="{i18n>ListEmail.ListEmail_FromName/ListEmail_FromName}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>ListEmail_HasAttachment/ListEmail_HasAttachment',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ListEmail_HtmlBody/ListEmail_HtmlBody',type:'sap.ui.model.type.String'}" linkText="{i18n>ListEmail.ListEmail_HtmlBody/ListEmail_HtmlBody}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ListEmail_Id/ListEmail_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>ListEmail.ListEmail_Id/ListEmail_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>ListEmail_IsDeleted/ListEmail_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<Text text="{dataModel>ListEmail_LastModifiedDate/ListEmail_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>ListEmail_LastReferencedDate/ListEmail_LastReferencedDate}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>ListEmail_LastViewedDate/ListEmail_LastViewedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ListEmail_Name/ListEmail_Name',type:'sap.ui.model.type.String'}" linkText="{i18n>ListEmail.ListEmail_Name/ListEmail_Name}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>ListEmail_ScheduledDate/ListEmail_ScheduledDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ListEmail_Status/ListEmail_Status',type:'sap.ui.model.type.String'}" linkText="{i18n>ListEmail.ListEmail_Status/ListEmail_Status}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ListEmail_Subject/ListEmail_Subject',type:'sap.ui.model.type.String'}" linkText="{i18n>ListEmail.ListEmail_Subject/ListEmail_Subject}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>ListEmail_SystemModstamp/ListEmail_SystemModstamp}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ListEmail_TextBody/ListEmail_TextBody',type:'sap.ui.model.type.String'}" linkText="{i18n>ListEmail.ListEmail_TextBody/ListEmail_TextBody}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>ListEmail_TotalSent/ListEmail_TotalSent',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>