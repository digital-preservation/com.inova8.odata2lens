<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>OrderFeeds.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/OrderFeed' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>OrderFeeds.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>OrderFeeds.OrderFeed_Body}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>OrderFeeds.OrderFeed_CommentCount}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>OrderFeeds.OrderFeed_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>OrderFeeds.OrderFeed_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>OrderFeeds.OrderFeed_IsDeleted}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>OrderFeeds.OrderFeed_IsRichText}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>OrderFeeds.OrderFeed_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>OrderFeeds.OrderFeed_LikeCount}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>OrderFeeds.OrderFeed_LinkUrl}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>OrderFeeds.OrderFeed_SystemModstamp}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>OrderFeeds.OrderFeed_Title}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>OrderFeeds.OrderFeed_Type}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="OrderFeed"/>
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>OrderFeed_Body/OrderFeed_Body',type:'sap.ui.model.type.String'}" linkText="{i18n>OrderFeed.OrderFeed_Body/OrderFeed_Body}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>OrderFeed_CommentCount/OrderFeed_CommentCount',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>OrderFeed_CreatedDate/OrderFeed_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>OrderFeed_Id/OrderFeed_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>OrderFeed.OrderFeed_Id/OrderFeed_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>OrderFeed_IsDeleted/OrderFeed_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>OrderFeed_IsRichText/OrderFeed_IsRichText',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<Text text="{dataModel>OrderFeed_LastModifiedDate/OrderFeed_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>OrderFeed_LikeCount/OrderFeed_LikeCount',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>OrderFeed_LinkUrl/OrderFeed_LinkUrl',type:'sap.ui.model.type.String'}" linkText="{i18n>OrderFeed.OrderFeed_LinkUrl/OrderFeed_LinkUrl}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>OrderFeed_SystemModstamp/OrderFeed_SystemModstamp}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>OrderFeed_Title/OrderFeed_Title',type:'sap.ui.model.type.String'}" linkText="{i18n>OrderFeed.OrderFeed_Title/OrderFeed_Title}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>OrderFeed_Type/OrderFeed_Type',type:'sap.ui.model.type.String'}" linkText="{i18n>OrderFeed.OrderFeed_Type/OrderFeed_Type}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>