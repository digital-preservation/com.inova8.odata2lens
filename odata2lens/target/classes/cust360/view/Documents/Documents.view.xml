<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>Documents.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/Document' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>Documents.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Documents.Document_Body}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Documents.Document_BodyLength}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Documents.Document_ContentType}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Documents.Document_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Documents.Document_Description}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Documents.Document_DeveloperName}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Documents.Document_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Documents.Document_IsBodySearchable}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Documents.Document_IsDeleted}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Documents.Document_IsInternalUseOnly}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Documents.Document_IsPublic}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Documents.Document_Keywords}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Documents.Document_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Documents.Document_LastReferencedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Documents.Document_LastViewedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Documents.Document_Name}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Documents.Document_NamespacePrefix}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Documents.Document_SystemModstamp}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Documents.Document_Type}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Documents.Document_Url}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="Document"/>
							<VBox>					
								<Text text="{dataModel>Document_Body/Document_Body}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>Document_BodyLength/Document_BodyLength',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Document_ContentType/Document_ContentType',type:'sap.ui.model.type.String'}" linkText="{i18n>Document.Document_ContentType/Document_ContentType}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>Document_CreatedDate/Document_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Document_Description/Document_Description',type:'sap.ui.model.type.String'}" linkText="{i18n>Document.Document_Description/Document_Description}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Document_DeveloperName/Document_DeveloperName',type:'sap.ui.model.type.String'}" linkText="{i18n>Document.Document_DeveloperName/Document_DeveloperName}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Document_Id/Document_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>Document.Document_Id/Document_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>Document_IsBodySearchable/Document_IsBodySearchable',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>Document_IsDeleted/Document_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>Document_IsInternalUseOnly/Document_IsInternalUseOnly',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>Document_IsPublic/Document_IsPublic',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Document_Keywords/Document_Keywords',type:'sap.ui.model.type.String'}" linkText="{i18n>Document.Document_Keywords/Document_Keywords}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>Document_LastModifiedDate/Document_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>Document_LastReferencedDate/Document_LastReferencedDate}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>Document_LastViewedDate/Document_LastViewedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Document_Name/Document_Name',type:'sap.ui.model.type.String'}" linkText="{i18n>Document.Document_Name/Document_Name}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Document_NamespacePrefix/Document_NamespacePrefix',type:'sap.ui.model.type.String'}" linkText="{i18n>Document.Document_NamespacePrefix/Document_NamespacePrefix}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>Document_SystemModstamp/Document_SystemModstamp}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Document_Type/Document_Type',type:'sap.ui.model.type.String'}" linkText="{i18n>Document.Document_Type/Document_Type}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Document_Url/Document_Url',type:'sap.ui.model.type.String'}" linkText="{i18n>Document.Document_Url/Document_Url}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>