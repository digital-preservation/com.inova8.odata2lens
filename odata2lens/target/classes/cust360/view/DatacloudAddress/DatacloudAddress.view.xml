					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>DatacloudAddress.DatacloudAddress_Latitude}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>DatacloudAddress_Latitude/DatacloudAddress_Latitude',type:'sap.ui.model.type.String' }" linkText="{i18n>DatacloudAddress.DatacloudAddress_Latitude/DatacloudAddress_Latitude}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>DatacloudAddress.DatacloudAddress_Longitude}" required="false"/> 
						<Text text=""/>
