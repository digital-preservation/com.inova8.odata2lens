						<rdf:TextLink text="{path:'dataModel>DatacloudAddress_GeoAccuracyCode/DatacloudAddress_GeoAccuracyCode',type:'sap.ui.model.type.String' }" linkText="{i18n>DatacloudAddress.DatacloudAddress_GeoAccuracyCode/DatacloudAddress_GeoAccuracyCode}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
