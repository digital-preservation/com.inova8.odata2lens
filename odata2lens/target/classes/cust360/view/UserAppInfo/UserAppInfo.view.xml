<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>UserAppInfo.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>UserAppInfo.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="UserAppInfo"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="UserAppInfo"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserAppInfo.UserAppInfo_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>UserAppInfo_CreatedById/sfdc_UserAppInfo_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserAppInfo.UserAppInfo_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>UserAppInfo_CreatedDate/UserAppInfo_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserAppInfo.UserAppInfo_FormFactor}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>UserAppInfo_FormFactor/UserAppInfo_FormFactor',type:'sap.ui.model.type.String' }" linkText="{i18n>UserAppInfo.UserAppInfo_FormFactor/UserAppInfo_FormFactor}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserAppInfo.UserAppInfo_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>UserAppInfo_Id/UserAppInfo_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>UserAppInfo.UserAppInfo_Id/UserAppInfo_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserAppInfo.UserAppInfo_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>UserAppInfo_IsDeleted/UserAppInfo_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserAppInfo.UserAppInfo_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>UserAppInfo_LastModifiedById/sfdc_UserAppInfo_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserAppInfo.UserAppInfo_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>UserAppInfo_LastModifiedDate/UserAppInfo_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserAppInfo.UserAppInfo_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>UserAppInfo_SystemModstamp/UserAppInfo_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserAppInfo.UserAppInfo_UserId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>UserAppInfo_UserId/sfdc_UserAppInfo_UserId" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserAppInfo.UserAppInfo_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>UserAppInfo_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserAppInfo.UserAppInfo_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>UserAppInfo_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserAppInfo.UserAppInfo_UserId}" required="false"/> 
						<rdf:ListResource path="dataModel>UserAppInfo_UserId" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>