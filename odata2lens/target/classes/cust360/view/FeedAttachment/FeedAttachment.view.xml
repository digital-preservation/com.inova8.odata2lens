<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>FeedAttachment.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>FeedAttachment.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="FeedAttachment"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="FeedAttachment"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>FeedAttachment.FeedAttachment_FeedEntityId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>FeedAttachment_FeedEntityId/sfdc_FeedAttachment_FeedEntityId" entitySet="AccountFeed"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>FeedAttachment.FeedAttachment_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>FeedAttachment_Id/FeedAttachment_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>FeedAttachment.FeedAttachment_Id/FeedAttachment_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>FeedAttachment.FeedAttachment_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>FeedAttachment_IsDeleted/FeedAttachment_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>FeedAttachment.FeedAttachment_RecordId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>FeedAttachment_RecordId/sfdc_FeedAttachment_RecordId" entitySet="ContentDocument"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>FeedAttachment.FeedAttachment_Title}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>FeedAttachment_Title/FeedAttachment_Title',type:'sap.ui.model.type.String' }" linkText="{i18n>FeedAttachment.FeedAttachment_Title/FeedAttachment_Title}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>FeedAttachment.FeedAttachment_Type}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>FeedAttachment_Type/FeedAttachment_Type',type:'sap.ui.model.type.String' }" linkText="{i18n>FeedAttachment.FeedAttachment_Type/FeedAttachment_Type}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>FeedAttachment.FeedAttachment_Value}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>FeedAttachment_Value/FeedAttachment_Value',type:'sap.ui.model.type.String' }" linkText="{i18n>FeedAttachment.FeedAttachment_Value/FeedAttachment_Value}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>FeedAttachment.FeedAttachment_FeedEntityId}" required="false"/> 
						<rdf:ListResource path="dataModel>FeedAttachment_FeedEntityId" entitySet="AccountFeed"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>FeedAttachment.FeedAttachment_RecordId}" required="false"/> 
						<rdf:ListResource path="dataModel>FeedAttachment_RecordId" entitySet="ContentDocument"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>