<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>UserFieldAccess.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>UserFieldAccess.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="UserFieldAccess"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="UserFieldAccess"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserFieldAccess.FieldDefinition_RunningUserFieldAccessId_Inverse}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>FieldDefinition_RunningUserFieldAccessId_Inverse/sfdc_FieldDefinition_RunningUserFieldAccessId_Inverse" entitySet="FieldDefinition"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserFieldAccess.UserFieldAccess_DurableId}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>UserFieldAccess_DurableId/UserFieldAccess_DurableId',type:'sap.ui.model.type.String' }" linkText="{i18n>UserFieldAccess.UserFieldAccess_DurableId/UserFieldAccess_DurableId}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserFieldAccess.UserFieldAccess_EntityDefinitionId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>UserFieldAccess_EntityDefinitionId/sfdc_UserFieldAccess_EntityDefinitionId" entitySet="EntityDefinition"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserFieldAccess.UserFieldAccess_FieldDefinitionId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>UserFieldAccess_FieldDefinitionId/sfdc_UserFieldAccess_FieldDefinitionId" entitySet="FieldDefinition"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserFieldAccess.UserFieldAccess_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>UserFieldAccess_Id/UserFieldAccess_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>UserFieldAccess.UserFieldAccess_Id/UserFieldAccess_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserFieldAccess.UserFieldAccess_IsAccessible}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>UserFieldAccess_IsAccessible/UserFieldAccess_IsAccessible',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserFieldAccess.UserFieldAccess_IsCreatable}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>UserFieldAccess_IsCreatable/UserFieldAccess_IsCreatable',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserFieldAccess.UserFieldAccess_IsUpdatable}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>UserFieldAccess_IsUpdatable/UserFieldAccess_IsUpdatable',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserFieldAccess.UserFieldAccess_UserId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>UserFieldAccess_UserId/sfdc_UserFieldAccess_UserId" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserFieldAccess.FieldDefinition_RunningUserFieldAccessId_Inverse}" required="false"/> 
						<rdf:ListResource path="dataModel>FieldDefinition_RunningUserFieldAccessId_Inverse" entitySet="FieldDefinition"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserFieldAccess.UserFieldAccess_EntityDefinitionId}" required="false"/> 
						<rdf:ListResource path="dataModel>UserFieldAccess_EntityDefinitionId" entitySet="EntityDefinition"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserFieldAccess.UserFieldAccess_FieldDefinitionId}" required="false"/> 
						<rdf:ListResource path="dataModel>UserFieldAccess_FieldDefinitionId" entitySet="FieldDefinition"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserFieldAccess.UserFieldAccess_UserId}" required="false"/> 
						<rdf:ListResource path="dataModel>UserFieldAccess_UserId" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>