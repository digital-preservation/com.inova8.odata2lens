<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>RecentlyVieweds.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/RecentlyViewed' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>RecentlyVieweds.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>RecentlyVieweds.RecentlyViewed_Alias}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>RecentlyVieweds.RecentlyViewed_Email}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>RecentlyVieweds.RecentlyViewed_FirstName}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>RecentlyVieweds.RecentlyViewed_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>RecentlyVieweds.RecentlyViewed_IsActive}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>RecentlyVieweds.RecentlyViewed_Language}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>RecentlyVieweds.RecentlyViewed_LastName}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>RecentlyVieweds.RecentlyViewed_LastReferencedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>RecentlyVieweds.RecentlyViewed_LastViewedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>RecentlyVieweds.RecentlyViewed_Name}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>RecentlyVieweds.RecentlyViewed_NameOrAlias}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>RecentlyVieweds.RecentlyViewed_Phone}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>RecentlyVieweds.RecentlyViewed_Title}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>RecentlyVieweds.RecentlyViewed_Type}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="RecentlyViewed"/>
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>RecentlyViewed_Alias/RecentlyViewed_Alias',type:'sap.ui.model.type.String'}" linkText="{i18n>RecentlyViewed.RecentlyViewed_Alias/RecentlyViewed_Alias}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>RecentlyViewed_Email/RecentlyViewed_Email',type:'sap.ui.model.type.String'}" linkText="{i18n>RecentlyViewed.RecentlyViewed_Email/RecentlyViewed_Email}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>RecentlyViewed_FirstName/RecentlyViewed_FirstName',type:'sap.ui.model.type.String'}" linkText="{i18n>RecentlyViewed.RecentlyViewed_FirstName/RecentlyViewed_FirstName}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>RecentlyViewed_Id/RecentlyViewed_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>RecentlyViewed.RecentlyViewed_Id/RecentlyViewed_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>RecentlyViewed_IsActive/RecentlyViewed_IsActive',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>RecentlyViewed_Language/RecentlyViewed_Language',type:'sap.ui.model.type.String'}" linkText="{i18n>RecentlyViewed.RecentlyViewed_Language/RecentlyViewed_Language}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>RecentlyViewed_LastName/RecentlyViewed_LastName',type:'sap.ui.model.type.String'}" linkText="{i18n>RecentlyViewed.RecentlyViewed_LastName/RecentlyViewed_LastName}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>RecentlyViewed_LastReferencedDate/RecentlyViewed_LastReferencedDate}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>RecentlyViewed_LastViewedDate/RecentlyViewed_LastViewedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>RecentlyViewed_Name/RecentlyViewed_Name',type:'sap.ui.model.type.String'}" linkText="{i18n>RecentlyViewed.RecentlyViewed_Name/RecentlyViewed_Name}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>RecentlyViewed_NameOrAlias/RecentlyViewed_NameOrAlias',type:'sap.ui.model.type.String'}" linkText="{i18n>RecentlyViewed.RecentlyViewed_NameOrAlias/RecentlyViewed_NameOrAlias}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>RecentlyViewed_Phone/RecentlyViewed_Phone',type:'sap.ui.model.type.String'}" linkText="{i18n>RecentlyViewed.RecentlyViewed_Phone/RecentlyViewed_Phone}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>RecentlyViewed_Title/RecentlyViewed_Title',type:'sap.ui.model.type.String'}" linkText="{i18n>RecentlyViewed.RecentlyViewed_Title/RecentlyViewed_Title}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>RecentlyViewed_Type/RecentlyViewed_Type',type:'sap.ui.model.type.String'}" linkText="{i18n>RecentlyViewed.RecentlyViewed_Type/RecentlyViewed_Type}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>