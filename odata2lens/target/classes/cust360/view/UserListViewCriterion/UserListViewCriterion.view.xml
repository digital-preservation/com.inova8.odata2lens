<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>UserListViewCriterion.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>UserListViewCriterion.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="UserListViewCriterion"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="UserListViewCriterion"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserListViewCriterion.UserListViewCriterion_ColumnName}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>UserListViewCriterion_ColumnName/UserListViewCriterion_ColumnName',type:'sap.ui.model.type.String' }" linkText="{i18n>UserListViewCriterion.UserListViewCriterion_ColumnName/UserListViewCriterion_ColumnName}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserListViewCriterion.UserListViewCriterion_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>UserListViewCriterion_CreatedById/sfdc_UserListViewCriterion_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserListViewCriterion.UserListViewCriterion_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>UserListViewCriterion_CreatedDate/UserListViewCriterion_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserListViewCriterion.UserListViewCriterion_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>UserListViewCriterion_Id/UserListViewCriterion_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>UserListViewCriterion.UserListViewCriterion_Id/UserListViewCriterion_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserListViewCriterion.UserListViewCriterion_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>UserListViewCriterion_IsDeleted/UserListViewCriterion_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserListViewCriterion.UserListViewCriterion_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>UserListViewCriterion_LastModifiedById/sfdc_UserListViewCriterion_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserListViewCriterion.UserListViewCriterion_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>UserListViewCriterion_LastModifiedDate/UserListViewCriterion_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserListViewCriterion.UserListViewCriterion_Operation}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>UserListViewCriterion_Operation/UserListViewCriterion_Operation',type:'sap.ui.model.type.String' }" linkText="{i18n>UserListViewCriterion.UserListViewCriterion_Operation/UserListViewCriterion_Operation}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserListViewCriterion.UserListViewCriterion_SortOrder}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>UserListViewCriterion_SortOrder/UserListViewCriterion_SortOrder',type:'sap.ui.model.type.Integer' }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserListViewCriterion.UserListViewCriterion_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>UserListViewCriterion_SystemModstamp/UserListViewCriterion_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserListViewCriterion.UserListViewCriterion_UserListViewId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>UserListViewCriterion_UserListViewId/sfdc_UserListViewCriterion_UserListViewId" entitySet="UserListView"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserListViewCriterion.UserListViewCriterion_Value}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>UserListViewCriterion_Value/UserListViewCriterion_Value',type:'sap.ui.model.type.String' }" linkText="{i18n>UserListViewCriterion.UserListViewCriterion_Value/UserListViewCriterion_Value}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserListViewCriterion.UserListViewCriterion_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>UserListViewCriterion_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserListViewCriterion.UserListViewCriterion_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>UserListViewCriterion_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>UserListViewCriterion.UserListViewCriterion_UserListViewId}" required="false"/> 
						<rdf:ListResource path="dataModel>UserListViewCriterion_UserListViewId" entitySet="UserListView"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>