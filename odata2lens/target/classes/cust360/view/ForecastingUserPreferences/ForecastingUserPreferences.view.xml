<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>ForecastingUserPreferences.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/ForecastingUserPreference' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>ForecastingUserPreferences.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ForecastingUserPreferences.ForecastingUserPreference_ExternalId}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ForecastingUserPreferences.ForecastingUserPreference_ForecastingPeriodDuration}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ForecastingUserPreferences.ForecastingUserPreference_ForecastingPeriodType}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ForecastingUserPreferences.ForecastingUserPreference_ForecastingStartPeriod}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ForecastingUserPreferences.ForecastingUserPreference_ForecastingViewCurrency}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ForecastingUserPreferences.ForecastingUserPreference_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ForecastingUserPreferences.ForecastingUserPreference_IsForecastingHideZeroRows}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ForecastingUserPreferences.ForecastingUserPreference_IsForecastingShowQuantity}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ForecastingUserPreferences.ForecastingUserPreference_IsHideForecastingGuidedTour}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ForecastingUserPreferences.ForecastingUserPreference_IsHideForecastingQuotaColumn}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ForecastingUserPreferences.ForecastingUserPreference_IsShowForecastingQuotaAttainment}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="ForecastingUserPreference"/>
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ForecastingUserPreference_ExternalId/ForecastingUserPreference_ExternalId',type:'sap.ui.model.type.String'}" linkText="{i18n>ForecastingUserPreference.ForecastingUserPreference_ExternalId/ForecastingUserPreference_ExternalId}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>ForecastingUserPreference_ForecastingPeriodDuration/ForecastingUserPreference_ForecastingPeriodDuration',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ForecastingUserPreference_ForecastingPeriodType/ForecastingUserPreference_ForecastingPeriodType',type:'sap.ui.model.type.String'}" linkText="{i18n>ForecastingUserPreference.ForecastingUserPreference_ForecastingPeriodType/ForecastingUserPreference_ForecastingPeriodType}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>ForecastingUserPreference_ForecastingStartPeriod/ForecastingUserPreference_ForecastingStartPeriod',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ForecastingUserPreference_ForecastingViewCurrency/ForecastingUserPreference_ForecastingViewCurrency',type:'sap.ui.model.type.String'}" linkText="{i18n>ForecastingUserPreference.ForecastingUserPreference_ForecastingViewCurrency/ForecastingUserPreference_ForecastingViewCurrency}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ForecastingUserPreference_Id/ForecastingUserPreference_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>ForecastingUserPreference.ForecastingUserPreference_Id/ForecastingUserPreference_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>ForecastingUserPreference_IsForecastingHideZeroRows/ForecastingUserPreference_IsForecastingHideZeroRows',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>ForecastingUserPreference_IsForecastingShowQuantity/ForecastingUserPreference_IsForecastingShowQuantity',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>ForecastingUserPreference_IsHideForecastingGuidedTour/ForecastingUserPreference_IsHideForecastingGuidedTour',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>ForecastingUserPreference_IsHideForecastingQuotaColumn/ForecastingUserPreference_IsHideForecastingQuotaColumn',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>ForecastingUserPreference_IsShowForecastingQuotaAttainment/ForecastingUserPreference_IsShowForecastingQuotaAttainment',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>