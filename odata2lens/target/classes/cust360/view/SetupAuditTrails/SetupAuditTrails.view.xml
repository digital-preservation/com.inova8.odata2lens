<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>SetupAuditTrails.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/SetupAuditTrail' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>SetupAuditTrails.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>SetupAuditTrails.SetupAuditTrail_Action}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>SetupAuditTrails.SetupAuditTrail_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>SetupAuditTrails.SetupAuditTrail_DelegateUser}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>SetupAuditTrails.SetupAuditTrail_Display}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>SetupAuditTrails.SetupAuditTrail_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>SetupAuditTrails.SetupAuditTrail_ResponsibleNamespacePrefix}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>SetupAuditTrails.SetupAuditTrail_Section}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="SetupAuditTrail"/>
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>SetupAuditTrail_Action/SetupAuditTrail_Action',type:'sap.ui.model.type.String'}" linkText="{i18n>SetupAuditTrail.SetupAuditTrail_Action/SetupAuditTrail_Action}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>SetupAuditTrail_CreatedDate/SetupAuditTrail_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>SetupAuditTrail_DelegateUser/SetupAuditTrail_DelegateUser',type:'sap.ui.model.type.String'}" linkText="{i18n>SetupAuditTrail.SetupAuditTrail_DelegateUser/SetupAuditTrail_DelegateUser}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>SetupAuditTrail_Display/SetupAuditTrail_Display',type:'sap.ui.model.type.String'}" linkText="{i18n>SetupAuditTrail.SetupAuditTrail_Display/SetupAuditTrail_Display}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>SetupAuditTrail_Id/SetupAuditTrail_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>SetupAuditTrail.SetupAuditTrail_Id/SetupAuditTrail_Id}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>SetupAuditTrail_ResponsibleNamespacePrefix/SetupAuditTrail_ResponsibleNamespacePrefix',type:'sap.ui.model.type.String'}" linkText="{i18n>SetupAuditTrail.SetupAuditTrail_ResponsibleNamespacePrefix/SetupAuditTrail_ResponsibleNamespacePrefix}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>SetupAuditTrail_Section/SetupAuditTrail_Section',type:'sap.ui.model.type.String'}" linkText="{i18n>SetupAuditTrail.SetupAuditTrail_Section/SetupAuditTrail_Section}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>