<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>EnvironmentHub.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>EnvironmentHub.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="EnvironmentHub"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="EnvironmentHub"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EnvironmentHub.EnvironmentHubInvitation_EnvironmentHubId_Inverse}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>EnvironmentHubInvitation_EnvironmentHubId_Inverse/sfdc_EnvironmentHubInvitation_EnvironmentHubId_Inverse" entitySet="EnvironmentHubInvitation"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EnvironmentHub.EnvironmentHubMemberRel_EnvironmentHubId_Inverse}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>EnvironmentHubMemberRel_EnvironmentHubId_Inverse/sfdc_EnvironmentHubMemberRel_EnvironmentHubId_Inverse" entitySet="EnvironmentHubMemberRel"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EnvironmentHub.EnvironmentHubMember_EnvironmentHubId_Inverse}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>EnvironmentHubMember_EnvironmentHubId_Inverse/sfdc_EnvironmentHubMember_EnvironmentHubId_Inverse" entitySet="EnvironmentHubMember"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EnvironmentHub.EnvironmentHub_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>EnvironmentHub_CreatedById/sfdc_EnvironmentHub_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EnvironmentHub.EnvironmentHub_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>EnvironmentHub_CreatedDate/EnvironmentHub_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EnvironmentHub.EnvironmentHub_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>EnvironmentHub_Id/EnvironmentHub_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>EnvironmentHub.EnvironmentHub_Id/EnvironmentHub_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EnvironmentHub.EnvironmentHub_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>EnvironmentHub_IsDeleted/EnvironmentHub_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EnvironmentHub.EnvironmentHub_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>EnvironmentHub_LastModifiedById/sfdc_EnvironmentHub_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EnvironmentHub.EnvironmentHub_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>EnvironmentHub_LastModifiedDate/EnvironmentHub_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EnvironmentHub.EnvironmentHub_Name}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>EnvironmentHub_Name/EnvironmentHub_Name',type:'sap.ui.model.type.String' }" linkText="{i18n>EnvironmentHub.EnvironmentHub_Name/EnvironmentHub_Name}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EnvironmentHub.EnvironmentHub_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>EnvironmentHub_SystemModstamp/EnvironmentHub_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EnvironmentHub.EnvironmentHubInvitation_EnvironmentHubId_Inverse}" required="false"/> 
						<rdf:ListResource path="dataModel>EnvironmentHubInvitation_EnvironmentHubId_Inverse" entitySet="EnvironmentHubInvitation"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EnvironmentHub.EnvironmentHubMemberRel_EnvironmentHubId_Inverse}" required="false"/> 
						<rdf:ListResource path="dataModel>EnvironmentHubMemberRel_EnvironmentHubId_Inverse" entitySet="EnvironmentHubMemberRel"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EnvironmentHub.EnvironmentHubMember_EnvironmentHubId_Inverse}" required="false"/> 
						<rdf:ListResource path="dataModel>EnvironmentHubMember_EnvironmentHubId_Inverse" entitySet="EnvironmentHubMember"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EnvironmentHub.EnvironmentHub_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>EnvironmentHub_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>EnvironmentHub.EnvironmentHub_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>EnvironmentHub_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>