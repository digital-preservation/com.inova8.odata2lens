<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>Territory2s.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/Territory2' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>Territory2s.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Territory2s.Territory2_AccountAccessLevel}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Territory2s.Territory2_CaseAccessLevel}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Territory2s.Territory2_ContactAccessLevel}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Territory2s.Territory2_Description}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Territory2s.Territory2_DeveloperName}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Territory2s.Territory2_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Territory2s.Territory2_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Territory2s.Territory2_Name}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Territory2s.Territory2_OpportunityAccessLevel}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Territory2s.Territory2_SystemModstamp}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="Territory2"/>
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Territory2_AccountAccessLevel/Territory2_AccountAccessLevel',type:'sap.ui.model.type.String'}" linkText="{i18n>Territory2.Territory2_AccountAccessLevel/Territory2_AccountAccessLevel}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Territory2_CaseAccessLevel/Territory2_CaseAccessLevel',type:'sap.ui.model.type.String'}" linkText="{i18n>Territory2.Territory2_CaseAccessLevel/Territory2_CaseAccessLevel}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Territory2_ContactAccessLevel/Territory2_ContactAccessLevel',type:'sap.ui.model.type.String'}" linkText="{i18n>Territory2.Territory2_ContactAccessLevel/Territory2_ContactAccessLevel}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Territory2_Description/Territory2_Description',type:'sap.ui.model.type.String'}" linkText="{i18n>Territory2.Territory2_Description/Territory2_Description}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Territory2_DeveloperName/Territory2_DeveloperName',type:'sap.ui.model.type.String'}" linkText="{i18n>Territory2.Territory2_DeveloperName/Territory2_DeveloperName}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Territory2_Id/Territory2_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>Territory2.Territory2_Id/Territory2_Id}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>Territory2_LastModifiedDate/Territory2_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Territory2_Name/Territory2_Name',type:'sap.ui.model.type.String'}" linkText="{i18n>Territory2.Territory2_Name/Territory2_Name}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Territory2_OpportunityAccessLevel/Territory2_OpportunityAccessLevel',type:'sap.ui.model.type.String'}" linkText="{i18n>Territory2.Territory2_OpportunityAccessLevel/Territory2_OpportunityAccessLevel}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>Territory2_SystemModstamp/Territory2_SystemModstamp}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>