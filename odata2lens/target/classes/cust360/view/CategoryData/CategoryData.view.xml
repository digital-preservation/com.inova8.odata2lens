<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>CategoryData.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>CategoryData.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="CategoryData"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="CategoryData"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CategoryData.CategoryData_CategoryNodeId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>CategoryData_CategoryNodeId/sfdc_CategoryData_CategoryNodeId" entitySet="CategoryNode"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CategoryData.CategoryData_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>CategoryData_CreatedById/sfdc_CategoryData_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CategoryData.CategoryData_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>CategoryData_CreatedDate/CategoryData_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CategoryData.CategoryData_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>CategoryData_Id/CategoryData_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>CategoryData.CategoryData_Id/CategoryData_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CategoryData.CategoryData_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>CategoryData_IsDeleted/CategoryData_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CategoryData.CategoryData_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>CategoryData_LastModifiedById/sfdc_CategoryData_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CategoryData.CategoryData_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>CategoryData_LastModifiedDate/CategoryData_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CategoryData.CategoryData_RelatedSobjectId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>CategoryData_RelatedSobjectId/sfdc_CategoryData_RelatedSobjectId" entitySet="Solution"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CategoryData.CategoryData_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>CategoryData_SystemModstamp/CategoryData_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CategoryData.CategoryData_CategoryNodeId}" required="false"/> 
						<rdf:ListResource path="dataModel>CategoryData_CategoryNodeId" entitySet="CategoryNode"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CategoryData.CategoryData_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>CategoryData_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CategoryData.CategoryData_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>CategoryData_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CategoryData.CategoryData_RelatedSobjectId}" required="false"/> 
						<rdf:ListResource path="dataModel>CategoryData_RelatedSobjectId" entitySet="Solution"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>