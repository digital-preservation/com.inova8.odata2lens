<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>AuthConfigProviders.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>AuthConfigProviders.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="AuthConfigProviders"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="AuthConfigProviders"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AuthConfigProviders.AuthConfigProviders_AuthConfigId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>AuthConfigProviders_AuthConfigId/sfdc_AuthConfigProviders_AuthConfigId" entitySet="AuthConfig"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AuthConfigProviders.AuthConfigProviders_AuthProviderId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>AuthConfigProviders_AuthProviderId/sfdc_AuthConfigProviders_AuthProviderId" entitySet="AuthProvider"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AuthConfigProviders.AuthConfigProviders_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>AuthConfigProviders_CreatedById/sfdc_AuthConfigProviders_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AuthConfigProviders.AuthConfigProviders_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>AuthConfigProviders_CreatedDate/AuthConfigProviders_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AuthConfigProviders.AuthConfigProviders_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>AuthConfigProviders_Id/AuthConfigProviders_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>AuthConfigProviders.AuthConfigProviders_Id/AuthConfigProviders_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AuthConfigProviders.AuthConfigProviders_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>AuthConfigProviders_IsDeleted/AuthConfigProviders_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AuthConfigProviders.AuthConfigProviders_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>AuthConfigProviders_LastModifiedById/sfdc_AuthConfigProviders_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AuthConfigProviders.AuthConfigProviders_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>AuthConfigProviders_LastModifiedDate/AuthConfigProviders_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AuthConfigProviders.AuthConfigProviders_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>AuthConfigProviders_SystemModstamp/AuthConfigProviders_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AuthConfigProviders.AuthConfigProviders_AuthConfigId}" required="false"/> 
						<rdf:ListResource path="dataModel>AuthConfigProviders_AuthConfigId" entitySet="AuthConfig"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AuthConfigProviders.AuthConfigProviders_AuthProviderId}" required="false"/> 
						<rdf:ListResource path="dataModel>AuthConfigProviders_AuthProviderId" entitySet="AuthProvider"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AuthConfigProviders.AuthConfigProviders_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>AuthConfigProviders_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>AuthConfigProviders.AuthConfigProviders_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>AuthConfigProviders_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>