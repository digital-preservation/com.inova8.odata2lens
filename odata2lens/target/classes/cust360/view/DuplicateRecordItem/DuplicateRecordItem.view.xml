<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>DuplicateRecordItem.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>DuplicateRecordItem.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="DuplicateRecordItem"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="DuplicateRecordItem"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>DuplicateRecordItem.DuplicateRecordItem_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>DuplicateRecordItem_CreatedById/sfdc_DuplicateRecordItem_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>DuplicateRecordItem.DuplicateRecordItem_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>DuplicateRecordItem_CreatedDate/DuplicateRecordItem_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>DuplicateRecordItem.DuplicateRecordItem_DuplicateRecordSetId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>DuplicateRecordItem_DuplicateRecordSetId/sfdc_DuplicateRecordItem_DuplicateRecordSetId" entitySet="DuplicateRecordSet"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>DuplicateRecordItem.DuplicateRecordItem_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>DuplicateRecordItem_Id/DuplicateRecordItem_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>DuplicateRecordItem.DuplicateRecordItem_Id/DuplicateRecordItem_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>DuplicateRecordItem.DuplicateRecordItem_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>DuplicateRecordItem_IsDeleted/DuplicateRecordItem_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>DuplicateRecordItem.DuplicateRecordItem_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>DuplicateRecordItem_LastModifiedById/sfdc_DuplicateRecordItem_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>DuplicateRecordItem.DuplicateRecordItem_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>DuplicateRecordItem_LastModifiedDate/DuplicateRecordItem_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>DuplicateRecordItem.DuplicateRecordItem_Name}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>DuplicateRecordItem_Name/DuplicateRecordItem_Name',type:'sap.ui.model.type.String' }" linkText="{i18n>DuplicateRecordItem.DuplicateRecordItem_Name/DuplicateRecordItem_Name}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>DuplicateRecordItem.DuplicateRecordItem_RecordId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>DuplicateRecordItem_RecordId/sfdc_DuplicateRecordItem_RecordId" entitySet="Account"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>DuplicateRecordItem.DuplicateRecordItem_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>DuplicateRecordItem_SystemModstamp/DuplicateRecordItem_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>DuplicateRecordItem.DuplicateRecordItem_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>DuplicateRecordItem_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>DuplicateRecordItem.DuplicateRecordItem_DuplicateRecordSetId}" required="false"/> 
						<rdf:ListResource path="dataModel>DuplicateRecordItem_DuplicateRecordSetId" entitySet="DuplicateRecordSet"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>DuplicateRecordItem.DuplicateRecordItem_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>DuplicateRecordItem_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>DuplicateRecordItem.DuplicateRecordItem_RecordId}" required="false"/> 
						<rdf:ListResource path="dataModel>DuplicateRecordItem_RecordId" entitySet="Account"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>