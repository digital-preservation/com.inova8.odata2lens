<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>ApexTestSuite.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>ApexTestSuite.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="ApexTestSuite"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="ApexTestSuite"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexTestSuite.ApexTestSuite_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ApexTestSuite_CreatedById/sfdc_ApexTestSuite_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexTestSuite.ApexTestSuite_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>ApexTestSuite_CreatedDate/ApexTestSuite_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexTestSuite.ApexTestSuite_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ApexTestSuite_Id/ApexTestSuite_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>ApexTestSuite.ApexTestSuite_Id/ApexTestSuite_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexTestSuite.ApexTestSuite_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>ApexTestSuite_IsDeleted/ApexTestSuite_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexTestSuite.ApexTestSuite_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ApexTestSuite_LastModifiedById/sfdc_ApexTestSuite_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexTestSuite.ApexTestSuite_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>ApexTestSuite_LastModifiedDate/ApexTestSuite_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexTestSuite.ApexTestSuite_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>ApexTestSuite_SystemModstamp/ApexTestSuite_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexTestSuite.ApexTestSuite_TestSuiteName}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ApexTestSuite_TestSuiteName/ApexTestSuite_TestSuiteName',type:'sap.ui.model.type.String' }" linkText="{i18n>ApexTestSuite.ApexTestSuite_TestSuiteName/ApexTestSuite_TestSuiteName}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexTestSuite.TestSuiteMembership_ApexTestSuiteId_Inverse}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>TestSuiteMembership_ApexTestSuiteId_Inverse/sfdc_TestSuiteMembership_ApexTestSuiteId_Inverse" entitySet="TestSuiteMembership"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexTestSuite.ApexTestSuite_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>ApexTestSuite_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexTestSuite.ApexTestSuite_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>ApexTestSuite_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexTestSuite.TestSuiteMembership_ApexTestSuiteId_Inverse}" required="false"/> 
						<rdf:ListResource path="dataModel>TestSuiteMembership_ApexTestSuiteId_Inverse" entitySet="TestSuiteMembership"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>