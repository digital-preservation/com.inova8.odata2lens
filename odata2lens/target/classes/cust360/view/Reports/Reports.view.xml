<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>Reports.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/Report' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>Reports.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Reports.Report_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Reports.Report_Description}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Reports.Report_DeveloperName}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Reports.Report_FolderName}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Reports.Report_Format}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Reports.Report_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Reports.Report_IsDeleted}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Reports.Report_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Reports.Report_LastReferencedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Reports.Report_LastRunDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Reports.Report_LastViewedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Reports.Report_Name}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Reports.Report_NamespacePrefix}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Reports.Report_SystemModstamp}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="Report"/>
							<VBox>					
							<Text text="{dataModel>Report_CreatedDate/Report_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Report_Description/Report_Description',type:'sap.ui.model.type.String'}" linkText="{i18n>Report.Report_Description/Report_Description}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Report_DeveloperName/Report_DeveloperName',type:'sap.ui.model.type.String'}" linkText="{i18n>Report.Report_DeveloperName/Report_DeveloperName}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Report_FolderName/Report_FolderName',type:'sap.ui.model.type.String'}" linkText="{i18n>Report.Report_FolderName/Report_FolderName}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Report_Format/Report_Format',type:'sap.ui.model.type.String'}" linkText="{i18n>Report.Report_Format/Report_Format}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Report_Id/Report_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>Report.Report_Id/Report_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>Report_IsDeleted/Report_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<Text text="{dataModel>Report_LastModifiedDate/Report_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>Report_LastReferencedDate/Report_LastReferencedDate}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>Report_LastRunDate/Report_LastRunDate}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>Report_LastViewedDate/Report_LastViewedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Report_Name/Report_Name',type:'sap.ui.model.type.String'}" linkText="{i18n>Report.Report_Name/Report_Name}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Report_NamespacePrefix/Report_NamespacePrefix',type:'sap.ui.model.type.String'}" linkText="{i18n>Report.Report_NamespacePrefix/Report_NamespacePrefix}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>Report_SystemModstamp/Report_SystemModstamp}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>