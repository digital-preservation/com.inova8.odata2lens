<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>FileSearchActivitys.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/FileSearchActivity' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>FileSearchActivitys.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>FileSearchActivitys.FileSearchActivity_AvgNumResults}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>FileSearchActivitys.FileSearchActivity_ClickRank}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>FileSearchActivitys.FileSearchActivity_CountQueries}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>FileSearchActivitys.FileSearchActivity_CountUsers}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>FileSearchActivitys.FileSearchActivity_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>FileSearchActivitys.FileSearchActivity_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>FileSearchActivitys.FileSearchActivity_IsDeleted}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>FileSearchActivitys.FileSearchActivity_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>FileSearchActivitys.FileSearchActivity_Name}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>FileSearchActivitys.FileSearchActivity_Period}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>FileSearchActivitys.FileSearchActivity_QueryDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>FileSearchActivitys.FileSearchActivity_QueryLanguage}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>FileSearchActivitys.FileSearchActivity_SearchTerm}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>FileSearchActivitys.FileSearchActivity_SystemModstamp}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="FileSearchActivity"/>
							<VBox>					
							<Text text="{path:'dataModel>FileSearchActivity_AvgNumResults/FileSearchActivity_AvgNumResults',type:'sap.ui.model.type.Float', formatOptions:{'style':'short','maxFractionDigits':2} }"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>FileSearchActivity_ClickRank/FileSearchActivity_ClickRank',type:'sap.ui.model.type.Float', formatOptions:{'style':'short','maxFractionDigits':2} }"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>FileSearchActivity_CountQueries/FileSearchActivity_CountQueries',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>FileSearchActivity_CountUsers/FileSearchActivity_CountUsers',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>FileSearchActivity_CreatedDate/FileSearchActivity_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>FileSearchActivity_Id/FileSearchActivity_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>FileSearchActivity.FileSearchActivity_Id/FileSearchActivity_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>FileSearchActivity_IsDeleted/FileSearchActivity_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<Text text="{dataModel>FileSearchActivity_LastModifiedDate/FileSearchActivity_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>FileSearchActivity_Name/FileSearchActivity_Name',type:'sap.ui.model.type.String'}" linkText="{i18n>FileSearchActivity.FileSearchActivity_Name/FileSearchActivity_Name}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>FileSearchActivity_Period/FileSearchActivity_Period',type:'sap.ui.model.type.String'}" linkText="{i18n>FileSearchActivity.FileSearchActivity_Period/FileSearchActivity_Period}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>FileSearchActivity_QueryDate/FileSearchActivity_QueryDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>FileSearchActivity_QueryLanguage/FileSearchActivity_QueryLanguage',type:'sap.ui.model.type.String'}" linkText="{i18n>FileSearchActivity.FileSearchActivity_QueryLanguage/FileSearchActivity_QueryLanguage}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>FileSearchActivity_SearchTerm/FileSearchActivity_SearchTerm',type:'sap.ui.model.type.String'}" linkText="{i18n>FileSearchActivity.FileSearchActivity_SearchTerm/FileSearchActivity_SearchTerm}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>FileSearchActivity_SystemModstamp/FileSearchActivity_SystemModstamp}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>