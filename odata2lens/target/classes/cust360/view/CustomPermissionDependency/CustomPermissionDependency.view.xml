<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>CustomPermissionDependency.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>CustomPermissionDependency.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="CustomPermissionDependency"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="CustomPermissionDependency"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CustomPermissionDependency.CustomPermissionDependency_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>CustomPermissionDependency_CreatedById/sfdc_CustomPermissionDependency_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CustomPermissionDependency.CustomPermissionDependency_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>CustomPermissionDependency_CreatedDate/CustomPermissionDependency_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CustomPermissionDependency.CustomPermissionDependency_CustomPermissionId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>CustomPermissionDependency_CustomPermissionId/sfdc_CustomPermissionDependency_CustomPermissionId" entitySet="CustomPermission"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CustomPermissionDependency.CustomPermissionDependency_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>CustomPermissionDependency_Id/CustomPermissionDependency_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>CustomPermissionDependency.CustomPermissionDependency_Id/CustomPermissionDependency_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CustomPermissionDependency.CustomPermissionDependency_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>CustomPermissionDependency_IsDeleted/CustomPermissionDependency_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CustomPermissionDependency.CustomPermissionDependency_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>CustomPermissionDependency_LastModifiedById/sfdc_CustomPermissionDependency_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CustomPermissionDependency.CustomPermissionDependency_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>CustomPermissionDependency_LastModifiedDate/CustomPermissionDependency_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CustomPermissionDependency.CustomPermissionDependency_RequiredCustomPermissionId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>CustomPermissionDependency_RequiredCustomPermissionId/sfdc_CustomPermissionDependency_RequiredCustomPermissionId" entitySet="CustomPermission"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CustomPermissionDependency.CustomPermissionDependency_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>CustomPermissionDependency_SystemModstamp/CustomPermissionDependency_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CustomPermissionDependency.CustomPermissionDependency_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>CustomPermissionDependency_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CustomPermissionDependency.CustomPermissionDependency_CustomPermissionId}" required="false"/> 
						<rdf:ListResource path="dataModel>CustomPermissionDependency_CustomPermissionId" entitySet="CustomPermission"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CustomPermissionDependency.CustomPermissionDependency_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>CustomPermissionDependency_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>CustomPermissionDependency.CustomPermissionDependency_RequiredCustomPermissionId}" required="false"/> 
						<rdf:ListResource path="dataModel>CustomPermissionDependency_RequiredCustomPermissionId" entitySet="CustomPermission"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>