<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>ThirdPartyAccountLink.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>ThirdPartyAccountLink.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="ThirdPartyAccountLink"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="ThirdPartyAccountLink"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ThirdPartyAccountLink.ThirdPartyAccountLink_Handle}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ThirdPartyAccountLink_Handle/ThirdPartyAccountLink_Handle',type:'sap.ui.model.type.String' }" linkText="{i18n>ThirdPartyAccountLink.ThirdPartyAccountLink_Handle/ThirdPartyAccountLink_Handle}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ThirdPartyAccountLink.ThirdPartyAccountLink_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ThirdPartyAccountLink_Id/ThirdPartyAccountLink_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>ThirdPartyAccountLink.ThirdPartyAccountLink_Id/ThirdPartyAccountLink_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ThirdPartyAccountLink.ThirdPartyAccountLink_IsNotSsoUsable}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>ThirdPartyAccountLink_IsNotSsoUsable/ThirdPartyAccountLink_IsNotSsoUsable',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ThirdPartyAccountLink.ThirdPartyAccountLink_Provider}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ThirdPartyAccountLink_Provider/ThirdPartyAccountLink_Provider',type:'sap.ui.model.type.String' }" linkText="{i18n>ThirdPartyAccountLink.ThirdPartyAccountLink_Provider/ThirdPartyAccountLink_Provider}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ThirdPartyAccountLink.ThirdPartyAccountLink_RemoteIdentifier}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ThirdPartyAccountLink_RemoteIdentifier/ThirdPartyAccountLink_RemoteIdentifier',type:'sap.ui.model.type.String' }" linkText="{i18n>ThirdPartyAccountLink.ThirdPartyAccountLink_RemoteIdentifier/ThirdPartyAccountLink_RemoteIdentifier}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ThirdPartyAccountLink.ThirdPartyAccountLink_SsoProviderId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ThirdPartyAccountLink_SsoProviderId/sfdc_ThirdPartyAccountLink_SsoProviderId" entitySet="AuthProvider"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ThirdPartyAccountLink.ThirdPartyAccountLink_SsoProviderName}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ThirdPartyAccountLink_SsoProviderName/ThirdPartyAccountLink_SsoProviderName',type:'sap.ui.model.type.String' }" linkText="{i18n>ThirdPartyAccountLink.ThirdPartyAccountLink_SsoProviderName/ThirdPartyAccountLink_SsoProviderName}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ThirdPartyAccountLink.ThirdPartyAccountLink_ThirdPartyAccountLinkKey}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ThirdPartyAccountLink_ThirdPartyAccountLinkKey/ThirdPartyAccountLink_ThirdPartyAccountLinkKey',type:'sap.ui.model.type.String' }" linkText="{i18n>ThirdPartyAccountLink.ThirdPartyAccountLink_ThirdPartyAccountLinkKey/ThirdPartyAccountLink_ThirdPartyAccountLinkKey}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ThirdPartyAccountLink.ThirdPartyAccountLink_UserId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ThirdPartyAccountLink_UserId/sfdc_ThirdPartyAccountLink_UserId" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ThirdPartyAccountLink.ThirdPartyAccountLink_SsoProviderId}" required="false"/> 
						<rdf:ListResource path="dataModel>ThirdPartyAccountLink_SsoProviderId" entitySet="AuthProvider"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ThirdPartyAccountLink.ThirdPartyAccountLink_UserId}" required="false"/> 
						<rdf:ListResource path="dataModel>ThirdPartyAccountLink_UserId" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>