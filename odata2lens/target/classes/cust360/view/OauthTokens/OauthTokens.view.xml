<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>OauthTokens.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/OauthToken' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>OauthTokens.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>OauthTokens.OauthToken_AccessToken}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>OauthTokens.OauthToken_AppName}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>OauthTokens.OauthToken_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>OauthTokens.OauthToken_DeleteToken}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>OauthTokens.OauthToken_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>OauthTokens.OauthToken_LastUsedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>OauthTokens.OauthToken_RequestToken}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>OauthTokens.OauthToken_UseCount}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="OauthToken"/>
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>OauthToken_AccessToken/OauthToken_AccessToken',type:'sap.ui.model.type.String'}" linkText="{i18n>OauthToken.OauthToken_AccessToken/OauthToken_AccessToken}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>OauthToken_AppName/OauthToken_AppName',type:'sap.ui.model.type.String'}" linkText="{i18n>OauthToken.OauthToken_AppName/OauthToken_AppName}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>OauthToken_CreatedDate/OauthToken_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>OauthToken_DeleteToken/OauthToken_DeleteToken',type:'sap.ui.model.type.String'}" linkText="{i18n>OauthToken.OauthToken_DeleteToken/OauthToken_DeleteToken}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>OauthToken_Id/OauthToken_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>OauthToken.OauthToken_Id/OauthToken_Id}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>OauthToken_LastUsedDate/OauthToken_LastUsedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>OauthToken_RequestToken/OauthToken_RequestToken',type:'sap.ui.model.type.String'}" linkText="{i18n>OauthToken.OauthToken_RequestToken/OauthToken_RequestToken}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>OauthToken_UseCount/OauthToken_UseCount',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>