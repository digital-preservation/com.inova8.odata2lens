					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>FlowInterview.FlowInterview_Name}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>FlowInterview_Name/FlowInterview_Name',type:'sap.ui.model.type.String' }" linkText="{i18n>FlowInterview.FlowInterview_Name/FlowInterview_Name}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>FlowInterview.FlowInterview_OwnerId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>FlowInterview_OwnerId/sfdc_FlowInterview_OwnerId" entitySet="Group"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
