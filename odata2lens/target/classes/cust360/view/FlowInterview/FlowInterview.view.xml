					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>FlowInterview.FlowInterview_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>FlowInterview_SystemModstamp/FlowInterview_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>FlowInterview.FlowInterviewShare_ParentId_Inverse}" required="false"/> 
						<rdf:ListResource path="dataModel>FlowInterviewShare_ParentId_Inverse" entitySet="FlowInterviewShare"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>FlowInterview.FlowInterview_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>FlowInterview_CreatedById" entitySet="User"/>
