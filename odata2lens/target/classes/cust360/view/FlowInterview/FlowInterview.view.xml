					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>FlowInterview.FlowInterview_Guid}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>FlowInterview_Guid/FlowInterview_Guid',type:'sap.ui.model.type.String' }" linkText="{i18n>FlowInterview.FlowInterview_Guid/FlowInterview_Guid}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>FlowInterview.FlowInterview_Id}" required="false"/> 
						<Text text=""/>
