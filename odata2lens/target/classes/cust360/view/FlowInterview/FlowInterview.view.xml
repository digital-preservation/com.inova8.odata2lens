				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="FlowInterview"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>FlowInterview.FlowInterviewShare_ParentId_Inverse}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>FlowInterviewShare_ParentId_Inverse/sfdc_FlowInterviewShare_ParentId_Inverse" entitySet="FlowInterviewShare"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>FlowInterview.FlowInterview_CreatedById}" required="false"/> 
