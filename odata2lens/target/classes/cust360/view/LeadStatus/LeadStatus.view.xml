<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>LeadStatus.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>LeadStatus.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="LeadStatus"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="LeadStatus"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>LeadStatus.LeadStatus_ApiName}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>LeadStatus_ApiName/LeadStatus_ApiName',type:'sap.ui.model.type.String' }" linkText="{i18n>LeadStatus.LeadStatus_ApiName/LeadStatus_ApiName}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>LeadStatus.LeadStatus_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>LeadStatus_CreatedById/sfdc_LeadStatus_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>LeadStatus.LeadStatus_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>LeadStatus_CreatedDate/LeadStatus_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>LeadStatus.LeadStatus_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>LeadStatus_Id/LeadStatus_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>LeadStatus.LeadStatus_Id/LeadStatus_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>LeadStatus.LeadStatus_IsConverted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>LeadStatus_IsConverted/LeadStatus_IsConverted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>LeadStatus.LeadStatus_IsDefault}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>LeadStatus_IsDefault/LeadStatus_IsDefault',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>LeadStatus.LeadStatus_LastModifiedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>LeadStatus_LastModifiedById/sfdc_LeadStatus_LastModifiedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>LeadStatus.LeadStatus_LastModifiedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>LeadStatus_LastModifiedDate/LeadStatus_LastModifiedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>LeadStatus.LeadStatus_MasterLabel}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>LeadStatus_MasterLabel/LeadStatus_MasterLabel',type:'sap.ui.model.type.String' }" linkText="{i18n>LeadStatus.LeadStatus_MasterLabel/LeadStatus_MasterLabel}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>LeadStatus.LeadStatus_SortOrder}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>LeadStatus_SortOrder/LeadStatus_SortOrder',type:'sap.ui.model.type.Integer' }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>LeadStatus.LeadStatus_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>LeadStatus_SystemModstamp/LeadStatus_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>LeadStatus.LeadStatus_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>LeadStatus_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>LeadStatus.LeadStatus_LastModifiedById}" required="false"/> 
						<rdf:ListResource path="dataModel>LeadStatus_LastModifiedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>