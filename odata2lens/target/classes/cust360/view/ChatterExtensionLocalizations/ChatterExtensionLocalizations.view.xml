<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>ChatterExtensionLocalizations.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/ChatterExtensionLocalization' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>ChatterExtensionLocalizations.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ChatterExtensionLocalizations.ChatterExtensionLocalization_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ChatterExtensionLocalizations.ChatterExtensionLocalization_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ChatterExtensionLocalizations.ChatterExtensionLocalization_Language}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ChatterExtensionLocalizations.ChatterExtensionLocalization_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ChatterExtensionLocalizations.ChatterExtensionLocalization_NamespacePrefix}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ChatterExtensionLocalizations.ChatterExtensionLocalization_SystemModstamp}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ChatterExtensionLocalizations.ChatterExtensionLocalization_Value}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="ChatterExtensionLocalization"/>
							<VBox>					
							<Text text="{dataModel>ChatterExtensionLocalization_CreatedDate/ChatterExtensionLocalization_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ChatterExtensionLocalization_Id/ChatterExtensionLocalization_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>ChatterExtensionLocalization.ChatterExtensionLocalization_Id/ChatterExtensionLocalization_Id}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ChatterExtensionLocalization_Language/ChatterExtensionLocalization_Language',type:'sap.ui.model.type.String'}" linkText="{i18n>ChatterExtensionLocalization.ChatterExtensionLocalization_Language/ChatterExtensionLocalization_Language}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>ChatterExtensionLocalization_LastModifiedDate/ChatterExtensionLocalization_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ChatterExtensionLocalization_NamespacePrefix/ChatterExtensionLocalization_NamespacePrefix',type:'sap.ui.model.type.String'}" linkText="{i18n>ChatterExtensionLocalization.ChatterExtensionLocalization_NamespacePrefix/ChatterExtensionLocalization_NamespacePrefix}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>ChatterExtensionLocalization_SystemModstamp/ChatterExtensionLocalization_SystemModstamp}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ChatterExtensionLocalization_Value/ChatterExtensionLocalization_Value',type:'sap.ui.model.type.String'}" linkText="{i18n>ChatterExtensionLocalization.ChatterExtensionLocalization_Value/ChatterExtensionLocalization_Value}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>