<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>IdpEventLogs.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/IdpEventLog' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>IdpEventLogs.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>IdpEventLogs.IdpEventLog_ErrorCode}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>IdpEventLogs.IdpEventLog_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>IdpEventLogs.IdpEventLog_IdentityUsed}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>IdpEventLogs.IdpEventLog_InitiatedBy}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>IdpEventLogs.IdpEventLog_OptionsHasLogoutUrl}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>IdpEventLogs.IdpEventLog_SamlEntityUrl}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>IdpEventLogs.IdpEventLog_SsoType}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>IdpEventLogs.IdpEventLog_Timestamp}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="IdpEventLog"/>
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>IdpEventLog_ErrorCode/IdpEventLog_ErrorCode',type:'sap.ui.model.type.String'}" linkText="{i18n>IdpEventLog.IdpEventLog_ErrorCode/IdpEventLog_ErrorCode}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>IdpEventLog_Id/IdpEventLog_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>IdpEventLog.IdpEventLog_Id/IdpEventLog_Id}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>IdpEventLog_IdentityUsed/IdpEventLog_IdentityUsed',type:'sap.ui.model.type.String'}" linkText="{i18n>IdpEventLog.IdpEventLog_IdentityUsed/IdpEventLog_IdentityUsed}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>IdpEventLog_InitiatedBy/IdpEventLog_InitiatedBy',type:'sap.ui.model.type.String'}" linkText="{i18n>IdpEventLog.IdpEventLog_InitiatedBy/IdpEventLog_InitiatedBy}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>IdpEventLog_OptionsHasLogoutUrl/IdpEventLog_OptionsHasLogoutUrl',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>IdpEventLog_SamlEntityUrl/IdpEventLog_SamlEntityUrl',type:'sap.ui.model.type.String'}" linkText="{i18n>IdpEventLog.IdpEventLog_SamlEntityUrl/IdpEventLog_SamlEntityUrl}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>IdpEventLog_SsoType/IdpEventLog_SsoType',type:'sap.ui.model.type.String'}" linkText="{i18n>IdpEventLog.IdpEventLog_SsoType/IdpEventLog_SsoType}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>IdpEventLog_Timestamp/IdpEventLog_Timestamp}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>