<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>ContentDistributionView.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>ContentDistributionView.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="ContentDistributionView"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="ContentDistributionView"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentDistributionView.ContentDistributionView_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ContentDistributionView_CreatedById/sfdc_ContentDistributionView_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentDistributionView.ContentDistributionView_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>ContentDistributionView_CreatedDate/ContentDistributionView_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentDistributionView.ContentDistributionView_DistributionId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ContentDistributionView_DistributionId/sfdc_ContentDistributionView_DistributionId" entitySet="ContentDistribution"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentDistributionView.ContentDistributionView_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ContentDistributionView_Id/ContentDistributionView_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>ContentDistributionView.ContentDistributionView_Id/ContentDistributionView_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentDistributionView.ContentDistributionView_IsDeleted}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>ContentDistributionView_IsDeleted/ContentDistributionView_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentDistributionView.ContentDistributionView_IsDownload}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>ContentDistributionView_IsDownload/ContentDistributionView_IsDownload',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentDistributionView.ContentDistributionView_IsInternal}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>ContentDistributionView_IsInternal/ContentDistributionView_IsInternal',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentDistributionView.ContentDistributionView_ParentViewId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ContentDistributionView_ParentViewId/sfdc_ContentDistributionView_ParentViewId" entitySet="ContentDistributionView"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentDistributionView.ContentDistributionView_ParentViewId_Inverse}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ContentDistributionView_ParentViewId_Inverse/sfdc_ContentDistributionView_ParentViewId_Inverse" entitySet="ContentDistributionView"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentDistributionView.ContentDistributionView_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>ContentDistributionView_SystemModstamp/ContentDistributionView_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentDistributionView.ContentDistributionView_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>ContentDistributionView_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentDistributionView.ContentDistributionView_DistributionId}" required="false"/> 
						<rdf:ListResource path="dataModel>ContentDistributionView_DistributionId" entitySet="ContentDistribution"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentDistributionView.ContentDistributionView_ParentViewId}" required="false"/> 
						<rdf:ListResource path="dataModel>ContentDistributionView_ParentViewId" entitySet="ContentDistributionView"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ContentDistributionView.ContentDistributionView_ParentViewId_Inverse}" required="false"/> 
						<rdf:ListResource path="dataModel>ContentDistributionView_ParentViewId_Inverse" entitySet="ContentDistributionView"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>