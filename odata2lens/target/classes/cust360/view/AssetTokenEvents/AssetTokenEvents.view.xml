<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>AssetTokenEvents.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/AssetTokenEvent' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>AssetTokenEvents.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AssetTokenEvents.AssetTokenEvent_ActorTokenPayload}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AssetTokenEvents.AssetTokenEvent_AssetName}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AssetTokenEvents.AssetTokenEvent_AssetSerialNumber}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AssetTokenEvents.AssetTokenEvent_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AssetTokenEvents.AssetTokenEvent_DeviceId}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AssetTokenEvents.AssetTokenEvent_DeviceKey}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AssetTokenEvents.AssetTokenEvent_Expiration}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AssetTokenEvents.AssetTokenEvent_Name}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AssetTokenEvents.AssetTokenEvent_ReplayId}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="AssetTokenEvent"/>
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>AssetTokenEvent_ActorTokenPayload/AssetTokenEvent_ActorTokenPayload',type:'sap.ui.model.type.String'}" linkText="{i18n>AssetTokenEvent.AssetTokenEvent_ActorTokenPayload/AssetTokenEvent_ActorTokenPayload}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>AssetTokenEvent_AssetName/AssetTokenEvent_AssetName',type:'sap.ui.model.type.String'}" linkText="{i18n>AssetTokenEvent.AssetTokenEvent_AssetName/AssetTokenEvent_AssetName}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>AssetTokenEvent_AssetSerialNumber/AssetTokenEvent_AssetSerialNumber',type:'sap.ui.model.type.String'}" linkText="{i18n>AssetTokenEvent.AssetTokenEvent_AssetSerialNumber/AssetTokenEvent_AssetSerialNumber}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>AssetTokenEvent_CreatedDate/AssetTokenEvent_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>AssetTokenEvent_DeviceId/AssetTokenEvent_DeviceId',type:'sap.ui.model.type.String'}" linkText="{i18n>AssetTokenEvent.AssetTokenEvent_DeviceId/AssetTokenEvent_DeviceId}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>AssetTokenEvent_DeviceKey/AssetTokenEvent_DeviceKey',type:'sap.ui.model.type.String'}" linkText="{i18n>AssetTokenEvent.AssetTokenEvent_DeviceKey/AssetTokenEvent_DeviceKey}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>AssetTokenEvent_Expiration/AssetTokenEvent_Expiration}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>AssetTokenEvent_Name/AssetTokenEvent_Name',type:'sap.ui.model.type.String'}" linkText="{i18n>AssetTokenEvent.AssetTokenEvent_Name/AssetTokenEvent_Name}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>AssetTokenEvent_ReplayId/AssetTokenEvent_ReplayId',type:'sap.ui.model.type.String'}" linkText="{i18n>AssetTokenEvent.AssetTokenEvent_ReplayId/AssetTokenEvent_ReplayId}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>