<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>ProcessInstanceStep.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>ProcessInstanceStep.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="ProcessInstanceStep"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="ProcessInstanceStep"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ProcessInstanceStep.ProcessInstanceStep_ActorId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ProcessInstanceStep_ActorId/sfdc_ProcessInstanceStep_ActorId" entitySet="Group"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ProcessInstanceStep.ProcessInstanceStep_Comments}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ProcessInstanceStep_Comments/ProcessInstanceStep_Comments',type:'sap.ui.model.type.String' }" linkText="{i18n>ProcessInstanceStep.ProcessInstanceStep_Comments/ProcessInstanceStep_Comments}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ProcessInstanceStep.ProcessInstanceStep_CreatedById}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ProcessInstanceStep_CreatedById/sfdc_ProcessInstanceStep_CreatedById" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ProcessInstanceStep.ProcessInstanceStep_CreatedDate}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>ProcessInstanceStep_CreatedDate/ProcessInstanceStep_CreatedDate}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ProcessInstanceStep.ProcessInstanceStep_ElapsedTimeInDays}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>ProcessInstanceStep_ElapsedTimeInDays/ProcessInstanceStep_ElapsedTimeInDays',type:'sap.ui.model.type.Float', formatOptions:{'style':'short','maxFractionDigits':2} }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ProcessInstanceStep.ProcessInstanceStep_ElapsedTimeInHours}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>ProcessInstanceStep_ElapsedTimeInHours/ProcessInstanceStep_ElapsedTimeInHours',type:'sap.ui.model.type.Float', formatOptions:{'style':'short','maxFractionDigits':2} }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ProcessInstanceStep.ProcessInstanceStep_ElapsedTimeInMinutes}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>ProcessInstanceStep_ElapsedTimeInMinutes/ProcessInstanceStep_ElapsedTimeInMinutes',type:'sap.ui.model.type.Float', formatOptions:{'style':'short','maxFractionDigits':2} }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ProcessInstanceStep.ProcessInstanceStep_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ProcessInstanceStep_Id/ProcessInstanceStep_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>ProcessInstanceStep.ProcessInstanceStep_Id/ProcessInstanceStep_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ProcessInstanceStep.ProcessInstanceStep_OriginalActorId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ProcessInstanceStep_OriginalActorId/sfdc_ProcessInstanceStep_OriginalActorId" entitySet="Group"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ProcessInstanceStep.ProcessInstanceStep_ProcessInstanceId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ProcessInstanceStep_ProcessInstanceId/sfdc_ProcessInstanceStep_ProcessInstanceId" entitySet="ProcessInstance"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ProcessInstanceStep.ProcessInstanceStep_StepNodeId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ProcessInstanceStep_StepNodeId/sfdc_ProcessInstanceStep_StepNodeId" entitySet="ProcessNode"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ProcessInstanceStep.ProcessInstanceStep_StepStatus}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ProcessInstanceStep_StepStatus/ProcessInstanceStep_StepStatus',type:'sap.ui.model.type.String' }" linkText="{i18n>ProcessInstanceStep.ProcessInstanceStep_StepStatus/ProcessInstanceStep_StepStatus}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ProcessInstanceStep.ProcessInstanceStep_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>ProcessInstanceStep_SystemModstamp/ProcessInstanceStep_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ProcessInstanceStep.ProcessInstanceStep_ActorId}" required="false"/> 
						<rdf:ListResource path="dataModel>ProcessInstanceStep_ActorId" entitySet="Group"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ProcessInstanceStep.ProcessInstanceStep_CreatedById}" required="false"/> 
						<rdf:ListResource path="dataModel>ProcessInstanceStep_CreatedById" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ProcessInstanceStep.ProcessInstanceStep_OriginalActorId}" required="false"/> 
						<rdf:ListResource path="dataModel>ProcessInstanceStep_OriginalActorId" entitySet="Group"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ProcessInstanceStep.ProcessInstanceStep_ProcessInstanceId}" required="false"/> 
						<rdf:ListResource path="dataModel>ProcessInstanceStep_ProcessInstanceId" entitySet="ProcessInstance"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ProcessInstanceStep.ProcessInstanceStep_StepNodeId}" required="false"/> 
						<rdf:ListResource path="dataModel>ProcessInstanceStep_StepNodeId" entitySet="ProcessNode"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>