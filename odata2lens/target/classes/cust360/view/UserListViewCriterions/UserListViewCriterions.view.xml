<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>UserListViewCriterions.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/UserListViewCriterion' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>UserListViewCriterions.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserListViewCriterions.UserListViewCriterion_ColumnName}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserListViewCriterions.UserListViewCriterion_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserListViewCriterions.UserListViewCriterion_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserListViewCriterions.UserListViewCriterion_IsDeleted}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserListViewCriterions.UserListViewCriterion_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserListViewCriterions.UserListViewCriterion_Operation}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserListViewCriterions.UserListViewCriterion_SortOrder}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserListViewCriterions.UserListViewCriterion_SystemModstamp}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserListViewCriterions.UserListViewCriterion_Value}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="UserListViewCriterion"/>
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>UserListViewCriterion_ColumnName/UserListViewCriterion_ColumnName',type:'sap.ui.model.type.String'}" linkText="{i18n>UserListViewCriterion.UserListViewCriterion_ColumnName/UserListViewCriterion_ColumnName}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>UserListViewCriterion_CreatedDate/UserListViewCriterion_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>UserListViewCriterion_Id/UserListViewCriterion_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>UserListViewCriterion.UserListViewCriterion_Id/UserListViewCriterion_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>UserListViewCriterion_IsDeleted/UserListViewCriterion_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<Text text="{dataModel>UserListViewCriterion_LastModifiedDate/UserListViewCriterion_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>UserListViewCriterion_Operation/UserListViewCriterion_Operation',type:'sap.ui.model.type.String'}" linkText="{i18n>UserListViewCriterion.UserListViewCriterion_Operation/UserListViewCriterion_Operation}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>UserListViewCriterion_SortOrder/UserListViewCriterion_SortOrder',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>UserListViewCriterion_SystemModstamp/UserListViewCriterion_SystemModstamp}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>UserListViewCriterion_Value/UserListViewCriterion_Value',type:'sap.ui.model.type.String'}" linkText="{i18n>UserListViewCriterion.UserListViewCriterion_Value/UserListViewCriterion_Value}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>