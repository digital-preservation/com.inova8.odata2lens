<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>UserLicenses.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/UserLicense' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>UserLicenses.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserLicenses.UserLicense_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserLicenses.UserLicense_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserLicenses.UserLicense_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserLicenses.UserLicense_LicenseDefinitionKey}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserLicenses.UserLicense_MasterLabel}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserLicenses.UserLicense_Name}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserLicenses.UserLicense_Status}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserLicenses.UserLicense_SystemModstamp}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserLicenses.UserLicense_TotalLicenses}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserLicenses.UserLicense_UsedLicenses}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>UserLicenses.UserLicense_UsedLicensesLastUpdated}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="UserLicense"/>
							<VBox>					
							<Text text="{dataModel>UserLicense_CreatedDate/UserLicense_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>UserLicense_Id/UserLicense_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>UserLicense.UserLicense_Id/UserLicense_Id}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>UserLicense_LastModifiedDate/UserLicense_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>UserLicense_LicenseDefinitionKey/UserLicense_LicenseDefinitionKey',type:'sap.ui.model.type.String'}" linkText="{i18n>UserLicense.UserLicense_LicenseDefinitionKey/UserLicense_LicenseDefinitionKey}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>UserLicense_MasterLabel/UserLicense_MasterLabel',type:'sap.ui.model.type.String'}" linkText="{i18n>UserLicense.UserLicense_MasterLabel/UserLicense_MasterLabel}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>UserLicense_Name/UserLicense_Name',type:'sap.ui.model.type.String'}" linkText="{i18n>UserLicense.UserLicense_Name/UserLicense_Name}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>UserLicense_Status/UserLicense_Status',type:'sap.ui.model.type.String'}" linkText="{i18n>UserLicense.UserLicense_Status/UserLicense_Status}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>UserLicense_SystemModstamp/UserLicense_SystemModstamp}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>UserLicense_TotalLicenses/UserLicense_TotalLicenses',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>UserLicense_UsedLicenses/UserLicense_UsedLicenses',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>UserLicense_UsedLicensesLastUpdated/UserLicense_UsedLicensesLastUpdated}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>