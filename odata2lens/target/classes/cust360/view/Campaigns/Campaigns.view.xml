							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>Campaign_IsDeleted/Campaign_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<Text text="{dataModel>Campaign_LastActivityDate/Campaign_LastActivityDate}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>Campaign_LastModifiedDate/Campaign_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>Campaign_LastReferencedDate/Campaign_LastReferencedDate}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>Campaign_LastViewedDate/Campaign_LastViewedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Campaign_Name/Campaign_Name',type:'sap.ui.model.type.String'}" linkText="{i18n>Campaign.Campaign_Name/Campaign_Name}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>Campaign_NumberOfContacts/Campaign_NumberOfContacts',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>Campaign_NumberOfConvertedLeads/Campaign_NumberOfConvertedLeads',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>Campaign_NumberOfLeads/Campaign_NumberOfLeads',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>Campaign_NumberOfOpportunities/Campaign_NumberOfOpportunities',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>Campaign_NumberOfResponses/Campaign_NumberOfResponses',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>Campaign_NumberOfWonOpportunities/Campaign_NumberOfWonOpportunities',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>Campaign_NumberSent/Campaign_NumberSent',type:'sap.ui.model.type.Float', formatOptions:{'style':'short','maxFractionDigits':2} }"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>Campaign_StartDate/Campaign_StartDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Campaign_Status/Campaign_Status',type:'sap.ui.model.type.String'}" linkText="{i18n>Campaign.Campaign_Status/Campaign_Status}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>Campaign_SystemModstamp/Campaign_SystemModstamp}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Campaign_Type/Campaign_Type',type:'sap.ui.model.type.String'}" linkText="{i18n>Campaign.Campaign_Type/Campaign_Type}"/>
							</VBox>							
