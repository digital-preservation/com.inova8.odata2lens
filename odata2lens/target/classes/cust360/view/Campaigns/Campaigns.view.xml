					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Campaigns.Campaign_StartDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Campaigns.Campaign_Status}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Campaigns.Campaign_SystemModstamp}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Campaigns.Campaign_Type}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="Campaign"/>
							<VBox>					
							<Text text="{path:'dataModel>Campaign_ActualCost/Campaign_ActualCost',type:'sap.ui.model.type.Float', formatOptions:{'style':'short','maxFractionDigits':2} }"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>Campaign_AmountAllOpportunities/Campaign_AmountAllOpportunities',type:'sap.ui.model.type.Float', formatOptions:{'style':'short','maxFractionDigits':2} }"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>Campaign_AmountWonOpportunities/Campaign_AmountWonOpportunities',type:'sap.ui.model.type.Float', formatOptions:{'style':'short','maxFractionDigits':2} }"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>Campaign_BudgetedCost/Campaign_BudgetedCost',type:'sap.ui.model.type.Float', formatOptions:{'style':'short','maxFractionDigits':2} }"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>Campaign_CreatedDate/Campaign_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Campaign_Description/Campaign_Description',type:'sap.ui.model.type.String'}" linkText="{i18n>Campaign.Campaign_Description/Campaign_Description}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>Campaign_EndDate/Campaign_EndDate}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>Campaign_ExpectedResponse/Campaign_ExpectedResponse',type:'sap.ui.model.type.Float', formatOptions:{'style':'short','maxFractionDigits':2} }"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>Campaign_ExpectedRevenue/Campaign_ExpectedRevenue',type:'sap.ui.model.type.Float', formatOptions:{'style':'short','maxFractionDigits':2} }"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>Campaign_Id/Campaign_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>Campaign.Campaign_Id/Campaign_Id}"/>
							</VBox>							
							<VBox>					
