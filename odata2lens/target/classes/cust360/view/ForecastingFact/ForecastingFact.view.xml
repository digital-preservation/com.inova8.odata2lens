<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>ForecastingFact.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>ForecastingFact.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="ForecastingFact"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="ForecastingFact"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ForecastingFact.ForecastingFact_ForecastCategoryName}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ForecastingFact_ForecastCategoryName/ForecastingFact_ForecastCategoryName',type:'sap.ui.model.type.String' }" linkText="{i18n>ForecastingFact.ForecastingFact_ForecastCategoryName/ForecastingFact_ForecastCategoryName}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ForecastingFact.ForecastingFact_ForecastedObjectId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ForecastingFact_ForecastedObjectId/sfdc_ForecastingFact_ForecastedObjectId" entitySet="OpportunityLineItem"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ForecastingFact.ForecastingFact_ForecastingItemId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ForecastingFact_ForecastingItemId/sfdc_ForecastingFact_ForecastingItemId" entitySet="ForecastingItem"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ForecastingFact.ForecastingFact_ForecastingTypeId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ForecastingFact_ForecastingTypeId/sfdc_ForecastingFact_ForecastingTypeId" entitySet="ForecastingType"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ForecastingFact.ForecastingFact_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ForecastingFact_Id/ForecastingFact_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>ForecastingFact.ForecastingFact_Id/ForecastingFact_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ForecastingFact.ForecastingFact_OpportunityId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ForecastingFact_OpportunityId/sfdc_ForecastingFact_OpportunityId" entitySet="Opportunity"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ForecastingFact.ForecastingFact_OwnerId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ForecastingFact_OwnerId/sfdc_ForecastingFact_OwnerId" entitySet="User"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ForecastingFact.ForecastingFact_PeriodId}" required="false"/> 
						<Text text=""/>
						<rdf:ListResource path="dataModel>ForecastingFact_PeriodId/sfdc_ForecastingFact_PeriodId" entitySet="Period"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ForecastingFact.ForecastingFact_SystemModstamp}" required="false"/> 
						<Text text=""/>
						<Text text="{dataModel>ForecastingFact_SystemModstamp/ForecastingFact_SystemModstamp}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ForecastingFact.ForecastingFact_ForecastedObjectId}" required="false"/> 
						<rdf:ListResource path="dataModel>ForecastingFact_ForecastedObjectId" entitySet="OpportunityLineItem"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ForecastingFact.ForecastingFact_ForecastingItemId}" required="false"/> 
						<rdf:ListResource path="dataModel>ForecastingFact_ForecastingItemId" entitySet="ForecastingItem"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ForecastingFact.ForecastingFact_ForecastingTypeId}" required="false"/> 
						<rdf:ListResource path="dataModel>ForecastingFact_ForecastingTypeId" entitySet="ForecastingType"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ForecastingFact.ForecastingFact_OpportunityId}" required="false"/> 
						<rdf:ListResource path="dataModel>ForecastingFact_OpportunityId" entitySet="Opportunity"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ForecastingFact.ForecastingFact_OwnerId}" required="false"/> 
						<rdf:ListResource path="dataModel>ForecastingFact_OwnerId" entitySet="User"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ForecastingFact.ForecastingFact_PeriodId}" required="false"/> 
						<rdf:ListResource path="dataModel>ForecastingFact_PeriodId" entitySet="Period"/>
						<Text text=""/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>