<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>ApexPageInfo.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>ApexPageInfo.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="ApexPageInfo"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
				<Column visible="true">
					<Text text="ApexPageInfo"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexPageInfo.ApexPageInfo_ApexPageId}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ApexPageInfo_ApexPageId/ApexPageInfo_ApexPageId',type:'sap.ui.model.type.String' }" linkText="{i18n>ApexPageInfo.ApexPageInfo_ApexPageId/ApexPageInfo_ApexPageId}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexPageInfo.ApexPageInfo_ApiVersion}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>ApexPageInfo_ApiVersion/ApexPageInfo_ApiVersion',type:'sap.ui.model.type.Float', formatOptions:{'style':'short','maxFractionDigits':2} }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexPageInfo.ApexPageInfo_Description}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ApexPageInfo_Description/ApexPageInfo_Description',type:'sap.ui.model.type.String' }" linkText="{i18n>ApexPageInfo.ApexPageInfo_Description/ApexPageInfo_Description}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexPageInfo.ApexPageInfo_DurableId}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ApexPageInfo_DurableId/ApexPageInfo_DurableId',type:'sap.ui.model.type.String' }" linkText="{i18n>ApexPageInfo.ApexPageInfo_DurableId/ApexPageInfo_DurableId}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexPageInfo.ApexPageInfo_Id}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ApexPageInfo_Id/ApexPageInfo_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>ApexPageInfo.ApexPageInfo_Id/ApexPageInfo_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexPageInfo.ApexPageInfo_IsAvailableInTouch}" required="false"/> 
						<Text text=""/>
						<Text text="{path:'dataModel>ApexPageInfo_IsAvailableInTouch/ApexPageInfo_IsAvailableInTouch',type:'sap.ui.model.type.Boolean'}"/>  
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexPageInfo.ApexPageInfo_IsShowHeader}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ApexPageInfo_IsShowHeader/ApexPageInfo_IsShowHeader',type:'sap.ui.model.type.String' }" linkText="{i18n>ApexPageInfo.ApexPageInfo_IsShowHeader/ApexPageInfo_IsShowHeader}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexPageInfo.ApexPageInfo_MasterLabel}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ApexPageInfo_MasterLabel/ApexPageInfo_MasterLabel',type:'sap.ui.model.type.String' }" linkText="{i18n>ApexPageInfo.ApexPageInfo_MasterLabel/ApexPageInfo_MasterLabel}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexPageInfo.ApexPageInfo_Name}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ApexPageInfo_Name/ApexPageInfo_Name',type:'sap.ui.model.type.String' }" linkText="{i18n>ApexPageInfo.ApexPageInfo_Name/ApexPageInfo_Name}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>ApexPageInfo.ApexPageInfo_NameSpacePrefix}" required="false"/> 
						<Text text=""/>
						<rdf:TextLink text="{path:'dataModel>ApexPageInfo_NameSpacePrefix/ApexPageInfo_NameSpacePrefix',type:'sap.ui.model.type.String' }" linkText="{i18n>ApexPageInfo.ApexPageInfo_NameSpacePrefix/ApexPageInfo_NameSpacePrefix}"/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>