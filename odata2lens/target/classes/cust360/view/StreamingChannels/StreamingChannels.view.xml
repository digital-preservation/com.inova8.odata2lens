<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>StreamingChannels.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/StreamingChannel' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>StreamingChannels.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>StreamingChannels.StreamingChannel_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>StreamingChannels.StreamingChannel_Description}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>StreamingChannels.StreamingChannel_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>StreamingChannels.StreamingChannel_IsDeleted}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>StreamingChannels.StreamingChannel_IsDynamic}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>StreamingChannels.StreamingChannel_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>StreamingChannels.StreamingChannel_LastReferencedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>StreamingChannels.StreamingChannel_LastViewedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>StreamingChannels.StreamingChannel_Name}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>StreamingChannels.StreamingChannel_SystemModstamp}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="StreamingChannel"/>
							<VBox>					
							<Text text="{dataModel>StreamingChannel_CreatedDate/StreamingChannel_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>StreamingChannel_Description/StreamingChannel_Description',type:'sap.ui.model.type.String'}" linkText="{i18n>StreamingChannel.StreamingChannel_Description/StreamingChannel_Description}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>StreamingChannel_Id/StreamingChannel_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>StreamingChannel.StreamingChannel_Id/StreamingChannel_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>StreamingChannel_IsDeleted/StreamingChannel_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>StreamingChannel_IsDynamic/StreamingChannel_IsDynamic',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<Text text="{dataModel>StreamingChannel_LastModifiedDate/StreamingChannel_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>StreamingChannel_LastReferencedDate/StreamingChannel_LastReferencedDate}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>StreamingChannel_LastViewedDate/StreamingChannel_LastViewedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>StreamingChannel_Name/StreamingChannel_Name',type:'sap.ui.model.type.String'}" linkText="{i18n>StreamingChannel.StreamingChannel_Name/StreamingChannel_Name}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>StreamingChannel_SystemModstamp/StreamingChannel_SystemModstamp}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>