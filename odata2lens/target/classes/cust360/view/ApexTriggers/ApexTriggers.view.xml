<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>ApexTriggers.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/ApexTrigger' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>ApexTriggers.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexTriggers.ApexTrigger_ApiVersion}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexTriggers.ApexTrigger_Body}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexTriggers.ApexTrigger_BodyCrc}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexTriggers.ApexTrigger_CreatedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexTriggers.ApexTrigger_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexTriggers.ApexTrigger_IsValid}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexTriggers.ApexTrigger_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexTriggers.ApexTrigger_LengthWithoutComments}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexTriggers.ApexTrigger_Name}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexTriggers.ApexTrigger_NamespacePrefix}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexTriggers.ApexTrigger_Status}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexTriggers.ApexTrigger_SystemModstamp}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexTriggers.ApexTrigger_TableEnumOrId}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexTriggers.ApexTrigger_UsageAfterDelete}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexTriggers.ApexTrigger_UsageAfterInsert}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexTriggers.ApexTrigger_UsageAfterUndelete}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexTriggers.ApexTrigger_UsageAfterUpdate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexTriggers.ApexTrigger_UsageBeforeDelete}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexTriggers.ApexTrigger_UsageBeforeInsert}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexTriggers.ApexTrigger_UsageBeforeUpdate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ApexTriggers.ApexTrigger_UsageIsBulk}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="ApexTrigger"/>
							<VBox>					
							<Text text="{path:'dataModel>ApexTrigger_ApiVersion/ApexTrigger_ApiVersion',type:'sap.ui.model.type.Float', formatOptions:{'style':'short','maxFractionDigits':2} }"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ApexTrigger_Body/ApexTrigger_Body',type:'sap.ui.model.type.String'}" linkText="{i18n>ApexTrigger.ApexTrigger_Body/ApexTrigger_Body}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>ApexTrigger_BodyCrc/ApexTrigger_BodyCrc',type:'sap.ui.model.type.Float', formatOptions:{'style':'short','maxFractionDigits':2} }"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>ApexTrigger_CreatedDate/ApexTrigger_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ApexTrigger_Id/ApexTrigger_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>ApexTrigger.ApexTrigger_Id/ApexTrigger_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>ApexTrigger_IsValid/ApexTrigger_IsValid',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<Text text="{dataModel>ApexTrigger_LastModifiedDate/ApexTrigger_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<Text text="{path:'dataModel>ApexTrigger_LengthWithoutComments/ApexTrigger_LengthWithoutComments',type:'sap.ui.model.type.Integer' }"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ApexTrigger_Name/ApexTrigger_Name',type:'sap.ui.model.type.String'}" linkText="{i18n>ApexTrigger.ApexTrigger_Name/ApexTrigger_Name}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ApexTrigger_NamespacePrefix/ApexTrigger_NamespacePrefix',type:'sap.ui.model.type.String'}" linkText="{i18n>ApexTrigger.ApexTrigger_NamespacePrefix/ApexTrigger_NamespacePrefix}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ApexTrigger_Status/ApexTrigger_Status',type:'sap.ui.model.type.String'}" linkText="{i18n>ApexTrigger.ApexTrigger_Status/ApexTrigger_Status}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>ApexTrigger_SystemModstamp/ApexTrigger_SystemModstamp}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ApexTrigger_TableEnumOrId/ApexTrigger_TableEnumOrId',type:'sap.ui.model.type.String'}" linkText="{i18n>ApexTrigger.ApexTrigger_TableEnumOrId/ApexTrigger_TableEnumOrId}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>ApexTrigger_UsageAfterDelete/ApexTrigger_UsageAfterDelete',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>ApexTrigger_UsageAfterInsert/ApexTrigger_UsageAfterInsert',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>ApexTrigger_UsageAfterUndelete/ApexTrigger_UsageAfterUndelete',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>ApexTrigger_UsageAfterUpdate/ApexTrigger_UsageAfterUpdate',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>ApexTrigger_UsageBeforeDelete/ApexTrigger_UsageBeforeDelete',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>ApexTrigger_UsageBeforeInsert/ApexTrigger_UsageBeforeInsert',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>ApexTrigger_UsageBeforeUpdate/ApexTrigger_UsageBeforeUpdate',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>ApexTrigger_UsageIsBulk/ApexTrigger_UsageIsBulk',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>