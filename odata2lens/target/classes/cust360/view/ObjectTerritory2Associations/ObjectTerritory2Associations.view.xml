<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>ObjectTerritory2Associations.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/ObjectTerritory2Association' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>ObjectTerritory2Associations.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ObjectTerritory2Associations.ObjectTerritory2Association_AssociationCause}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ObjectTerritory2Associations.ObjectTerritory2Association_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ObjectTerritory2Associations.ObjectTerritory2Association_IsDeleted}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ObjectTerritory2Associations.ObjectTerritory2Association_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ObjectTerritory2Associations.ObjectTerritory2Association_SobjectType}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ObjectTerritory2Associations.ObjectTerritory2Association_SystemModstamp}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="ObjectTerritory2Association"/>
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ObjectTerritory2Association_AssociationCause/ObjectTerritory2Association_AssociationCause',type:'sap.ui.model.type.String'}" linkText="{i18n>ObjectTerritory2Association.ObjectTerritory2Association_AssociationCause/ObjectTerritory2Association_AssociationCause}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ObjectTerritory2Association_Id/ObjectTerritory2Association_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>ObjectTerritory2Association.ObjectTerritory2Association_Id/ObjectTerritory2Association_Id}"/>
							</VBox>							
							<VBox>					
								<Text text="{path:'dataModel>ObjectTerritory2Association_IsDeleted/ObjectTerritory2Association_IsDeleted',type:'sap.ui.model.type.Boolean'}"/>  
							</VBox>							
							<VBox>					
							<Text text="{dataModel>ObjectTerritory2Association_LastModifiedDate/ObjectTerritory2Association_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>ObjectTerritory2Association_SobjectType/ObjectTerritory2Association_SobjectType',type:'sap.ui.model.type.String'}" linkText="{i18n>ObjectTerritory2Association.ObjectTerritory2Association_SobjectType/ObjectTerritory2Association_SobjectType}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>ObjectTerritory2Association_SystemModstamp/ObjectTerritory2Association_SystemModstamp}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>