						<Text text="{i18n>CaseSubjectParticles.CaseSubjectParticle_Type}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="CaseSubjectParticle"/>
							<VBox>					
							<Text text="{dataModel>CaseSubjectParticle_CreatedDate/CaseSubjectParticle_CreatedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>CaseSubjectParticle_DeveloperName/CaseSubjectParticle_DeveloperName',type:'sap.ui.model.type.String'}" linkText="{i18n>CaseSubjectParticle.CaseSubjectParticle_DeveloperName/CaseSubjectParticle_DeveloperName}"/>
