<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>AuraDefinitionInfos.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/AuraDefinitionInfo' , sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entitySetView>/tableNoDataText}" busyIndicatorDelay="{entitySetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" required="false"/>
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entityView>text}"/>
							</ComboBox>
							<Title text=")" required="false"/>
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>AuraDefinitionInfos.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AuraDefinitionInfos.AuraDefinitionInfo_AuraDefinitionId}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AuraDefinitionInfos.AuraDefinitionInfo_DefType}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AuraDefinitionInfos.AuraDefinitionInfo_DeveloperName}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AuraDefinitionInfos.AuraDefinitionInfo_DurableId}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AuraDefinitionInfos.AuraDefinitionInfo_Format}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AuraDefinitionInfos.AuraDefinitionInfo_Id}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AuraDefinitionInfos.AuraDefinitionInfo_LastModifiedDate}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AuraDefinitionInfos.AuraDefinitionInfo_NamespacePrefix}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>AuraDefinitionInfos.AuraDefinitionInfo_Source}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="AuraDefinitionInfo"/>
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>AuraDefinitionInfo_AuraDefinitionId/AuraDefinitionInfo_AuraDefinitionId',type:'sap.ui.model.type.String'}" linkText="{i18n>AuraDefinitionInfo.AuraDefinitionInfo_AuraDefinitionId/AuraDefinitionInfo_AuraDefinitionId}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>AuraDefinitionInfo_DefType/AuraDefinitionInfo_DefType',type:'sap.ui.model.type.String'}" linkText="{i18n>AuraDefinitionInfo.AuraDefinitionInfo_DefType/AuraDefinitionInfo_DefType}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>AuraDefinitionInfo_DeveloperName/AuraDefinitionInfo_DeveloperName',type:'sap.ui.model.type.String'}" linkText="{i18n>AuraDefinitionInfo.AuraDefinitionInfo_DeveloperName/AuraDefinitionInfo_DeveloperName}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>AuraDefinitionInfo_DurableId/AuraDefinitionInfo_DurableId',type:'sap.ui.model.type.String'}" linkText="{i18n>AuraDefinitionInfo.AuraDefinitionInfo_DurableId/AuraDefinitionInfo_DurableId}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>AuraDefinitionInfo_Format/AuraDefinitionInfo_Format',type:'sap.ui.model.type.String'}" linkText="{i18n>AuraDefinitionInfo.AuraDefinitionInfo_Format/AuraDefinitionInfo_Format}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>AuraDefinitionInfo_Id/AuraDefinitionInfo_Id',type:'sap.ui.model.type.String'}" linkText="{i18n>AuraDefinitionInfo.AuraDefinitionInfo_Id/AuraDefinitionInfo_Id}"/>
							</VBox>							
							<VBox>					
							<Text text="{dataModel>AuraDefinitionInfo_LastModifiedDate/AuraDefinitionInfo_LastModifiedDate}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>AuraDefinitionInfo_NamespacePrefix/AuraDefinitionInfo_NamespacePrefix',type:'sap.ui.model.type.String'}" linkText="{i18n>AuraDefinitionInfo.AuraDefinitionInfo_NamespacePrefix/AuraDefinitionInfo_NamespacePrefix}"/>
							</VBox>							
							<VBox>					
							<rdf:TextLink text="{path:'dataModel>AuraDefinitionInfo_Source/AuraDefinitionInfo_Source',type:'sap.ui.model.type.String'}" linkText="{i18n>AuraDefinitionInfo.AuraDefinitionInfo_Source/AuraDefinitionInfo_Source}"/>
							</VBox>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>