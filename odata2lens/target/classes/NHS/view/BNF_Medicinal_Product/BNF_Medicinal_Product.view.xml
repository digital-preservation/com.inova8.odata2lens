<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>BNF_Medicinal_Product.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>BNF_Medicinal_Product.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="BNF_Medicinal_Product"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>BNF_Medicinal_Product.BNF_Medicinal_Product_Chemical_Substance_Code}" required="false"/> 
						<rdf:TextLink text="{path:'dataModel>BNF_Medicinal_Product_Chemical_Substance_Code',type:'sap.ui.model.type.String' }" linkText="{i18n>BNF_Medicinal_Product.BNF_Medicinal_Product_Chemical_Substance_Code}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>BNF_Medicinal_Product.BNF_Medicinal_Product_Chemical_Substance_Name}" required="false"/> 
						<rdf:TextLink text="{path:'dataModel>BNF_Medicinal_Product_Chemical_Substance_Name',type:'sap.ui.model.type.String' }" linkText="{i18n>BNF_Medicinal_Product.BNF_Medicinal_Product_Chemical_Substance_Name}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>BNF_Medicinal_Product.BNF_Medicinal_Product_DMD_Product_Description}" required="false"/> 
						<rdf:TextLink text="{path:'dataModel>BNF_Medicinal_Product_DMD_Product_Description',type:'sap.ui.model.type.String' }" linkText="{i18n>BNF_Medicinal_Product.BNF_Medicinal_Product_DMD_Product_Description}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>BNF_Medicinal_Product.BNF_Medicinal_Product_Id}" required="false"/> 
						<rdf:TextLink text="{path:'dataModel>BNF_Medicinal_Product_Id',type:'sap.ui.model.type.String' }" linkText="{i18n>BNF_Medicinal_Product.BNF_Medicinal_Product_Id}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>BNF_Medicinal_Product.BNF_Medicinal_Product_MDR_Product_Description}" required="false"/> 
						<rdf:TextLink text="{path:'dataModel>BNF_Medicinal_Product_MDR_Product_Description',type:'sap.ui.model.type.String' }" linkText="{i18n>BNF_Medicinal_Product.BNF_Medicinal_Product_MDR_Product_Description}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>BNF_Medicinal_Product.BNF_Medicinal_Product_Presentation_Code}" required="false"/> 
						<rdf:TextLink text="{path:'dataModel>BNF_Medicinal_Product_Presentation_Code',type:'sap.ui.model.type.String' }" linkText="{i18n>BNF_Medicinal_Product.BNF_Medicinal_Product_Presentation_Code}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>BNF_Medicinal_Product.BNF_Medicinal_Product_Presentation_Name}" required="false"/> 
						<rdf:TextLink text="{path:'dataModel>BNF_Medicinal_Product_Presentation_Name',type:'sap.ui.model.type.String' }" linkText="{i18n>BNF_Medicinal_Product.BNF_Medicinal_Product_Presentation_Name}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>BNF_Medicinal_Product.BNF_Medicinal_Product_Product_Code}" required="false"/> 
						<rdf:TextLink text="{path:'dataModel>BNF_Medicinal_Product_Product_Code',type:'sap.ui.model.type.String' }" linkText="{i18n>BNF_Medicinal_Product.BNF_Medicinal_Product_Product_Code}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>BNF_Medicinal_Product.BNF_Medicinal_Product_Product_Name}" required="false"/> 
						<rdf:TextLink text="{path:'dataModel>BNF_Medicinal_Product_Product_Name',type:'sap.ui.model.type.String' }" linkText="{i18n>BNF_Medicinal_Product.BNF_Medicinal_Product_Product_Name}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>BNF_Medicinal_Product.BNF_Medicinal_Product_AMPP_Id}" required="false"/> 
						<rdf:ListResource path="dataModel>BNF_Medicinal_Product_AMPP_Id" entitySet="Actual_Medicinal_Product_Pack"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>BNF_Medicinal_Product.BNF_Medicinal_Product_AMP_Id}" required="false"/> 
						<rdf:ListResource path="dataModel>BNF_Medicinal_Product_AMP_Id" entitySet="Actual_Medicinal_Product"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>BNF_Medicinal_Product.BNF_Medicinal_Product_VMP_Id}" required="false"/> 
						<rdf:ListResource path="dataModel>BNF_Medicinal_Product_VMP_Id" entitySet="Virtual_Medicinal_Product"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>BNF_Medicinal_Product.Prescription_Order_BNF_CODE_inverse}" required="false"/> 
						<rdf:ListResource path="dataModel>Prescription_Order_BNF_CODE_inverse" entitySet="Prescription_Order"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>BNF_Medicinal_Product.Virtual_Medicinal_Product_BNF_Id_inverse}" required="false"/> 
						<rdf:ListResource path="dataModel>Virtual_Medicinal_Product_BNF_Id_inverse" entitySet="Virtual_Medicinal_Product"/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>