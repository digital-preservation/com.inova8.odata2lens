<mvc:View controllerName="com.inova8.lens.controller.Entity" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>Amended_Record_Indicator.Title}"
		busy="{entityView>/busy}" busyIndicatorDelay="{entityView>/delay}">
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<Title text="{i18n>Amended_Record_Indicator.this}"/>
				<rdf:TextResource path="dataModel>" entitySet="Amended_Record_Indicator"/>
				<Title text="{i18n>Amended_Record_Indicator.Title}" />
				<ToolbarSpacer/>
			</Toolbar>
		</customHeader>
		<content>
			<f:SimpleForm class="editableForm" columnsL="1" columnsM="1" editable="true" emptySpanL="4" emptySpanM="4" 
				labelSpanL="2" labelSpanM="2" layout="ResponsiveGridLayout" maxContainerCols="2" minWidth="1024" singleContainerFullSize="true">
				<f:content>
					<!--<Label text="{i18n>Amended_Record_Indicator.this}" required="false"/><rdf:TextResource path="dataModel>" entitySet="Amended_Record_Indicator"/>-->
					<Label text="{i18n>Amended_Record_Indicator.Amended_Record_Indicator_Code}" required="false"/>   <rdf:TextLink text="{path:'dataModel>Amended_Record_Indicator_Code',type:'sap.ui.model.type.String' }" linkText="{i18n>Amended_Record_Indicator.Amended_Record_Indicator_Code}"/>
					<Label text="{i18n>Amended_Record_Indicator.Amended_Record_Indicator_Name}" required="false"/>   <rdf:TextLink text="{path:'dataModel>Amended_Record_Indicator_Name',type:'sap.ui.model.type.String' }" linkText="{i18n>Amended_Record_Indicator.Amended_Record_Indicator_Name}"/>
				</f:content>
			</f:SimpleForm>
		</content>
	</Page>
</mvc:View>