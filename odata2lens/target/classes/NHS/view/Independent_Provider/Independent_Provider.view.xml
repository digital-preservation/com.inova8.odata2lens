<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>Independent_Provider.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>Independent_Provider.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="Independent_Provider"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Independent_Provider.Independent_Provider_Address_Line_1}" required="false"/> 
						<rdf:TextLink text="{path:'dataModel>Independent_Provider_Address_Line_1',type:'sap.ui.model.type.String' }" linkText="{i18n>Independent_Provider.Independent_Provider_Address_Line_1}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Independent_Provider.Independent_Provider_Address_Line_2}" required="false"/> 
						<rdf:TextLink text="{path:'dataModel>Independent_Provider_Address_Line_2',type:'sap.ui.model.type.String' }" linkText="{i18n>Independent_Provider.Independent_Provider_Address_Line_2}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Independent_Provider.Independent_Provider_Address_Line_3}" required="false"/> 
						<rdf:TextLink text="{path:'dataModel>Independent_Provider_Address_Line_3',type:'sap.ui.model.type.String' }" linkText="{i18n>Independent_Provider.Independent_Provider_Address_Line_3}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Independent_Provider.Independent_Provider_Address_Line_4}" required="false"/> 
						<rdf:TextLink text="{path:'dataModel>Independent_Provider_Address_Line_4',type:'sap.ui.model.type.String' }" linkText="{i18n>Independent_Provider.Independent_Provider_Address_Line_4}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Independent_Provider.Independent_Provider_Address_Line_5}" required="false"/> 
						<rdf:TextLink text="{path:'dataModel>Independent_Provider_Address_Line_5',type:'sap.ui.model.type.String' }" linkText="{i18n>Independent_Provider.Independent_Provider_Address_Line_5}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Independent_Provider.Independent_Provider_Close_Date}" required="false"/> 
						<Text text="{dataModel>Independent_Provider_Close_Date}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Independent_Provider.Independent_Provider_Contact_Telephone_Number}" required="false"/> 
						<rdf:TextLink text="{path:'dataModel>Independent_Provider_Contact_Telephone_Number',type:'sap.ui.model.type.String' }" linkText="{i18n>Independent_Provider.Independent_Provider_Contact_Telephone_Number}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Independent_Provider.Independent_Provider_Name}" required="false"/> 
						<rdf:TextLink text="{path:'dataModel>Independent_Provider_Name',type:'sap.ui.model.type.String' }" linkText="{i18n>Independent_Provider.Independent_Provider_Name}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Independent_Provider.Independent_Provider_Open_Date}" required="false"/> 
						<Text text="{dataModel>Independent_Provider_Open_Date}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Independent_Provider.Independent_Provider_Organisation_Code}" required="false"/> 
						<rdf:TextLink text="{path:'dataModel>Independent_Provider_Organisation_Code',type:'sap.ui.model.type.String' }" linkText="{i18n>Independent_Provider.Independent_Provider_Organisation_Code}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Independent_Provider.Independent_Provider_Postcode}" required="false"/> 
						<rdf:TextLink text="{path:'dataModel>Independent_Provider_Postcode',type:'sap.ui.model.type.String' }" linkText="{i18n>Independent_Provider.Independent_Provider_Postcode}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Independent_Provider.Independent_Provider_Amended_Record_Indicator}" required="false"/> 
						<rdf:ListResource path="dataModel>Independent_Provider_Amended_Record_Indicator" entitySet="Amended_Record_Indicator"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Independent_Provider.Independent_Provider_High_Level_Health_Geography}" required="false"/> 
						<rdf:ListResource path="dataModel>Independent_Provider_High_Level_Health_Geography" entitySet="Geographical_Region"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Independent_Provider.Independent_Provider_National_Grouping}" required="false"/> 
						<rdf:ListResource path="dataModel>Independent_Provider_National_Grouping" entitySet="Geographical_Region"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Independent_Provider.Independent_Provider_Organisation_Sub_Type_Code}" required="false"/> 
						<rdf:ListResource path="dataModel>Independent_Provider_Organisation_Sub_Type_Code" entitySet="Organisation_Sub_Type"/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>