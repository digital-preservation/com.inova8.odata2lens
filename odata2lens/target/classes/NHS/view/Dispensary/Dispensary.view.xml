<mvc:View controllerName="com.inova8.lens.controller.Entity" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>Dispensary.Title}"
		busy="{entityView>/busy}" busyIndicatorDelay="{entityView>/delay}">
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<Title text="{i18n>Dispensary.this}"/>
				<rdf:TextResource path="dataModel>" entitySet="Dispensary"/>
				<Title text="{i18n>Dispensary.Title}" />
				<ToolbarSpacer/>
			</Toolbar>
		</customHeader>
		<content>
			<f:SimpleForm class="editableForm" columnsL="1" columnsM="1" editable="true" emptySpanL="4" emptySpanM="4" 
				labelSpanL="2" labelSpanM="2" layout="ResponsiveGridLayout" maxContainerCols="2" minWidth="1024" singleContainerFullSize="true">
				<f:content>
					<!--<Label text="{i18n>Dispensary.this}" required="false"/><rdf:TextResource path="dataModel>" entitySet="Dispensary"/>-->
					<Label text="{i18n>Dispensary.Dispensary_Address_Line_1}" required="false"/>   <rdf:TextLink text="{path:'dataModel>Dispensary_Address_Line_1',type:'sap.ui.model.type.String' }" linkText="{i18n>Dispensary.Dispensary_Address_Line_1}"/>
					<Label text="{i18n>Dispensary.Dispensary_Address_Line_2}" required="false"/>   <rdf:TextLink text="{path:'dataModel>Dispensary_Address_Line_2',type:'sap.ui.model.type.String' }" linkText="{i18n>Dispensary.Dispensary_Address_Line_2}"/>
					<Label text="{i18n>Dispensary.Dispensary_Address_Line_3}" required="false"/>   <rdf:TextLink text="{path:'dataModel>Dispensary_Address_Line_3',type:'sap.ui.model.type.String' }" linkText="{i18n>Dispensary.Dispensary_Address_Line_3}"/>
					<Label text="{i18n>Dispensary.Dispensary_Address_Line_4}" required="false"/>   <rdf:TextLink text="{path:'dataModel>Dispensary_Address_Line_4',type:'sap.ui.model.type.String' }" linkText="{i18n>Dispensary.Dispensary_Address_Line_4}"/>
					<Label text="{i18n>Dispensary.Dispensary_Address_Line_5}" required="false"/>   <rdf:TextLink text="{path:'dataModel>Dispensary_Address_Line_5',type:'sap.ui.model.type.String' }" linkText="{i18n>Dispensary.Dispensary_Address_Line_5}"/>
					<Label text="{i18n>Dispensary.Dispensary_Close_Date}" required="false"/>   <Text text="{dataModel>Dispensary_Close_Date}"/>
					<Label text="{i18n>Dispensary.Dispensary_Contact_Telephone_Number}" required="false"/>   <rdf:TextLink text="{path:'dataModel>Dispensary_Contact_Telephone_Number',type:'sap.ui.model.type.String' }" linkText="{i18n>Dispensary.Dispensary_Contact_Telephone_Number}"/>
					<Label text="{i18n>Dispensary.Dispensary_Join_Parent_Date}" required="false"/>   <Text text="{dataModel>Dispensary_Join_Parent_Date}"/>
					<Label text="{i18n>Dispensary.Dispensary_Left_Parent_Date}" required="false"/>   <Text text="{dataModel>Dispensary_Left_Parent_Date}"/>
					<Label text="{i18n>Dispensary.Dispensary_Name}" required="false"/>   <rdf:TextLink text="{path:'dataModel>Dispensary_Name',type:'sap.ui.model.type.String' }" linkText="{i18n>Dispensary.Dispensary_Name}"/>
					<Label text="{i18n>Dispensary.Dispensary_Open_Date}" required="false"/>   <Text text="{dataModel>Dispensary_Open_Date}"/>
					<Label text="{i18n>Dispensary.Dispensary_Organisation_Code}" required="false"/>   <rdf:TextLink text="{path:'dataModel>Dispensary_Organisation_Code',type:'sap.ui.model.type.String' }" linkText="{i18n>Dispensary.Dispensary_Organisation_Code}"/>
					<Label text="{i18n>Dispensary.Dispensary_Postcode}" required="false"/>   <rdf:TextLink text="{path:'dataModel>Dispensary_Postcode',type:'sap.ui.model.type.String' }" linkText="{i18n>Dispensary.Dispensary_Postcode}"/>
					<Label text="{i18n>Dispensary.Dispensary_Care_Organisation}" required="false"/><rdf:TextResource path="dataModel>Dispensary_Care_Organisation" entitySet="Care_Organisation"/>		
				</f:content>
			</f:SimpleForm>
		</content>
	</Page>
</mvc:View>