<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>Practice_Membership.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" required="false"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" required="false"/>
				</HBox>
				<Title text="{i18n>Practice_Membership.this}"/>
				<Title text="(" required="false"/>
				<rdf:TextResource path="dataModel>" entitySet="Practice_Membership"/>
				<Title text=")" required="false"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Practice_Membership.Practice_Membership_Join_Parent_Date}" required="false"/> 
						<Text text="{dataModel>Practice_Membership_Join_Parent_Date}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Practice_Membership.Practice_Membership_Left_Parent_Date}" required="false"/> 
						<Text text="{dataModel>Practice_Membership_Left_Parent_Date}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Practice_Membership.Practice_Membership_Parent_Organisation_Type}" required="false"/> 
						<rdf:TextLink text="{path:'dataModel>Practice_Membership_Parent_Organisation_Type',type:'sap.ui.model.type.String' }" linkText="{i18n>Practice_Membership.Practice_Membership_Parent_Organisation_Type}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Practice_Membership.Practice_Membership_Organisation_Code}" required="false"/> 
						<rdf:TextResource path="dataModel>Practice_Membership_Organisation_Code" entitySet="GP_Practice"/>		
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Practice_Membership.Practice_Membership_Amended_Record_Indicator}" required="false"/> 
						<rdf:ListResource path="dataModel>Practice_Membership_Amended_Record_Indicator" entitySet="Amended_Record_Indicator"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Practice_Membership.Practice_Membership_Parent_Organisation_Code}" required="false"/> 
						<rdf:ListResource path="dataModel>Practice_Membership_Parent_Organisation_Code" entitySet="Parent_Organisation"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Practice_Membership.Practice_Membership_Practitioner_Code}" required="false"/> 
						<rdf:ListResource path="dataModel>Practice_Membership_Practitioner_Code" entitySet="General_Practitioner"/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>