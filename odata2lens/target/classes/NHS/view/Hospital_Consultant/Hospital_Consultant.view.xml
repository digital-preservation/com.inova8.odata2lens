<mvc:View controllerName="com.inova8.lens.controller.Entity" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>Hospital_Consultant.Title}"
		busy="{entityView>/busy}" busyIndicatorDelay="{entityView>/delay}">
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<Title text="{i18n>Hospital_Consultant.this}"/>
				<rdf:TextResource path="dataModel>" entitySet="Hospital_Consultant"/>
				<Title text="{i18n>Hospital_Consultant.Title}" />
				<ToolbarSpacer/>
			</Toolbar>
		</customHeader>
		<content>
			<f:SimpleForm class="editableForm" columnsL="1" columnsM="1" editable="true" emptySpanL="4" emptySpanM="4" 
				labelSpanL="2" labelSpanM="2" layout="ResponsiveGridLayout" maxContainerCols="2" minWidth="1024" singleContainerFullSize="true">
				<f:content>
					<!--<Label text="{i18n>Hospital_Consultant.this}" required="false"/><rdf:TextResource path="dataModel>" entitySet="Hospital_Consultant"/>-->
					<Label text="{i18n>Hospital_Consultant.Hospital_Consultant_GMC_Code}" required="false"/>   <rdf:TextLink text="{path:'dataModel>Hospital_Consultant_GMC_Code',type:'sap.ui.model.type.String' }" linkText="{i18n>Hospital_Consultant.Hospital_Consultant_GMC_Code}"/>
					<Label text="{i18n>Hospital_Consultant.Hospital_Consultant_Initials}" required="false"/>   <rdf:TextLink text="{path:'dataModel>Hospital_Consultant_Initials',type:'sap.ui.model.type.String' }" linkText="{i18n>Hospital_Consultant.Hospital_Consultant_Initials}"/>
					<Label text="{i18n>Hospital_Consultant.Hospital_Consultant_Practitioner_Code}" required="false"/>   <rdf:TextLink text="{path:'dataModel>Hospital_Consultant_Practitioner_Code',type:'sap.ui.model.type.String' }" linkText="{i18n>Hospital_Consultant.Hospital_Consultant_Practitioner_Code}"/>
					<Label text="{i18n>Hospital_Consultant.Hospital_Consultant_Practitioner_Type}" required="false"/>   <rdf:TextLink text="{path:'dataModel>Hospital_Consultant_Practitioner_Type',type:'sap.ui.model.type.String' }" linkText="{i18n>Hospital_Consultant.Hospital_Consultant_Practitioner_Type}"/>
					<Label text="{i18n>Hospital_Consultant.Hospital_Consultant_Speciality_Function_Code}" required="false"/>   <rdf:TextLink text="{path:'dataModel>Hospital_Consultant_Speciality_Function_Code',type:'sap.ui.model.type.String' }" linkText="{i18n>Hospital_Consultant.Hospital_Consultant_Speciality_Function_Code}"/>
					<Label text="{i18n>Hospital_Consultant.Hospital_Consultant_Surname}" required="false"/>   <rdf:TextLink text="{path:'dataModel>Hospital_Consultant_Surname',type:'sap.ui.model.type.String' }" linkText="{i18n>Hospital_Consultant.Hospital_Consultant_Surname}"/>
					<Label text="{i18n>Hospital_Consultant.Hospital_Consultant_Location_Organisation_Code}" required="false"/><rdf:TextResource path="dataModel>Hospital_Consultant_Location_Organisation_Code" entitySet="Location_Organisation"/>		
				</f:content>
			</f:SimpleForm>
		</content>
	</Page>
</mvc:View>