							<Text text="{dataModel>Independent_Healthcare_Provider_Site_Open_Date}"/>
							<rdf:TextLink text="{path:'dataModel>Independent_Healthcare_Provider_Site_Organisation_Code',type:'sap.ui.model.type.String'}" linkText="{i18n>Independent_Healthcare_Provider_Site.Independent_Healthcare_Provider_Site_Organisation_Code}"/>
							<rdf:TextLink text="{path:'dataModel>Independent_Healthcare_Provider_Site_Postcode',type:'sap.ui.model.type.String'}" linkText="{i18n>Independent_Healthcare_Provider_Site.Independent_Healthcare_Provider_Site_Postcode}"/>
