<mvc:View controllerName="com.inova8.lens.controller.EntitySet360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>Independent_Healthcare_Provider_Sites.ViewTitle}">
		<content>
			<Table id="table360" inset="false" items="{ path: 'dataModel>/Independent_Healthcare_Provider_Site' , sorter: { path: 'label', descending: false } 
