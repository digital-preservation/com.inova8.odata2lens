					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Independent_Healthcare_Provider_Sites.Independent_Healthcare_Provider_Site_Name}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Independent_Healthcare_Provider_Sites.Independent_Healthcare_Provider_Site_Open_Date}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Independent_Healthcare_Provider_Sites.Independent_Healthcare_Provider_Site_Organisation_Code}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Independent_Healthcare_Provider_Sites.Independent_Healthcare_Provider_Site_Postcode}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template360" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="Independent_Healthcare_Provider_Site"/>
							<rdf:TextLink text="{path:'dataModel>Independent_Healthcare_Provider_Site_Address_Line_1',type:'sap.ui.model.type.String'}" linkText="{i18n>Independent_Healthcare_Provider_Site.Independent_Healthcare_Provider_Site_Address_Line_1}"/>
							<rdf:TextLink text="{path:'dataModel>Independent_Healthcare_Provider_Site_Address_Line_2',type:'sap.ui.model.type.String'}" linkText="{i18n>Independent_Healthcare_Provider_Site.Independent_Healthcare_Provider_Site_Address_Line_2}"/>
							<rdf:TextLink text="{path:'dataModel>Independent_Healthcare_Provider_Site_Address_Line_3',type:'sap.ui.model.type.String'}" linkText="{i18n>Independent_Healthcare_Provider_Site.Independent_Healthcare_Provider_Site_Address_Line_3}"/>
							<rdf:TextLink text="{path:'dataModel>Independent_Healthcare_Provider_Site_Address_Line_4',type:'sap.ui.model.type.String'}" linkText="{i18n>Independent_Healthcare_Provider_Site.Independent_Healthcare_Provider_Site_Address_Line_4}"/>
