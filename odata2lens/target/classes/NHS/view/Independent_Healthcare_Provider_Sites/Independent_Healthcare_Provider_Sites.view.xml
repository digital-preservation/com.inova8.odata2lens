					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Independent_Healthcare_Provider_Sites.Independent_Healthcare_Provider_Site_Address_Line_1}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Independent_Healthcare_Provider_Sites.Independent_Healthcare_Provider_Site_Address_Line_2}"/>
