<mvc:View controllerName="com.inova8.lens.controller.EntityNavigationSet" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>BNF_Medicinal_ProductPrescription_Order_BNF_CODE_inverse.ViewTitle}">
		<content>
			<Table id="table" inset="false" items="{ path: '/{objectId}/Prescription_Order_BNF_CODE_inverse', sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entityNavigationSetView>/tableNoDataText}" busyIndicatorDelay="{entityNavigationSetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>					
					<ToolbarSpacer/>
						<Title id="entitySet"/>
						<rdf:TextResource id="resourceTitle" />
						<Title id="tableHeader" text="{entityNavigationSetView>/entityNavigationSetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>BNF_Medicinal_ProductPrescription_Order_BNF_CODE_inverse.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>BNF_Medicinal_ProductPrescription_Order_BNF_CODE_inverse.Prescription_Order_ACT_COST}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>BNF_Medicinal_ProductPrescription_Order_BNF_CODE_inverse.Prescription_Order_BNF_NAME}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>BNF_Medicinal_ProductPrescription_Order_BNF_CODE_inverse.Prescription_Order_ITEMS}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>BNF_Medicinal_ProductPrescription_Order_BNF_CODE_inverse.Prescription_Order_NIC}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>BNF_Medicinal_ProductPrescription_Order_BNF_CODE_inverse.Prescription_Order_PERIOD}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>BNF_Medicinal_ProductPrescription_Order_BNF_CODE_inverse.Prescription_Order_QUANTITY}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="Prescription_Order"/>
  <Text text="{dataModel>Prescription_Order_ACT_COST}"/>
  <rdf:TextLink text="{path:'dataModel>Prescription_Order_BNF_NAME',type:'sap.ui.model.type.String' }" linkText="{i18n>Prescription_Order.Prescription_Order_BNF_NAME}"/>
  <Text text="{path:'dataModel>Prescription_Order_ITEMS',type:'sap.ui.model.type.Integer' }"/>
  <Text text="{dataModel>Prescription_Order_NIC}"/>
  <Text text="{dataModel>Prescription_Order_PERIOD}"/>
  <Text text="{dataModel>Prescription_Order_QUANTITY}"/>
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>