#macro( entitymaptypes  $propertyObject $target )
#set( $property =  $propertyObject.property)
#set( $range =  $propertyObject.range)
#set( $formatOptions =  $propertyObject.formatOptions)
  #if ($range == "Edm.Binary")						<Text text="{dataModel>$property}" tooltip="{i18n>$target.$property}"/>
  #elseif ($range == "Edm.Boolean")						<Text text="{path:'dataModel>$property',type:'sap.ui.model.type.Boolean'}" tooltip="{i18n>$target.$property}"/>
  #elseif ($range == "Edm.Byte")						<Text text="{dataModel>$property}" tooltip="{i18n>$target.$property}"/>
  #elseif ($range == "Edm.SByte")						<Text text="{dataModel>$property}" tooltip="{i18n>$target.$property}"/>
  #elseif ($range == "Edm.Date")						<Text text="{dataModel>$property}" tooltip="{i18n>$target.$property}"/>
  #elseif ($range == "Edm.DateTimeOffset")						<Text text="{dataModel>$property}" tooltip="{i18n>$target.$property}"/>
  #elseif ($range == "Edm.TimeOfDay")						<Text text="{dataModel>$property}" tooltip="{i18n>$target.$property}"/>
  #elseif ($range == "Edm.Duration")						<Text text="{dataModel>$property}" tooltip="{i18n>$target.$property}"/>
  #elseif ($range == "Edm.Decimal")						<Text text="{dataModel>$property}" tooltip="{i18n>$target.$property}"/>
  #elseif ($range == "Edm.Single")						<Text text="{dataModel>$property}" tooltip="{i18n>$target.$property}"/>
  #elseif ($range == "Edm.Double")						<Text text="{path:'dataModel>$property',type:'sap.ui.model.type.Float'$!formatOptions }" tooltip="{i18n>$target.$property}"/>
  #elseif ($range == "Edm.Guid")						<Text text="{path:'dataModel>$property',type:'sap.ui.model.type.String'}" tooltip="{i18n>$target.$property}"/>
  #elseif ($range == "Edm.Int16")						<Text text="{path:'dataModel>$property',type:'sap.ui.model.type.Integer'$!formatOptions }" tooltip="{i18n>$target.$property}"/>
  #elseif ($range == "Edm.Int32")						<Text text="{path:'dataModel>$property',type:'sap.ui.model.type.Integer'$!formatOptions }" tooltip="{i18n>$target.$property}"/>
  #elseif ($range == "Edm.Int64")						<Text text="{path:'dataModel>$property',type:'sap.ui.model.type.Integer'$!formatOptions}" tooltip="{i18n>$target.$property}"/>
  #elseif ($range == "Edm.String")						<rdf:TextLink text="{path:'dataModel>$property',type:'sap.ui.model.type.String' }" linkText="{i18n>$target.$property}" tooltip="{i18n>$target.$property}"/>
  #elseif ($range == "Edm.Stream")						<Text text="{dataModel>$property}" tooltip="{i18n>$target.$property}"/>
  #elseif ($range == "Edm.Geography")						<Text text="{dataModel>$property}" tooltip="{i18n>$target.$property}"/>
  #elseif ($range == "Edm.GeographyPoint")						<Text text="{dataModel>$property}" tooltip="{i18n>$target.$property}"/>
  #elseif ($range == "Edm.GeographyLineString")						<Text text="{dataModel>$property}" tooltip="{i18n>$target.$property}"/>
  #elseif ($range == "Edm.GeographyPolygon")						<Text text="{dataModel>$property}" tooltip="{i18n>$target.$property}"/>
  #elseif ($range == "Edm.GeographyMultiPoint")						<Text text="{dataModel>$property}" tooltip="{i18n>$target.$property}"/>
  #elseif ($range == "Edm.GeographyMultiLineString")						<Text text="{dataModel>$property}" tooltip="{i18n>$target.$property}"/>
  #elseif ($range == "Edm.GeographyMultiPolygon")						<Text text="{dataModel>$property}" tooltip="{i18n>$target.$property}"/>
  #elseif ($range == "Edm.GeographyCollection")						<Text text="{dataModel>$property}" tooltip="{i18n>$target.$property}"/>
  #elseif ($range == "Edm.Geometry")						<Text text="{dataModel>$property}" tooltip="{i18n>$target.$property}"/>
  #elseif ($range == "Edm.GeometryPoint")						<Text text="{dataModel>$property}" tooltip="{i18n>$target.$property}"/>
  #elseif ($range == "Edm.GeometryLineString")						<Text text="{dataModel>$property}" tooltip="{i18n>$target.$property}"/>
  #elseif ($range == "Edm.GeometryPolygon")						<Text text="{dataModel>$property}" tooltip="{i18n>$target.$property}"/>
  #elseif ($range == "Edm.GeometryMultiPoint")						<Text text="{dataModel>$property}" tooltip="{i18n>$target.$property}"/>
  #elseif ($range == "Edm.GeometryMultiLineString")						<Text text="{dataModel>$property}" tooltip="{i18n>$target.$property}"/>
  #elseif ($range == "Edm.GeometryMultiPolygon")						<Text text="{dataModel>$property}" tooltip="{i18n>$target.$property}"/>
  #elseif ($range == "Edm.GeometryCollection")						<Text text="{dataModel>$property}" tooltip="{i18n>$target.$property}"/>
  #else
  #end  
#end
#set ($ViewTitle	= "ViewTitle") 
#set ($This	= "this") 
#set ($SearchTooltip	= "SearchTooltip") 
<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>$entity.target#[[.Title]]#}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigationText}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="(" width="4px"/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" width="5px"/>
				</HBox>
				<Title id="entityTypeTitle" text="{i18n>$entity.target.$This}"/>
				<Title text="(" width="4px"/>
				<rdf:TextResource path="dataModel>" entitySet="$entitySet.name"/>
				<Title text=")" width="5px"/>
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed"#if($entity.hasMultipleColumns) visible="true"#else visible="false"#end	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Title text="{i18n>Lens.field}"/>
				</Column>
				#if($entity.ownProperties)
				<Column visible="true">
					<Title text="{i18n>Lens.common}"/>
				</Column>
				#end
				#foreach($subTypeName in $entity.subTypeNames)
				<Column visible="true">
					<Title text="$subTypeName"/>
				</Column>
				#end	
			</columns>
			<items>
			#foreach($property in $uiTemplate.properties)
				#if($property.visible)
				#set( $complexTypeName =  $property.range)
				<ColumnListItem >
					<cells>
						<Label text="{i18n>$entity.target.$property.property}" required="false"/> 
					#if($entity.ownProperties)
						#if($property.propertyType == "D")
						#entitymaptypes( $property $entity.target  ) 
						#elseif ($property.propertyType == "O")
						<rdf:TextResource path="dataModel>$property.property" entitySet="$property.range" tooltip="{i18n>$entity.target.$property.property}"/>		
						#elseif ($property.propertyType == "C")
						<rdf:ListResource path="dataModel>$property.property" entitySet="$property.range" tooltip="{i18n>$entity.target.$property.property}"/>
						#elseif ($property.propertyType == "S")
						<Text text=""/>
						#end
					#end
					#foreach($subTypeName in $entity.subTypeNames)				
						#set( $complexType = $complexTypes.get($complexTypeName))
						#if(  $complexType.getPropertyTemplates($subTypeName)  )
							#set( $complexProperties = $complexType.getPropertyTemplates($subTypeName))
							#if( $complexProperties.size() > 1 )
						<VBox>
							#end
							#foreach( $complexProperty in   $complexProperties)
								#if($complexProperty.propertyType == "D")
						#entitymaptypes( $complexProperty $entity.target  ) 
								#elseif ($complexProperty.propertyType == "O")
						<rdf:TextResource path="dataModel>$complexProperty.property" entitySet="$complexProperty.range" tooltip="{i18n>$entity.target.$complexProperty.property}"/>		
								#elseif ($complexProperty.propertyType == "C")
						<rdf:ListResource path="dataModel>$complexProperty.property" entitySet="$complexProperty.range" tooltip="{i18n>$entity.target.$complexProperty.property}"/>
								#end	
							#end
							#if($complexProperties.size() > 1 )
						</VBox>
							#end
						#else
						<Text text=""/>
						#end
					#end	
					</cells>
				</ColumnListItem>
				#end					
			#end
			</items>
		</Table>
	</Page>
</mvc:View>