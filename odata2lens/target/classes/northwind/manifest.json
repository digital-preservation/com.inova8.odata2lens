{
	"_version": "1.4.0",
	"sap.app": {
		"id": "com.inova8.lens",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{Lens.appTitle}}",
		"description": "{{Lens.appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"dataSources": {
			"dataService": {
				"serviceUrl": "./../../odata2sparql/northwind/",
				"odataVersion": "4.0",
				"autoExpandSelect": true,
				"operationMode": "Server",
				"synchronizationMode": "None"
			},
			"local": {
				"serviceUrl": "http://127.0.0.1:8080/odata2sparql/northwind/",
				"odataVersion": "4.0",
				"autoExpandSelect": true,
				"operationMode": "Server",
				"synchronizationMode": "None"
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "sap-icon://task",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_bluecrystal"
		]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "com.inova8.lens.view.Lens",
			"type": "XML",
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.38.0",
			"libs": {
				"sap.ui.core": {
					"minVersion": "1.38.0"
				},
				"sap.m": {
					"minVersion": "1.38.0"
				}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "apartment.northwind.i18n.i18n"
				}
			},
			"dataModel": {
				"dataSource": "dataService",
				"settings": {
					"odataVersion": "4.0",
					"autoExpandSelect": true,
					"operationMode": "Server",
					"synchronizationMode": "None"
				},
				"type": "sap.ui.model.odata.v4.ODataModel"
			},
			"metaModel": {
				"type": "sap.ui.model.odata.v4.ODataMetaModel"
			},
			"sideMenu": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/sideMenu.json"
			},
			"contextMenu": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/contextMenu.json"
			},
			"alerts": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/alerts.json"
			},
			"routing": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/routing.json"
			},
			"history": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/history.json"
			},
			"resourceCache": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/resourceCache.json"
			}
		}
	}
}