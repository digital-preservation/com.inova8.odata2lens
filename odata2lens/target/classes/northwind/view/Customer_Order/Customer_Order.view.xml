<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>Customer_Order.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<Title text="{i18n>Customer_Order.this}"/>
				<rdf:TextResource path="dataModel>" entitySet="Customer_Order"/>
				<Title text="{i18n>Customer_Order.Title}"/>
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Customer_Order.customer}" required="false"/> 
						<rdf:TextLink text="{path:'dataModel>customer',type:'sap.ui.model.type.String' }" linkText="{i18n>Customer_Order.customer}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Customer_Order.customer_Order_customerCity}" required="false"/> 
						<rdf:TextLink text="{path:'dataModel>customer_Order_customerCity',type:'sap.ui.model.type.String' }" linkText="{i18n>Customer_Order.customer_Order_customerCity}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Customer_Order.customer_Order_customerCompanyName}" required="false"/> 
						<rdf:TextLink text="{path:'dataModel>customer_Order_customerCompanyName',type:'sap.ui.model.type.String' }" linkText="{i18n>Customer_Order.customer_Order_customerCompanyName}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Customer_Order.customer_Order_shipCity}" required="false"/> 
						<rdf:TextLink text="{path:'dataModel>customer_Order_shipCity',type:'sap.ui.model.type.String' }" linkText="{i18n>Customer_Order.customer_Order_shipCity}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Customer_Order.order}" required="false"/> 
						<rdf:TextLink text="{path:'dataModel>order',type:'sap.ui.model.type.String' }" linkText="{i18n>Customer_Order.order}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Customer_Order.customer_Order_customer}" required="false"/> 
						<rdf:TextResource path="dataModel>customer_Order_customer" entitySet="Customer"/>		
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Customer_Order.customer_Order_order}" required="false"/> 
						<rdf:TextResource path="dataModel>customer_Order_order" entitySet="Order"/>		
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>