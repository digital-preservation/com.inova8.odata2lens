<mvc:View controllerName="com.inova8.lens.controller.Entity" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>Customer_Order.Title}"
		busy="{entityView>/busy}" busyIndicatorDelay="{entityView>/delay}">
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<Title text="{i18n>Customer_Order.this}"/>
				<rdf:TextResource path="dataModel>" entitySet="Customer_Order"/>
				<Title text="{i18n>Customer_Order.Title}" />
				<ToolbarSpacer/>
			</Toolbar>
		</customHeader>
		<content>
			<f:SimpleForm class="editableForm" columnsL="1" columnsM="1" editable="true" emptySpanL="4" emptySpanM="4" 
				labelSpanL="2" labelSpanM="2" layout="ResponsiveGridLayout" maxContainerCols="2" minWidth="1024" singleContainerFullSize="true">
				<f:content>
					<!--<Label text="{i18n>Customer_Order.this}" required="false"/><rdf:TextResource path="dataModel>" entitySet="Customer_Order"/>-->
					<Label text="{i18n>Customer_Order.customer}" required="false"/>   <rdf:TextLink text="{path:'dataModel>customer',type:'sap.ui.model.type.String' }" linkText="{i18n>Customer_Order.customer}"/>
					<Label text="{i18n>Customer_Order.customer_Order_customerCity}" required="false"/>   <rdf:TextLink text="{path:'dataModel>customer_Order_customerCity',type:'sap.ui.model.type.String' }" linkText="{i18n>Customer_Order.customer_Order_customerCity}"/>
					<Label text="{i18n>Customer_Order.customer_Order_customerCompanyName}" required="false"/>   <rdf:TextLink text="{path:'dataModel>customer_Order_customerCompanyName',type:'sap.ui.model.type.String' }" linkText="{i18n>Customer_Order.customer_Order_customerCompanyName}"/>
					<Label text="{i18n>Customer_Order.customer_Order_shipCity}" required="false"/>   <rdf:TextLink text="{path:'dataModel>customer_Order_shipCity',type:'sap.ui.model.type.String' }" linkText="{i18n>Customer_Order.customer_Order_shipCity}"/>
					<Label text="{i18n>Customer_Order.order}" required="false"/>   <rdf:TextLink text="{path:'dataModel>order',type:'sap.ui.model.type.String' }" linkText="{i18n>Customer_Order.order}"/>
					<Label text="{i18n>Customer_Order.customer_Order_customer}" required="false"/><rdf:TextResource path="dataModel>customer_Order_customer" entitySet="Customer"/>		
					<Label text="{i18n>Customer_Order.customer_Order_order}" required="false"/><rdf:TextResource path="dataModel>customer_Order_order" entitySet="Order"/>		
				</f:content>
			</f:SimpleForm>
		</content>
	</Page>
</mvc:View>