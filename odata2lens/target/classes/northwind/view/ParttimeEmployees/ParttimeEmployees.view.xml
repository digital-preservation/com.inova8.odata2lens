  <rdf:TextLink text="{path:'dataModel>employeePostalCode',type:'sap.ui.model.type.String'}" linkText="{i18n>ParttimeEmployee.employeePostalCode}"/>
  <rdf:TextLink text="{path:'dataModel>extension',type:'sap.ui.model.type.String'}" linkText="{i18n>ParttimeEmployee.extension}"/>
  <rdf:TextLink text="{path:'dataModel>firstName',type:'sap.ui.model.type.String'}" linkText="{i18n>ParttimeEmployee.firstName}"/>
  <Text text="{dataModel>hireDate}"/>
  <rdf:TextLink text="{path:'dataModel>homePhone',type:'sap.ui.model.type.String'}" linkText="{i18n>ParttimeEmployee.homePhone}"/>
  <rdf:TextLink text="{path:'dataModel>isDefinedBy',type:'sap.ui.model.type.String'}" linkText="{i18n>ParttimeEmployee.isDefinedBy}"/>
  <rdf:TextLink text="{path:'dataModel>lastName',type:'sap.ui.model.type.String'}" linkText="{i18n>ParttimeEmployee.lastName}"/>
  <Text text="{path:'dataModel>lat',type:'sap.ui.model.type.Float' }"/>
  <rdf:TextLink text="{path:'dataModel>lat_long',type:'sap.ui.model.type.String'}" linkText="{i18n>ParttimeEmployee.lat_long}"/>
  <Text text="{path:'dataModel>long',type:'sap.ui.model.type.Float' }"/>
  <rdf:TextLink text="{path:'dataModel>notes',type:'sap.ui.model.type.String'}" linkText="{i18n>ParttimeEmployee.notes}"/>
  <Text text="{dataModel>parttimeHours}"/>
  <rdf:TextLink text="{path:'dataModel>photo',type:'sap.ui.model.type.String'}" linkText="{i18n>ParttimeEmployee.photo}"/>
  <rdf:TextLink text="{path:'dataModel>regionDescription',type:'sap.ui.model.type.String'}" linkText="{i18n>ParttimeEmployee.regionDescription}"/>
  <rdf:TextLink text="{path:'dataModel>reportsToNotation',type:'sap.ui.model.type.String'}" linkText="{i18n>ParttimeEmployee.reportsToNotation}"/>
  <rdf:TextLink text="{path:'dataModel>territoryNotation',type:'sap.ui.model.type.String'}" linkText="{i18n>ParttimeEmployee.territoryNotation}"/>
  <rdf:TextLink text="{path:'dataModel>title',type:'sap.ui.model.type.String'}" linkText="{i18n>ParttimeEmployee.title}"/>
  <rdf:TextLink text="{path:'dataModel>titleOfCourtesy',type:'sap.ui.model.type.String'}" linkText="{i18n>ParttimeEmployee.titleOfCourtesy}"/>
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>