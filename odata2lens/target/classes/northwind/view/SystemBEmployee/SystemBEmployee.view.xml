					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SystemBEmployee.lat}" required="false"/> 
						<Text text="{path:'dataModel>lat',type:'sap.ui.model.type.Float' }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SystemBEmployee.lat_long}" required="false"/> 
						<rdf:TextLink text="{path:'dataModel>lat_long',type:'sap.ui.model.type.String' }" linkText="{i18n>SystemBEmployee.lat_long}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
