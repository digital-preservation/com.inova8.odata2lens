				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SystemBEmployee.managesEmployee}" required="false"/> 
						<rdf:ListResource path="dataModel>managesEmployee" entitySet="Employee"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>SystemBEmployee.managesOrder}" required="false"/> 
						<rdf:ListResource path="dataModel>managesOrder" entitySet="Order"/>
