<mvc:View controllerName="com.inova8.lens.controller.Entity" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>Customer.Title}"
		busy="{entityView>/busy}" busyIndicatorDelay="{entityView>/delay}">
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<Title text="{i18n>Customer.this}"/>
				<rdf:TextResource path="dataModel>" entitySet="Customer"/>
				<Title text="{i18n>Customer.Title}" />
				<ToolbarSpacer/>
			</Toolbar>
		</customHeader>
		<content>
			<f:SimpleForm class="editableForm" columnsL="1" columnsM="1" editable="true" emptySpanL="4" emptySpanM="4" 
				labelSpanL="2" labelSpanM="2" layout="ResponsiveGridLayout" maxContainerCols="2" minWidth="1024" singleContainerFullSize="true">
				<f:content>
					<!--<Label text="{i18n>Customer.this}" required="false"/><rdf:TextResource path="dataModel>" entitySet="Customer"/>-->
					<Label text="{i18n>Customer.comment}" required="false"/>   <rdf:TextLink text="{path:'dataModel>comment',type:'sap.ui.model.type.String' }" linkText="{i18n>Customer.comment}"/>
					<Label text="{i18n>Customer.customerAddress}" required="false"/>   <rdf:TextLink text="{path:'dataModel>customerAddress',type:'sap.ui.model.type.String' }" linkText="{i18n>Customer.customerAddress}"/>
					<Label text="{i18n>Customer.customerCity}" required="false"/>   <rdf:TextLink text="{path:'dataModel>customerCity',type:'sap.ui.model.type.String' }" linkText="{i18n>Customer.customerCity}"/>
					<Label text="{i18n>Customer.customerCompanyName}" required="false"/>   <rdf:TextLink text="{path:'dataModel>customerCompanyName',type:'sap.ui.model.type.String' }" linkText="{i18n>Customer.customerCompanyName}"/>
					<Label text="{i18n>Customer.customerContactName}" required="false"/>   <rdf:TextLink text="{path:'dataModel>customerContactName',type:'sap.ui.model.type.String' }" linkText="{i18n>Customer.customerContactName}"/>
					<Label text="{i18n>Customer.customerContactTitle}" required="false"/>   <rdf:TextLink text="{path:'dataModel>customerContactTitle',type:'sap.ui.model.type.String' }" linkText="{i18n>Customer.customerContactTitle}"/>
					<Label text="{i18n>Customer.customerCountry}" required="false"/>   <rdf:TextLink text="{path:'dataModel>customerCountry',type:'sap.ui.model.type.String' }" linkText="{i18n>Customer.customerCountry}"/>
					<Label text="{i18n>Customer.customerFax}" required="false"/>   <rdf:TextLink text="{path:'dataModel>customerFax',type:'sap.ui.model.type.String' }" linkText="{i18n>Customer.customerFax}"/>
					<Label text="{i18n>Customer.customerPhone}" required="false"/>   <rdf:TextLink text="{path:'dataModel>customerPhone',type:'sap.ui.model.type.String' }" linkText="{i18n>Customer.customerPhone}"/>
					<Label text="{i18n>Customer.customerPostalCode}" required="false"/>   <rdf:TextLink text="{path:'dataModel>customerPostalCode',type:'sap.ui.model.type.String' }" linkText="{i18n>Customer.customerPostalCode}"/>
					<Label text="{i18n>Customer.customerWebsite}" required="false"/>   <rdf:TextLink text="{path:'dataModel>customerWebsite',type:'sap.ui.model.type.String' }" linkText="{i18n>Customer.customerWebsite}"/>
					<Label text="{i18n>Customer.isDefinedBy}" required="false"/>   <rdf:TextLink text="{path:'dataModel>isDefinedBy',type:'sap.ui.model.type.String' }" linkText="{i18n>Customer.isDefinedBy}"/>
					<Label text="{i18n>Customer.lat}" required="false"/>   <Text text="{path:'dataModel>lat',type:'sap.ui.model.type.Float' }"/>
					<Label text="{i18n>Customer.lat_long}" required="false"/>   <rdf:TextLink text="{path:'dataModel>lat_long',type:'sap.ui.model.type.String' }" linkText="{i18n>Customer.lat_long}"/>
					<Label text="{i18n>Customer.long}" required="false"/>   <Text text="{path:'dataModel>long',type:'sap.ui.model.type.Float' }"/>
					<Label text="{i18n>Customer.regionDescription}" required="false"/>   <rdf:TextLink text="{path:'dataModel>regionDescription',type:'sap.ui.model.type.String' }" linkText="{i18n>Customer.regionDescription}"/>
				</f:content>
			</f:SimpleForm>
		</content>
	</Page>
</mvc:View>