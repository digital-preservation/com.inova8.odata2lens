<mvc:View controllerName="com.inova8.lens.controller.Entity" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>Employee.Title}"
		busy="{entityView>/busy}" busyIndicatorDelay="{entityView>/delay}">
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<Title text="{i18n>Employee.this}"/>
				<rdf:TextResource path="dataModel>" entitySet="Employee"/>
				<Title text="{i18n>Employee.Title}" />
				<ToolbarSpacer/>
			</Toolbar>
		</customHeader>
		<content>
			<f:SimpleForm class="editableForm" columnsL="1" columnsM="1" editable="true" emptySpanL="4" emptySpanM="4" 
				labelSpanL="2" labelSpanM="2" layout="ResponsiveGridLayout" maxContainerCols="2" minWidth="1024" singleContainerFullSize="true">
				<f:content>
					<!--<Label text="{i18n>Employee.this}" required="false"/><rdf:TextResource path="dataModel>" entitySet="Employee"/>-->
					<Label text="{i18n>Employee.birthDate}" required="false"/>   <Text text="{dataModel>birthDate}"/>
					<Label text="{i18n>Employee.comment}" required="false"/>   <rdf:TextLink text="{path:'dataModel>comment',type:'sap.ui.model.type.String' }" linkText="{i18n>Employee.comment}"/>
					<Label text="{i18n>Employee.employeeAddress}" required="false"/>   <rdf:TextLink text="{path:'dataModel>employeeAddress',type:'sap.ui.model.type.String' }" linkText="{i18n>Employee.employeeAddress}"/>
					<Label text="{i18n>Employee.employeeCity}" required="false"/>   <rdf:TextLink text="{path:'dataModel>employeeCity',type:'sap.ui.model.type.String' }" linkText="{i18n>Employee.employeeCity}"/>
					<Label text="{i18n>Employee.employeeCountry}" required="false"/>   <rdf:TextLink text="{path:'dataModel>employeeCountry',type:'sap.ui.model.type.String' }" linkText="{i18n>Employee.employeeCountry}"/>
					<Label text="{i18n>Employee.employeeLinkedIn}" required="false"/>   <rdf:TextLink text="{path:'dataModel>employeeLinkedIn',type:'sap.ui.model.type.String' }" linkText="{i18n>Employee.employeeLinkedIn}"/>
					<Label text="{i18n>Employee.employeePostalCode}" required="false"/>   <rdf:TextLink text="{path:'dataModel>employeePostalCode',type:'sap.ui.model.type.String' }" linkText="{i18n>Employee.employeePostalCode}"/>
					<Label text="{i18n>Employee.extension}" required="false"/>   <rdf:TextLink text="{path:'dataModel>extension',type:'sap.ui.model.type.String' }" linkText="{i18n>Employee.extension}"/>
					<Label text="{i18n>Employee.firstName}" required="false"/>   <rdf:TextLink text="{path:'dataModel>firstName',type:'sap.ui.model.type.String' }" linkText="{i18n>Employee.firstName}"/>
					<Label text="{i18n>Employee.hireDate}" required="false"/>   <Text text="{dataModel>hireDate}"/>
					<Label text="{i18n>Employee.homePhone}" required="false"/>   <rdf:TextLink text="{path:'dataModel>homePhone',type:'sap.ui.model.type.String' }" linkText="{i18n>Employee.homePhone}"/>
					<Label text="{i18n>Employee.isDefinedBy}" required="false"/>   <rdf:TextLink text="{path:'dataModel>isDefinedBy',type:'sap.ui.model.type.String' }" linkText="{i18n>Employee.isDefinedBy}"/>
					<Label text="{i18n>Employee.lastName}" required="false"/>   <rdf:TextLink text="{path:'dataModel>lastName',type:'sap.ui.model.type.String' }" linkText="{i18n>Employee.lastName}"/>
					<Label text="{i18n>Employee.lat}" required="false"/>   <Text text="{path:'dataModel>lat',type:'sap.ui.model.type.Float' }"/>
					<Label text="{i18n>Employee.lat_long}" required="false"/>   <rdf:TextLink text="{path:'dataModel>lat_long',type:'sap.ui.model.type.String' }" linkText="{i18n>Employee.lat_long}"/>
					<Label text="{i18n>Employee.long}" required="false"/>   <Text text="{path:'dataModel>long',type:'sap.ui.model.type.Float' }"/>
					<Label text="{i18n>Employee.notes}" required="false"/>   <rdf:TextLink text="{path:'dataModel>notes',type:'sap.ui.model.type.String' }" linkText="{i18n>Employee.notes}"/>
					<Label text="{i18n>Employee.photo}" required="false"/>   <rdf:TextLink text="{path:'dataModel>photo',type:'sap.ui.model.type.String' }" linkText="{i18n>Employee.photo}"/>
					<Label text="{i18n>Employee.regionDescription}" required="false"/>   <rdf:TextLink text="{path:'dataModel>regionDescription',type:'sap.ui.model.type.String' }" linkText="{i18n>Employee.regionDescription}"/>
					<Label text="{i18n>Employee.reportsToNotation}" required="false"/>   <rdf:TextLink text="{path:'dataModel>reportsToNotation',type:'sap.ui.model.type.String' }" linkText="{i18n>Employee.reportsToNotation}"/>
					<Label text="{i18n>Employee.territoryNotation}" required="false"/>   <rdf:TextLink text="{path:'dataModel>territoryNotation',type:'sap.ui.model.type.String' }" linkText="{i18n>Employee.territoryNotation}"/>
					<Label text="{i18n>Employee.title}" required="false"/>   <rdf:TextLink text="{path:'dataModel>title',type:'sap.ui.model.type.String' }" linkText="{i18n>Employee.title}"/>
					<Label text="{i18n>Employee.titleOfCourtesy}" required="false"/>   <rdf:TextLink text="{path:'dataModel>titleOfCourtesy',type:'sap.ui.model.type.String' }" linkText="{i18n>Employee.titleOfCourtesy}"/>
					<Label text="{i18n>Employee.reportsTo}" required="false"/><rdf:TextResource path="dataModel>reportsTo" entitySet="Employee"/>		
				</f:content>
			</f:SimpleForm>
		</content>
	</Page>
</mvc:View>