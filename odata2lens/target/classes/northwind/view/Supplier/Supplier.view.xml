<mvc:View controllerName="com.inova8.lens.controller.Entity360" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>Supplier.Title}" busy="{entityView>/busy}"
		busyIndicatorDelay="{entityView>/delay}" >
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox id="sourceTitle" alignItems="Center">
						<rdf:TextResource id="selectedResource" path="" entitySet=""/>
				</HBox>	
				<Title id="pathTitle" text="{entityView>/navigation}"/>
				<HBox id="versionsTitle" alignItems="Center">
					<Title text="("/>
					<ComboBox id="versionSelection" items="{ path: 'entityView>/versions' }" selectionChange="onVersionChange">
						<core:Item key="{entityView>navigation}" text="{entityView>text}"/>
					</ComboBox>
					<Title text=")" />
				</HBox>
				<Title text="{i18n>Supplier.this}"/>
				<Title text="(" />
				<rdf:TextResource path="dataModel>" entitySet="Supplier"/>
				<Title text=")" />
	
				<ToolbarSpacer/>
				<ToggleButton id="toggleColumns" icon="sap-icon://hide" activeIcon="sap-icon://show" press="onSettingsPressed" visible="false"	>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</ToggleButton>
				
			</Toolbar>
		</customHeader>
		<Table id="form360" inset="false" fixedLayout="true" class="multicolumnform" updateFinished="hideColumns">
			<columns>
				<Column hAlign="Right" width="200px">
					<Text text=""/>
				</Column>
				<Column visible="true">
				</Column>
			</columns>
			<items>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Supplier.comment}" required="false"/> 
						<rdf:TextLink text="{path:'dataModel>comment',type:'sap.ui.model.type.String' }" linkText="{i18n>Supplier.comment}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Supplier.homePage}" required="false"/> 
						<rdf:TextLink text="{path:'dataModel>homePage',type:'sap.ui.model.type.String' }" linkText="{i18n>Supplier.homePage}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Supplier.lat}" required="false"/> 
						<Text text="{path:'dataModel>lat',type:'sap.ui.model.type.Float' }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Supplier.lat_long}" required="false"/> 
						<rdf:TextLink text="{path:'dataModel>lat_long',type:'sap.ui.model.type.String' }" linkText="{i18n>Supplier.lat_long}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Supplier.long}" required="false"/> 
						<Text text="{path:'dataModel>long',type:'sap.ui.model.type.Float' }"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Supplier.regionDescription}" required="false"/> 
						<rdf:TextLink text="{path:'dataModel>regionDescription',type:'sap.ui.model.type.String' }" linkText="{i18n>Supplier.regionDescription}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Supplier.seeAlso}" required="false"/> 
						<rdf:TextLink text="{path:'dataModel>seeAlso',type:'sap.ui.model.type.String' }" linkText="{i18n>Supplier.seeAlso}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Supplier.supplierAddress}" required="false"/> 
						<rdf:TextLink text="{path:'dataModel>supplierAddress',type:'sap.ui.model.type.String' }" linkText="{i18n>Supplier.supplierAddress}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Supplier.supplierCity}" required="false"/> 
						<rdf:TextLink text="{path:'dataModel>supplierCity',type:'sap.ui.model.type.String' }" linkText="{i18n>Supplier.supplierCity}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Supplier.supplierCompanyName}" required="false"/> 
						<rdf:TextLink text="{path:'dataModel>supplierCompanyName',type:'sap.ui.model.type.String' }" linkText="{i18n>Supplier.supplierCompanyName}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Supplier.supplierContactName}" required="false"/> 
						<rdf:TextLink text="{path:'dataModel>supplierContactName',type:'sap.ui.model.type.String' }" linkText="{i18n>Supplier.supplierContactName}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Supplier.supplierContactTitle}" required="false"/> 
						<rdf:TextLink text="{path:'dataModel>supplierContactTitle',type:'sap.ui.model.type.String' }" linkText="{i18n>Supplier.supplierContactTitle}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Supplier.supplierCountry}" required="false"/> 
						<rdf:TextLink text="{path:'dataModel>supplierCountry',type:'sap.ui.model.type.String' }" linkText="{i18n>Supplier.supplierCountry}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Supplier.supplierFax}" required="false"/> 
						<rdf:TextLink text="{path:'dataModel>supplierFax',type:'sap.ui.model.type.String' }" linkText="{i18n>Supplier.supplierFax}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Supplier.supplierHomePage}" required="false"/> 
						<rdf:TextLink text="{path:'dataModel>supplierHomePage',type:'sap.ui.model.type.String' }" linkText="{i18n>Supplier.supplierHomePage}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Supplier.supplierPhone}" required="false"/> 
						<rdf:TextLink text="{path:'dataModel>supplierPhone',type:'sap.ui.model.type.String' }" linkText="{i18n>Supplier.supplierPhone}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Supplier.supplierPostalCode}" required="false"/> 
						<rdf:TextLink text="{path:'dataModel>supplierPostalCode',type:'sap.ui.model.type.String' }" linkText="{i18n>Supplier.supplierPostalCode}"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Supplier.supplierRegion}" required="false"/> 
						<rdf:ListResource path="dataModel>supplierRegion" entitySet="Region"/>
					</cells>
				</ColumnListItem>
				<ColumnListItem >
					<cells>
						<Label text="{i18n>Supplier.suppliesProduct}" required="false"/> 
						<rdf:ListResource path="dataModel>suppliesProduct" entitySet="Product"/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Page>
</mvc:View>