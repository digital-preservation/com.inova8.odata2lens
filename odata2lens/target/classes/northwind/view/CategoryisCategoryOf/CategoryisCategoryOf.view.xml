<mvc:View controllerName="com.inova8.lens.controller.EntityNavigationSet" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>CategoryisCategoryOf.ViewTitle}">
		<content>
			<Table id="table" inset="false" items="{ path: '/{objectId}/isCategoryOf', sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entityNavigationSetView>/tableNoDataText}" busyIndicatorDelay="{entityNavigationSetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>					
					<ToolbarSpacer/>
						<Title id="entitySet"/>
						<rdf:TextResource id="resourceTitle" />
						<Title id="tableHeader" text="{entityNavigationSetView>/entityNavigationSetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>CategoryisCategoryOf.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CategoryisCategoryOf.categoryNotation}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CategoryisCategoryOf.comment}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CategoryisCategoryOf.discontinued}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CategoryisCategoryOf.isDefinedBy}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CategoryisCategoryOf.productName}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CategoryisCategoryOf.productUnitPrice}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CategoryisCategoryOf.quantityPerUnit}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CategoryisCategoryOf.reorderLevel}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CategoryisCategoryOf.supplierNotation}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CategoryisCategoryOf.unitsInStock}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>CategoryisCategoryOf.unitsOnOrder}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="Product"/>
  <rdf:TextLink text="{path:'dataModel>categoryNotation',type:'sap.ui.model.type.String' }" linkText="{i18n>Product.categoryNotation}"/>
  <rdf:TextLink text="{path:'dataModel>comment',type:'sap.ui.model.type.String' }" linkText="{i18n>Product.comment}"/>
  <Text text="{path:'dataModel>discontinued',type:'sap.ui.model.type.Boolean'}"/>  
  <rdf:TextLink text="{path:'dataModel>isDefinedBy',type:'sap.ui.model.type.String' }" linkText="{i18n>Product.isDefinedBy}"/>
  <rdf:TextLink text="{path:'dataModel>productName',type:'sap.ui.model.type.String' }" linkText="{i18n>Product.productName}"/>
  <Text text="{path:'dataModel>productUnitPrice',type:'sap.ui.model.type.Float' }"/>
  <rdf:TextLink text="{path:'dataModel>quantityPerUnit',type:'sap.ui.model.type.String' }" linkText="{i18n>Product.quantityPerUnit}"/>
  <Text text="{path:'dataModel>reorderLevel',type:'sap.ui.model.type.Integer' }"/>
  <rdf:TextLink text="{path:'dataModel>supplierNotation',type:'sap.ui.model.type.String' }" linkText="{i18n>Product.supplierNotation}"/>
  <Text text="{path:'dataModel>unitsInStock',type:'sap.ui.model.type.Integer' }"/>
  <Text text="{path:'dataModel>unitsOnOrder',type:'sap.ui.model.type.Integer' }"/>
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>