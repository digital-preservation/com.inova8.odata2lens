<mvc:View controllerName="com.inova8.lens.controller.Entity" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>Shipper.Title}"
		busy="{entityView>/busy}" busyIndicatorDelay="{entityView>/delay}">
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<Title text="{i18n>Shipper.this}"/>
				<rdf:TextResource path="dataModel>" entitySet="Shipper"/>
				<Title text="{i18n>Shipper.Title}" />
				<ToolbarSpacer/>
			</Toolbar>
		</customHeader>
		<content>
			<f:SimpleForm class="editableForm" columnsL="1" columnsM="1" editable="true" emptySpanL="4" emptySpanM="4" 
				labelSpanL="2" labelSpanM="2" layout="ResponsiveGridLayout" maxContainerCols="2" minWidth="1024" singleContainerFullSize="true">
				<f:content>
					<!--<Label text="{i18n>Shipper.this}" required="false"/><rdf:TextResource path="dataModel>" entitySet="Shipper"/>-->
					<Label text="{i18n>Shipper.comment}" required="false"/>   <rdf:TextLink text="{path:'dataModel>comment',type:'sap.ui.model.type.String' }" linkText="{i18n>Shipper.comment}"/>
					<Label text="{i18n>Shipper.isDefinedBy}" required="false"/>   <rdf:TextLink text="{path:'dataModel>isDefinedBy',type:'sap.ui.model.type.String' }" linkText="{i18n>Shipper.isDefinedBy}"/>
					<Label text="{i18n>Shipper.shipperCompanyName}" required="false"/>   <rdf:TextLink text="{path:'dataModel>shipperCompanyName',type:'sap.ui.model.type.String' }" linkText="{i18n>Shipper.shipperCompanyName}"/>
					<Label text="{i18n>Shipper.shipperPhone}" required="false"/>   <rdf:TextLink text="{path:'dataModel>shipperPhone',type:'sap.ui.model.type.String' }" linkText="{i18n>Shipper.shipperPhone}"/>
				</f:content>
			</f:SimpleForm>
		</content>
	</Page>
</mvc:View>