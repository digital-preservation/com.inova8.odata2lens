<mvc:View controllerName="com.inova8.lens.controller.Entity" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>Product.Title}"
		busy="{entityView>/busy}" busyIndicatorDelay="{entityView>/delay}">
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<Title text="{i18n>Product.this}"/>
				<rdf:TextResource path="dataModel>" entitySet="Product"/>
				<Title text="{i18n>Product.Title}" />
				<ToolbarSpacer/>
			</Toolbar>
		</customHeader>
		<content>
			<f:SimpleForm class="editableForm" columnsL="1" columnsM="1" editable="true" emptySpanL="4" emptySpanM="4" 
				labelSpanL="2" labelSpanM="2" layout="ResponsiveGridLayout" maxContainerCols="2" minWidth="1024" singleContainerFullSize="true">
				<f:content>
					<!--<Label text="{i18n>Product.this}" required="false"/><rdf:TextResource path="dataModel>" entitySet="Product"/>-->
					<Label text="{i18n>Product.categoryNotation}" required="false"/>   <rdf:TextLink text="{path:'dataModel>categoryNotation',type:'sap.ui.model.type.String' }" linkText="{i18n>Product.categoryNotation}"/>
					<Label text="{i18n>Product.comment}" required="false"/>   <rdf:TextLink text="{path:'dataModel>comment',type:'sap.ui.model.type.String' }" linkText="{i18n>Product.comment}"/>
					<Label text="{i18n>Product.discontinued}" required="false"/>   <Text text="{path:'dataModel>discontinued',type:'sap.ui.model.type.Boolean'}"/>  
					<Label text="{i18n>Product.isDefinedBy}" required="false"/>   <rdf:TextLink text="{path:'dataModel>isDefinedBy',type:'sap.ui.model.type.String' }" linkText="{i18n>Product.isDefinedBy}"/>
					<Label text="{i18n>Product.productName}" required="false"/>   <rdf:TextLink text="{path:'dataModel>productName',type:'sap.ui.model.type.String' }" linkText="{i18n>Product.productName}"/>
					<Label text="{i18n>Product.productUnitPrice}" required="false"/>   <Text text="{path:'dataModel>productUnitPrice',type:'sap.ui.model.type.Float' }"/>
					<Label text="{i18n>Product.quantityPerUnit}" required="false"/>   <rdf:TextLink text="{path:'dataModel>quantityPerUnit',type:'sap.ui.model.type.String' }" linkText="{i18n>Product.quantityPerUnit}"/>
					<Label text="{i18n>Product.reorderLevel}" required="false"/>   <Text text="{path:'dataModel>reorderLevel',type:'sap.ui.model.type.Integer' }"/>
					<Label text="{i18n>Product.supplierNotation}" required="false"/>   <rdf:TextLink text="{path:'dataModel>supplierNotation',type:'sap.ui.model.type.String' }" linkText="{i18n>Product.supplierNotation}"/>
					<Label text="{i18n>Product.unitsInStock}" required="false"/>   <Text text="{path:'dataModel>unitsInStock',type:'sap.ui.model.type.Integer' }"/>
					<Label text="{i18n>Product.unitsOnOrder}" required="false"/>   <Text text="{path:'dataModel>unitsOnOrder',type:'sap.ui.model.type.Integer' }"/>
					<Label text="{i18n>Product.category}" required="false"/><rdf:TextResource path="dataModel>category" entitySet="Category"/>		
					<Label text="{i18n>Product.supplier}" required="false"/><rdf:TextResource path="dataModel>supplier" entitySet="Supplier"/>		
				</f:content>
			</f:SimpleForm>
		</content>
	</Page>
</mvc:View>