<mvc:View controllerName="com.inova8.lens.controller.EntityNavigationSet" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>Customercustomer_customer_Order.ViewTitle}">
		<content>
			<Table id="table" inset="false" items="{ path: '/{objectId}/customer_customer_Order', sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entityNavigationSetView>/tableNoDataText}" busyIndicatorDelay="{entityNavigationSetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>					
					<ToolbarSpacer/>
						<Title id="entitySet"/>
						<rdf:TextResource id="resourceTitle" />
						<Title id="tableHeader" text="{entityNavigationSetView>/entityNavigationSetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>Customercustomer_customer_Order.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Customercustomer_customer_Order.customer}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Customercustomer_customer_Order.customer_Order_customerCity}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Customercustomer_customer_Order.customer_Order_customerCompanyName}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Customercustomer_customer_Order.customer_Order_shipCity}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>Customercustomer_customer_Order.order}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="Customer_Order"/>
  <rdf:TextLink text="{path:'dataModel>customer',type:'sap.ui.model.type.String' }" linkText="{i18n>Customer_Order.customer}"/>
  <rdf:TextLink text="{path:'dataModel>customer_Order_customerCity',type:'sap.ui.model.type.String' }" linkText="{i18n>Customer_Order.customer_Order_customerCity}"/>
  <rdf:TextLink text="{path:'dataModel>customer_Order_customerCompanyName',type:'sap.ui.model.type.String' }" linkText="{i18n>Customer_Order.customer_Order_customerCompanyName}"/>
  <rdf:TextLink text="{path:'dataModel>customer_Order_shipCity',type:'sap.ui.model.type.String' }" linkText="{i18n>Customer_Order.customer_Order_shipCity}"/>
  <rdf:TextLink text="{path:'dataModel>order',type:'sap.ui.model.type.String' }" linkText="{i18n>Customer_Order.order}"/>
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>