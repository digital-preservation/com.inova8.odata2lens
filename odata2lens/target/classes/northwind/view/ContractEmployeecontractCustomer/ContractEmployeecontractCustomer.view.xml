<mvc:View controllerName="com.inova8.lens.controller.EntityNavigationSet" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout"
	xmlns:rdf="com.inova8.lens.control">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>ContractEmployeecontractCustomer.ViewTitle}">
		<content>
			<Table id="table" inset="false" items="{ path: '/{objectId}/contractCustomer', sorter: { path: 'label', descending: false } 
				}"
				noDataText="{entityNavigationSetView>/tableNoDataText}" busyIndicatorDelay="{entityNavigationSetView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished"  fixedLayout="false">
				<headerToolbar>
					<Toolbar>					
					<ToolbarSpacer/>
						<Title id="entitySet"/>
						<rdf:TextResource id="resourceTitle" />
						<Title id="tableHeader" text="{entityNavigationSetView>/entityNavigationSetTableTitle}"/>
						<ToolbarSpacer/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column >
						<Text text="{i18n>ContractEmployeecontractCustomer.this}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContractEmployeecontractCustomer.comment}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContractEmployeecontractCustomer.customerAddress}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContractEmployeecontractCustomer.customerCity}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContractEmployeecontractCustomer.customerCompanyName}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContractEmployeecontractCustomer.customerContactName}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContractEmployeecontractCustomer.customerContactTitle}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContractEmployeecontractCustomer.customerCountry}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContractEmployeecontractCustomer.customerFax}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContractEmployeecontractCustomer.customerPhone}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContractEmployeecontractCustomer.customerPostalCode}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContractEmployeecontractCustomer.customerWebsite}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContractEmployeecontractCustomer.isDefinedBy}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContractEmployeecontractCustomer.lat}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContractEmployeecontractCustomer.lat_long}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContractEmployeecontractCustomer.long}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>ContractEmployeecontractCustomer.regionDescription}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="template" type="Navigation" press="onPress">
						<cells>
							<rdf:TextResource path="dataModel>" entitySet="Customer"/>
  <rdf:TextLink text="{path:'dataModel>comment',type:'sap.ui.model.type.String' }" linkText="{i18n>Customer.comment}"/>
  <rdf:TextLink text="{path:'dataModel>customerAddress',type:'sap.ui.model.type.String' }" linkText="{i18n>Customer.customerAddress}"/>
  <rdf:TextLink text="{path:'dataModel>customerCity',type:'sap.ui.model.type.String' }" linkText="{i18n>Customer.customerCity}"/>
  <rdf:TextLink text="{path:'dataModel>customerCompanyName',type:'sap.ui.model.type.String' }" linkText="{i18n>Customer.customerCompanyName}"/>
  <rdf:TextLink text="{path:'dataModel>customerContactName',type:'sap.ui.model.type.String' }" linkText="{i18n>Customer.customerContactName}"/>
  <rdf:TextLink text="{path:'dataModel>customerContactTitle',type:'sap.ui.model.type.String' }" linkText="{i18n>Customer.customerContactTitle}"/>
  <rdf:TextLink text="{path:'dataModel>customerCountry',type:'sap.ui.model.type.String' }" linkText="{i18n>Customer.customerCountry}"/>
  <rdf:TextLink text="{path:'dataModel>customerFax',type:'sap.ui.model.type.String' }" linkText="{i18n>Customer.customerFax}"/>
  <rdf:TextLink text="{path:'dataModel>customerPhone',type:'sap.ui.model.type.String' }" linkText="{i18n>Customer.customerPhone}"/>
  <rdf:TextLink text="{path:'dataModel>customerPostalCode',type:'sap.ui.model.type.String' }" linkText="{i18n>Customer.customerPostalCode}"/>
  <rdf:TextLink text="{path:'dataModel>customerWebsite',type:'sap.ui.model.type.String' }" linkText="{i18n>Customer.customerWebsite}"/>
  <rdf:TextLink text="{path:'dataModel>isDefinedBy',type:'sap.ui.model.type.String' }" linkText="{i18n>Customer.isDefinedBy}"/>
  <Text text="{path:'dataModel>lat',type:'sap.ui.model.type.Float' }"/>
  <rdf:TextLink text="{path:'dataModel>lat_long',type:'sap.ui.model.type.String' }" linkText="{i18n>Customer.lat_long}"/>
  <Text text="{path:'dataModel>long',type:'sap.ui.model.type.Float' }"/>
  <rdf:TextLink text="{path:'dataModel>regionDescription',type:'sap.ui.model.type.String' }" linkText="{i18n>Customer.regionDescription}"/>
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>