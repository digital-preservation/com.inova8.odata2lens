#set ($ViewTitle	= "ViewTitle") 
#set ($This	= "this") 
#set ($SearchThis	= "searchThis") 
#set ($SearchTooltip	= "SearchTooltip") 
#macro( entitySetmaptypes $propertyObject $target )
#set( $range =  $propertyObject.range)
#set( $formatOptions =  $propertyObject.formatOptions)
  #if ($range == "Edm.Binary")							<Text text="{dataModel>$propertyObject.property}" tooltip="{i18n>$target.$propertyObject.property}"/>
  #elseif ($range == "Edm.Boolean")							<Text text="{path:'dataModel>$propertyObject.property',type:'sap.ui.model.type.Boolean'}" tooltip="{i18n>$target.$propertyObject.property}"/>
  #elseif ($range == "Edm.Byte")							<Text text="{dataModel>$propertyObject.property}" tooltip="{i18n>$target.$propertyObject.property}"/>
  #elseif ($range == "Edm.SByte")							<Text text="{dataModel>$propertyObject.property}" tooltip="{i18n>$target.$propertyObject.property}"/>
  #elseif ($range == "Edm.Date")							<Text text="{dataModel>$propertyObject.property}" tooltip="{i18n>$target.$propertyObject.property}"/>
  #elseif ($range == "Edm.DateTimeOffset")							<Text text="{dataModel>$propertyObject.property}" tooltip="{i18n>$target.$propertyObject.property}"/>
  #elseif ($range == "Edm.TimeOfDay")							<Text text="{dataModel>$propertyObject.property}" tooltip="{i18n>$target.$propertyObject.property}"/>
  #elseif ($range == "Edm.Duration")							<Text text="{dataModel>$propertyObject.property}" tooltip="{i18n>$target.$propertyObject.property}"/>
  #elseif ($range == "Edm.Decimal")							<Text text="{dataModel>$propertyObject.property}" tooltip="{i18n>$target.$propertyObject.property}"/>
  #elseif ($range == "Edm.Single")							<Text text="{dataModel>$propertyObject.property}" tooltip="{i18n>$target.$propertyObject.property}"/>
  #elseif ($range == "Edm.Double")							<Text text="{path:'dataModel>$propertyObject.property',type:'sap.ui.model.type.Float'$!formatOptions }" tooltip="{i18n>$target.$propertyObject.property}"/>
  #elseif ($range == "Edm.Guid")							<Text text="{path:'dataModel>$propertyObject.property',type:'sap.ui.model.type.String'}" tooltip="{i18n>$target.$propertyObject.property}"/>
  #elseif ($range == "Edm.Int16")							<Text text="{path:'dataModel>$propertyObject.property',type:'sap.ui.model.type.Integer'$!formatOptions }" tooltip="{i18n>$target.$propertyObject.property}"/>
  #elseif ($range == "Edm.Int32")							<Text text="{path:'dataModel>$propertyObject.property',type:'sap.ui.model.type.Integer'$!formatOptions }" tooltip="{i18n>$target.$propertyObject.property}"/>
  #elseif ($range == "Edm.Int64")							<Text text="{path:'dataModel>$propertyObject.property',type:'sap.ui.model.type.Integer'$!formatOptions}" tooltip="{i18n>$target.$propertyObject.property}"/>
  #elseif ($range == "Edm.String")							<rdf:TextLink text="{path:'dataModel>$propertyObject.property',type:'sap.ui.model.type.String' }" linkText="{i18n>$target.$propertyObject.property}" tooltip="{i18n>$target.$propertyObject.property}"/>
  #elseif ($range == "Edm.Stream")							<Text text="{dataModel>$propertyObject.property}" tooltip="{i18n>$target.$propertyObject.property}"/>
  #elseif ($range == "Edm.Geography")							<Text text="{dataModel>$propertyObject.property}" tooltip="{i18n>$target.$propertyObject.property}"/>
  #elseif ($range == "Edm.GeographyPoint")							<Text text="{dataModel>$propertyObject.property}" tooltip="{i18n>$target.$propertyObject.property}"/>
  #elseif ($range == "Edm.GeographyLineString")							<Text text="{dataModel>$propertyObject.property}" tooltip="{i18n>$target.$propertyObject.property}"/>
  #elseif ($range == "Edm.GeographyPolygon")							<Text text="{dataModel>$propertyObject.property}" tooltip="{i18n>$target.$propertyObject.property}"/>
  #elseif ($range == "Edm.GeographyMultiPoint")							<Text text="{dataModel>$propertyObject.property}" tooltip="{i18n>$target.$propertyObject.property}"/>
  #elseif ($range == "Edm.GeographyMultiLineString")							<Text text="{dataModel>$propertyObject.property}" tooltip="{i18n>$target.$propertyObject.property}"/>
  #elseif ($range == "Edm.GeographyMultiPolygon")							<Text text="{dataModel>$propertyObject.property}" tooltip="{i18n>$target.$propertyObject.property}"/>
  #elseif ($range == "Edm.GeographyCollection")							<Text text="{dataModel>$propertyObject.property}" tooltip="{i18n>$target.$propertyObject.property}"/>
  #elseif ($range == "Edm.Geometry")							<Text text="{dataModel>$propertyObject.property}" tooltip="{i18n>$target.$propertyObject.property}"/>
  #elseif ($range == "Edm.GeometryPoint")							<Text text="{dataModel>$propertyObject.property}" tooltip="{i18n>$target.$propertyObject.property}"/>
  #elseif ($range == "Edm.GeometryLineString")							<Text text="{dataModel>$propertyObject.property}" tooltip="{i18n>$target.$propertyObject.property}"/>
  #elseif ($range == "Edm.GeometryPolygon")							<Text text="{dataModel>$propertyObject.property}" tooltip="{i18n>$target.$propertyObject.property}"/>
  #elseif ($range == "Edm.GeometryMultiPoint")							<Text text="{dataModel>$propertyObject.property}" tooltip="{i18n>$target.$propertyObject.property}"/>
  #elseif ($range == "Edm.GeometryMultiLineString")							<Text text="{dataModel>$propertyObject.property}" tooltip="{i18n>$target.$propertyObject.property}"/>
  #elseif ($range == "Edm.GeometryMultiPolygon")							<Text text="{dataModel>$propertyObject.property}" tooltip="{i18n>$target.$propertyObject.property}"/>
  #elseif ($range == "Edm.GeometryCollection")							<Text text="{dataModel>$propertyObject.property}" tooltip="{i18n>$target.$propertyObject.property}"/>
  #else
  #end 
#end
#macro( entitySetfiltertypes $propertyObject  )
#set( $range =  $propertyObject.range)
#set( $formatOptions =  $propertyObject.formatOptions)
  #if ($range == "Edm.Binary")	defaultFilterOperator="EQ" filterType="sap.ui.model.type.String"
  #elseif ($range == "Edm.Boolean")	defaultFilterOperator="EQ" filterType="sap.ui.model.type.Boolean"
  #elseif ($range == "Edm.Byte") defaultFilterOperator="EQ" filterType="sap.ui.model.type.Integer"
  #elseif ($range == "Edm.SByte") defaultFilterOperator="EQ" filterType="sap.ui.model.type.Integer"
  #elseif ($range == "Edm.Date") defaultFilterOperator="EQ" filterType="sap.ui.model.type.String"
  #elseif ($range == "Edm.DateTimeOffset") defaultFilterOperator="EQ" filterType="sap.ui.model.type.String"
  #elseif ($range == "Edm.TimeOfDay") defaultFilterOperator="EQ" filterType="sap.ui.model.type.Time"
  #elseif ($range == "Edm.Duration") defaultFilterOperator="EQ" filterType="sap.ui.model.type.String"
  #elseif ($range == "Edm.Decimal") defaultFilterOperator="EQ" filterType="sap.ui.model.type.Float"
  #elseif ($range == "Edm.Single") defaultFilterOperator="EQ" filterType="sap.ui.model.type.Float"
  #elseif ($range == "Edm.Double") defaultFilterOperator="EQ" filterType="sap.ui.model.type.Float"
  #elseif ($range == "Edm.Guid") defaultFilterOperator="EQ" filterType="sap.ui.model.type.String"
  #elseif ($range == "Edm.Int16") defaultFilterOperator="EQ" filterType="sap.ui.model.type.Integer"
  #elseif ($range == "Edm.Int32") defaultFilterOperator="EQ" filterType="sap.ui.model.type.Integer"
  #elseif ($range == "Edm.Int64") defaultFilterOperator="EQ" filterType="sap.ui.model.type.Integer"
  #elseif ($range == "Edm.String") defaultFilterOperator="Contains" filterType="sap.ui.model.type.String"
  #elseif ($range == "Edm.Stream") defaultFilterOperator="EQ" filterType="sap.ui.model.type.String"
  #elseif ($range == "Edm.Geography") defaultFilterOperator="Contains" filterType="sap.ui.model.type.String"
  #elseif ($range == "Edm.GeographyPoint") defaultFilterOperator="Contains" filterType="sap.ui.model.type.String"
  #elseif ($range == "Edm.GeographyLineString") defaultFilterOperator="Contains" filterType="sap.ui.model.type.String"/>
  #elseif ($range == "Edm.GeographyPolygon") defaultFilterOperator="Contains" filterType="sap.ui.model.type.String"
  #elseif ($range == "Edm.GeographyMultiPoint") defaultFilterOperator="Contains" filterType="sap.ui.model.type.String"
  #elseif ($range == "Edm.GeographyMultiLineString") defaultFilterOperator="Contains" filterType="sap.ui.model.type.String"
  #elseif ($range == "Edm.GeographyMultiPolygon") defaultFilterOperator="Contains" filterType="sap.ui.model.type.String"
  #elseif ($range == "Edm.GeographyCollection")	defaultFilterOperator="Contains" filterType="sap.ui.model.type.String"
  #elseif ($range == "Edm.Geometry") defaultFilterOperator="Contains" filterType="sap.ui.model.type.String"
  #elseif ($range == "Edm.GeometryPoint") defaultFilterOperator="Contains" filterType="sap.ui.model.type.String"
  #elseif ($range == "Edm.GeometryLineString")	defaultFilterOperator="Contains" filterType="sap.ui.model.type.String"
  #elseif ($range == "Edm.GeometryPolygon")	defaultFilterOperator="Contains" filterType="sap.ui.model.type.String"
  #elseif ($range == "Edm.GeometryMultiPoint")	defaultFilterOperator="Contains" filterType="sap.ui.model.type.String"
  #elseif ($range == "Edm.GeometryMultiLineString")	defaultFilterOperator="Contains" filterType="sap.ui.model.type.String"
  #elseif ($range == "Edm.GeometryMultiPolygon")	defaultFilterOperator="Contains" filterType="sap.ui.model.type.String"
  #elseif ($range == "Edm.GeometryCollection")	defaultFilterOperator="Contains" filterType="sap.ui.model.type.String"
  #else
  #end 
#end
<mvc:View controllerName="com.inova8.lens.controller.EntitySetTable" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc"
	xmlns:layout="sap.ui.layout" xmlns:rdf="com.inova8.lens.control" xmlns:core="sap.ui.core" xmlns:table="sap.ui.table" xmlns:cd="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
	<Page showHeader="false" id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>$entitySet.target.$ViewTitle}">
		<content>
			<table:Table id="table"
					cd:parameters="{ 
					#set($numOfExpand = 0)	
					#set($selectprops = "")	
					#foreach($property in $uiTemplate.properties)
						#if($property.visible)
							#set( $complexTypeName =  $property.range)
							#if($property.propertyType == "D")
								#set($selectprops = $selectprops + "," + $property.property)
							#elseif($property.propertyType == "O") 
								#if($numOfExpand == 0) 
							    $expand:{#else 
							  			, #end $property.property:{$select:'subjectId,label'}	
								#set($numOfExpand = $numOfExpand + 1)
							#elseif($property.propertyType == "C") 
								#if($numOfExpand == 0) 
							    $expand:{#else 
							  			, #end $property.property:{ $count:true}	
								#set($numOfExpand = $numOfExpand + 1)	
							#elseif ($property.propertyType == "S")
								#set($selectprops = $selectprops + "," + $property.property)
							#end
							#foreach($subTypeName in $entity.subTypeNames)				
								#set( $complexType = $complexTypes.get($complexTypeName))
								#if(  $complexType.getPropertyTemplates($subTypeName)  )
									#set( $complexProperties = $complexType.getPropertyTemplates($subTypeName))
									#foreach( $complexProperty in   $complexProperties)
										#if($complexProperty.propertyType == "D")
										#elseif ($complexProperty.propertyType == "O")
											#if($numOfExpand == 0) 
							    $expand:{#else 
							  			, #end '$complexProperty.property':{$select:'subjectId,label'}
							  				#set($numOfExpand = $numOfExpand + 1)					
										#elseif ($complexProperty.propertyType == "C")
											#if($numOfExpand == 0) 
							    $expand:{#else 
							  			, #end '$complexProperty.property':{$select:'subjectId,label'}	
							  				#set($numOfExpand = $numOfExpand + 1)			
										#end	
									#end
								#else
								#end
							#end	
						#end				
					#end
					#if($numOfExpand > 0)										},#end
					
								$select:'subjectId,label$selectprops'}"			
				enableColumnFreeze="true" enableCellFilter="true" enableCustomFilter="false" showColumnVisibilityMenu="true" enableColumnReordering="true" enableBusyIndicator="true"
				selectionMode="Single" width="auto" visibleRowCountMode="Auto" >
				<table:toolbar>
					<Toolbar>
						<ToolbarSpacer/>
						<HBox id="sourceTitle" alignItems="Center">
							<rdf:TextResource id="selectedResource" path="" entitySet=""/>
						</HBox>
						<Title id="pathTitle" text="{entitySetView>/navigationText}"/>
						<HBox id="versionsTitle" alignItems="Center">
							<Title text="(" />
							<ComboBox id="versionSelection" items="{ path: 'entitySetView>/versions' }" selectionChange="onVersionChange">
								<core:Item key="{entitySetView>navigation}" text="{entitySetView>text}"/>
							</ComboBox>
							<Title text=")" />
						</HBox>
						<Title id="tableHeader" text="{entitySetView>/entitySetTableTitle}"/>
						<ToolbarSpacer/>
						<Button
							icon="sap-icon://sort"
							tooltip="{i18n>search.clearAllSorts}"
							press="clearAllSorts"/>
						<Button
							icon="sap-icon://clear-filter"
							tooltip="{i18n>search.clearAllFilters}"
							press="clearAllFilters"/>
						<SearchField id="searchField" tooltip="{i18n>search.searchTooltip}" search="onSearch" width="auto" placeholder="{i18n>$entitySet.target.$SearchThis}"></SearchField>
					</Toolbar>
				</table:toolbar>

				<table:columns>
					<table:Column  width="200px" autoResizable="true">
						<Label text="{i18n>$entitySet.target.$This}"/>
						<table:template>
							<rdf:TextResource path="dataModel>" entitySet="$entitySet.name"/>
						</table:template>
					</table:Column>
							#foreach($property in $uiTemplate.properties)					
								#if($property.visible)
								#if ($property.propertyType == "C")	
					<table:Column id="$property.property" minWidth="150" autoResizable="true" showFilterMenuEntry="false" showSortMenuEntry="false">
								#elseif ($property.propertyType == "S")	
					<table:Column id="$property.property" minWidth="150" autoResizable="true" showFilterMenuEntry="false" showSortMenuEntry="false">
								#else
					<table:Column id="$property.property"  minWidth="150" autoResizable="true" sortProperty="$property.property" filterProperty="$property.property" showFilterMenuEntry="true" showSortMenuEntry="true" #entitySetfiltertypes($property ) >								
								#end
						<Label text="{i18n>$entitySet.target.$property.property}"/>
						<table:template>
									#if($property.propertyType == "D")
							#entitySetmaptypes($property $entitySet.target  ) 	
									#elseif ($property.propertyType == "O")
							<rdf:TextResource path="dataModel>$property.property" entitySet="$property.range"/>
									#elseif ($property.propertyType == "C")
							<rdf:NavigationPropertyCollection path="dataModel>$property.property/$count" navigationProperty="$entitySet.name/$property.property"/>
									#elseif ($property.propertyType == "S")		
							<VBox>					
										#foreach($subTypeName in $entity.subTypeNames)
											#set( $complexTypeName =  $property.range)				
											#set( $complexType = $complexTypes.get($complexTypeName))
											#if(  $complexType.getPropertyTemplates($subTypeName)  )
												#set( $complexProperties = $complexType.getPropertyTemplates($subTypeName))
								<HBox  justifyContent="Center" alignItems="Stretch">
												#foreach( $complexProperty in   $complexProperties)
													#if($complexProperty.propertyType == "D")
									#entitySetmaptypes( $complexProperty $entitySet.target  ) 
													#elseif ($complexProperty.propertyType == "O")
							<rdf:TextResource path="dataModel>$complexProperty.property" entitySet="$complexProperty.range" tooltip="{i18n>$entitySet.target.$complexProperty.property}"/>		
													#elseif ($complexProperty.propertyType == "C")
							<rdf:ListResource path="dataModel>$complexProperty.property" entitySet="$complexProperty.range" tooltip="{i18n>$entitySet.target.$complexProperty.property}"/>
													#end	
													#if ( $foreach.hasNext ) 
							<Text text="  " renderWhitespace="true"/>
													#end
												#end
								</HBox>
											#else
											<Text text=""/>
											#end
										#end
							</VBox>							
									#end
						</table:template>
				 	</table:Column>									
								#end
							#end							
				</table:columns>
			</table:Table>
		</content>
	</Page>
</mvc:View>