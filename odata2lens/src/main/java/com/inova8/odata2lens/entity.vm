#macro( entitymaptypes  $propertyObject $target )
#set( $property =  $propertyObject.property)
#set( $range =  $propertyObject.range)
#set( $formatOptions =  $propertyObject.formatOptions)
  #if ($range == "Edm.Binary")  <Text text="{dataModel>$property}"/>
  #elseif ($range == "Edm.Boolean")  <Text text="{path:'dataModel>$property',type:'sap.ui.model.type.Boolean'}"/>  
  #elseif ($range == "Edm.Byte")  <Text text="{dataModel>$property}"/>
  #elseif ($range == "Edm.SByte")  <Text text="{dataModel>$property}"/>
  #elseif ($range == "Edm.Date")  <Text text="{dataModel>$property}"/>
  #elseif ($range == "Edm.DateTimeOffset")  <Text text="{dataModel>$property}"/>
  #elseif ($range == "Edm.TimeOfDay")  <Text text="{dataModel>$property}"/>
  #elseif ($range == "Edm.Duration")  <Text text="{dataModel>$property}"/>
  #elseif ($range == "Edm.Decimal")  <Text text="{dataModel>$property}"/>
  #elseif ($range == "Edm.Single")  <Text text="{dataModel>$property}"/>
  #elseif ($range == "Edm.Double")  <Text text="{path:'dataModel>$property',type:'sap.ui.model.type.Float'$!formatOptions }"/>
  #elseif ($range == "Edm.Guid")  <Text text="{path:'dataModel>$property',type:'sap.ui.model.type.String'}"/>
  #elseif ($range == "Edm.Int16")  <Text text="{path:'dataModel>$property',type:'sap.ui.model.type.Integer'$!formatOptions }"/>
  #elseif ($range == "Edm.Int32")  <Text text="{path:'dataModel>$property',type:'sap.ui.model.type.Integer'$!formatOptions }"/>
  #elseif ($range == "Edm.Int64")  <Text text="{path:'dataModel>$property',type:'sap.ui.model.type.Integer'$!formatOptions}"/>
  #elseif ($range == "Edm.String")  <rdf:TextLink text="{path:'dataModel>$property',type:'sap.ui.model.type.String' }" linkText="{i18n>$target.$property}"/>
  #elseif ($range == "Edm.Stream")  <Text text="{dataModel>$property}"/>
  #elseif ($range == "Edm.Geography")  <Text text="{dataModel>$property}"/>
  #elseif ($range == "Edm.GeographyPoint")  <Text text="{dataModel>$property}"/>
  #elseif ($range == "Edm.GeographyLineString")  <Text text="{dataModel>$property}"/>
  #elseif ($range == "Edm.GeographyPolygon")  <Text text="{dataModel>$property}"/>
  #elseif ($range == "Edm.GeographyMultiPoint")  <Text text="{dataModel>$property}"/>
  #elseif ($range == "Edm.GeographyMultiLineString")  <Text text="{dataModel>$property}"/>
  #elseif ($range == "Edm.GeographyMultiPolygon")  <Text text="{dataModel>$property}"/>
  #elseif ($range == "Edm.GeographyCollection")  <Text text="{dataModel>$property}"/>
  #elseif ($range == "Edm.Geometry")<Text text="{dataModel>$property}"/>
  #elseif ($range == "Edm.GeometryPoint")  <Text text="{dataModel>$property}"/>
  #elseif ($range == "Edm.GeometryLineString")  <Text text="{dataModel>$property}"/>
  #elseif ($range == "Edm.GeometryPolygon")  <Text text="{dataModel>$property}"/>
  #elseif ($range == "Edm.GeometryMultiPoint")  <Text text="{dataModel>$property}"/>
  #elseif ($range == "Edm.GeometryMultiLineString")  <Text text="{dataModel>$property}"/>
  #elseif ($range == "Edm.GeometryMultiPolygon")  <Text text="{dataModel>$property}"/>
  #elseif ($range == "Edm.GeometryCollection")  <Text text="{dataModel>$property}"/>
  #else
  #end 
#end
#set ($ViewTitle	= "ViewTitle") 
#set ($This	= "this") 
#set ($SearchTooltip	= "SearchTooltip") 
<mvc:View controllerName="com.inova8.lens.controller.Entity" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:rdf="com.inova8.lens.control">
	<Page showHeader="true" id="page" navButtonPress="onNavBack" showNavButton="false" title="{i18n>$entity.target#[[.Title]]#}"
		busy="{entityView>/busy}" busyIndicatorDelay="{entityView>/delay}">
		<content>
			<f:SimpleForm class="editableForm" columnsL="1" columnsM="1" editable="true" emptySpanL="4" emptySpanM="4" 
				labelSpanL="2" labelSpanM="2" layout="ResponsiveGridLayout" maxContainerCols="2" minWidth="1024" singleContainerFullSize="true">
				<f:content>
					<Label text="{i18n>$entity.target.$This}" required="false"/><rdf:TextResource path="dataModel>" entitySet="$entity.name"/>
					#foreach($property in $uiTemplate.properties)
						#if(!$property.hidden)
							#if($property.propertyType == "D")
					<Label text="{i18n>$entity.target.$property.property}" required="false"/> #entitymaptypes( $property $entity.target  ) 
							#elseif ($property.propertyType == "O")
					<Label text="{i18n>$entity.target.$property.property}" required="false"/><rdf:TextResource path="dataModel>$property.property" entitySet="$property.range"/>		
							#elseif ($property.propertyType == "C")
					<Label text="{i18n>$entity.target.$property.property}" required="false"/><rdf:ListResource path="dataModel>$property.property" entitySet="$property.range"/>
							#end
						#end
					#end		
				</f:content>
			</f:SimpleForm>
		</content>
	</Page>
</mvc:View>